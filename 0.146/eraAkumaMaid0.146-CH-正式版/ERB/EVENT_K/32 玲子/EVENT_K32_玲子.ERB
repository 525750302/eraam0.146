;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K32, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你/你_你
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 主人様_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
; 以下ライブラリ
CASE "愛液"
	LOCALS = お汁_おつゆ/おツユ_おつゆ
	SIF MARK:屈服刻印 > 1
		LOCALS += "/マン汁_まんじる"
