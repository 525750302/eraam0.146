	;DEBUGPRINTFORML エラー！　%ARGS%は未定義の単語です
ENDSELECT

	IF STRCOUNT(LOCALS, "/")
		SPLIT LOCALS, "/", LOCALS
		LOCALS = %LOCALS:(RAND:RESULT)%
	ENDIF

RETURNF LOCALS

