					LOCALS = %CALLNAME:PLAYER%君_%CALLNAME:PLAYER%君
				ELSE
					LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
				ENDIF
			ELSE
				LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
			ENDIF
		ENDSELECT
CASEELSE
;動いてるはず
	IF ISNUMERIC(ARGS)
		ACTOR = TOINT(ARGS)
	ELSEIF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)
	ENDIF
	IF ACTOR == MASTER
		IF TALENT:ACTOR:男性
			IF ABL:信頼 < 3
