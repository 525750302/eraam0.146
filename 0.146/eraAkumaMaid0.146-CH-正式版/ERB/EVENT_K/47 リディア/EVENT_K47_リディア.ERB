			ELSEIF TRUE_SEX(MASTER) != "男性"
				LOCALS = %CALLNAME:MASTER%_%CALLNAME:MASTER%
			ELSE
				LOCALS = 君_你
			ENDIF
		ENDIF
	ELSEIF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CALLNAME:ACTOR%老师
	ELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部"
			;仲良くなったら呼び捨てにしたいが保留
			IF TALENT:ACTOR:男性
				LOCALS = %CALLNAME:ACTOR%君_%CALLNAME:ACTOR%君
