;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K55, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	IF TALENT:相思相愛
		LOCALS = 我_我
	ELSE
		LOCALS = 僕_我
	ENDIF
CASE "二人称"
	;相手が主人
	;相手が主人ではない時の特殊二人称もここ(ナンバー指定あり)
	IF PLAYER == MASTER
		LOCALS = 部長_部長
	ELSEIF NO:PLAYER == 10 && TALENT:相思相愛
		LOCALS = 義母桑_おかあ桑
	ELSEIF NO:PLAYER == 10
		LOCALS = 麦莉爱娜桑_めりぃえーな桑
	
	;相手が主人ではない時の汎用口上
	;学生限定。
	;中等部以下…くんちゃん付け
	;高等部以上…先輩
	;教師…先生
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:PLAYER%君_%CALLNAME:PLAYER%君
			ELSE
				LOCALS = %CALLNAME:PLAYER%酱_%CALLNAME:PLAYER%酱
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%前輩_%CALLNAME:PLAYER%前輩
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%老师_%CALLNAME:PLAYER%老师
			ELSE
				LOCALS = %CALLNAME:PLAYER%桑_%CALLNAME:PLAYER%桑
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長_部長
CASE "助手"
	IF COND("麦莉爱娜", ASSI) && TALENT:相思相愛
		LOCALS = 義母桑_おかあ桑
	ELSEIF COND("麦莉爱娜", ASSI)
		LOCALS = 麦莉爱娜桑_めりぃえーな桑
	ELSEIF COND("森賀棗", ASSI)
		LOCALS = 棗桑_なつめ桑
	ELSEIF COND("小鳥遊静菜", ASSI)
		LOCALS = 静菜桑_しずな桑
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:男性
				LOCALS = %CALLNAME:ASSI%君_%CALLNAME:ASSI%君
			ELSE
				LOCALS = %CALLNAME:ASSI%酱_%CALLNAME:ASSI%酱
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CALLNAME:ASSI%前輩
		CASE "教師"
			LOCALS = %CALLNAME:ASSI%老师_%CALLNAME:ASSI%老师
		CASEELSE
	    	LOCALS = %CALLNAME:ASSI%桑_%CALLNAME:ASSI%桑
		ENDSELECT
   ENDIF
CASEELSE
	;ARGSにキャラ名が入れられてる場合にはキャラへの呼び方なのでチェック
	IF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)

		IF NO:ACTOR == 10 && TALENT:相思相愛
			LOCALS = 義母桑_おかあ桑
		ELSEIF NO:ACTOR == 51
			LOCALS = 棗桑_なつめ桑
		ELSEIF NO:ACTOR == 52
			LOCALS = 静菜桑_しずな桑
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部", "１年生", "２年生"
				IF TALENT:ACTOR:男性
					LOCALS = %CALLNAME:ACTOR%君_%CALLNAME:ACTOR%君
				ELSE
					LOCALS = %CALLNAME:ACTOR%酱_%CALLNAME:ACTOR%酱
				ENDIF
		CASE "３年生", "研究生"
				LOCALS = %CALLNAME:ACTOR%前輩_%CALLNAME:ACTOR%前輩
		CASEELSE
				IF COND("教員", ACTOR)
					LOCALS = %CALLNAME:ACTOR%老师_%CALLNAME:ACTOR%老师
				ELSE
					LOCALS = %CALLNAME:ACTOR%桑_%CALLNAME:ACTOR%桑
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
ENDSELECT
RETURNF LOCALS


