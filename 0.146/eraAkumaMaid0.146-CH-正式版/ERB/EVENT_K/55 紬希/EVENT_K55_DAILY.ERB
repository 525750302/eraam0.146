	PRINTFORML 「！？ もう、あんまりじろじろ見ないでよー……」
CASE "低腰臀圍"
	PRINTFORML 
CASE "掀起裙子"
	PRINTFORML 「……もう」
	PRINTFORML 「%CNAME("二人称")%だけだよ……こういうことしていいよって言うのは……」
CASE "检查内褲"
	IF COND("合意：检查内褲") && CHARA_NUM("室内の邪魔者") == 1
		PRINTFORML 「%CNAME("二人称")%、人前では他の人にこういうのはやっちゃダメだからね……？」
	ELSEIF TALENT:恋慕 && CHARA_NUM("室内の邪魔者") == 0
		CALL PRINT_STRL, @" 「%CNAME("二人称")%に胖次見られるのも慣れちゃった……_HH_」"
		CALL PRINT_STRL, @" 「……毎日胖次の自撮りおしつけちゃおっかなー……_HH_」"
