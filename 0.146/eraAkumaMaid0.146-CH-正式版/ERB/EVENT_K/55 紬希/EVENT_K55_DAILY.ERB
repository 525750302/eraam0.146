		PRINTFORML 「%CNAME("二人称")%、%CNAME("一人称")%の胖次ほんっと好きだよねえ」
		PRINTFORML 「えっ？『%CALLNAME:TARGET%のぱんつ是くら見ても飽きない』って？」
		CALL PRINT_STRL, @" 「……%CNAME("二人称")%の、ヘン領帯_HH_」"
	ENDIF
CASE "检查阴部"
	;人が居るとき分岐させる(つもり)
	IF COND("合意：检查阴部") && CHARA_NUM("室内の邪魔者") == 1
		PRINTFORML 「%CNAME("二人称")%～……えっちじゃないのにこういうことは、いけないことだと思うんだけどなー……」
	ELSEIF TALENT:恋慕 && CHARA_NUM("室内の邪魔者") == 0
		CALL PRINT_STRL, @" 「あは……_HH_ ここ、見るの好き～？」"
		CALL PRINT_STRL, @" 「いっつも見てくれてるから頑張って手入れしている甲斐があるよ～_HH_」"
