;コンドーム装着
;-------------------------------------------------
CASE "Ｖ避孕套装着"
	;挿入コマンド
	IF TRAIN_GENRE("挿入", SELECTCOM)
		;プレイヤーは着ける気満々
		IF TEQUIP:PLAYER:避孕套
			;初めてのゴム有りセックス
			IF EXP:避孕套性交経験 == 0
				PRINTFORMW 「啊啦、准备得很好呢。该重新审视你了」
			ELSE
				PRINTFORMW 「呵呵、谢谢您的関心♪」
			ENDIF
			IF TALENT:PLAYER:童貞
				CALL PRINT_STRW, "「ですが今日は特別な日ですから、よければどうぞ何もつけずに……_H_」"
				IF COND("危険日")
					PRINTFORMW 「因为是危险日所以要求做太多次会很困扰的、稍微多一点的话……没関系吧？」
				ELSE
					PRINTFORMW 「因为是第一次……没有用避孕器具，想确认一下是什么样的东西吧？」
				ENDIF
				CALL PRINT_SELECT, "即使是那样也要着装（蒸着）/就听话生挿入！"
				IF RESULT == 0
					PRINTFORMW 「……比想象中更能接受诱惑呢」
					PRINTFORMW 「%CNAME("二人称")%の事、也许是小看你了……」
				ELSE
					PRINTFORMW 「……呼呼」
					PRINTFORMW 「%CNAME("二人称")%の初めて……どうぞ%CNAME("一人称")%で経験しちゃってくださいね♪」
					CALL SETFLAG, "避孕套中止", PLAYER
					SIF COND("危険日")
						TCVAR:ワンモア生挿入中 = 1
				ENDIF
			ELSEIF COND("安全日")
				CALL PRINT_STRW, "「ですが今日は大丈夫な日ですから、よければどうぞ何もつけずに……_H_」"
				CALL PRINT_SELECT, "即使是那样也要着装（蒸着）/就听话生挿入！"
				IF RESULT == 0
					PRINTFORMW 「啊啦啦、被拒绝了。%CNAME("一人称")%の話術、还差得远呢♪」
				ELSE
					PRINTFORMW 「……呵呵~♪现在在这里说的话是、我喜欢生插入、%CNAME("一人称")%」
					CALL SETFLAG, "避孕套中止", PLAYER
				ENDIF
			ELSE
				PRINTFORMW %CALLNAME:TARGET%看着%CALLNAME:MASTER%带上避孕套而微笑着。
			ENDIF
		;避妊処置をしなくて良い事がキャラに分かっている状態
		ELSEIF COND("避妊不要") || TCVAR:ご褒美生Ｈ
			SIF CHOSECOMS("Ｖ挿入") == 0
				PRINTFORMW 「呵呵。挺期待的呢♪」
		;ゴムが無い
		ELSEIF ITEM:避孕套 == 0
			;ゴム付けてやるはずだったけどゴムが切れてしまった！
			IF TCVAR:Ｖ避孕套挿入許可
				PRINTFORMW 「啊啦？难道说避孕套不见了吗？」
			ELSE
				PRINTFORMW 「啊啦……？%CNAME("二人称")%、难道是没有准备避孕套吗？」
			ENDIF
			IF TALENT:PLAYER:童貞
				PRINTFORMW 「真是个废柴啊。最低限的礼节都没有吗？」
				PRINTFORMW 「我是想这么说……呼呼」
				CALL PRINT_STRW, "「今日は特別な日ですからねぇ。……いいですよ、このまま何も着けなくても……_H_」"
				IF COND("危険日")
					PRINTFORMW 「因为是危险日所以要求做太多次会很困扰的、但稍微可以多做一点的话……对吧？」
				ELSE
					PRINTFORMW 「因为是第一次……没有用避孕器具，想确认一下是什么样的东西吧？」
				ENDIF
				SIF COND("危険日")
					TCVAR:ワンモア生挿入中 = 1
			ELSEIF COND("安全日") || COND("女性主導生性交") || COND("合意：危険日生性交")
