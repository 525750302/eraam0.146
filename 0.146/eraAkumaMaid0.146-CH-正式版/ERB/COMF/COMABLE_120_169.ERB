@COM_ABLE120
;正常位奉仕
SIF COMABLE_DEF(120)
	RETURN 0
;調教者がオトコだとダメ
SIF TALENT:PLAYER:男性
	RETURN 0
;ペニスあり
IF PENIS(TARGET)
	;ペニスを使う時には股間が露出できないとダメ
	SIF !CHECK_CLO("Ｖ露出可")
		RETURN 0
;ペニスなし
ELSE
	;ペニスバンドがないとダメ
	SIF COND("假阳具") == 0
		RETURN 0
ENDIF
IF TEQUIP:推倒
	;サドっ気が必要
	SIF ABL:施虐属性 == 0
		RETURN 0
	SIF TALENT:PLAYER:処女 && ABL:施虐属性 < 3
		RETURN 0
ENDIF
CALL COM_ORDER_COM120, 1
RETURN LIMIT(RESULT, 0, 1)

