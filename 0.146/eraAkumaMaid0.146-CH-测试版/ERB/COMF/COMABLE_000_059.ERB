@COM_ABLE15
;尿道指挿入れ実行判定
SIF COMABLE_DEF(15)
	RETURN 0
SIF COND("指の使用", PLAYER) == 0
	RETURN 0
;アソコ剥き出しにできないとダメ
SIF !CHECK_CLO("Ｖ露出可")
	RETURN 0
;カテーテル使用中はダメ
SIF TEQUIP:カテーテル
	RETURN 0
;指を余裕で受け入れることが出来ないとダメ(指の直径+1)mmの拡張度が必要
SIF CALCF("尿道径") <= SIZE("指の直径", PLAYER)
	RETURN 0
RETURN 1

