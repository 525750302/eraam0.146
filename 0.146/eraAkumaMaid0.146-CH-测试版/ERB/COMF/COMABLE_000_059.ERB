;-------------------------------------------------
;0〜29番,愛撫系やソフトコマンド
;-------------------------------------------------
@COM_ABLE0
;愛撫実行判定
SIF COMABLE_DEF(0)
	RETURN 0
SIF COND("指の使用", PLAYER) == 0
	RETURN 0
RETURN 1

@COM_ABLE1
;クンニ実行判定
SIF COMABLE_DEF(1)
	RETURN 0
SIF TALENT:オトコ
	RETURN 0
IF TEQUIP:押し倒し
	SIF TEQUIP:バイブ || TEQUIP:アナルバイブ
		RETURN 0
ELSE
	SIF CHECK_OUTER("ズボン") && !CHECK_CLO("Ｖ露出可")
		RETURN 0
ENDIF
RETURN 1

@COM_ABLE2
;アナル愛撫実行判定
SIF COMABLE_DEF(2)
	RETURN 0
SIF COND("指の使用", PLAYER) == 0
	RETURN 0
;アナル剥き出しにできないとダメ
SIF !CHECK_CLO("Ａ露出可")
	RETURN 0
RETURN 1

@COM_ABLE3
;自慰実行判定
SIF COMABLE_DEF(3)
	RETURN 0
;縄・拘束中で手が使えない時はダメ
SIF GETBIT(TEQUIP:拘束, 20) || STATE("バインド：手")
	RETURN 0
CALL COM_ORDER_COM3, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE4
;フェラする実行判定
SIF COMABLE_DEF(4)
	RETURN 0
;アソコ剥き出しにできないとダメ
SIF !CHECK_CLO("Ｖ露出可")
	RETURN 0
;オトコかふたなりじゃないとダメ
SIF PENIS(TARGET) == 0
	RETURN 0
RETURN 1

@COM_ABLE5
;胸愛撫実行判定
SIF COMABLE_DEF(5)
	RETURN 0
RETURN 1

@COM_ABLE6
;キスする実行判定
SIF COMABLE_DEF(6)
	RETURN 0
;押し倒されているとダメ…キス経験が豊富（キス好き）or恋慕なら良し！
SIF TEQUIP:押し倒し && TALENT:恋慕 == 0 && EXP:キス経験 < 20
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷
	RETURN 0
CALL COM_ORDER_COM6, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE7
;アソコ愛撫実行判定
SIF COMABLE_DEF(7)
	RETURN 0
IF TALENT:オトコ
	RETURN 0
ELSEIF TALENT:ふたなり
	IF CHECK_COM("ふきふき", 7)
	ELSEIF TEQUIP:バイブ || INSERT("Ｖインサート") || INSERT("挿入中")
		RETURN 0
	ENDIF
ENDIF
RETURN 1

@COM_ABLE8
;指挿入れ実行判定
SIF COMABLE_DEF(8)
	RETURN 0
SIF COND("指の使用", PLAYER) == 0
	RETURN 0
;アソコ使用判定（例外的にバイブはＯＫ）
SIF USE_V(TARGET, "バイブ") == 0
	RETURN 0
;処女だとだめ
SIF TALENT:処女
	RETURN 0
RETURN 1

@COM_ABLE9
;アナル舐め実行判定
SIF COMABLE_DEF(9)
	RETURN 0
;アナル使用判定
SIF USE_A(TARGET) == 0
	RETURN 0
RETURN 1

