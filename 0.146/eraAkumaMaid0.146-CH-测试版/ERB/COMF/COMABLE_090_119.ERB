@COM_ABLE106
;逆バイブ実行判定
SIF COMABLE_DEF(106)
	RETURN 0
;アイテムを持っていないとダメ
SIF ITEM:バイブセット == 0
	RETURN 0
;オトコだとダメ
SIF TALENT:PLAYER:オトコ
	RETURN 0
;手が使えないとダメ
SIF COND("指の使用") == 0
	RETURN 0
;アイマスク使用中は不可
SIF TEQUIP:アイマスク
	RETURN 0
;処女の時はマゾっ気が必要
IF TALENT:PLAYER:処女
	SIF ABL:PLAYER:マゾっ気 < 3
		RETURN 0
ENDIF

CALL COM_ORDER_COM106, 1
RETURN LIMIT(RESULT, 0, 1)


