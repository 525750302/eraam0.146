SIF TEQUIP:口枷
	RETURN 0

CALL COM_ORDER_COM96, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE97
;ぱんつコキ実行判定
SIF COMABLE_DEF(97)
	RETURN 0
;ぱんつコキできるパンツじゃないとダメ
SIF CHECK_CLO("ぱんつコキ") == 0
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0

;ラミア等、足が無い人はダメ
SIF COND("脚が無い")
	RETURN 0

CALL COM_ORDER_COM97, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE98
;奉仕クンニ実行判定
SIF COMABLE_DEF(98)
	RETURN 0
;調教者がオトコだとダメ
SIF TALENT:PLAYER:オトコ
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷
	RETURN 0

CALL COM_ORDER_COM98, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE99
;シックスナイン実行判定
SIF COMABLE_DEF(99)
	RETURN 0
;解除はいつでも可能
SIF TEQUIP:シックスナイン
	RETURN 1
CALL COM_ABLE91
IF RESULT == 0
	CALL COM_ABLE98
	SIF RESULT == 0
		RETURN 0
ENDIF
CALL COM_ABLE1
IF RESULT == 0
	CALL COM_ABLE4
	SIF RESULT == 0
		RETURN 0
ENDIF

CALL COM_ORDER_COM99, 1
RETURN LIMIT(RESULT, 0, 1)

@COM_ABLE100
;脚を舐める実行判定
SIF COMABLE_DEF(100)
	RETURN 0
;調教者のサドっ気が必要
SIF ABL:PLAYER:サドっ気 == 0
	RETURN 0
;調教者のサドっ気か相手のマゾっ気が3LV必要
SIF MAX(ABL:マゾっ気, ABL:PLAYER:サドっ気) < 3
	RETURN 0
;調教者がラミア等、足が無い人や脚が使えない状況だとダメ
SIF COND("脚の使用", PLAYER) == 0
	RETURN 0
SIF COND("口の使用") == 0
	RETURN 0

CALL COM_ORDER_COM100, 1
RETURN LIMIT(RESULT, 0, 1)


