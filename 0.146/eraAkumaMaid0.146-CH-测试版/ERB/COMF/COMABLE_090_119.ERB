;-------------------------------------------------
;90〜119番,奉仕系コマンド
;-------------------------------------------------
@COM_ABLE90
;手淫実行判定
SIF COMABLE_DEF(90)
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0

CALL COM_ORDER_COM90, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE91
;フェラチオ実行判定
SIF COMABLE_DEF(91)
	RETURN 0
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷 == 1
	RETURN 0

CALL COM_ORDER_COM91, 1
RETURN LIMIT(RESULT, 0, 1)

@COM_ABLE92
;パイズリ実行判定
SIF COMABLE_DEF(92)
	RETURN 0
;胸を剥き出しにできるor胸露出でないとダメ
SIF CHECK_CLO("谷間露出") + CHECK_CLO("はだけ可") == 0
	RETURN 0
;オトコだとダメ
;SIF TALENT:オトコ
;	RETURN 0
;[巨乳]でなければ技巧2以上、[貧乳]だと技巧3以上必要
IF TALENT:貧乳 || TALENT:オトコ
	SIF ABL:技巧 < 3
		RETURN 0
ELSEIF TALENT:巨乳 == 0
	SIF ABL:技巧 < 2
		RETURN 0
ENDIF
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0

CALL COM_ORDER_COM92, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE93
;素股実行判定
SIF COMABLE_DEF(93)
	RETURN 0
;見せ槍の時以外は、アソコ剥き出しにできないとダメ
IF CHECK_COM("見せ槍", 93)
ELSEIF CHECK_CLO("Ｖ露出可") == 0
	RETURN 0
ENDIF
;調教者がオトコかふたなりじゃないとダメ
SIF PENIS(PLAYER) == 0
	RETURN 0
;オトコだとダメ
;SIF TALENT:オトコ
;	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:バイブ
	RETURN 0

CALL COM_ORDER_COM93, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE94
;泡踊り実行判定
SIF COMABLE_DEF(94)
	RETURN 0
;ハダカでないとダメ
SIF !CHECK_CLO("ハダカ")
	RETURN 0
;お風呂場プレイ中でないとダメ
SIF TEQUIP:お風呂プレイ == 0
	RETURN 0
;Ｗ押し倒し中ならオンナでないとダメ
SIF TEQUIP:Ｗ押し倒し && TALENT:オトコ
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:バイブ
	RETURN 0
;アナルバイブ挿入中はダメ
SIF TEQUIP:アナルバイブ
	RETURN 0
;ボールギャグ使用中は不可
SIF TEQUIP:口枷
	RETURN 0
;アイマスク使用中は不可
SIF TEQUIP:アイマスク
	RETURN 0
;浣腸使用中は不可
SIF TEQUIP:浣腸
	RETURN 0

CALL COM_ORDER_COM94, 1
RETURN LIMIT(RESULT, 0, 1)


@COM_ABLE95
;足コキ実行判定
SIF COMABLE_DEF(95)
	RETURN 0
;ラミア等、足が無い人や脚が使えない状況だとダメ
SIF COND("脚の使用") == 0
	RETURN 0
;アイマスク使用中は不可
SIF TEQUIP:アイマスク
	RETURN 0
IF TEQUIP:押し倒し
	;サドっ気が必要
	SIF ABL:サドっ気 == 0
		RETURN 0
ENDIF

CALL COM_ORDER_COM95, 1
RETURN LIMIT(RESULT, 0, 1)


