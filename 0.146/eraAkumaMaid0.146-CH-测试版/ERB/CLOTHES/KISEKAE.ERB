FOR LCOUNT, 0, 10
	STAIN:ARG:(LCOUNT + 10) = MEMO_STAIN:LCOUNT
	EQUIP:ARG:(LCOUNT + 40) = MEMO_EQUIP:LCOUNT
	CSTR:ARG:(LCOUNT + 40) = %MEMO_CSTR:LCOUNT%
NEXT


@SET_MEMO_DRESS, ARGS, ARG
#DIM LCOUNT
SIF ARG <= 0
	ARG = TARGET

SELECTCASE ARGS
CASE "着替え希望衣装"
	CFLAG:ARG:コスプレ中 = 1
