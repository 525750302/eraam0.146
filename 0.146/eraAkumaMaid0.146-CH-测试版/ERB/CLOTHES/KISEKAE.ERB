	SELECTCASE NAME_SHIRT(LCOUNT)
	CASE ""
		CONTINUE
	CASEELSE
		SIF SHIRT(TARGET) == NAME_SHIRT(LCOUNT)
			SETCOLOR DEF_COLOR("黄色")
		PRINTFORM  [{LCOUNT, 2}] %TEXT_LJ(NAME_SHIRT(LCOUNT), 22)%
		RESETCOLOR

		NEXT_LINE += 1
		SIF NEXT_LINE%3 == 0
			PRINTL 
	ENDSELECT
NEXT
