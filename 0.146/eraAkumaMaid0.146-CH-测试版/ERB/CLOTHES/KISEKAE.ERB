	CSTR:ARG:ARGS = %CSTR:ARG:ARGS%{EQUIP:ARG:LCOUNT},%CSTR:ARG:LCOUNT%,
NEXT

@PRINT_MEMO_DRESS, ARGS, ARG
#DIM LCOUNT
#DIM MEMO_STAIN, 10
#DIM MEMO_EQUIP, 10
#DIMS MEMO_CSTR, 10

SIF ARG <= 0
	ARG = TARGET

FOR LCOUNT, 0, 10
	MEMO_STAIN:LCOUNT = STAIN:ARG:(LCOUNT + 10)
	MEMO_EQUIP:LCOUNT = EQUIP:ARG:(LCOUNT + 40)
	MEMO_CSTR:LCOUNT = %CSTR:ARG:(LCOUNT + 40)%
NEXT

