;-------------------------------------------------
;メガネなどその他小物に関する関数
;-------------------------------------------------
@SELECT_OTHER
#DIM LCOUNT
#DIM MEMO_LINECOUNT
#DIM CHOICE
#DIM NEXT_LINE

MEMO_LINECOUNT = LINECOUNT

PRINTL 
CALL PRINT_MAIDFACE, TARGET
CALL PRINT_STRL, @"身に着けている物は_黄色_色が変わり_、選択する事で外せます。\@ CONFIG("顔グラＯＮ") ? ものによっては顔グラに変化が起きます。 # \@"
PRINTFORM 現在:

VARSET LOCALS
IF EQUIP:アクセサリ
	SPLIT CSTR:アクセサリ, "/", LOCALS
	FOR LCOUNT, 0, 10
		SIF LOCALS:LCOUNT != ""
			PRINTFORM [%LOCALS:LCOUNT%]
	NEXT
	PRINTL 
ELSE
	PRINTFORML 無し
ENDIF
PRINTL 

NEXT_LINE = 0
FOR LCOUNT, 0, 90
	SELECTCASE NAME_OTHER(LCOUNT)
	CASE ""
		CONTINUE
	CASEELSE
		SELECTCASE LCOUNT
		CASE 20, 30, 50, 60, 70
			SIF NEXT_LINE%3
				PRINTL 
			NEXT_LINE = 0
		ENDSELECT
		SELECTCASE LCOUNT
		CASE 0
			PRINTFORML ・首周りのアクセサリ
		CASE 20
			PRINTFORML ・眼鏡
		CASE 30
			PRINTFORML ・帽子
		CASE 50
			PRINTFORML ・つけ耳
		CASE 60
			PRINTFORML ・髪飾り
		CASE 70
			PRINTFORML ・外套、その他
		ENDSELECT

		SIF STRCOUNT(@"\/%CSTR:アクセサリ%", @"\/%NAME_OTHER(LCOUNT)%\/")
			SETCOLOR DEF_COLOR("黄色")
		PRINTFORM  [{LCOUNT, 2}] %TEXT_LJ(NAME_OTHER(LCOUNT), 20)%
		RESETCOLOR

		NEXT_LINE += 1
		SIF NEXT_LINE%3 == 0
			PRINTL 
	ENDSELECT
NEXT
PRINTL 
IF EQUIP:アクセサリ
	PRINTFORML  [99] すべて外してもらう
ELSE
	CALL PRINT_STRL, "暗灰色_ [×] すべて外してもらう"
ENDIF
PRINTFORML [100] 戻る

$INPUT_LOOP
INPUT

CHOICE = RESULT

IF CHOICE == 100
	RETURN 0
ELSEIF CHOICE == 99 && EQUIP:アクセサリ
	CALL SET_OTHER, ""
ELSEIF NAME_OTHER(CHOICE) != ""
	;すでに身に着けているものと完全一致したものを選んだら外す
	CALL INVERTSET_OTHER, NAME_OTHER(CHOICE)
ELSE
	GOTO INPUT_LOOP
ENDIF

