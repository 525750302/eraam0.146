	RETURN 0
ELSEIF CHOICE == 90 && EQUIP:服 + EQUIP:スカート
	CALL EVENT_DATUI, "衣服"
ELSEIF CHOICE == 91 && EQUIP:スカート && !CHECK_SKIRT("ワンピース")
