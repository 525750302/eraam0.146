			ASSI = 0
			ASSI:1 = 0
			REDRAW 1
			RETURN 0
		ELSEIF CHOICE < MASTER || CHOICE >= CHARANUM
			GOTO INPUT_LOOP
		ELSEIF CHOICE == ASSI

		ELSEIF COND("助手可能", CHOICE) == 0
			GOTO INPUT_LOOP
		ELSEIF CONFIG("助手への指示") && ABL:CHOICE:助手 == 0
