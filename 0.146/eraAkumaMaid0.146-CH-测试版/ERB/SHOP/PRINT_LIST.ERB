		CHOICE = TOINT(CHOICES)
		IF CHOICE == 100
			REDRAW 1
			RETURN 0
		ELSEIF CHOICE == 101
			CLEARLINE LINECOUNT - MEMO_LINECOUNT
			CALL SHOW_CHARADATA
			RETURN 0
		ELSEIF CHOICE == 102 && CHARA_NUM("控えキャラ")
			CLEARLINE LINECOUNT - MEMO_LINECOUNT
