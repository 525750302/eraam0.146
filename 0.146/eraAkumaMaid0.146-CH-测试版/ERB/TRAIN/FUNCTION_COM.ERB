@ENERGY_DRAIN
#DIM LCOUNT
#DIM MEMO_TARGET, 2
#DIM VALUE_ED_PRE
#DIM VALUE_ED

;ちんこが無いor既にエナジードレインが発生した場合はここで終わり
SIF PENIS(PLAYER) == 0 || CHARA_NUM("エナジードレイン")
	RETURN 0


VALUE_ED_PRE = 0
VARSET MEMO_TARGET
MEMO_TARGET:1 = TARGET:1
;PLAYERがTARGET:1だった場合の処理
SIF PLAYER == TARGET:1
	TARGET:1 = 0

;エナジードレインが発生するかチェック
FOR LCOUNT, 2, CHARANUM
	SELECTCASE LCOUNT
	CASE PLAYER
		CONTINUE
	CASE TARGET

	CASE TARGET:1
		SIF SELECTCOM:1 < 0 || STATE("撃沈", TARGET:1)
			CONTINUE
	CASEELSE
		CONTINUE
	ENDSELECT

	SIF TALENT:LCOUNT:吸精体質 == 0
		CONTINUE
	SIF NOWEX:LCOUNT:Ｖ中出し + NOWEX:LCOUNT:Ａ中出し + NOWEX:LCOUNT:口内射精 + NOWEX:LCOUNT:乳内射精 + NOWEX:LCOUNT:尿道内射精 == 0
		CONTINUE
	SIF NOWEX:LCOUNT:暴発 == 0 && COND("絶頂", PLAYER) < RAND:4
		CONTINUE
	SIF NOWEX:LCOUNT:暴発 == 0 && RAND:(3 + TALENT:LCOUNT:吸精体質 + TALENT:LCOUNT:恋慕 + TALENT:LCOUNT:淫乱) < 3
		CONTINUE

	VALUE_ED_PRE = TALENT:LCOUNT:吸精体質

	SELECTCASE SELECTCOM
	CASE 65, 75, 88
		VALUE_ED_PRE += 1
	CASE 60 TO 89, 91, 140, 150
	CASEELSE
		SIF INSERT("インサート", LCOUNT, PLAYER) == 0 && TEQUIP:LCOUNT:シックスナイン != 91
			VALUE_ED_PRE = 0
	ENDSELECT

	NOWEX:LCOUNT:エナジードレイン = MAX(RAND:(VALUE_ED_PRE + 1), NOWEX:LCOUNT:暴発)

	SIF NOWEX:LCOUNT:エナジードレイン == 0
		CONTINUE

	EX:LCOUNT:エナジードレイン += NOWEX:LCOUNT:エナジードレイン
NEXT


VARSET LOCALS
SELECTCASE CHARA_NUM("エナジードレイン")
;エナジードレインが起きないならここまで
CASE 0
	;PLAYERがTARGET:1だった場合の後処理
	TARGET:1 = MEMO_TARGET:1
	RETURN 0
CASE 1
	IF NOWEX:エナジードレイン
		LOCALS = %CALLNAME:TARGET%
	ELSEIF NOWEX:(TARGET:1):エナジードレイン
		LOCALS = %CALLNAME:(TARGET:1)%
	ENDIF
CASE 2
	LOCALS = 二人
ENDSELECT

PRINTL 
CALL PRINT_STRL, "ハートピンク_＜エナジードレイン！＞"

;表記の簡略
VALUE_ED = NOWEX:エナジードレイン + NOWEX:(TARGET:1):エナジードレイン

PRINTFORML %CALLNAME:PLAYER%のペニスから凄まじい快感とともに精力が%LOCALS%に吸い取られる！
TCVAR:PLAYER:絶頂度 = MIN(TCVAR:PLAYER:絶頂度 + VALUE_ED, 5)

BASE:PLAYER:精力 = MAX(BASE:PLAYER:精力 - VALUE_ED*3, 0)
TCVAR:PLAYER:精力減少 += VALUE_ED*3
PRINTFORM 精力-{VALUE_ED*3}
IF PLAYER == MASTER
	PRINTFORM  (%CALLNAME:PLAYER%)
ELSEIF PLAYER == ASSI
	PRINTFORM （%TEXTS("助手の名称")%）
ENDIF
PRINTL 

FOR LCOUNT, 2, CHARANUM
	SIF NOWEX:LCOUNT:エナジードレイン == 0
		CONTINUE

	EXP:LCOUNT:吸精経験 += 1
	PRINTFORML 吸精経験＋1\@ LCOUNT != TARGET ? （%CALLNAME:LCOUNT%） # \@
	PRINTFORML %CALLNAME:LCOUNT%の体力が{NOWEX:LCOUNT:エナジードレイン*100}回復した！
	DOWNBASE:LCOUNT:体力 -= NOWEX:LCOUNT:エナジードレイン*100

	BASE:LCOUNT:精力 += NOWEX:LCOUNT:エナジードレイン*3
	TCVAR:LCOUNT:精力減少 -= NOWEX:LCOUNT:エナジードレイン*3
	PRINTFORML さらに%CALLNAME:LCOUNT%の精力が{NOWEX:LCOUNT:エナジードレイン*3}回復した！
	;魂のやり取り
	SIF NOWEX:LCOUNT:エナジードレイン
		CALL EXCHANGE_SOUL, "絶頂", NOWEX:LCOUNT:エナジードレイン*3, LCOUNT
NEXT

;PLAYERがTARGET:1だった場合の後処理
TARGET:1 = MEMO_TARGET:1
