@EXP_LV(ARG)
#FUNCTION
SELECTCASE ARG
CASE IS <= 0
	RETURNF 0
CASE 1
	RETURNF 1
CASE 2
	RETURNF 5
CASE 3
	RETURNF 20
CASE 4
	RETURNF 50
