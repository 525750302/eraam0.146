CASE 11
	RETURNF 600
CASE 12
	RETURNF 700
CASE 13
	RETURNF 800
CASE 14
	RETURNF 900
CASE 15
	RETURNF 1000
CASEELSE
	RETURNF 1000 + (ARG - 15)*200
ENDSELECT

