;ここのARGにはキャラ番号が入っている
CASE "ワイルドスウィング"
	SIF ARG == 0
		ARG = TARGET
	SIF COND("疲労", ARG) || TEQUIP:ARG:押し倒し == 0 || INSERT("騎乗位", ARG) == 0 || COND("脚の使用", ARG) == 0
		RETURNF 0
	;連続ではあまりしてこない
	SIF INSERT("ワイルドスウィング", ARG) && RAND:4
		RETURNF 0
	SIF TEQUIP:ARG:Ｗ押し倒し || SELECTCOM:1 >= 0 || COND("前Ｔ調教者") != PLAYER || PENIS(PLAYER) == 0
		RETURNF 0
	;基本値は欲望
	LOCAL = ABL:ARG:欲望
	IF INSERT("Ｖインサート", ARG) && SELECTCOM >= 60 && SELECTCOM <= 69
		SIF EXP:ARG:Ｖ経験 + EXP:ARG:Ｖ性交経験*2 + EXP:ARG:腰使い経験 <= 50
			RETURNF 0
		IF TALENT:ARG:淫壷
			LOCAL += 5
		ELSE
			;恥じらい
			SIF TALENT:ARG:恥じらい
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSEIF INSERT("Ａインサート", ARG) && SELECTCOM >= 70 && SELECTCOM <= 79
		SIF EXP:ARG:Ａ経験 + EXP:ARG:Ａ性交経験*2 <= 50 || EXP:ARG:腰使い経験 <= 20
			RETURNF 0
		SIF ABL:ARG:Ａ感覚 < 3
			LOCAL -= 5
		SELECTCASE PALAM:ARG:Ａ潤 + PALAM:PLAYER:Ｐ潤
		CASE IS < 3000
			LOCAL -= 5
		CASE IS < 10000
			LOCAL -= 2
		ENDSELECT
		IF TALENT:ARG:尻穴狂い
			LOCAL += 5
		ELSE
			;恥じらい
			SIF TALENT:ARG:恥じらい
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSEIF INSERT("尿道インサート", ARG) && SELECTCOM >= 83 && SELECTCOM <= 88
		SIF EXP:ARG:尿道経験 + EXP:ARG:尿道性交経験*2 <= 50 || EXP:ARG:腰使い経験 <= 20 || PENIS(ARG)
			RETURNF 0
		SIF TALENT:ARG:尿道性感 == 0
			LOCAL -= 5

		IF TALENT:ARG:尿道狂い
			LOCAL += 5
		ELSE
			;恥じらい
			SIF TALENT:ARG:恥じらい
				LOCAL -= 5
			;清楚
			SIF TALENT:ARG:清楚
				LOCAL -= 3
		ENDIF
	ELSE
		RETURNF 0
	ENDIF
	;強気
	IF TALENT:ARG:強気
		LOCAL += 2
		;PLAYERがあまりイってない
		SIF EXP_GAIN("イかせた回数", ARG) < 5
			LOCAL += 1
	ENDIF
	;好色
	SIF TALENT:ARG:好色
		LOCAL += 1
	;淫乱
	SIF TALENT:ARG:淫乱
		LOCAL += 2
	;腰使い
	SIF ABL:ARG:腰使い || EXP:ARG:腰使い経験 >= 100
