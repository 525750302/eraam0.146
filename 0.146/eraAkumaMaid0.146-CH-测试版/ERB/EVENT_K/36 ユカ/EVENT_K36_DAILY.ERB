;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K36, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;見ないと思ったらデートの約束がない状態で日曜限定なのか。そりゃ出ないわ。
CASE "朝拘束"
	IF ABL:MASTER:受虐属性 >= 3 && ABL:施虐属性
		CALL EMOTION_PLUS, "躁动难忍", TARGET
		PRINTFORMW 「いい機会だから、今日は徹底的に搾り尽くしてやるよ」
		PRINTFORMW 「すっからかんになるまで許してやらないから、覚悟しとけよな……♪」
	ELSE
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「おっはよー！　状況は理解できたか？」
		PRINTFORMW 「じゃあ目が覚めたところで、ここからが本番な？」
	ENDIF
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください


CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"
	CASE "朝乳交"

	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝袭")
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW 「%TEXTR("あはっ……♪　起きたっ……♪/んっ……♪　もう出そう……？/んふふー、出していいよ……♪")%」
		ENDIF
	CASE "朝Ａ騎乗位"

	CASE "朝接吻"
		PRINTFORMW 「%TEXTR("＿/ちゅ")%……♪」
	CASE "朝添い寝"
	IF RAND:2 && CEVENT("初回朝袭")
		CALL SETFLAG, "地の文スキップ"
		IF RAND:3 && CFLAG:MASTER:约会的约定 == 0 && TEXTS("曜日") == "日" && COND("同居中")
			PRINTFORMW 朝、ふと目が覚めると、%CALLNAME:TARGET%が被窝の中にもぐり込んですやすやと寝息を立てていた。
			PRINTFORMW どうやら%CALLNAME:MASTER%を起こし来了ようだが、休日のため気が緩んでいたのか、釣られて眠ってしまったようだ。
			CALL EMOTION_PLUS, "睡眠", TARGET
			PRINTFORMW 「すぅすぅ……」
			PRINTFORMW 平和そうな%CALLNAME:TARGET%の寝顔をぼんやりと眺めているうちに、こちらも二度寝したい気分になってきた……。
		ELSEIF RAND:2 && COND("同居中")
			PRINTFORMW 朝、何やら%TEXTR("香ばしい/食欲を刺激する/おいしそうな")%气味がして睁開眼睛と、%CALLNAME:TARGET%が開け被释放进窓枠に腰を降ろしてトーストと%TEXTR("目玉焼き/スクランブルエッグ/ベーコン/ウィンナー")%を%TEXTR("頬張って/食べて")%いた。
			CALL EMOTION_PLUS, "夸奖", TARGET
			PRINTFORMW 「もぐもぐ……。%CNAME("二人称")%の分も酱と持ってきたぞ。一緒に食べよー！」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "夸奖", TARGET
			PRINTFORMW 「%TEXTR("起きろー！/朝ですよー！")%」
			PRINTFORMW %CALLNAME:TARGET%が部屋のカーテンを思いっきり全開にしたことで、ぐうぐうと%TEXTR("睡眠こけている/惰眠を貪っている")%%CALLNAME:MASTER%の顔に%TEXTR("眩しい/清々しい/爽やかな")%朝の日差しが直撃した。
			IF RAND:3 == 0 && COND("同居中")
				PRINTFORMW 「へい、おはよー！」
				PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
			ELSEIF RAND:2
				PRINTFORMW 「へい！　%CALLNAME:TARGET%桑が起こし来了ぞー！」
			ELSE
				PRINTFORMW 「んふふー、今日もいい天気だぞ！」
			ENDIF
		ELSE
			PRINTFORMW 朝、%CALLNAME:MASTER%が%TEXTR("睡眠こけている/惰眠を貪っている")%と、突然誰かに%TEXTR("わきの下/足の裏/首筋")%をこちょこちょとくすぐられて、急に意識が覚醒した。
			PRINTFORMW %TEXTR("困倦眼をこすりながら身を叫醒と/惊嚇して飛び起きると")%、%CALLNAME:TARGET%が%TEXTR("いたずらっぽく/にこにこと/楽しそうに")%笑いながら朝の打招呼をしてきた。
			CALL EMOTION_PLUS, "夸奖", TARGET
			IF RAND:3 == 0 && COND("同居中")
				PRINTFORMW 「あははっ♪　目は覚めたか？」
				PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
			ELSEIF RAND:2
				PRINTFORMW 「へい！　%CALLNAME:TARGET%桑が起こし来了ぞー！」
			ELSE
				PRINTFORMW 「あははっ、声かけても全然起きない方が悪いんだぞー？」
			ENDIF
		ENDIF
	ENDIF
	CASE "朝口交"
		SIF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝袭")
			PRINTFORMW 「んふふー、起きた？　このまま一回抜いて给……♪」

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "打招呼"
;あんまり唐突過ぎるので、一個会話を入れる。
;OPによると寝込みを襲う番とかなんだかよく分からんもんがあるらしいが、まぁよく分からんし……
	IF CEVENT("初回朝袭") == 0
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「へへ、おはよー！　目は覚めたか？」
		PRINTFORMW 「%CNAME("二人称")%さぁ。毎朝弗理希艾尔桑とかに起こしてもらってるらしいじゃん？」
		PRINTFORMW 「%CNAME("一人称")%も気が向いたら来てやることにしたから、ちょっとは楽しみにしてろよなー♪」
		CALL SET_CEVENT, "初回朝袭", TARGET
		RETURN 1
	ENDIF
;デート前。同棲人数が少ない場合は選ばれる率が上がるのでちょっと頻度落とす。
IF CFLAG:MASTER:约会的约定 && CFLAG:约会的约定 && TEXTS("曜日") == "日" && (RAND:4 || CHARA_NUM("同居中")  >= 3)
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「おはよー！　%TEXTR("目は覚めたか？/もう朝だぞー！/朝ごはん出来てるぞー！")%」
		PRINTFORMW 「今日は%TEXTR("玩耍に/约会に")%行君%TEXTR("だろ/でしょ")%？　ぐうぐう寝てないでとっとと%TEXTR("顔を洗ってくるのだ！/歯を磨くのだ！/淋浴を浴びるのだ！")%」
;さらに4分の1の確率で下の汎用挨拶に流す。
ELSEIF RAND:4
	SELECTCASE CSTR:朝袭
	CASE "朝乳交"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「もう、出し過ぎだって。ベタベタになっちゃったじゃん……♪」_W"
		ELSE
			PRINTFORMW 「どう？　%CNAME("一人称")%の胸部で元気出た？」
		ENDIF
	CASE "朝口交", "朝六九式"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, @"「おはよー。%TEXTR("朝から濃いなぁ/朝からすごい量/今日も元気そうだな")%……_HH_」_W"
		ELSE
			PRINTFORMW 「おはよー！」
			PRINTFORMW 「……で、どうする、これ？　まだ時間あるし、このまま抜いてやろうか？」
		ENDIF
	CASE "朝騎乗位","朝Ａ騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「ふぅ……_HH_　おはよー♪」_W"
		ELSE
			PRINTFORMW 「ふぅー、いい汗かいた！　一緒に淋浴浴びようぜ！」
		ENDIF
	CASE "朝接吻"
		CALL EMOTION_PLUS, "夸奖", TARGET
		IF RAND:3 == 0 && TALENT:相思相愛 == 0 && COND("同居中")
			PRINTFORMW 「あはは。目覚めの接吻をするのって、普通逆だよなー」
			PRINTFORMW 「今度、%CNAME("二人称")%の方からこうやって起こしに来ない？　どうよ？」
		ELSEIF RAND:2
			PRINTFORMW 「あはは、なんだよー。照れなくってもいいじゃん？」
		ELSE
			PRINTFORMW 「ほら、起きて起きてー。起きないと、もっかい接吻しちゃうぞー？」
		ENDIF
	CASE "朝足交"
			IF RAND:3 == 0 && EXP:足交経験 >= 30 && EXP_GAIN("射精経験", PLAYER) 
				PRINTFORMW 「んふふー。%CALLNAME:TARGET%桑の足捌きはどうよ？　だいぶ擅長变成了でしょ？」
			ELSEIF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「あははっ、遅刻しそうなお寝坊桑にはこうだぞー♪」
			ELSE
				PRINTFORMW 「目は覚めた？　早く起きないと、ふみふみーを続行しちゃうぞ？」
			ENDIF
	CASE "朝の新妻PLAY"
			CALL EMOTION_PLUS, "夸奖", TARGET
			IF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「んふふー、どう？　今日も一日%TEXTR("がんばる/勉強する")%気变成了？」
			ELSE
				PRINTFORMW 「はぁ、気持ちよかった……♪」
				PRINTFORMW 「じゃあ、次は朝ご飯なっ！　いや、その前に淋浴がいいかなぁ……」
			ENDIF
	ENDSELECT
ENDIF

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	CALL EMOTION_PLUS, "夸奖", TARGET
	IF RAND:3 == 0 && COND("同居中")
			PRINTFORMW 「へい、おはよー！」
		IF COND("水泳")
			IF RAND:3 == 0 && COND("夏")
				PRINTFORMW 「今日も暑くなりそう！　絶好の水泳日和だな！」
			ELSE
				PRINTFORMW 「泳装と毛巾、ここに置いとくから忘れんなよー？」
			ENDIF
		ELSEIF COND("体育")
			IF RAND:5 == 0
				PRINTFORMW 「何だか天気が可疑んだよなぁ。体育の授業まで降らないといいけど……」
			ELSEIF RAND:3
				PRINTFORMW 「今日の体育、女子は%TEXTR("サッカー/バレー/バスケ/バドミントン/ソフトボール")%なんだ！　楽しみ！」
			ELSE
				PRINTFORMW 「体操服、ここに置いとくから忘れんなよー？」
			ENDIF
		ENDIF
	ELSE
		IF RAND:3 == 0 && COND("同居中")
			PRINTFORMW 「おはよー！」
			PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
		ELSEIF RAND:2
			PRINTFORMW 「%TEXTR("おう/うむ")%っ。おはよー！」
		ELSE
			PRINTFORMW 「目は覚めたか？　ご飯の前に淋浴行くぞー！」
		ENDIF
	ENDIF

CASE "一番乗り"
	CALL EMOTION_PLUS, "夸奖", TARGET
	IF COND("同伴中")
			IF RAND:3 == 0 && COND("Ｈ的サプライズ可能") && COND("欲求不満") >= 2
				CALL EMOTION_PLUS, "躁动难忍", TARGET
				PRINTFORMW 「ねぇ、%CALLNAME:MASTER%。ちょうどいいし、今日は%CNAME("一人称")%としない？」
				PRINTFORMW 「今さ、すっごく%CNAME("二人称")%としたい気分なんだよ。……ダメ？」
			ELSE
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「んー、ちょっと早かったかな？」
				PRINTFORMW 「まぁいいや。%CNAME("二人称")%はそこに座ってろよ。茶淹れてやるからさ」
			ENDIF
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				CALL EMOTION_PLUS, "夸奖", TARGET
				PRINTFORMW 「へいらっしゃい！　茶飲む？」
		ELSE
				;一番乗りの時はすでにシャワーを浴びてる。
				CFLAG:淋浴済 = 1
				CFLAG:汗 = 0
				CALL RESET_STAIN_EX, "老廃物以外全部", TARGET
				;このタイミングで欲求不満気味or淫乱な時はスパッツをキャストオフする
				IF (COND("欲求不満") && CEVENT("調教開始：Ｈに慣れた")) || CEVENT("調教開始：淫乱")
					SIF CHECK_OUTER("紧身短褲")
						CALL SET_OUTER, "", TARGET
				ENDIF
			IF COND("浑身是汗", MASTER)
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				CALL EMOTION_PLUS, "夸奖", TARGET
				PRINTFORMW 「うーわ、%CNAME("二人称")%。汗びっしょりだぞー？」
				PRINTFORMW 「拭いてやるからこっち来いよ。カモンカモン♪」
				PRINTFORMW %CALLNAME:TARGET%は毛巾を引っぱり出してくると、浑身是汗の%CALLNAME:MASTER%に手招きをした。
				CFLAG:MASTER:汗 = 0
			ELSEIF COND("夏") && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				SAVESTR:茶 = 麦茶
				PRINTFORMW 「%CNAME("二人称")%も麦茶飲む？」
				PRINTFORMW 「よしよし。それじゃあ、グラス持ってくるからなー」
			ELSEIF COND("冬") && RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「あったかいものでも飲む？　何がいい？」
			ELSEIF RAND:2
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「おっ、今日は早いじゃん！　ご褒美に茶でも淹れてやるかな」
			ELSE
				CALL SETFLAG_DAILY, "茶を用意", TARGET
				PRINTFORMW 「おっ、今日は%CNAME("二人称")%が二着だな！　茶淹れるから待ってろなー」
			ENDIF
		ENDIF
	ENDIF
CASE "ただいま"
	IF COND("冬")
		IF RAND:3 == 0
			PRINTFORMW 「%TEXTR("ただいまー/ただいま！/ただいまー♪")%」
			PRINTFORMW 「……っと、手を洗ってこなくっちゃ」
		ELSE
			PRINTFORMW 「ただいまー。あー、さむさむ……」
		ENDIF
	ELSEIF TEXTS("季節詳細") == "晩秋" && RAND:2
		PRINTFORMW 「ただいまっ。もう日が暮れるのがだいぶ早くなったなー」
	ELSEIF COND("夏") && RAND:2
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「はぁ、クーラーが効いてて涼しいのう……♪」
	ELSEIF RAND:2
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「へい！　ただいまっ！」
	ELSE
		PRINTFORMW 「%TEXTR("ただいまー/ただいま！/ただいまー♪")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TEXTS("曜日") == "土"
			CALL EMOTION_PLUS, "夸奖", TARGET
			PRINTFORMW 「おかえり、%CALLNAME:MASTER%！　今週も%TEXTR("お疲れ大人！/おつかれー！")%」
		ELSE
			PRINTFORM 「
			SIF COND("同伴中", MASTER)
				PRINTFORM 二人とも、
			PRINTFORMW %TEXTR("おかえりー/おかえり！/おかえり♪/おつかれー！")%」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR("よっす！/おつかれ！/へい！")%」
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "夸奖", TARGET
		;おやつ当番が料理上手だとこう。フェリさん作なら確率上げてもいい
		IF COND("点心の作り手が料理擅長") && COND("亲手自制点心") && RAND:2
			PRINTFORMW 「今日は亲手自制点心があると聞いて！　%CNAME("一人称")%、%TEXTR("颯爽と/華麗に")%%TEXTR("参上/登場")%！」
		ELSE
			PRINTFORMW 「%TEXTR(@"お待たせー！/%CNAME("一人称")%、参上！/へい、お待たせ！")%」
		ENDIF
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		IF RAND:2
			CALL EMOTION_PLUS, "躁动难忍", TARGET
			PRINTFORMW 「おー、今日も盛ってるなー……」
		ELSE
			CALL EMOTION_PLUS, "夸奖", TARGET
			PRINTFORMW 「あははっ、何やってんだよー」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす！/%CNAME("一人称")%、参上！/はろはろー！")%」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR(@"あれっ、みんないるじゃん。今日って何かあったっけ？/ごめん。ちょっと遅れちゃった！/よっす！")%」
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす！/%CNAME("一人称")%、参上！/はろはろー！")%」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "打招呼返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		IF RAND:2
			PRINTFORMW 『こんにちはー』
		ELSE
			PRINTFORMW 『お疲れ大人です』
		ENDIF
	;絡みやすそうな人たち(独断)
	ELSEIF (CALLNAME_ACTOR == "哈娜" || CALLNAME_ACTOR == "杏珠" || CALLNAME_ACTOR == "織") && COND("相性○", ACTOR)
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 『おっ、%CNAME(CALLNAME_ACTOR)%じゃん！　%TEXTR(@"よっす！/はろはろー！/こっちこっちー！/待ってたぞー！")%』
	;同級生以下(三十路は弾く)はよっす！
	ELSEIF TALENT:ACTOR:外見年齢 <= 3 && COND("学生", ACTOR) && COND("学年") >= COND("学年", ACTOR)
			PRINTFORMW 『%TEXTR(@"よっす！/お疲れー/おつかれー")%』
	;先輩とか目上相手は敬語。
	ELSE
		IF RAND:2
			PRINTFORMW 『こんにちはー』
		ELSE
			PRINTFORMW 『お疲れ大人です』
		ENDIF
	ENDIF


CASE "居間に現れた"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "夸奖", TARGET
		PRINTFORMW 「%TEXTR(@"呼んだ？/%CNAME("一人称")%、参上！/何か用かー？")%」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		IF STATE("困倦",MASTER) && RAND:3
			PRINTFORMW 「なんだか眠そうじゃない？　そろそろ部屋に戻ったらどうよ？」
		ELSEIF STATE("酔酒",MASTER)
			CALL EMOTION_PLUS, "夸奖", TARGET
			PRINTFORMW 「あははっ、まだ飲んでんのかよー」
		ELSE
			PRINTFORMW 「ふんふーん……」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす/へい！")%」
	ENDIF
CASE "全裸化への反応"
	IF ABL:欲望 >= 3 && ABL:露出癖 >= 3
		IF TALENT:伪娘
