;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K35, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		PRINTFORMW 「やーっと起きたな、この寝ぼすけめ」
		PRINTFORMW 「あんまりかわいい顔してるから、我慢できなくなっちゃったよ」
	CASE "朝クンニ"
		PRINTFORMW 「へへ、起きた起きた」
		PRINTFORMW 「ちょっと寝てる間にお掃除をと思ってね」
	CASE "朝足コキ"
		PRINTFORMW 「やーっと起きたな、この寝ぼすけめ」
		PRINTFORMW 「こんだけやっても起きないとか、大した奴だなぁ。くっく」
	CASE "朝の新妻プレイ"
		PRINTFORMW 「ったく、この%CNAME("一人称")%様が珍しく飯作ってやってるってのに……」
		PRINTFORMW 「ほら、先に食ってろ。%CNAME("一人称")%はちょっと洗い物ができた」
	CASE "朝パイズリ"
		PRINTFORMW 「んっ……あ、起きた？」
		PRINTFORMW 「わりぃわりぃ。ちょっとコイツの使い心地を確かめようかなって。ふふっ」
	CASE "朝キス"
		PRINTFORMW 「ふふっ……王子様のキス」
		PRINTFORMW 「なんつって。%CNAME("二人称")%にとってはお姫様かな？」
	CASE "朝添い寝"
		PRINTFORMW 「なんだよ、もう起きちまったの？」
		PRINTFORMW 「折角だから二度寝でもしようぜ……ふふっ」
	CASE "朝フェラ", "朝シックスナイン"
		PRINTFORMW 「んっふふ……一番搾り、ごちそうさん」
		PRINTFORMW 「んじゃ、ちょっと洗面台借りるから」
	CASE "朝騎乗位", "朝Ａ騎乗位"
		CALL PRINT_STRW, "「んっ……_H_ あー……やっと起きた？」"
		PRINTFORMW 「よし、じゃあちょっと……手ぇ貸せ。立てねぇから」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「%TEXTR("おら、気張れ。今日は体育だぞ/きょう体育だってよ。だりー……")%」
	ELSE
		PRINTFORMW 「%TEXTR("ん、おはよ/ほら、起きた起きた")%」
	ENDIF
CASE "一番乗り"
	PRINTFORMW 「%TEXTR("なんだ、他の奴いないのか……/ん……二人っきりか")%」
CASE "ただいま"
	CALL EMOTION_PLUS, "不機嫌", TARGET
	PRINTFORMW 「ただいまー。%TEXTR("あー、疲れた……/なあ、なんか遊ぼうぜー")%」

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「%TEXTR("なんだよ、遅かったじゃん/お、おかえりー。いま暇か？")%」
	ELSE
		PRINTFORMW 「%TEXTR("よっ。先に来てるぜ/おつかれ。今日は何する？")%」
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		PRINTFORMW 「お待たせ、待った？ ……%TEXTR("なーんて/とかいって")%」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		IF TALENT:恋慕
			CALL EMOTION_PLUS, "不機嫌", TARGET
			PRINTFORMW 「%TEXTR("おい、もうおっ始めてんのかよ。ったく/ああ？ なんだよ、勝手に楽しみやがって")%」
		ELSE
			PRINTFORMW 「お？ やってんねぇ」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR("お邪魔しまーす/うーっす。来てるか？")%」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("あー、遅れた遅れた……/おー、もうみんな来てんじゃん")%」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「%TEXTR("お待たせ/来たぞ")%ー」
	ELSE
		PRINTFORMW 「%TEXTR("どもー/うっす")%」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『どうもー、先生』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF FIND_COND("入室挨拶中", "真", "莉")
			PRINTFORMW 『ずいぶん遅かったなーお前。何してたん？』
		ELSE
			PRINTFORMW 『よ。遅かったな』
		ENDIF
	ELSEIF FIND_COND("入室挨拶中", "真", "莉")
		PRINTFORMW 『%TEXTR("おう/ん")%、なんだお前か』
	ELSE
		PRINTFORMW 『%TEXTR("どもー/おつかれー/うーす")%』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「呼んだかー？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ただいま、っと」
	ELSE
		PRINTFORMW 「うーっす、お邪魔しますよっと」
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「%TEXTR("やれやれ/元気だなぁ")%……」
	ELSE
		PRINTFORMW 「%TEXTR("なんだ、お楽しみか〜？/おーおー、お盛んなこって")%」
	ENDIF
CASE "お弁当"
	IF CEVENT("初めてのお弁当") == 0
		PRINTFORMW 「ほれ」
		PRINTFORMW ぶっきらぼうに弁当箱を差し出した%CALLNAME:TARGET%の顔は、珍しく緊張の色が見て取れた。
		PRINTFORMW 「なんだよ、早く食えよ」
		PRINTFORMW 不機嫌そうに急かされつつ弁当箱の蓋を開けると、思ったより整ったお弁当の姿が現れた。
		PRINTFORMW 試しに一口食べてみるが、味も申し分ない。
		PRINTFORMW 「……うまいか？」
		PRINTFORMW 躊躇いなく頷きを返すと、%CALLNAME:TARGET%は溜め息を漏らした。
		PRINTFORMW 「ったく、無茶振りしやがって……」
		PRINTFORMW 悪態をつく口は、ほんの僅かにニヤけていた。
		CALL SET_CEVENT, "初めてのお弁当", TARGET
	ELSE
		PRINTFORMW 「物好きだよなぁ、%CNAME("二人称")%」
		PRINTFORMW 自分の手料理を満足そうに食べる%CALLNAME:MASTER%の横顔を、%CALLNAME:TARGET%はぼーっと眺めていた。
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「%TEXTR("おーおー、気にせず始めやがって……/おいおい、こっちは生殺しかー？")%」
	ELSE
		CALL EMOTION_PLUS, "びっくり", TARGET
		PRINTFORMW 「%TEXTR("おいマジか。遠慮しねえな/え、なに。見てていいの？")%」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K35, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「%NAME:TARGET%です♪ よろしくお願いしまーす♪」
	PRINTFORML %CALLNAME:TARGET%はわざとらしく白々しい笑顔で挨拶した。
CASE "汗だく"
	IF RAND:2
		PRINTFORML 「%TEXTR(@"おーい、拭いてくれよー……/%CNAME("主人")%ー、アイス食べたいー")%」
