	IF PLACE("ラブホテル")
		PRINTFORML 「待たせたな、\@ TALENT:MASTER:オトコ ? お兄さん # お嬢さん \@」
	;ヤリ部屋
	ELSE
		PRINTFORML 「よ。お待たせ」
	ENDIF
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;標語の反応。書いたらコメントアウトを外してください
IF CFLAG:標語実践
	IF STRCOUNT(SAVESTR:標語, "喉の渇きに注意")
		;IF DEVENT("標語お茶") == 0
		;	PRINTFORML 
		;	CALL SET_DEVENT, "標語お茶"
		;ENDIF
