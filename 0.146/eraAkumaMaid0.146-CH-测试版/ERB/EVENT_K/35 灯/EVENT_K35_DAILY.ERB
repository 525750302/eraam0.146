		PRINTFORML 「%TEXTR("さ、個撮でもするか？/ちょうど二人きりだ。じっくり堪能していいんだぜ")%」
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K35
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
