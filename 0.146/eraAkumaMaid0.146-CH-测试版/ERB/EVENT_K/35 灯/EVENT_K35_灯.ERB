;エンディングを書いたら、下のRETURN 0を消してください
;RETURN 0

;ENDを選択する際の表示。下で満たしたEND条件に応じて全角21文字以内で好きに変えて良い
STR:ＥＮＤタイトル = 個別イベントを見る

;魂の量によるEND
;SIF BASE:魂 >= 300
	;ENDの番号_END名 となっている
	;番号は1〜60の範囲(でもできれば１から順に詰めて使用)の半角数字で、ＥＮＤ名は好きなものに変更してください
	;STR:エンディング += "1_ＥＮＤ名１/"
;その他の条件(例えば淫乱)によるEND
;SIF TALENT:淫乱
	;STR:エンディング += "2_ＥＮＤ名２/"
;魂の量とその他の条件(例えば相思相愛)によるEND
;SIF BASE:魂 >= 300 && TALENT:相思相愛
	;STR:エンディング += "3_ＥＮＤ名３/"
STR:エンディング += "1_イベント閲覧条件/"
SIF (MARK:屈服刻印 >= 1) + CEVENT("全開放")
	STR:エンディング += "2_高慢な美少年の憂鬱/"
SIF (MARK:屈服刻印 >= 2) + CEVENT("全開放")
	STR:エンディング += "3_うわさの恋人/"
SIF (MARK:屈服刻印 == 3) + CEVENT("全開放")
	STR:エンディング += "4_恋の自覚/"
SIF (TALENT:相思相愛 && CEVENT("調教開始：相思相愛")) + CEVENT("全開放")
	STR:エンディング += "5_男の娘の理由/"
SIF (CFLAG:取引回数 >= 3) + CEVENT("全開放")
	STR:エンディング += "6_セックスをするお友達サークル/"
SIF (CEVENT("あなたの許可") && CEVENT("あなたの許可", FINDCHARA(NO, 34)) && CONFIG("部員間Ｈ")) + CEVENT("全開放")
	STR:エンディング += "7_最後の一線/"
RETURN 1


;ARGSには@MENU_SINGLE_ENDING_K1で設定したEND名のどれかが入っている
;見たENDはCEVENTで記録される
;例）ＥＮＤ名１を閲覧した場合は CEVENT("ＥＮＤ名１") = 1 となる
@SINGLE_ENDING_K35, ARGS
SELECTCASE ARGS
CASE "イベント閲覧条件"
	PRINTL
	PRINTFORML [高慢な美少年の憂鬱]　屈服刻印LV1を取得
	PRINTL
	PRINTFORML [うわさの恋人]　屈服刻印LV2を取得
	PRINTL
	PRINTFORML [恋の自覚]　屈服刻印LV3を取得
	PRINTL
	PRINTFORML [男の娘の理由]　相思相愛素質を取得し、特定の調教開始時の会話を見る
	PRINTL
	PRINTFORML [セックスをするお友達サークル]　望木灯とフェリシエルの取引イベントが3回以上
	PRINTL
	PRINTFORML [最後の一線]　望木真の“兄妹の言い訳”イベント閲覧後、真と灯の%CALLNAME:MASTER%との特定の会話イベントを行う
	PRINTFORML ※コンフィグであなた以外とＨする事を誰相手でも許さない設定以外にすることが必要
	PRINTW
	CALL PRINT_STRW, "黄色_望木灯が相思相愛取得後、あなたの魂を2個消費することでイベントを全開放できます"
	PRINTL
	IF CEVENT("全開放") == 0
		[IF_DEBUG]
			PRINTFORML デバッグモードの特権で条件を無視してイベントを全開放しますか？
			PRINTFORML ※一度開放すると元には戻せません また、予期せぬ不具合が発生する場合があります
			CALL PRINT_SELECT, "開放する/開放しない"
			IF RESULT == 0
				CALL SET_CEVENT, "全開放"
				CALL PRINT_STRW, "黄色_イベントを全開放しました"
			ENDIF
			RETURN 1
		[ENDIF]
		IF BASE:MASTER:魂 > 200 && TALENT:相思相愛
			PRINTFORML イベントを全開放しますか？
			PRINTFORML ※一度開放すると元には戻せません また、予期せぬ不具合が発生する場合があります
			CALL PRINT_SELECT, "開放する/開放しない"
			IF RESULT == 0
				BASE:MASTER:魂 -= 200
				CALL SET_CEVENT, "全開放"
				CALL PRINT_STRW, "黄色_イベントを全開放しました"
			ENDIF
		ENDIF
	ENDIF
CASE "男の娘の理由"
	CALL K35_LONG_01
CASE "最後の一線"
	IF CEVENT("最後の一線")
		PRINTFORML どれを閲覧しますか？
		CALL PRINT_SELECT, "分岐前/兄妹の処女同時喪失/真の処女喪失/灯の処女喪失/処女喪失を含まないセックス"
		IF RESULT == 0
			CALL K35_LONG_02
		ELSEIF RESULT == 1
			CALL K35_LONG_02_1
		ELSEIF RESULT == 2
			CALL K35_LONG_02_2
		ELSEIF RESULT == 3
			CALL K35_LONG_02_3
		ELSE
			CALL K35_LONG_02_4
		ENDIF
	ELSE
		CALL K35_LONG_02
		IF TALENT:処女 && TALENT:FINDCHARA(NO, 34):処女
			CALL K35_LONG_02_1
			EXP:FIND_COND("真"):特別経験 += 1
			CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%バイブで処女喪失： ???日目 %CALLNAME:TARGET%のバイブを処女穴に受け入れ、女にして貰った
			EXP:特別経験 += 1
			CSTR:特別経験 = %CSTR:特別経験%バイブで処女喪失： ???日目 %CALLNAME:FINDCHARA(NO, 34)%のバイブを処女穴に受け入れ、女にして貰った
			TALENT:処女 = 0
			TALENT:FINDCHARA(NO, 34):処女 = 0
		ELSEIF TALENT:FINDCHARA(NO, 34):処女
			CALL K35_LONG_02_2
			EXP:FIND_COND("真"):特別経験 += 1
			CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%処女喪失： ???日目 %CALLNAME:TARGET%のペニスに処女を捧げた
			IF EXP:Ｖ挿入経験 == 0
				EXP:特別経験 += 1
				CSTR:特別経験 = %CSTR:特別経験%童貞喪失： ???日目 %CALLNAME:FINDCHARA(NO, 34)%に筆下ろしして貰った
				EXP:FIND_COND("真"):特別経験 += 1
				CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%筆下ろし： ???日目 %CALLNAME:TARGET%の童貞を美味しく頂いた
			ENDIF
			TALENT:FINDCHARA(NO, 34):処女 = 0
		ELSEIF TALENT:処女
			CALL K35_LONG_02_3
			EXP:特別経験 += 1
			CSTR:特別経験 = %CSTR:特別経験%処女喪失： ???日目 %CALLNAME:FINDCHARA(NO, 34)%のペニスに処女を捧げた
			IF EXP:FIND_COND("真"):Ｖ挿入経験 == 0
				EXP:FIND_COND("真"):特別経験 += 1
				CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%童貞喪失： ???日目 %CALLNAME:TARGET%に筆下ろしして貰った
				EXP:特別経験 += 1
				CSTR:特別経験 = %CSTR:特別経験%筆下ろし： ???日目 %CALLNAME:FINDCHARA(NO, 34)%の童貞を美味しく頂いた
			ENDIF
			TALENT:処女 = 0
		ELSE
			CALL K35_LONG_02_4
			IF EXP:FIND_COND("真"):Ｖ挿入経験 == 0
				EXP:FIND_COND("真"):特別経験 += 1
				CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%童貞喪失： ???日目 %CALLNAME:TARGET%に筆下ろしして貰った
				EXP:特別経験 += 1
				CSTR:特別経験 = %CSTR:特別経験%筆下ろし： ???日目 %CALLNAME:FINDCHARA(NO, 34)%の童貞を美味しく頂いた
			ENDIF
			IF EXP:Ｖ挿入経験 == 0
				EXP:特別経験 += 1
				CSTR:特別経験 = %CSTR:特別経験%童貞喪失： ???日目 %CALLNAME:FINDCHARA(NO, 34)%に筆下ろしして貰った
				EXP:FIND_COND("真"):特別経験 += 1
				CSTR:FIND_COND("真"):特別経験 = %CSTR:特別経験%筆下ろし： ???日目 %CALLNAME:TARGET%の童貞を美味しく頂いた
			ENDIF
		ENDIF
		PRINTFORML ------------------------------------------------------------------------------------------------
		IF TALENT:倒錯的 == 0
			TALENT:倒錯的 = 1
			PRINTL
			CALL PRINT_STR, @"イエロー_%CALLNAME:TARGET%は[倒錯的]を獲得しました。_W_"
		ENDIF
		PRINTFORML 次回から本イベントを選択した場合、異なる分岐イベントの個別回想が可能です
	ENDIF
CASE "高慢な美少年の憂鬱"
	CALL K35_LONG_03
CASE "うわさの恋人"
	CALL K35_LONG_04
CASE "恋の自覚"
	CALL K35_LONG_05
CASE "セックスをするお友達サークル"
	CALL K35_LONG_06
ENDSELECT
RETURN 1
