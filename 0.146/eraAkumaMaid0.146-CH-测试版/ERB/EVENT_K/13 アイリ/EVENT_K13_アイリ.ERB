;頻繁に表示されるためランダムや表示条件の設定推奨です
;-------------------------------------------------
@KOJO_MESSAGE_SHOOT_K13
;初期は表示されないようにしてあるため、書いた後はRETURN 0を消す
;毎回出るのもアレなのでとりあえず３分の１の確率で表示
IF RAND:3 || SELECTCOM == 282
	RETURN 0
ELSEIF INSERT("Ｖインサート")
	;発情期　おねだりセリフ入れるならここかなあ？
	IF COND("発情中")
		IF TEQUIP:甘い言葉 && TALENT:恋慕 && RAND:3 == 0
			PRINTFORML 「愛してるっ、%CNAME("一人称")%も、愛してるからっ…！」
			IF RAND:2
				CALL PRINT_STR, "「だから、来てっ_H_早く来てぇっ_H_」_W"
			ELSE
				CALL PRINT_STRW, "「だから、愛してる証拠欲しいのっ_H_お腹の中に欲しいのぉっ_H_」"
			ENDIF
		ELSEIF TEQUIP:PLAYER:コンドーム == 0 && COND("避妊不要") == 0 && RAND:2
			IF TEQUIP:新妻プレイ
				PRINTFORMW 「抜いちゃ、やだ…中がいいの…」
				PRINTFORMW 「……夫婦なんだから……いいでしょ？」
			ELSEIF RAND:2
				PRINTFORML 「…%CNAME("二人称")%の、全部中で受け止めてあげる…」
				CALL PRINT_STR, @"「だから、一番奥で思いっきり%TEXTR("ぶっ放して/イって/ビューっ、てして")%…_H_」_W"
			ELSE
				PRINTFORML 「ビクビクいってる…出るのね？出ちゃうのね？」
				PRINTFORMW 「お願い、そのまま中に出して…%TEXTR("元気な/可愛い")%%TEXTR("仔猫/赤ちゃん")%…孕ませて？」
			ENDIF
		ELSEIF COND("合意：危険日生セックス") && COND("避妊不要") == 0 && TEQUIP:PLAYER:コンドーム && RAND:2
			PRINTFORMW 「いっぱい出して…！ゴム突き破って孕ませるぐらいいっぱい出してぇっ…！」
		ELSE
			PRINTFORMW 「%TEXTR("我慢しないで…そのままイって/ほーら、最後までちゃんと頑張って")%…♪」
		ENDIF
	;それ以外
	ELSE
		IF TEQUIP:PLAYER:コンドーム == 0 && RAND:3 == 0
			PRINTFORML 「その…もし、どうしても我慢できないなら…」
			PRINTFORMW 「このまま抜かなくても、いいから…」
		ELSEIF RAND:2
			PRINTFORMW 「え？もう%TEXTR("イきそう/限界/出ちゃいそう")%、って……やぁっ！？は、激しっ…待ってってば…！」
		ELSE
			PRINTFORMW 「んうぅっ…！な、中で、ビクビク震えて…！」
		ENDIF
	ENDIF
ELSE
	IF SELECTCOM == 140
		IF RAND:2
			PRINTFORMW 「んんっ…！？ぅっぶ！！んﾞーっ！んﾞーーっ！」
		ELSE
			PRINTFORMW 「んぶっ、%TEXTR("ぶぐっ/ぐぶっ/ぐぅ")%……んむっ、んんぅ〜〜っ…！」
		ENDIF
	;足コキ
	ELSEIF SELECTCOM == 95
		;サドっ気Lv3
		IF ABL:サドっ気 >= 3
			IF RAND:2 && !COND("発情中")
				PRINTFORMW 「なんか、足疲れてきた…」
				PRINTFORMW 「早く出しちゃってよ、手間かけさせないでよね…」
			ELSE
				PRINTFORMW 「ん…？足に出しちゃうの？」
				PRINTFORMW 「ま、別に私は構わないけど……」
			ENDIF
		ELSE
			PRINTFORMW 「え……ほ、本当に出しちゃうの？こんなので？」
		ENDIF
	ELSEIF COND("発情中")
		IF RAND:2
			PRINTFORMW 「好きな所にかけて…%CNAME("一人称")%の体、オスの匂い%TEXTR("まみれ/でいっぱい")%にして…♪」
		ELSE
			PRINTFORMW 「イきたい…？それとも、もうちょっと頑張る？ふふ♪」
		ENDIF
	ELSEIF TALENT:恋慕
		IF RAND:2
			PRINTFORMW 「もうダメなの？まだいけるの？もう、ハッキリしてよね…」
		ELSE
			PRINTFORMW 「ん、そろそろみたいね…いいわ、好きなだけ出して…」
		ENDIF
	ELSE
		PRINTFORMW 「出ちゃいそう…？ったく、しょうがないんだから…」
	ENDIF
ENDIF

