;-------------------------------------------------
;イベント口上集
;-------------------------------------------------
@KOJO_EVENT_K13, ARGS
SELECTCASE ARGS
;-------------------------------------------------
;調教終了不可能
;-------------------------------------------------
CASE "調教終了不可能"
	;発情期
	IF COND("発情中")
		PRINTFORMW 「…ダメ、まだ離してあげない…」
		PRINTFORMW 「こんな中途半端な所で終わっちゃ嫌なんだから…♪」
	;それ以外
	ELSE
		PRINTFORMW 「その…悪いけど、もうちょっとだけ付き合って…」
	ENDIF
;-------------------------------------------------
;コンドーム装着
;-------------------------------------------------
CASE "Ｖコンドーム装着"
	;挿入コマンド
	IF TRAIN_GENRE("挿入", SELECTCOM)
		;プレイヤーは着ける気満々
		IF TEQUIP:PLAYER:コンドーム
			IF COND("発情中")
				IF COND("合意：危険日生セックス") && !COND("避妊不要")
					IF CHOSECOMS("Ｖ挿入") == 0
						IF TEQUIP:新妻プレイ
							CALL PRINT_STR, "「このまま……つけないで、最後までして…_H_」_W"
							CALL PRINT_STR, "「それで、もし当たっちゃったら……本当のお嫁さんにして…_H_」_W"
						ELSEIF RAND:2
							PRINTFORMW 「今の%CNAME("一人称")%、子供作るためにせっかく発情してるのよ？」
							PRINTFORMW 「だからね、子供作るためのちゃんとした交尾、したい…」
							PRINTFORMW 「…覚悟ならとっくにできてるわ、あとは%CNAME("二人称")%が%TEXTR("欲望に負ける/本能に従う/理性消し飛ばす")%のを待つだけよ♪」
						ELSE
							PRINTFORMW 「その…今日、%TEXTR("ゴムなし/ナマ/つけない")%でしない？」
							PRINTFORMW 「うん、わかってる…まずいってのはわかってるけど…」
							PRINTFORMW 「でも……本能に逆らえないの…%TEXTR("子供、欲しい/繁殖、したい/子宮、疼く")%の…お願い…♪」
						ENDIF
						CALL PRINT_SELECT, "それでも着ける/お言葉に甘えて生挿入！"
						IF RESULT == 0
							IF RAND:2
								PRINTFORMW 「ふーん、まだまだヤリ足りないってこと？しょうがないわね…♪」
							ELSE
								PRINTFORMW 「…交尾じゃなくて、普通のセックスの方がいいの？」
								PRINTFORMW 「それならそれで…存分に楽しめるわね♪」
							ENDIF
						ELSE
							PRINTFORMW 「元気な遺伝子、いっぱい注ぎ込んでね…♪」
							CALL SETFLAG, "ゴムキャンセル", PLAYER
						ENDIF
					ENDIF
				ELSE
					PRINTFORMW 「ふふ、これで心置きなく楽しめるわね…」
				ENDIF
			ELSE
				IF TCVAR:ご褒美生Ｈ
					PRINTFORMW 「待って……忘れたの？」
					PRINTFORMW 「今日は、ご褒美あげるって話だった…でしょ？」
					CALL PRINT_STR, "「だからその……それ、付けなくても…_／／／_」_W"
					CALL PRINT_SELECT, "それでも着ける/お言葉に甘えて生挿入！"
					IF RESULT == 0
						PRINTFORMW 「なによ、人がせっかく……！まあ、いいけど…」
					ELSE
						PRINTFORMW 「……ん、どうぞ…」
						CALL SETFLAG, "ゴムキャンセル", PLAYER
					ENDIF
				ELSE
					PRINTFORMW 「ちゃんと着いた？途中で取れたりしないわね？」
				ENDIF
			ENDIF
		;避妊処置をしなくて良い事がキャラに分かっている状態
		ELSEIF COND("避妊不要") || TCVAR:ご褒美生Ｈ
			
		;ゴムが無い
		ELSEIF ITEM:コンドーム == 0
			PRINTFORMW 「あ、ゴム…無いの？」
			IF COND("発情中")
				IF COND("合意：危険日生セックス")
					PRINTFORMW 「じゃあ…そのまま、やっちゃう？」
					PRINTFORMW 「そのままはそのままよ…ナマでいい、ってことだけど？」
					CALL PRINT_SELECT, "やっぱりやめておく/お言葉に甘えて生挿入！"
					IF RESULT == 0
						PRINTFORMW 「もう…じゃあこれどうやって治めればいいのよ…こんなの生殺しよ…」
						TFLAG:コマンドキャンセル = 1
					ELSE
						PRINTFORMW 「そのまま入れて、そのまま中で出しちゃったら、そのまま…どうなっちゃうのかしらね♪」
					ENDIF
				ELSE
					PRINTFORMW 「もう…じゃあこれどうやって治めればいいのよ…こんなの生殺しよ…」
					TFLAG:コマンドキャンセル = 1
				ENDIF
			ELSE
				IF TALENT:恋慕 && COND("安全日")
					PRINTFORMW 「じゃ…い、いいわ、着けなくても…ないものはしょうがないでしょ…」
				ELSE
					PRINTFORMW 「準備悪いわね…今日はやめときましょ」
					PRINTFORMW 「…そんなに残念そうな顔しないの」
					TFLAG:コマンドキャンセル = 1
				ENDIF
			ENDIF
		;発情期以外に生希望
		ELSEIF !COND("発情期")
			IF TALENT:恋慕 && COND("安全日")
				IF CHOSECOMS("Ｖ挿入") == 0
					IF RAND:2
						PRINTFORMW 「…今日は大丈夫な日だから」
						PRINTFORMW 「%CNAME("二人称")%が、そうしたいなら…」
					ELSE
						PRINTFORMW 「え、えっ…？」
						PRINTFORMW 「いや…何でもないわ…勝手にすれば……？」
					ENDIF
				ENDIF
			;キャラメイクで発情体質除外した場合　たぶんほぼないと思うけど
			ELSEIF COND("合意：危険日生セックス") && !COND("安全日")
				
			ELSE
				PRINTFORMW 「ゴムあるんなら着けない理由なんてないでしょ！さっさと着けなさい！」
				PRINTFORMW %CALLNAME:PLAYER%は仕方なくゴムを装着した…
				CALL SETFLAG, "ゴム装着", PLAYER
			ENDIF
		;発情期に生ＯＫ
		ELSEIF COND("合意：危険日生セックス")
			IF CHOSECOMS("Ｖ挿入") == 0
				IF COND("発情中")
