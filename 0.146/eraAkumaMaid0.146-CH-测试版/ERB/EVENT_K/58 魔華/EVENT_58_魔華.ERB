;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K58, ARGS
#FUNCTIONS
;名前を呼ぶ相手のキャラ番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = センパイ_せんぱい
	;相手が主人ではない
	ELSE
		LOCALS = あなた_あなた
	ENDIF
;ここリアラセンセのマネ,CASEELSEの下に持ってくるのはまずいらしい
CASE "精子"
	LOCALS  = 精子_せいし/せーし_せーし/精子_せいし/せーし_せーし/精子_せいし/子種_こだね/
	LOCALS += "精子_せいし/子種_こだね/遺伝子_いでんし/精虫_せいちゅう/精細胞_せいさいぼう/"
CASE "精液"
	LOCALS  = ザーメン_ざーめん/ザー汁_ざーじる/スペルマ_すぺるま/ちんぽ汁_ちんぽじる/濃厚ミルク_のうこうみるく/
	LOCALS += "性欲の塊_せいよくのかたまり/オス汁_おすしる/オスの獣液_おすのじゅうえき/せーえき_せーえき/蛋白液_たんぱくえき/"
	LOCALS += "精液_せいえき/せーえき_せーえき/精液_せいえき/ザーメン_ざーめん/精液_せいえき/ザーメン_ざーめん/"
CASE "射精音"
	LOCALS  = びゅりゅびゅりゅ_びゅりゅびゅりゅ/びゅーびゅー_びゅーびゅー/どくどく_どくどく/ぴゅっぴゅ_ぴゅっぴゅ/びゅくびゅく_びゅくびゅく/ぴゅゆぴゅりゅ_ぴゅるぴゅる/
	LOCALS += "ビュリュビュリュ_びゅりゅびゅりゅ/ビュービュー_びゅーびゅー/ドクドク_どくどく/ピュッピュ_ぴゅっぴゅ/ビュクビュク_びゅくびゅく/ピュルピュル_ぴゅるぴゅる/"
CASE "子宮"
	LOCALS  = 子宮_しきゅう/子宮_しきゅう/子宮_しきゅう/お腹の奥_おなかのおく/メスの器官_めすのきかん/精液タンク_せいえきたんく/
	LOCALS += "子宮_しきゅう/子宮_しきゅう/子宮_しきゅう/お腹の奥_おなかのおく/"
	LOCALS += "ザーメン捨場_ざーめんすてば/スペルマ廃棄所_すぺるまはいきじょ/オス液投棄場_おすえきとうきば/排泄先_はいせつさき/"
CASE "膣"
	LOCALS  = おまんこ_おまんこ/肉オナホ_にくおなほ/チンポしごき穴_ちんぽしごきあな/ザーメン絞り器_ざーめんしぼりき/シコシコ穴_しこしこあな/
	LOCALS += "オネダリ穴_おねだりあな/オナホ_オナホ/チンコキホール_ちんこきほーる/"
;この下は三人称。
CASE "主人"
	LOCALS = センパイ_せんぱい
CASEELSE
	;名前を呼ぶ相手のキャラ番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSにキャラのCALLNAMEが入ったりしている場合(ARGS == "フェリシエル" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%センセ_%CSTR:ACTOR:ふりがな%せんせ

	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSE
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ENDIF
		;相手が上級生
		ELSE
			LOCALS = %CALLNAME:ACTOR%先輩_%CSTR:ACTOR:ふりがな%せんぱい
		ENDIF

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
	ENDIF

ENDSELECT
RETURNF LOCALS

