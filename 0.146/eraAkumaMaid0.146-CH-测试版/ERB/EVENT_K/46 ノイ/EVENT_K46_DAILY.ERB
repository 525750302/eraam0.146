;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K46, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 3+RAND:3)%」"
	CALL PRINT_STRW, @"「こんだけ構っといて、飽きたら放っておくたあひでえじゃねえか_H_」"
	IF RAND:2
		CALL PRINT_STRW, @"「さて、お返しだ。今日はたっぷり『使わせて』もらうぞ。覚悟しておけ_H_」"
	ELSE
		CALL PRINT_STRW, @"「さあて、たっぷりお仕置きさせてもらうとするかね_H_」"
	ENDIF
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 3+RAND:3)%」"
		CALL PRINT_STRW, @"「マンコが……%PANT_K46(TARGET, "要", "通常")%絡みついて……くっ_H_きやがる_H_」"
	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		SELECTCASE RAND:3
		CASE 1
			PRINTFORMW 「お、ようやくお目覚めか」
			PRINTFORMW 「こんなに激しくヤってりゃ嫌でも目覚めるだろって思ってたが、外れたな」
			PRINTFORMW 「もし体調が悪いのなら気兼ねなく言ってくれ、一発射精したら考えてやる」
		CASE 2
			PRINTFORMW 「お、おはよう」
			PRINTFORMW 「……ったく、こんだけ激しくヤっても起きねえとは、そうとう疲れてたのか？」
			PRINTFORMW 「次はスッキリ起きられるように、%CNAME("一人称")%が直々に『マッサージ』してやるよ」
		CASE 3
			PRINTFORMW 「おっと、おはよう」
			PRINTFORMW 「しっかし%CNAME("二人称")%は本当にやらしいなぁ」
			CALL PRINT_STRW, @"「……ほら、起きたばっかだってのに、マンコが物欲しそうにうねうねと動いているぞ_H_」"
		ENDSELECT
	CASE "朝フェラ", "朝クンニ"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 4+RAND:3)%」"
		PRINTFORMW 「……ん？ ああ、起きたのか」
		PRINTFORMW 「もうちょっと待っててくれよ」
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "Ｍ異物", 4+RAND:3)%」"
		SIF RAND:3 == 0
			CALL PRINT_STRW, @"「……ククッ_H_朝から\@CSTR:朝駆け == "朝フェラ" ? おっ勃てて # 濡らしまくって\@元気なこったなぁ_H_」"
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "通常", 4+RAND:3)%」"
		CALL PRINT_STRW, @"「んっ_H_おはっ_H_ようっ_H_」"
		CALL PRINT_STRW, @"「もうちょっとっ_H_まってっ_H_くれっ_H_」"
	CASE "朝キス"
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「お、起きたか」
			PRINTFORMW 「ん？ 何してたってそりゃキスさ」
			IF TALENT:相思相愛
				PRINTFORMW 「恋人同士なんだ。キスくらいしてもいいだろ？」
			ELSE
				PRINTFORMW 「……とはいえ同意を得ていないのは問題だな。すまなかった」
			ENDIF
		CASE 1
			CALL PRINT_STRW, @"「%PANT_K46(TARGET, "要", "キス", 5+RAND:2)%」"
			PRINTFORMW 「……ふぅ、ようやくお目覚めか」
			PRINTFORMW 「ったく、さっきまで結構本気でキスしてたんだがな」
			PRINTFORMW 「これだけ情熱的に求めても気付かれないのは、流石に傷つくぜ？」
			PRINTFORMW 「……なんてな」
		CASE 2
			PRINTFORMW 「お、おはよう%CNAME("主人")%」
			PRINTFORMW 「ん？　なんでキスしてたかって？」
			PRINTFORMW 「そりゃあ眠れるお姫様にゃキスだろ……って、我ながら言ってて鳥肌が立つな」
			PRINTFORMW 「……ま、気を取り直してだ」
			CALL PRINT_STRW, @"「せっかく起きたんだから、もう一回キスしないか？　%CNAME("主人")%_H_」"
		ENDSELECT
	CASE "朝添い寝"
		PRINTFORMW 「ん？　……ああ、おはよう」
		IF COND("冬") && RAND:2
			PRINTFORM 「思っていたより寒いからな、
		ELSE
			PRINTFORM 「中々抱き心地が良いからな、
		ENDIF
		PRINTFORMW もう少し抱き着かれていてくれ」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育の日か」
		PRINTFORMW 「しっかり\@FIND_COND("ホムラ") ? %CNAME("ホムラ")% # \@先生に絞られなよ」
	ELSE
		IF TEXTS("曜日") == "日"
			PRINTFORMW 「ん、そういや今日は休みか」
			IF COND("朝駆け相手") && COND("欲求不満")
				CALL PRINT_STRW, @"「……せっかくだ、このまま二回戦としゃれ込もうじゃねえか_H_」"
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORMW 「ふぁ……なら二度寝でもしちまうか……」
				CASE 1
					PRINTFORMW 「ならさっそくゲームでも……」
				CASE 2
					PRINTFORMW 「一緒にだらけちまわないか？　%CNAME("主人")%」
				CASE 3
					PRINTFORMW 「……ああ、そういや積んじまってたゲームがあったな……」
				ENDSELECT
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORM 「今日は授業が……
				IF RAND:3 == 0
					PRINTFORMW あるな」
					PRINTFORMW 「%CNAME("二人称")%相手だからって贔屓する気はないからな。ちゃんと勉強しろよ？」
				ELSE
					PRINTFORMW ないな」
					PRINTFORMW 「今日は部活まで合わないからって、手を抜くんじゃないぞー」
				ENDIF
			ELSE
				PRINTFORMW 「さて、そろそろ時間だ」
				PRINTFORMW 「%CNAME("一人称")%は先に行くが、%CNAME("二人称")%も遅れるんじゃないぞ？」
			ENDIF
		ENDIF
	ENDIF
CASE "一番乗り"
	PRINTFORMW 「よーす……って、%CNAME("二人称")%だけか」
	PRINTFORMW 「他の奴らが来るまでに掃除でもしておく？」
CASE "ただいま"
	PRINTFORMW 「ただいま」
CASE "既に入室"
	IF RAND:2
		PRINTFORMW 「お、早かったな」
	;家で出迎えてくれる時
	ELSEIF COND("夜")
		PRINTFORMW 「おう、お帰り」
	ELSE
		PRINTFORMW 「よう、失礼してるぞ」
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		PRINTFORMW 「よっ」
		PRINTFORMW 「……で、授業で分からない所でもあったのか？」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		PRINTFORMW 「遅くなっちま……」
		IF RAND:2
			PRINTFORMW 「…………ほーう？」
		ELSE
			PRINTFORMW 「……おっと、こりゃ失礼」
		ENDIF
	ELSE
		PRINTFORMW 「よーす」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「お、みんな揃ってるな」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「ん、何かあったか？」
	ELSE
		PRINTFORMW 「よーす」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『お、%CNAME(CALLNAME_ACTOR)%も来たか』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『お、%CNAME(CALLNAME_ACTOR)%も来たか』
	ELSE
		PRINTFORMW 『おお、%CNAME(CALLNAME_ACTOR)%か』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「なんかあったか？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「お、面白そうなテレビやってんな」
	ELSE
		PRINTFORMW 「水ー」
	ENDIF
CASE "抜け出された"
	PRINTFORMW 「おっと、別のところにいくのか」
	SIF PLACE("自宅") == 0 && COND("夜")
		PRINTFORMW 「夜道には気をつけるんだぞー」
CASE "お弁当"
	PRINTFORMW 「ほい、今日の弁当だ」
	PRINTFORM %CALLNAME:MASTER%%TEXTR("の教室に出向いた/の家に出向いた/を職員室に呼び出した")%%CALLNAME:TARGET%は、
	IF TALENT:料理上手 && RAND:5
		PRINTFORM 綺麗に盛り付けられた
	ELSEIF CFLAG:弁当日数 >= 50
		SELECTCASE RAND:3
		CASE 0
			PRINTFORM 全て%CALLNAME:TARGET%の手作りと思われる
		CASE 1
			PRINTFORM 忙しかったのか冷凍食品で構成された
		CASEELSE
			PRINTFORM 時間短縮のためかところどころに冷凍食品を使った
		ENDSELECT
	ELSEIF CFLAG:弁当日数 >= 10
		PRINTFORM 冷凍食品に少し手を加えたと思われる
	ELSE
		PRINTFORM コンビニで買ってきた
	ENDIF
	SELECTCASE RAND:3
	CASE 0
		PRINTFORM 幕ノ内弁当
	CASE 1
		PRINTFORM スタミナ弁当
	CASEELSE
		PRINTFORM %TEXTR("カレー/牛丼/カツ丼/ナポリタン")%
	ENDSELECT
	PRINTFORMW を差し出してきた……

	SIF CFLAG:弁当日数 <= 500
		CFLAG:弁当日数++
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	PRINTFORMW 「お、元気だねぇ」
	PRINTFORMW 「じゃ、お邪魔虫はお邪魔虫らしく、ここで%TEXTR("コソコソする/出歯亀させてもらう")%としますかね」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K46, ARGS
#DIM MEMO_LINECOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ほー、この教室を使ったのか。ここなら確かに人もこないだろうしな」
	PRINTFORML 「さて、今回この部活の見学をする%CALLNAME:TARGET%だ」
	PRINTFORML 「ま、ほどほどになー」
CASE "汗だく"
	IF COND("夏") && RAND:2
		PRINTFORML 「あーあっつ……」
		PRINTFORML 「昔と比べたら段違いに暑くなったな……」
	ELSE
		PRINTFORML 「うわ、汗でビショビショじゃねえか……」
	ENDIF
CASE "寝起き"
	PRINTFORML 「ん……？」
	PRINTFORML 「……もしかして、%CNAME("一人称")%寝てた？」
CASE "コスプレ中"
	SELECTCASE SHIRT(TARGET)
	CASE "女教師"
		PRINTFORML 「ふむ、あんま変わんないと思ったが、結構違うモンだな」
		PRINTFORML 「……つってもコスプレ用のなんだから当然か」
	CASE "和服"
		PRINTFORML 「ほー、こんな感じなのか」
		PRINTFORML 「今まで着た事なかったからな、中々新鮮だったよ」
	CASE "メイド服"
		PRINTFORML 「ご主人様、何をいたしましょうか？　なんてな」
	CASE "体操服"
		PRINTFORML 「さすがにこんなんを着る年じゃないんだがなぁ……」
		PRINTFORML 「……ま、いいか。%CNAME("主人")%、どう思う？」
	CASE "ナース服"
		PRINTFORML 「お？　%CNAME("主人")%は堕天使の俺に白衣の天使になってほしいのか？」
		CALL PRINT_STRL, @"「フフッ_H_いいぜ、%CNAME("二人称")%に快楽を届けてやるよ……_H_」"
	CASE "チャイナドレス"
		PRINTFORML 「ほー、こんな風になってんだな」
		PRINTFORML 「案外動きやすそうで助かるよ」
	CASE "セーラー服"
		PRINTFORML 「へえ、%CNAME("一人称")%に学生のコスプレしてほしいのか」
		PRINTFORML 「……じゃ、どういう設定にするんだ？　先輩？　後輩？　それとも同級生か？」
	CASE "巫女"
		PRINTFORML 「ほー、こっちの聖職者はこんな感じのを着るんだな」
		PRINTFORML 「へえ……ふむ……これはこれで中々……」
	CASE "ベビードール"
		IF CEVENT("女装")
			PRINTFORML 「こりゃあまた着こなすのが大変そうなのを選んできたな……」
			PRINTFORML 「……%CNAME("一人称")%はこういうのに慣れてねえんだ、キツくても我慢してくれよ」
		ELSE
			PRINTFORML 「%CNAME("二人称")%がこんな格好してほしいって頼んでくるとは、正直意外だった」
			PRINTFORML 「今までの格好的にこんなのを選んでくるとは思わなかったしな」
		ENDIF
	CASE "ウェディングドレス"
		PRINTFORML 「ほー、もしかして%CNAME("主人")%には結婚願望があるのか？」
	CASE "ゴスロリドレス"
		PRINTFORML 「へえ、最近はこういうのが流行ってんのか」
		PRINTFORML 「勉強になるなー」
	CASE "チアユニフォーム"
		PRINTFORML 「ほー、この格好してほしいのか」
		CALL PRINT_STRL, @"「……下半身は応援しなくてもいいのか_H_」"
	CASE "パジャマ"
		PRINTFORML 「これを着てほしいとは、中々マニアックな趣味してんな……」
	CASE "怪獣パジャマ"
		PRINTFORML 「……着てきたぞ」
		PRINTFORML 「ったく……こんなナリだが%CNAME("一人称")%は子どもじゃねえんだぞ……」
	CASEELSE
		PRINTFORML 「ん、どうだ？」
	ENDSELECT
CASE "おやつの感想"
	;おやつを用意した人
	ACTOR = FIND_NO(TEXTS("おやつ当番"))

	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
		IF RAND:2
			PRINTFORML 「やっぱこれウマいな！」
		ELSE
			PRINTFORML 「どうだ？ ウマいだろ？」
		ENDIF
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				PRINTFORML 「……！」
				PRINTFORML 「これメチャクチャうまいな……！ こういうのはかなり高いだろうに……」
				PRINTFORML 「……え。これ、%TEXTS("おやつ当番")%が作ったのか……！？」
			ELSE
				PRINTFORML 「……！」
				PRINTFORML 「こりゃあまた難しいだろうなうまく作ったもんだ……」
				PRINTFORML 「お？ これ%TEXTS("おやつ当番")%が作ったのか？ これは磨けばさらに高みへ行けるぞ……！」
			ENDIF
		ELSE
			PRINTFORML 「ほう、今日のおやつは%TEXTS("おやつ当番")%が作ったのか」
			IF COND("現在存在", ACTOR)
				PRINTFORML 「%TEXTR("難しいだろうに、よくやったな/うまいぞ、ありがとうな/初めて見るが……うまそうだ")%」
			ELSE
				PRINTFORML 「%TEXTR("難しいだろうに、よくやったもんだ/初めて見るが……うまそうだ")%」
			ENDIF
		ENDIF
	;おやつが手作りでない
	ELSE
		PRINTFORML 「ほう、今日のおやつは%TEXTS("おやつ当番")%が買ったのか」
		PRINTFORML 「%TEXTR("これ、安いのにうまいんだよな/高かったろうに……/見たことがないヤツだな……")%」
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K46
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「……じゃあ改めて」
	CALL PRINT_STRL, @"「これからは恋人としてよろしくな？ %CNAME("主人")%_H_」"
CASE "デートの約束"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「今度の日曜、楽しみにしてるぞ」
	CASE 1
		PRINTFORML 「今度の日曜、よろしくな」
	CASE 2
		PRINTFORML 「デート、忘れんなよ」
	ENDSELECT
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「終わったぞー」
	CASE 1
		PRINTFORML 「よし、体の調子はどうだ？」
	CASEELSE
		PRINTFORML 「さて……こんなもんか？」
	ENDSELECT
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	IF SIZE("ペニス") == SIZE("初期ペニスサイズ")
		PRINTFORML 「……やっぱりこの大きさが一番しっくりくるな」
	ELSE
		SELECTCASE CONDS("ペニスサイズ")
		CASE "普通"
			PRINTFORML 「……よし、こんなもんか？」
		CASE "巨根"
			PRINTFORML 「中々良い大きさになったじゃないか」
		CASE "馬並み"
			IF RAND:2
				PRINTFORML 「……まあ、これくらいでヤって欲しいってヤツもいたが……」
			ELSE
				PRINTFORML 「……念のため聞いとくが、大丈夫か？」
			ENDIF
		CASEELSE
			IF RAND:2
				PRINTFORML 「……ちょっとデカすぎじゃないか……？」
				PRINTFORML 「%CNAME("二人称")%が良いなら、まあ良いが……」
			ELSE
				PRINTFORML 「ここまでさせるのか……？」
				PRINTFORML 「いくら何でもヤるための大きさじゃないだろコレ……」
			ENDIF
		ENDSELECT
	ENDIF
CASE "短小化契約成立"
	IF SIZE("ペニス") == SIZE("初期ペニスサイズ")
		PRINTFORML 「……やっぱりこの大きさが一番しっくりくるな」
	ELSE
		SELECTCASE CONDS("ペニスサイズ")
		CASE "短小"
			PRINTFORML 「ほう……%CNAME("二人称")%はこれくらいの大きさが好きなのか？」
		CASE "普通"
			PRINTFORML 「まあ、これくらいが一番無難だよな」
		CASE "巨根"
			PRINTFORML 「さすがに大きすぎたか？」
		CASE "馬並み"
			PRINTFORML 「ようやくそれなりの大きさになったな……」
		CASEELSE
			PRINTFORML 「……まだこんなにデカいか……」
		ENDSELECT
	ENDIF
CASE "抜け出す"
	PRINTFORML 「おっと、お気に召さなかったか」
CASE "あなたの部屋に連れ込まれた"
	IF RAND:2
