;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K32, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = きみ_きみ/キミ_きみ
	;相手が主人ではない
	ELSE
		LOCALS = あなた_あなた
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
; 以下ライブラリ
CASE "愛液"
	LOCALS = お汁_おつゆ/おツユ_おつゆ
	SIF MARK:屈服刻印 > 1
		LOCALS += "/マン汁_まんじる"
CASE "アナル"
	LOCALS = お尻の穴_おしりのあな/アナル_あなる/アヌス_アヌス
	SIF MARK:屈服刻印 >= 2
		LOCALS += "/ケツ穴_けつあな/おケツマンコ_おけつまんこ"
CASE "陰毛"
	LOCALS = マン毛_まんげ/おけけ_おけけ/ケ_け
CASE "オナニー", "自慰"
	LOCALS = オナニー_おなにー/ひとりエッチ_ひとりえっち/ソロプレイ_そろぷれい
CASE "オマンコ"
	LOCALS = オマンコ_おまんこ/アソコ_あそこ
CASE "危険日"
	LOCALS = 危険日_きけんび/排卵日_はいらんび/赤ちゃんのデキちゃう日_あかちゃんのできちゃうひ
CASE "コンドーム", "ゴム"
	LOCALS = コンドーム_こんどーむ/ゴム_ごむ
CASE "子宮"
	LOCALS = 子宮_しきゅう/赤ちゃんの部屋_あかちゃんのへや
CASE "射精"
	LOCALS = 射精_しゃせい/お射精_おしゃせい/びゅるびゅる_びゅるびゅる/どっぴゅん_どっぴゅん
CASE "射精擬音"
	LOCALS = びゅるびゅる/びゅくびゅく/どぴゅどぴゅ/どっぴゅん/びゅーびゅー
CASE "精液"
	LOCALS = 精液_せいえき/ザーメン_ざーめん
	SIF MARK:屈服刻印 > 0
		LOCALS += "/ザー汁_ざーじる/赤ちゃんミルク_あかちゃんみるく"
	SIF MARK:屈服刻印 > 1
		LOCALS += "/ちんぽ汁_ちんぽじる/おちんぽミルク_おちんぽみるく/孕ませ汁_はらませじる"
CASE "中出し"
	LOCALS = 中出し_なかだし/ナカダシ_なかだし/膣内射精_ちつないしゃせい
CASE "ぱんつ", "パンツ"
	LOCALS = ぱんつ_ぱんつ/パンティ_ぱんてぃ/下着_したぎ
CASE "ペニス"
	LOCALS = おちんちん_おちんちん/おちんぽ_おちんぽ
CASEELSE
	;DEBUGPRINTFORML エラー！　%ARGS%は未定義の単語です
ENDSELECT

	IF STRCOUNT(LOCALS, "/")
		SPLIT LOCALS, "/", LOCALS
		LOCALS = %LOCALS:(RAND:RESULT)%
	ENDIF

RETURNF LOCALS

