				CALL PRINT_STRL, @"（や、やだ_BH_　なんでこんなに%TEXTR("湿的てる/盛ってる")%のよ、%CNAME("一人称")%……_BH_）"
				CALL PRINT_STRL,  "（まさか……欲求不満とか……？　若い子の性欲に付き合ってるうちに？　う、嘘でしょ……？_／／／_）"
				CALL PRINT_STRL,  "「……ぅー」"
				PRINTFORML %CNAME("玲子")%は有点害臊的红着脸ちらちらと%CALLNAME:PLAYER%を見ている……
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_DAILY_LP1
	ENDIF

	; 汎用
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRL,  "「………………」"
			PRINTFORML %CNAME("玲子")%は焦点の定まらない困倦眼のまま自分の胸を枕にして机に突っ伏している……
		CASE 1
			CALL PRINT_STRL,  "「ぅ……ん……。ふわぁあ……」"
			PRINTFORML %CNAME("玲子")%は間の抜けた顔でお大号的欠伸をした……
	ENDSELECT
CASE "Cosplay中"
	PRINTFORML 
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K32
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

