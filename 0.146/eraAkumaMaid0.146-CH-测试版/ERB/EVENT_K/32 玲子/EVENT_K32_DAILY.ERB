;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K32, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

$RYOKO_GREETING_LP1
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	RETURN 0
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	RETURN 0
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「朝からこんなにするなんて……もう……_BH_」_W"
				CALL PRINT_STR,  "「スッキリさせて给わね……んちゅ_BH_」_W"
		ENDSELECT
	CASE "朝騎乗位"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「んっ_BH_　ぁっ_BH_　んっ_BH_　んん～～_BH_」_W"
				CALL PRINT_STR,  "「ぁ、ん、も、もう少し、寝ててもいい……_BH_　わよ_BH_」_W"
				CALL PRINT_STR,  "「勝手、に_BH_　楽しませて、もらう、から、ぁ、ぁあっ_BH_　おぅっ_BH_」_W"
				PRINTFORMW %CALLNAME:PLAYER%に跨った%CNAME("玲子")%は大股開きで結合部を見せつけながら貪欲に快楽を貪っている……
				CALL PRINT_STR, @"（ぅぅぅ_BH_　朝、朝なのにすごい、しゅうごいガチガチ%CNAME("阴茎")%……_BH_）_W"
				CALL PRINT_STR, @"（こんなの、少し、少しでいいから_BH_　ちょっとだけでいいから_BH_　朝イチで%TEXTR("絞って/ヌイて")%あげなきゃ_BH_　きっと苦しい、よね_BH_）_W"
				CALL PRINT_STR, @"「ぅぅ_BH_　%CNAME("阴茎")%%TEXTR("ぷるぷる/びくびく")%してきたぁ……_BH_」_W"
				CALL PRINT_STR, @"「だして_BH_　このまま出して_BH_　射精して_BH_　朝勃ち%CNAME("阴茎")%の新鮮%CNAME("精液")%_BH_　いっぱいどぷどぷして_BH_」_W"
				CALL PRINT_STR,  "「ぅ、うう～～～～_BH_　お～……_BH_　おおー……_BH_」_W"
		ENDSELECT
	CASEELSE
		RETURN 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「あら、遅かったわね」"
				CALL PRINT_STRW,  "「どこか繞道でもしてきたの？」"
		ENDSELECT
	ENDSELECT

CASE "入室打招呼"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW,  "「失礼します」"
		CASE 1
			CALL PRINT_STRW,  "「遅くなっちゃった……ごめんなさいね」"
	ENDSELECT
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0 && CHARANUM > 6
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「……あら？　ひょっとして結構重役出勤しちゃったかしら」"
				CALL PRINT_STRL,  "（重役っていうかお局よね、正確には……はぁ）"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「ごめんなさい、ちょっと遅れちゃったわね」"
		ENDSELECT
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "打招呼返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, "『あら、ご一緒だったんですか？』"
		ENDSELECT

	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "『遅かったわね。みんなもう揃ってるわよ』"
			CASE 1
				CALL PRINT_STRW,  "『どこかに寄ってきたの？』"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, @"『こんにちは、%CALLNAME_ACTOR%』"
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「…………坏心眼ね、もう」"
				CALL PRINT_STRW,  "（せめて悄悄的出て行ってくれないかしら……）"
			CASE 1
				CALL PRINT_STRW, @"「お盛んなのは%CNAME("一人称")%も重々承知してるけど……」"
				CALL PRINT_STRW, @"「……%CNAME("一人称")%の分も残しておいてね？_／／／_」"
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_GREETING_LP1
	ENDIF

	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW, @"「あら、二人お出かけ？　%CNAME("一人称")%は置いてけぼりかしら」"
			CALL PRINT_STRW,  "（これは……隠れて着いて行くべきかしら？　でもバレたら拙いわね……）"
		CASE 1
			CALL PRINT_STRW,  "「あまり羽目を外しすぎないようにね」"
	ENDSELECT

CASE "便当"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR, @"「是、%TEXTR("三明治/カレー胖次/あん胖次/クロワッ桑")%。……あ、便当の方が良かった？」_W"
			CALL PRINT_STR,  "「そう？　こっちのが食べたかったら言ってね。……ふぅ」_W"
			PRINTFORM 便利店のレジ袋から未開封の%TEXTR("三明治/カレー胖次/あん胖次/クロワッ桑")%を取り出して%CALLNAME:PLAYER%に手渡した%CNAME("玲子")%は、
			PRINTFORMW %CALLNAME:PLAYER%の隣に座ると自分の分の%TEXTR("三明治/カレー胖次/あん胖次/クロワッ桑")%を取り出した。
			CALL PRINT_STR,  "「…………はぁ」_W"
			IF TALENT:MASTER:男性
				CALL PRINT_STR,  "（制服着て、異性と一緒に便当、かぁ……）_W"
			ELSE
				CALL PRINT_STR,  "（制服着て、女の子と一緒に便当、かぁ……）_W"
			ENDIF
			CALL PRINT_STR,  "（父桑の言う街道高校通ってたらこういう機会もあったのかなぁ……）_W"
			PRINTFORMW どこか物悲しげな雰囲気の%CNAME("玲子")%だが、一緒にいるのを嫌がっているわけではなさそうだ……
		CASE 1
			CALL PRINT_STR,  "「色々買ってきたけど、何が食べたい？」_W"
			PRINTFORMW %CNAME("玲子")%は手に提げた便利店のレジ袋から取り出した三明治や飯団を%CALLNAME:PLAYER%の前に並べていく……
			CALL PRINT_STR,  "「あら、多かった？　でも午後も体力使うんでしょう？」_W"
			CALL PRINT_STR,  "「……べ、別に間違ったことは言ってないわよ」_W"
		CASE 2
			CALL PRINT_STR,  "「今日はちょっと甜食很多ね。ホント、糖分が足らなくて……」_W"
			CALL PRINT_STR,  "「……さっきの授業で分からない所があったんだけど、食べながら聞いてもいい？」_W"
			CALL PRINT_STR,  "（中学卒業してからすぐに修行と研修ばっかりだったから正直授業に着いていくの辛いのよね……）_W"
	ENDSELECT
CASEELSE
	DEBUGPRINTFORM KOJO_GREETING_K32, ARGS:%ARGS%
	RETURN 0
ENDSELECT

SIF MEMO_LINECOUNT - LINECOUNT
	RETURN 1
GOTO RYOKO_GREETING_LP1

