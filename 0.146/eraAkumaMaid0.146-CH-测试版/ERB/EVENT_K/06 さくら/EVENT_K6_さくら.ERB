;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K6, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		;本来の性別で判定
		IF TRUE_SEX(MASTER) == "オトコ"
			LOCALS = 兄さん_にいさん
		ELSE
			LOCALS = 姉さん_ねえさん
		ENDIF
	;相手がメリィエーナかつ実母モード
	ELSEIF COND("実母", PLAYER)
		LOCALS = お母さん_おかあさん
	;相手が主人ではない
	ELSE
		LOCALS = あなた_あなた
	ENDIF
CASE "主人"
	;本来の性別で判定
	IF TRUE_SEX(MASTER) == "オトコ"
		LOCALS = 兄さん_にいさん
	ELSE
		LOCALS = 姉さん_ねえさん
	ENDIF
CASE "助手"
	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
ENDSELECT
RETURNF LOCALS

