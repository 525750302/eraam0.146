CASE "伸皮薬"
	;薬を使用した後の状態で分岐
	IF COND("真性包茎")
		PRINTFORMW 
	ELSEIF COND("仮性包茎")
		PRINTFORMW 
	ENDIF
CASE "豊胸薬"
	;薬を使用した後の状態で分岐
	SELECTCASE TALENT:巨乳
	;並乳
	CASE 0
		PRINTFORMW 
	;巨乳
	CASE 1
		PRINTFORMW 
	;爆乳
	CASE 2
		PRINTFORMW 
	;超乳
	CASE IS >= 3
		PRINTFORMW 
	ENDSELECT
CASE "貧乳薬"
	;薬を使用した後の状態で分岐
	SELECTCASE TALENT:巨乳
	;並乳以下
	CASE 0
		IF TALENT:貧乳
			PRINTFORMW 
		;並乳
		ELSE
			PRINTFORMW 
		ENDIF
	;巨乳
	CASE 1
		PRINTFORMW 
	;爆乳
	CASE 2
		PRINTFORMW 
	ENDSELECT
CASE "巨人の薬"
	;薬を使用した後の状態で分岐
	IF TALENT:長身
		PRINTFORMW 
	;普通身長
	ELSE
		PRINTFORMW 
	ENDIF
CASE "小人の薬"
	;薬を使用した後の状態で分岐
	IF TALENT:小柄
		PRINTFORMW 
	;普通身長
	ELSE
		PRINTFORMW 
	ENDIF
CASE "虹の薬"
	;薬を使用した後の状態で分岐
	SELECTCASE CONDS("肌の色")
	CASE "褐色肌"
		PRINTFORMW 
	CASE "色白"
		PRINTFORMW 
	CASE "青肌"
		PRINTFORMW 
	;普通
	CASEELSE
		PRINTFORMW 
	ENDSELECT
CASE "桃尻薬"
	;薬を使用した後の状態で分岐
	IF TALENT:巨尻 >= 2
		PRINTFORMW 
	ELSEIF TALENT:巨尻
