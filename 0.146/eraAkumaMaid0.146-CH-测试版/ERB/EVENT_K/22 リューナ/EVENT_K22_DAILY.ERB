			PRINTFORMW 「%TEXTR(@"向那些搭讪者们/向那些用色情的目光注视%CNAME("一人称")%的家伙们")%、展示你对这个女人拥有的主权吧…嗯、哈嗯…♪」
		ELSE
			PRINTFORMW 「这样的话、%TEXTR(@"那些搭讪者们/那些用色情的目光注视%CNAME("一人称")%的家伙们")%都会放弃吧…」
		ENDIF
	CASEELSE
		PRINTFORMW 「%TEXTR("呀嗯/呀呜/呀")%！　喂、等等…」
		PRINTFORMW 「%TEXTR("周围的人都在看啊/不要向周围炫耀啊")%、真是的…」
	ENDSELECT
;あなたが物陰フェラやキスやセクハラをしない紳士な場合はここに来ます
CASE "次は何処に行こうか？と話を振る"
	PRINTFORMW 「%TEXTR(@"嗯？　怎么办呢…/呼呼、交给%CNAME("二人称")%了/是啊…那这里怎么样？")%」
;帰り際のセリフです。さりげなくエッチに誘うのもいいでしょう
CASE "帰り際"
	IF EXP:TARGET:デート経験 == 0
		PRINTFORMW 「…这么说来%CNAME("一人称")%、好久没有像这样和谁出去玩了」
		PRINTFORMW 「所以…今天真的很开心。谢谢、%CALLNAME:MASTER%」
	ELSEIF RAND:3 == 0
		PRINTFORMW 「嗯…差不多到了%TEXTR("结束的时候/回去的时候")%了」
		IF COND("同棲中") == 0
			PRINTFORMW 「呼呼、稍微有点%TEXTR("舍不得啊/寂寞啊")%…没什么」
		ELSE
			PRINTFORMW 「那么、今天的晚饭%TEXTR("吃什么呢/怎么办呢")%」
		ENDIF
	ELSEIF RAND:2
		PRINTFORMW 「哎呀、已经这个时间了啊」
		PRINTFORMW 「…怎么办？　今天就这样回去？　还是说…」
		PRINTFORMW %CALLNAME:TARGET%看向%CALLNAME:MASTER%的视线中好像有着对什么的期待…。
	ELSE
		PRINTFORMW 「啊、果然和%CNAME("二人称")%在一起就会很开心啊♪」
		PRINTFORMW 「%TEXTR("还想两个人一起/下次还想这样")%%TEXTR("出去玩啊/出门")%啊」
	ENDIF
CASE "デート終了"
	;デートの〆にエッチをする場合
	IF TFLAG:ベッドイン
		IF RAND:3 == 0
			CALL PRINT_STRW, @"「想在这里稍微休息一下？　嗯、%CNAME("一人称")%也…_／／／_」"
			PRINTFORMW %CALLNAME:TARGET%看到%CALLNAME:MASTER%慢慢地站在了情人旅馆前面、马上就察觉到了意图。
		ELSEIF RAND:2
			CALL PRINT_STRW, "「嗯…_／／／_」"
			PRINTFORMW 被搂住腰的%CALLNAME:TARGET%发出甜美的娇声、怯怯懦懦地将自己交给了%CALLNAME:MASTER%…。
		ELSE
			PRINTFORMW 「…什么？　已经忍不住了吗？」
			IF COND("同棲中") == 0
				CALL PRINT_STRW, @"「好啊、%CNAME("一人称")%也想%CNAME("二人称")%两个人再多待一会…_／／／_」"
			ELSE
				CALL PRINT_STRW, @"「呼呼、%CNAME("一人称")%好像也没法等到回家了…来、做吧_／／／_」"
			ENDIF
		ENDIF
	ELSE
		IF COND("同棲中") == 0
			IF RAND:2
				PRINTFORMW 「那么、%TEXTR("回家小心点哦/明天见/拜拜/ciao（意大利舶来语：再见）♪")%」
				PRINTFORMW %CALLNAME:TARGET%满脸都是不舍的表情、知道看不到%CALLNAME:MASTER%背影为止都一直在挥手告别…。
			ELSE
				PRINTFORMW 「天变得很暗了啊…因为很担心、所以%CNAME("一人称")%会送你的」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「啊、肚子饿了～、早点回去吃晚饭吧♪」
			ELSE
				PRINTFORMW 「嗯、机会难得、牵着手回家吧♪」
				PRINTFORMW 虽然不知道有什么好难得的、%CALLNAME:MASTER%还是默默地和%CALLNAME:TARGET%牵起手来。
			ENDIF
		ENDIF
	ENDIF
ENDSELECT


