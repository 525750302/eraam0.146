CASE "帰り際"
	IF EXP:TARGET:デート経験 == 0
		PRINTFORMW 「…そういえば%CNAME("一人称")%、こうして誰かと遊びに出かけたのってすっごい久々だったなぁ」
		PRINTFORMW 「だから…今日は本当に楽しかったよ。ありがとね、%CALLNAME:MASTER%」
	ELSEIF RAND:3 == 0
		PRINTFORMW 「ん…そろそろ%TEXTR("お開きの時間/帰らなきゃ")%だね」
		IF COND("同棲中") == 0
			PRINTFORMW 「ふふ、ちょっと%TEXTR("名残惜しいかな/寂しいなぁ")%…なーんて」
		ELSE
			PRINTFORMW 「さーて、今日の夕飯%TEXTR("は何だろうね/どうしようかな")%」
		ENDIF
	ELSEIF RAND:2
		PRINTFORMW 「ありゃ、もうこんな時間かぁ」
		PRINTFORMW 「…どうする？　今日はもう帰っちゃう？　それとも…」
		PRINTFORMW %CALLNAME:TARGET%は何かを期待するような目を%CALLNAME:MASTER%に向けている…。
	ELSE
		PRINTFORMW 「あー、やっぱ%CNAME("二人称")%と一緒だと楽しいよ♪」
		PRINTFORMW 「また%TEXTR("二人で/今度も")%こうやって%TEXTR("出かけたい/行きたい")%なぁ」
	ENDIF
CASE "デート終了"
	;デートの〆にエッチをする場合
	IF TFLAG:ベッドイン
		IF RAND:3 == 0
			CALL PRINT_STRW, @"「ここでちょっと休んで行きたいって？　うん、%CNAME("一人称")%も…_／／／_」"
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%がおもむろにラブホテルの前で立ち止まったのを見てすぐにその意図を察したようだ。
		ELSEIF RAND:2
			CALL PRINT_STRW, "「んっ…_／／／_」"
			PRINTFORMW 腰を抱き寄せられた%CALLNAME:TARGET%は甘い声を上げ、おずおずと%CALLNAME:MASTER%に身を寄せてきた…。
		ELSE
			PRINTFORMW 「…なぁに？　ムラムラしちゃったの？」
			IF COND("同棲中") == 0
				CALL PRINT_STRW, @"「良いよ、%CNAME("一人称")%も%CNAME("二人称")%ともうちょっと一緒にいたいから…_／／／_」"
			ELSE
				CALL PRINT_STRW, @"「ふふ、%CNAME("一人称")%も家まで我慢出来なさそうだし…シてっちゃおっか_／／／_」"
			ENDIF
		ENDIF
	ELSE
		IF COND("同棲中") == 0
			IF RAND:2
				PRINTFORMW 「それじゃ、%TEXTR("気をつけて帰ってね/また明日/バイバイ/チャオ♪")%」
				PRINTFORMW %CALLNAME:TARGET%は名残惜しげな表情で、%CALLNAME:MASTER%の姿が見えなくなるまで手を振って見送っていた…。
			ELSE
				PRINTFORMW 「だいぶ暗くなっちゃったね…心配だから%CNAME("一人称")%が途中まで送ってくよ」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORMW 「あーお腹空いた〜、早いとこ帰って夕飯夕飯っと♪」
			ELSE
				PRINTFORMW 「んー、せっかくだし手ぇつないで帰ろっか♪」
				PRINTFORMW 何がせっかくなのか分からないが、%CALLNAME:MASTER%は何も言わず%CALLNAME:TARGET%と手を繋いだ。
			ENDIF
		ENDIF
	ENDIF
ENDSELECT


