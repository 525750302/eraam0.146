@KOJO_GREETING_K22, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STR, @"「%TEXTR("ふふ/……")%…_H_」_W_"
	CALL PRINT_STR, "「今日はもうずっと相手してもらうから…_H_　覚悟してね_H_」_W_"
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝の新妻プレイ"
		;恋慕
		IF TALENT:恋慕
			CALL PRINT_STRW, "「♪〜〜〜_H_　♪〜♪〜〜_H_　♪〜♪♪〜〜〜_H_　…あっ、おはよう♪」"
			PRINTFORMW 「もうすぐ朝ご飯出来るから、手洗って待っててね♪」
			PRINTFORML 朝食の匂いに釣られて起きだした%CALLNAME:PLAYER%に満面の笑顔で挨拶する%CALLNAME:TARGET%は、
			PRINTFORMW 裸エプロン姿で鼻歌まで歌いながら上機嫌で朝食の用意をしている。
			PRINTFORML %CALLNAME:TARGET%が動くたびに、まるで誘うように尻と尻尾が揺れる様子に股間が熱くなった%CALLNAME:PLAYER%は、
			PRINTFORM 一度背後から忍び寄り
			IF RAND:5 == 0
				PRINTFORMW エプロンの隙間に手を入れ胸を鷲掴みにした。
			ELSEIF RAND:4 == 0
				PRINTFORMW ペニスをクリトリスと秘裂に荒々しく擦り付けた。
			ELSEIF RAND:3 == 0
				PRINTFORMW 尻尾の付け根と尻を優しく撫でた。
			ELSEIF RAND:2 == 0
				PRINTFORMW グイっと抱き寄せると、唇を奪った。
			ELSE
				PRINTFORMW 首筋と翼の付け根に舌を這わせ、甘噛みした。
			ENDIF
			CALL PRINT_STR, "「んっ、あぁンッ…！_H_　…ふふん、やっぱりぃ…_H_　この恰好してれば来ると思ったんだよね_H_L_"
			CALL PRINT_STR, "　でもこう、実際に誘ってみてガッと来られるとやっぱ恥ずかしいなぁ…_／／／_」_W_"
			PRINTFORML 欲情した%CALLNAME:PLAYER%に後ろから抱きつかれた%CALLNAME:TARGET%は甘ったるい声を上げ、
			PRINTFORMW 恥じらいながらも振り返り、蠱惑的な流し目でニィッと微笑む。どうやら本当に誘っていたようだ。
			CALL PRINT_STR, @"「朝ご飯の前に%CNAME("一人称")%を食べたいんでしょ？_H_　…うん、良いよ_H_」_W_"
			PRINTFORML 興奮半分羞恥半分といった様子の%CALLNAME:TARGET%が%CALLNAME:PLAYER%に向けて尻を突き出し、
			CALL PRINT_STR, @"秘所を膣口ごと『くぱぁ_H_』と音がしそうなほど広げると、とろりと愛液が零れ落ちた…。_W_"
			CALL PRINT_STR, @"「%CNAME("一人称")%のココはもう準備出来てるから…ッ_H__L_"
			CALL PRINT_STR, @"　だから%CNAME("二人称")%のおチンポたくさん食べさせてへぇえぇぇぇッ！？_HH_」_W_"
			PRINTFORML 顔を真っ赤にしながらおねだりをする%CALLNAME:TARGET%の痴態に辛抱たまらなくなった%CALLNAME:PLAYER%は、
			PRINTFORMW 膣奥まで一息にペニスを突きこむと、朝食の事も忘れて%CALLNAME:TARGET%と何度も激しくまぐわった…。
		;それ以外
		ELSE
			PRINTFORMW 「キャッ！？　ちょっと危ないでしょ！　今%TEXTR("火/包丁")%使ってんだから！」
			PRINTFORML 裸エプロンで料理する%CALLNAME:TARGET%の後ろ姿にムラッと来てしまい、衝動的に後ろから抱きついた%CALLNAME:PLAYER%は、
			PRINTFORMW 珍しく可愛らしい悲鳴をあげた%CALLNAME:TARGET%に尻尾で張り倒されてしまった…。
			PRINTFORML しかしめげずに立ち上がった%CALLNAME:PLAYER%は、睨んでくる%CALLNAME:TARGET%に謝りながら優しく抱きすくめ、
			PRINTFORMW エプロンの下のしなやかな肢体を緩やかに愛撫しながら、ねだるように秘裂へ肉棒の先端を浅く出し入れした。
			CALL PRINT_STR, "「あっ、んっ、ちょっとぉ…優しくしたって駄目だってば………ああもう、しょうがないなぁ…_H_」_W_"
			PRINTFORML 困惑する%CALLNAME:TARGET%だったが、しばらく続けているとため息をつきながら料理の手を止め
			PRINTFORMW 顔を赤くしつつ自分から腰を突き出し、準備の整った秘所の奥まで%CALLNAME:PLAYER%を受け入れたのだった。
			CALL PRINT_STR, "「くッ、んぁっ、ああっ…！_H_　ほら、挿入ったよ…ちゃっちゃと射精して終わらせてね？」_W_"
			PRINTFORML %CALLNAME:PLAYER%は%CALLNAME:TARGET%の頼みに生返事を返しながら彼女の腰と尻尾をがっちりと掴むと、
			CALL PRINT_STR, @"朝食をとる時間と%CALLNAME:TARGET%の「ご飯作れないよぉ_H_」という抗議が無くなるまでたっぷり犯しぬいた…。_L_"
		ENDIF
	CASE "朝騎乗位"
		;発情期
		IF COND("発情期")
			CALL PRINT_STR, @"「あぁッ_H_　…ほーらぁ%CALLNAME:PLAYER%、起きてっ_H_起、き、てぇ_H_　んあぁっ_H_」_W_"
			PRINTFORML 甘い嬌声や粘着質な水音の混じった呼びかけと、下半身に伝わる快感で目を覚ました%CALLNAME:PLAYER%。
			PRINTFORMW 視界に飛び込んで来たのは理性が飛んだだらしない笑みを浮かべ、真っ赤に染まった%CALLNAME:TARGET%の顔だった。
			IF RAND:3 == 0 && TALENT:恋慕
				CALL PRINT_STR, "「えへへ、おはよ_H_　って言ってもおちんぽはずーっと前から起きてるけど_H_」_W_"
				CALL PRINT_STR, @"「%CALLNAME:PLAYER%ってば、寝てるのに濃いのを何度も種付けするから、%CNAME("一人称")%の子宮、もう%TEXTR("たぷたぷ/満タン")%なんだぁ…_H_」_W_"
				PRINTFORM %CALLNAME:TARGET%が言う通り、彼女の
				SIF MARK:淫紋
					PRINTFORM 淫紋が刻まれた
				PRINTFORML なめらかな下腹部は、
				PRINTFORMW %CALLNAME:PLAYER%が寝ている間に何発も射精した濃厚な精液でぽっこりと膨らんでいる。
				CALL PRINT_STR, @"「んふふ…%CALLNAME:PLAYER%の、また勃って来てるよ…？_H_」_W_"
				CALL PRINT_STR, @"「_H_」_W_"
				PRINTFORML その光景とちゅうちゅう吸い付いてくる子宮口の感触に%CALLNAME:PLAYER%の肉棒はまた元気になり、
				PRINTFORMW そのまま何度か%CALLNAME:TARGET%に求められるまま、子宮へ追加の精液を注ぎ込んだ…。
	;			PRINTFORML 
	;			PRINTFORML …その後%CALLNAME:PLAYER%が身支度をして朝食をとりに行き、部屋に残った%CALLNAME:TARGET%はベッドに横たわり、
	;			PRINTFORMW 多幸感に満ちた表情で、%CALLNAME:PLAYER%の精液で膨らんだ下腹部をゆっくり愛おしそうに擦っていた…。
	;			CALL PRINT_STR, @"（…%CALLNAME:PLAYER%と%CNAME("一人称")%の子供、出来るかな…　出来たら良いなぁ…_H_）_W_"
			ELSEIF RAND:2
				CALL PRINT_STR, "「あは、やーっと起きたぁ_H_　この寝坊助さんっ_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%はそう言いながら、きゅんきゅんと切なげに締め上げる秘所から肉棒を開放すると、
				PRINTFORMW 栓が抜けて膣内から溢れ出た精液と愛液のカクテルを指ですくって、味わうように舐め始めた…。
				CALL PRINT_STR, @"「えへへ、いただきまーす_H_　ちゅぱ、じゅる、コクン…_H_」_W_"
				CALL PRINT_STR, @"「んーっ、%CNAME("二人称")%と%CNAME("一人称")%のねばねば、すごくエッチで美味しいよぉ…_H_」_W_"
			ELSE
				PRINTFORMW だいぶ長い時間シていたのか、二人の股間では互いの分泌した体液が白く泡立ち、淫靡な%TEXTS("匂い")%が漂っている。
				PRINTFORML %CALLNAME:TARGET%が名残惜しそうに腰を上げると、開きっぱなしでくぱくぱと痙攣する秘所から、
				PRINTFORMW 精液と愛液が混じり合った粘液がごぽっと卑猥な音と共に溢れ、シーツに染みを作っていく。
				CALL PRINT_STR, @"発情して思考が蕩け切ってしまった%CALLNAME:TARGET%は「やだ、もったいない_H_」などと言いながら、_L_"
				PRINTFORMW 零れてしまった粘液を指でかき集めては膣内に戻し、自慰まがいの動きでぐちゅぐちゅと擦りこみだしたのだった…。
			ENDIF
		;それ以外
		ELSE
			CALL PRINT_STR, "「あんっ、はぁっ……_H_　ンッ、あぁんっ…！_H_」_W_"
			PRINTFORML 控えめな喘ぎ声と腰への重みと快感で目が覚めた%CALLNAME:PLAYER%が頭を起こすと、
			PRINTFORMW 真っ赤な顔で%CALLNAME:PLAYER%に跨って腰を振る%CALLNAME:TARGET%と目が合った。
			IF TALENT:恋慕
				CALL PRINT_STR, "「ん、おはよう………ふふ、良く眠れた？_H_」_W_"
				PRINTFORMW %CALLNAME:PLAYER%が起きたことに気付いた%CALLNAME:TARGET%は、腰の動きを止めずに笑顔で挨拶してきた。
				CALL PRINT_STR, @"「%CNAME("二人称")%のおちんちん、ガッチガチに勃ってて苦しそうだったよ？_H_」_W_"
				CALL PRINT_STR, @"「んんっ…今楽にしてあげるから、このまま%CNAME("一人称")%に任せて寝ててよ_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%はそう言って%CALLNAME:PLAYER%が起き上がろうとするのを手で制し、
				PRINTFORMW ぱちゅっ、ぱちゅっと愛液を飛び散らせながら、少しずつ腰のピストンを激しくしていく。
				CALL PRINT_STR, @"「ぁはっ、おちんちんっ、ビクビクしてるぅっ…_H_　ふあぁんっ、そろそろっ、出るんだっ？_H_」_W_"
				PRINTFORML %CALLNAME:PLAYER%の射精が近い事を察した%CALLNAME:TARGET%はニッと淫蕩な笑みを浮かべると、
				PRINTFORML そのまま上体を倒して%CALLNAME:PLAYER%に覆いかぶさり、手を重ね合わせて優しく握り締め、
				PRINTFORMW ペニスの先端を子宮口にめり込ませながら耳元で囁くようなおねだりをした…。
				CALL PRINT_STR, @"「朝から%TEXTR("大好きな/愛する/逞しい")%雄の%TEXTR("精液/子種")%が欲しくて寝床に押しかけてぇ…_H_」_W_"
				CALL PRINT_STR, @"「自分から腰を振っておねだりする%TEXTR("欲しがり/変態/エッチな/ドスケベ/いやらしい")%雌ドラゴンの子宮にぃ…_H_」_W_"
				CALL PRINT_STR, @"「%TEXTR("びゅーっ/どぴゅっ/どぽどぽっ/びゅるるっ/どくどく")%_H_って%TEXTR("アツアツの/どろっどろの/濃厚な")%%TEXTR("精液/子種")%を恵んで下さいっ…〜〜〜ッ_HH_」_W_"
				PRINTFORML 限界に達した%CALLNAME:PLAYER%が朝一番の濃く熱い精液を一分近くかけて子宮に注ぎ込むと、
				PRINTFORMW 覆いかぶさった%CALLNAME:TARGET%も幸せそうな表情で甲高い嬌声を上げて絶頂に達した。
				PRINTFORML 互いの絶頂が落ち着いた後、%CALLNAME:TARGET%が名残惜しげに身を起こすと、
				PRINTFORMW 膣内に入りきらなかった、ゼリーのように濃厚な白濁が結合部からドロドロと押し出されていた…。
			ELSE
				PRINTFORMW 「あっ………お、おはよう………え、えーと、これは…その………」
				PRINTFORML 目が覚めた%CALLNAME:PLAYER%とばっちり目が合ってしまった%CALLNAME:TARGET%は、
				PRINTFORMW %CALLNAME:PLAYER%に跨ったまま、しどろもどろになっている…。
				PRINTFORML %CALLNAME:PLAYER%がモゴモゴと口籠る%CALLNAME:TARGET%に続きを促すように腰を突き上げると、
				PRINTFORMW %CALLNAME:TARGET%は膣をきゅっと締め、腰を上下させながら甘い声で弁解し始めた…。
				CALL PRINT_STR, @"「ぁっ、%CNAME("二人称")%を、起こしに行ったらぁ…あッ_H_」_W_"
				CALL PRINT_STR, @"「おぁっ、%CNAME("二人称")%のおちんちんがっ、ぁは、すっごく元気になっててぇ_H_」_W_"
				CALL PRINT_STR, @"「そっ、それで%CNAME("一人称")%ッ、我慢できなくなっちゃったのおぉッ！_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%が弁解を最後まで言い切った瞬間、%CALLNAME:PLAYER%が腰を強く突き上げて射精すると、
				PRINTFORMW %CALLNAME:TARGET%も同時に仰け反りながら絶頂し、ペニスの脈動に合わせて身体と尻尾を跳ねさせた…。
			ENDIF
		ENDIF
	CASE "朝フェラ", "朝シックスナイン"
		IF COND("発情期")
			CALL PRINT_STR, "「……ングッ、ゴクッ…ぷは、はぁぁ………あっ、お、おはよう…_H_」_W_"
			CALL PRINT_STR, "「起こしに行ったらさ…その…すっごく元気で、雄のニオイが凄かったから…つい…_H_」_W_"
			IF RAND:4 == 0 && TALENT:恋慕
				PRINTFORML 発情している%CALLNAME:TARGET%は片方の手と指で飛び散った精液を掻きとって味わうように舐めながら
				PRINTFORMW もう片方の手でぐちゅぐちゅとわざと大きな水音を立てながら秘所を弄り回している…。
				CALL PRINT_STR, "「こんなに濃厚なの舐めてたら、身体がどんどん火照って来ちゃってさ…_H_」_W_"
				CALL PRINT_STR, "「もうあんまり時間無いし、今は我慢するけど…後で下の口にもお願いして良い？_H_」_W_"
			ELSEIF RAND:3 == 0
				CALL PRINT_STR, "「ふふ、今『お掃除』してあげる_H_　遠慮しないで、ほら……あーん…_H_」_W_"
				PRINTFORML 瞳を蕩かせた%CALLNAME:TARGET%はそう言いながら%CALLNAME:PLAYER%の腰を腕でがっちりと抑え込むと、
				PRINTFORMW 舌全体を精液と唾液で濡れた一物に巻き付けながら、%TEXTR("喉の奥まで/吸い付くように")%咥えこんで来た…！
				CALL PRINT_STR, "「んぶ、じゅるるっ…ぷはっ、んぐ_H_　ぢゅぽっぢゅぽっぢゅぽっ！_H_」_W_"
				PRINTFORML %CALLNAME:PLAYER%が%CALLNAME:TARGET%の明らかに『お掃除』の範疇を超えている激しい口淫に耐え切れず、
				PRINTFORML たっぷりと精液を吐き出すと、%CALLNAME:TARGET%は喉を鳴らしながら精液を飲み干していった…。
			ELSEIF RAND:2
				CALL PRINT_STR, "「今綺麗にしてあげるから、動かないでね………れろぉ……_H_」_W_"
				PRINTFORMW %CALLNAME:TARGET%はそう言って、舌先を精液と唾液にまみれた一物の先端に絡ませながら
				PRINTFORML そのまま深く咥えこみ、隅々まで丁寧に掃除してくれた…。
				CALL PRINT_STR, "「ん………じゅる……ちゅぽっ_H_　はぁ…ほら、綺麗になったよ_H_」_W_"
			ELSE
				PRINTFORML 口元に精液を付けたまま恥ずかしそうにはにかむ%CALLNAME:TARGET%の姿を見ていると、
				PRINTFORMW 再び股間に血が集まり、が硬さを取り戻していく。
				CALL PRINT_STR, "「ん、またおっきくなってる……ふふ、しょうがない、鎮めなきゃね…_H_」_W_"
				PRINTFORML %CALLNAME:TARGET%は小さく舌なめずりしてから大きく口を開くと、
				PRINTFORML ペニスを咥えこみ、ねちっこく丁寧な口使いで再び射精に導いてくれた…。
			ENDIF
		ELSE
			PRINTFORMW 「あ、おはよう…びっくりさせちゃった？」
			PRINTFORMW 「えぇっとね、起こそうと思ったら…その…立派になってたからさ…」
			PRINTFORMW 顔を真っ赤にした%CALLNAME:TARGET%はそう言い訳しながら、いまだに臨戦態勢の剛直の先端へ
			IF RAND:2
				PRINTFORM ちゅっ、と愛しそうに優しいキスを落として
			ELSE
				PRINTFORM チロチロと舌を這わせ、甘い刺激を与えて
			ENDIF
			PRINTFORMW から、少し名残惜しそうに%CALLNAME:PLAYER%の身体から離れた。
			PRINTFORMW 「え、続き？　…朝ご飯冷めちゃうから後でね…」
		ENDIF
	ENDSELECT
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
		CASE "朝キス"
			PRINTFORML 「ちゅ…お、%TEXTR("おはよう/起きたね")%」
			IF TEXTS("曜日") == "日"
				PRINTFORML 「今日は
				IF CFLAG:デートの約束
					PRINTFORML デートの日だね。たくさん楽しめると良いなぁ」
				ELSE
					PRINTFORML %TEXTR("予定とかある/暇")%？　%TEXTR("何も無いならこのままダラダラするのもアリかもね/何なら一緒に出掛けない？/どっちにしろ、とりあえず朝ご飯食べようか")%」
				ENDIF
			ELSE
				PRINTFORML 「%TEXTR(@"ふふ、良い目覚めだった？　なんちゃって♪/今日も元気に頑張ろうか？/ほらほら、目が覚めてるんならサクッと起きる！")%」
			ENDIF
		CASE "朝フェラ", "朝シックスナイン"
			IF TEXTS("曜日") == "日"
				PRINTFORML 「ふふ、スッキリしたみたいだね」
				PRINTFORM 「続き？　んー、
				IF CFLAG:デートの約束
					PRINTFORML デートの%TEXTR("帰り/後で")%ね♪」
				ELSE
					PRINTFORML %TEXTR("朝ご飯食べてから/夜になったら")%ね♪」
				ENDIF
			ELSE
				PRINTFORML 「ほーら、スッキリしたんだから起きた起きた！」
				PRINTFORML 「続きなら%TEXTR("学校/後")%でしてあげるから…ね？」
			ENDIF
		CASE "朝騎乗位"
			CALL PRINT_STR, @"「%TEXTR("ちゅっ、ちゅぅぅ…/んふっ、はぁぁ…")%_H_　%TEXTR("改めて…/ふふ、")%おはよう_H_」_L"
			IF RAND:4 && TEXTS("曜日") == "日"
				CALL PRINT_STR, @"「今日は休みだし…もうちょっと続けちゃう？_H_」_L"
			ELSE
				CALL PRINT_STR, @"「%TEXTR(@"起き抜けにこんなドロドロのが出せるなんて、%CNAME("一人称")%の本能も目覚めちゃいそうだよ…なんてね/朝から襲っちゃってゴメンね…でも朝勃ちしてるの見たら我慢出来なくってさ/続き、したい…？　ふふ、ご飯冷めちゃうから後でね…")%_H_」_L"
			ENDIF
		CASE "朝の新妻プレイ"
				PRINTFORML 「ああもう、%TEXTR("ちょっとだけご飯/せっかく作ったのに")%冷めちゃってる…」
				IF TALENT:恋慕
					PRINTFORM 「まぁ、止めなかった%CNAME("一人称")%も悪いもんね…
				ELSE
					PRINTFORM 「
				ENDIF
				PRINTFORML 今温め直すから座って待っててね」
		CASE "朝添い寝"
			IF RAND:3 == 0
				PRINTFORMW 「んんぅ…？　もう朝かぁ………」
			ELSEIF RAND:2
				PRINTFORMW 「ふぁあぁ〜………う〜ん………おはよー………」
			ELSE
				PRINTFORMW 「う〜ん………あー…よく寝たぁ………」
			ENDIF
			SELECTCASE TEXTS("季節")
				CASE "冬"
					PRINTFORMW 目を覚ました%CALLNAME:TARGET%は、尻尾まで使って抱きしめていた%CALLNAME:MASTER%を開放しながら起き上がった。
