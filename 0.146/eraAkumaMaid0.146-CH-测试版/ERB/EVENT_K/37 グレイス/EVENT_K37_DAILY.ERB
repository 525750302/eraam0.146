;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K37, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝シックスナイン"
		PRINTFORMW 「ちゅぅっ…ちゅちゅっ…%TEXTR("あら/ふふ")%、%TEXTR("おはようございます/やっ…と起きましたわね")%♪」
		PRINTFORMW 「%TEXTR(@"ごめんなさい、%CNAME("二人称")%の%TEXTR("御子息/御立派様/おチンポ")%をつまみ食いしてしまって/もっとも、%CNAME("二人称")%の%TEXTR("御子息/御立派様/おチンポ/おチンチン")%はとっくに早起きしてらしたけど…")%♪」
	CASE "朝騎乗位"
		PRINTFORMW 「%TEXTR("あら/ふふ")%、上から失礼して…おはようございます♪」
		PRINTFORMW 「%TEXTR(@"ごめんなさい、%CNAME("二人称")%の%TEXTR("御子息/御立派様/おチンポ")%をつまみ食いしてしまって/もっとも、%CNAME("二人称")%の%TEXTR("御子息/御立派様/おチンポ/おチンチン")%はとっくに早起きしてらしたけど…")%♪」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		IF RAND:2
			PRINTFORMW 「今日は体育の授業がありますわね」
			PRINTFORMW 「%CNAME("一人称")%大活躍の予感ですわ！」
		ELSE
			PRINTFORMW 「今日は体育の授業がありますわね。体育着は忘れずにお持ちになりまして？」
		ENDIF
	ELSEIF COND("水泳")
		IF RAND:2
			PRINTFORMW 「今日は水泳の授業がありますわね」
			PRINTFORMW 「ふふ、%CNAME("一人称")%の水着姿に見とれて溺れないように注意ですわよ♪」
		ELSE
			PRINTFORMW 「今日は水泳の授業がありますわね。水着は忘れずにお持ちになりまして？」
		ENDIF
	ELSE
		IF RAND:2
			PRINTFORMW 「あ、あら、おはようございます！」
			PRINTFORMW （むぅ、おはようエアバズーカをぶっ放すチャンスを失いましたわ…）
		ELSE
			PRINTFORMW 「おはようございます%TEXTR("＿/ッ")%！」
			PRINTFORMW 「今日も良い天気ですわよ！」
		ENDIF
	ENDIF
CASE "一番乗り"
	IF RAND:3 == 0
		PRINTFORMW 「今なら人目を気にせずセクハラし放題ですわよ♪　ほらほら♪」
	ELSEIF RAND:2
		PRINTFORMW 「あら、二人っきりですわね」
	ELSE
		PRINTFORMW 「他の方はどうしたのかしら？」
	ENDIF
CASE "ただいま"
	IF RAND:2
		PRINTFORMW 「只今帰りましたわ」
	ELSE
		PRINTFORMW 「あら、良い匂い…%TEXTR("玄関の芳香剤、変えました？/今日のおゆはんかしら…(ｸﾞｩｩ…)")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF RAND:3
			PRINTFORMW 「お帰りなさいませ、%TEXTR("今日も学校楽しかったですわね！ねっ！/お先に寛がせていただいてますわ")%」
		ELSEIF RAND:2
			PRINTFORML 「まったく…だいぶ寄り道してらしたようですわね？」
			PRINTFORMW 「%CNAME("一人称")%、すこーしだけ心配しましたのよ？」
		ELSE
			PRINTFORML 「あら、お帰りなさいませ〜」
			PRINTFORMW 玄関のドアを開けると、%CALLNAME:TARGET%が僅かに身体を宙に浮かせて漂っていた…。
		ENDIF
	ELSE
		IF RAND:3
			PRINTFORMW 「あら、お先に寛がせていただいてますわ」
		ELSEIF RAND:2
			PRINTFORML 「だいぶ重役出勤でしたわね」
			PRINTFORMW 「補習でしたら%CNAME("一人称")%の分かる範囲で教えますわよ？」
		ELSE
			PRINTFORMW 「もう、せっかくのお茶が温くなっちゃいましたわ…」
		ENDIF
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		PRINTFORMW 
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		IF RAND:3 == 0
			PRINTFORMW 「…何をやってるのかしら？」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の様子を伺っている…。
		ELSEIF RAND:2
			PRINTFORMW 「ふむ…やはり%CNAME("二人称")%ってばもしかしなくても変態ですわね！」
			PRINTFORMW 呆れたように言う%CALLNAME:TARGET%だが、%CALLNAME:MASTER%を見る目は愉快そうに弓なりに反っている…。
		ELSE
			PRINTFORMW 「あら失礼…いえ、%CNAME("一人称")%にお構いなく続けなさいな」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%達を面白そうに眺めながら続きを促して来た…。
		ENDIF
	ELSE
		SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
		CASE 1
			SELECTCASE CSTR:MASTER:PREV_DAILYCOM
			CASE "甘い言葉"
				IF RAND:2
					PRINTFORMW 「あらあら、お熱いこと…ちょっと妬けちゃいますわ」
					PRINTFORMW %CALLNAME:TARGET%はそう言いながら唇を尖らせるが、その目は楽しそうに笑っている。
				ELSE
					PRINTFORMW 「これは失礼…いえ、%CNAME("一人称")%にお構いなく続けてくださいな」
					PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%達を面白そうに見つめながら続きを促して来た…。
				ENDIF
	;		CASE "挨拶クンニ", "挨拶フェラ"
			ENDSELECT
		;もてなす
		CASE 2
		;隙を見せる
		CASE 3
			SELECTCASE CSTR:MASTER:PREV_DAILYCOM
				CASE "Ｈのお誘い", "押し倒し"
					PRINTFORMW 「あらあら、近づけないほどに熱々ですわね！」
					PRINTFORMW %CALLNAME:TARGET%は楽しそうな顔で%CALLNAME:MASTER%達を遠巻きに眺めている。
				CASE "膝枕", "抱きつき", "膝を枕にしてくる"
					IF RAND:2
						PRINTFORMW 「あらあら、本当にお熱いこと…妬けちゃいますわ」
						SIF TALENT:恋慕
							PRINTFORMW 「%CNAME("一人称")%の惚れた方ですから、しょうがないですわね」
						PRINTFORMW %CALLNAME:TARGET%はそう言いながら唇を尖らせるが、その目は楽しそうに笑っている。
					ELSE
						PRINTFORMW 「あら、砂糖菓子の如く甘々な空気ですわね。嫌いじゃありませんわ」
						PRINTFORMW %CALLNAME:TARGET%は甘い雰囲気を漂わせる%CALLNAME:MASTER%達をニコニコしながら見つめている。
					ENDIF
	;			CASE "パンツ見せ", "股間擦り付け", "手淫", "股間擦り付け", "口淫", "キス", "おっぱい押し付け"
					
			ENDSELECT
		;パンツ
		CASE 4
			IF RAND:3 == 0
				PRINTFORMW 「ふむ…やはり%CNAME("二人称")%ってばもしかしなくても変態ですわね！」
				PRINTFORMW 呆れたように言う%CALLNAME:TARGET%だが、%CALLNAME:MASTER%を見る目は愉快そうに弓なりに反っている…。
			ELSEIF RAND:2
				PRINTFORMW 「…あまりそういう事はやらない方がよろしいのではなくて？」
				SIF TALENT:恋慕
					PRINTFORMW 「ほら、%CNAME("一人称")%のパンツならいくらでも見せて差し上げますわよ？」
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の行為をやんわりと咎めてきた。
			ELSE
				PRINTFORMW 「訴えられたり捕まったりしないようにお気をつけなさいな」
				SIF TALENT:恋慕
					PRINTFORMW 「もしそうなったら%CNAME("一人称")%、悲しいですわ…」
				PRINTFORMW %CALLNAME:TARGET%はパンツを堪能する%CALLNAME:MASTER%を心配そうに見つめている…。
			ENDIF
		ENDSELECT
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「大変お待たせしましたわ」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「うふ、%CNAME("一人称")%を呼びまして？」
	ELSE
		PRINTFORMW 「皆様ごきげんよう！」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『あら先生、どうもこんにちは』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『あら、ずいぶん遅かったですわね？』
	ELSE
		PRINTFORMW 『ふふ、今日も元気そうで何よりですわ』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
	IF RAND:3 == 0
		PRINTFORMW 「%CNAME("一人称")%を置いて逢瀬ですの？ふ〜ん…」
	ELSEIF RAND:2
		PRINTFORMW 「あら、二人揃って御用事かしら？いってらっしゃいませ〜」
	ELSE
		PRINTFORMW 「まぁ…懇ろなんですのね」
	ENDIF
CASE "お弁当"
	IF COND("同棲中")
		IF RAND:3 == 0
			PRINTFORML 「はい、どうぞ。今日のは自信ありますわよ！」
			PRINTFORMW （…ほとんどフェリシエルが作ってくれたことは秘密ですわ）
		ELSEIF RAND:2
			PRINTFORML 「ごめんなさい、きょ、今日は焦ってたからいつにも増して酷い出来ですわ…」
			PRINTFORMW 「次はもっと手作りの…多分ちゃんとしたのを作りますわ…きっと」
		ELSE
			PRINTFORML 「つ、作ってきましたわよ！とくとご賞味くださいまし！」
			PRINTFORMW 「…出来るなら見た目の悪さなどには目をつぶってくれるとありがたいですわ…」
		ENDIF
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「ささ、早く食べましょう？%CNAME("一人称")%、もうお腹ペコペコですわぁ〜」
			PRINTFORMW %CALLNAME:TARGET%はいそいそと弁当を取り出して%CALLNAME:MASTER%の隣に陣取った。
		ELSEIF RAND:2
			PRINTFORMW 「はい、どうぞ。%CNAME("二人称")%の好物だけ詰め込んでもら…みましたわ！」
		ELSE
			PRINTFORMW %CALLNAME:TARGET%が持ってきたのは三段重ねの重箱だった。
			PRINTFORMW 「つ、つい作りすぎてしまいましたの。皆も呼んで食べましょう？」
		ENDIF
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K37, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「皆さん、これからよろしくお願いいたしますわ！！」
	PRINTFORML %CALLNAME:TARGET%は眩しい笑顔で部員一人一人と順番に握手をしている。
CASE "汗だく"
	IF CHECK_CLO("体操服姿") && COND("ブラ透け") && EQUIP:ブラ && RAND:3 == 0
		PRINTFORML 「あら、服が透けてブラが見えますわね」
		PRINTFORML 「こういうのはお好きかしら？」
	ELSEIF RAND:2
		PRINTFORML 「はぁ〜、髪が汗でビショビショですわ」
		PRINTFORML 「突風で吹き散らしてしまうのが一番かしら…」
	ELSE
		PRINTFORML 「ふぅ…ハッスルし過ぎて溶けそうですわ〜…」
	ENDIF
CASE "寝起き"
	IF EXP_GAIN("睡眠姦経験")
		IF RAND:3 && CHOSE_DEVENT("睡眠姦Ｖ中出し")
			PRINTFORML 「うーん………ふぁ〜………」
			PRINTFORML （何だかお腹に幸せな重みがありますわね…）
			PRINTFORML %CALLNAME:TARGET%は寝てる間に子種を注がれた下腹部をゆっくり擦っている。
		ELSEIF RAND:2
			PRINTFORML 「………………」
			PRINTFORML %CALLNAME:TARGET%は顔を赤く染めながらぼんやりと%CALLNAME:MASTER%の事を見ている。
			PRINTFORML （%CNAME("一人称")%ったら%CALLNAME:MASTER%に抱かれる夢なんて見ちゃいましたわ…）
		ELSE
			PRINTFORML 「ん………」
			PRINTFORML （何でしょう…？　オマンコが凄く濡れてますわ…）
			PRINTFORML %CALLNAME:TARGET%は首を傾げながらしきりに太ももを擦り合わせている。
		ENDIF
	ELSE
		IF RAND:4 == 0
			PRINTFORML 「あら…何だか元気が無いですわね？　顔色もあまり良くないようで…」
			PRINTFORML 「冬の寒さに負けそう？それは鍛練が…鍛え方が足りないのですわ！！」
			PRINTFORML 「身体を鍛えれば冬の厳しい寒さにも負けませんわッ！！」
			PRINTFORML 「さあ、準備は出来まして！？行きますわよ、いざ雪山へ！！！」
			PRINTFORML 
			PRINTFORML 「っていう悪夢を見ましたわ…何でマッチョになってたのかしら…」
		ELSEIF RAND:3 == 0
			PRINTFORML 「ふわぁ〜〜〜………」
			PRINTFORML %CALLNAME:TARGET%は大欠伸をすると額に張り付いた髪を軽く払っている。
		ELSEIF RAND:2
			PRINTFORML 「（ゴキッ、ボキボキ…）…あらごめんなさい、見苦しい所を見せてしまいましたわね」
			PRINTFORML %CALLNAME:TARGET%は首と肩を鳴らして眠気を覚まそうとしている。
		ELSE
			PRINTFORML 「んん〜っ、良く寝ましたわ〜」
			PRINTFORML %CALLNAME:TARGET%は豊かな胸を強調するように伸びをしている。
		ENDIF
	ENDIF
CASE "コスプレ中"
	PRINTFORML 
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K37
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K37, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "デートの約束"
	PRINTFORML 「うふふっ♪デート、デートッ♪」
	PRINTFORML 「週末が待ちきれませんわぁ〜♪」
;これは悪魔のみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K37, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "抜け出す"
	SELECTCASE SAVESTR:現在位置
		CASE "ヤリ部屋"
			PRINTFORML 「あら、もう一部屋ありましたの？」
			PRINTFORML 「こちらはまさしくそういうことをする部屋、ですわね」
			PRINTFORML 「うふふ、沢山可愛がってくださいまし♪」
		CASE "体育倉庫", "更衣室", "保健室"
			PRINTFORML 「まさかここでなさるの？そんな…」
			PRINTFORML 「…先生方や他の生徒の方々にバレたりしないか、スリル満点ですわね♪」
		CASE "ラブホテル"
			PRINTFORML 「ここまでの道もデートしているみたいでとても楽しかったのですが…」
			PRINTFORML 「やはりここでの本命が一番楽しみですわね♪」
			PRINTFORML 「先にシャワー浴びてますわね♪　後で入って来ても良いですわよ♪」
	ENDSELECT
CASE "あなたの部屋に連れ込まれた"
	IF RAND:2
		PRINTFORML 「ｽﾝｽﾝ…%CNAME("二人称")%の部屋の匂い、何だか落ち着きますわ…」
	ELSE
		PRINTFORML 「あら、%CNAME("一人称")%を連れ込んで何をするつもりかしら？」
		IF RAND:2
			PRINTFORML 「一緒に勉強したいなら分かる範囲で何でも教えますわよ♪」
		ELSE
			PRINTFORML 「ふふ、おしゃべりでもエッチなことでも%CNAME("一人称")%は大歓迎ですわ♪」
		ENDIF
	ENDIF
CASE "自室に招待"
	PRINTFORML 「ふふ、%CNAME("一人称")%の部屋に来てくださってありがとうございます♪」
	IF RAND:3 == 0
		PRINTFORML 「そうそう…良かったらこちらの%TEXTR("ケープ/ブランケット")%、使ってくださいませ」
		PRINTFORML 「%TEXTR("一人称")%の手編みですの。持って帰って使っても構いませんわよ♪」
	ELSEIF RAND:2
		PRINTFORML 「紅茶の匂いがする？ええ、たった今淹れたところですの。もちろん%CNAME("二人称")%の分もありますわよ♪」
		PRINTFORML 「はいどうぞ、%TEXTR("アッツアツな/熱い/激熱な/滅茶苦茶ＨＯＴな")%ので気をつけてくださいませ」
	ELSE
		PRINTFORML 「それで何をいたしましょうか？」
		IF RAND:3 == 0
			PRINTFORML 「ふふ、おしゃべりでもエッチなことでも%CNAME("一人称")%は大歓迎ですわよ♪」
		ELSEIF RAND:2
			PRINTFORML 「一緒に勉強したいなら分かる範囲で何でも教えますわよ♪」
		ELSE
			PRINTFORML 「…とりあえずお茶を用意いたしますわね。適当な所に座って待っていてくださいませ」
		ENDIF
	ENDIF
CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 「あら、%CNAME("二人称")%に%CNAME(CSVCALLNAME(NO:ASSI) )%。わざわざ訪ねて来てくださったのね」
		IF RAND:2
			PRINTFORML 「二人揃ってどうかなさいましたの？」
		ELSE
			PRINTFORML 「今お茶を用意いたしますわ。適当な所に座って待っていてくださいまし」
		ENDIF
	ELSE
		PRINTFORML 「あら、%CNAME("二人称")%。わざわざ訪ねて来てくださったのね」
		PRINTFORML 「一体どうなさいましたの？」
		IF RAND:3 == 0
			PRINTFORML 「…顔が見たかった？まあ、嬉しいですわ♪」
		ELSEIF RAND:2
			PRINTFORML 「一緒に勉強したいなら何でも教えますわよ♪」
		ELSE
			PRINTFORML 「とりあえずお茶を用意いたしますわね。適当な所に座って待っててくださいまし」
		ENDIF
	ENDIF
CASE "アドバイザー部屋訪問"
	PRINTFORML 「お二人は%CNAME("一人称")%がいないものと思って振舞ってくださいませ」
	PRINTFORML 「ええ、%CNAME("一人称")%はここで見守ってますわ♪」
	PRINTFORML %CALLNAME:TARGET%はニヤニヤしながら二人の視界にギリギリ入るか入らないかの所に陣取った…。
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 
CASE "オナバレ"
	PRINTFORML 
CASE "ミニスカパンチラ"
	PRINTFORML 「どうしたんですの？　ニヤニヤ笑って…」
	PRINTFORML 「もしかして今%CNAME("一人称")%のパンツが見えてらして？」
