;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K48, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		CALL PRINT_STRW, @"「ん、ふ……_H_ あ、%CNAME("二人称")%……起こしちゃいましたか……？」"
		PRINTFORMW 「すみません……勝手に使ってます……っ」
	CASE "朝クンニ"
		PRINTFORMW 「ん、ふ……あ、起きちゃいました？」
		PRINTFORMW 「すみませんね、ちょっと……遊びたくなっちゃって。ふふ」
	CASE "朝足コキ"
		PRINTFORMW 「あ……やっと起きましたか、%CNAME("二人称")%？」
		PRINTFORMW 「くふふ……全然起きないんだから、ちょっと楽しくなってたところですよ」
	CASE "朝の新妻プレイ"
		PRINTFORMW 「あ、ちょ……邪魔しないでくださいってば」
		PRINTFORMW 「もう……料理ダメになっても知りませんからね。ちゃんと全部食べるんですよ？」
	CASE "朝パイズリ"
		PRINTFORMW 「んしょ……と。お目覚めですか？」
		PRINTFORMW 「すみませんね、ちょっと個人的な練習台に使わせてもらってます……」
	CASE "朝キス", "朝添い寝"
		PRINTFORMW 「……その……おはようございます」
	CASE "朝フェラ", "朝シックスナイン"
		PRINTFORMW 「んむ……ふぁ……おはようございます、%CNAME("二人称")%」
		PRINTFORMW 「すみませんね。なんかやたら目立ってたので、つい……ね」
	CASE "朝騎乗位", "朝Ａ騎乗位"
		CALL PRINT_STRW, "「んっ、ふっ……_H_ あぁ……おはようございます……」"
		PRINTFORMW 「くふふ……たまにはこっちが勝手に使っても構いませんよね……？」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	PRINTFORMW 「ほら、%TEXTR("いつまで寝てるんですか。/わざわざ起こしにきてあげたんだから、")%さっさと起きてくださいよ」
CASE "一番乗り"
	PRINTFORMW 「%TEXTR("あれ/ええ")%……%CNAME("二人称")%だけ……？」
CASE "ただいま"
	PRINTFORMW 「……%TEXTR("どうも/ただいま/帰りました")%」
CASE "既に入室"
	PRINTFORM 「ああ……
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW お帰りなさい」
	ELSE
		PRINTFORMW どうも。お邪魔してます」
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		PRINTFORMW 「……%TEXTR(@"来ましたけど、%CNAME("二人称")%/お待たせしました")%」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		PRINTFORMW 「%TEXTR("どうも/失礼します")%……%TEXTR("って、なにやってんですか/げ、タイミングわる……")%」
	ELSE
		PRINTFORMW 「%TEXTR("お邪魔/失礼")%します……」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("……すいません、遅くなりました/ん……集まってますね")%」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「……来ました」
	ELSE
		PRINTFORMW 「……どうも」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『あ、先生……%CNAME("どうも/こんにちは")%』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF FIND_COND("入室挨拶中", "真")
			PRINTFORMW 『ん……あぁ、お姉ちゃん。何してたの？』
		ELSEIF FIND_COND("入室挨拶中", "灯")
			PRINTFORMW 『あぁ、お兄ちゃん。遅かったね』
		ELSE
			PRINTFORMW 『あぁ……どうも。遅かったですね』
		ENDIF
	ELSEIF FIND_COND("入室挨拶中", "真")
		PRINTFORMW 『ん……なんだ、お姉ちゃんか』
	ELSEIF FIND_COND("入室挨拶中", "灯")
		PRINTFORMW 『あぁ、お兄ちゃんか』
	ELSE
		PRINTFORMW 『あぁ……%TEXTR("どうも/こんにちは/いらっしゃい")%』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「……なんの用です？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ちょっと……通りますよ」
	ELSE
		PRINTFORMW 「あぁ、いたんですか。ちょっと失礼します」
	ENDIF
CASE "抜け出された"
	PRINTFORMW 「はぁ……お%TEXTR("元気/盛ん")%ですね……」
CASE "お弁当：地の文前"
	IF TALENT:反抗的 == 0 && CEVENT("初めてのお弁当") == 0
		PRINTFORMW 「……はい」
		PRINTFORMW 昼休み、%CALLNAME:MASTER%との待ち合わせ場所に現れた%CALLNAME:TARGET%は目を逸らしながらお弁当を差し出した。
		PRINTFORMW 「じゃあ%CNAME("一人称")%、もう行きますから」
		PRINTFORMW いきなり帰ろうとする%CALLNAME:TARGET%を、%CALLNAME:MASTER%は慌てて引き止める。
		PRINTFORMW 「な、なんですか。%CNAME("一人称")%もうお腹いっぱいですから、一人で食べててくださいよ」
		PRINTFORMW 理由をつけて帰ろうとする%CALLNAME:TARGET%をなんとか座らせ、%CALLNAME:MASTER%はいよいよお弁当の蓋をあけた。
		PRINTL
		PRINTFORMW ……おにぎりと卵焼きとソーセージのみだ……！
		PRINTL
		PRINTFORMW 「わ、悪かったですね。女の子っぽくなくて」
		PRINTFORMW %CALLNAME:TARGET%は膝を抱えて顔を背けてしまうが、%CALLNAME:MASTER%は綺麗に卷けている卵焼きに釣られて一口齧り付いた。
		PRINTFORMW ……味の濃さは気になるが、なかなかおいしい……！
		PRINTFORMW %CALLNAME:MASTER%は上手にできていることを伝えようと%CALLNAME:TARGET%に声をかけた。
		PRINTFORMW 「――けふっ」
		PRINTFORMW ……%CALLNAME:TARGET%の口から、控えめなゲップが漏れた。
		PRINTFORMW 「もう……卵は見たくないです……」
		PRINTFORMW ……頑張って作ってくれたようだ……
		CALL SET_CEVENT, "初めてのお弁当", TARGET
	;↑を閲覧済みだと↓の分岐は使われない
	ELSEIF CEVENT("初めてのお弁当：冷凍") == 0 && CEVENT("初めてのお弁当") == 0
		PRINTFORMW 「はい」
		PRINTFORMW 昼休み、約束通り%CALLNAME:TARGET%は%CALLNAME:MASTER%のお弁当を作って持ってきてくれた。
		PRINTFORMW さっそく中身を見てみると、とても整った彩りのおかずが散りばめられている。
		PRINTFORMW 一口食べると、不思議と覚えのある無難な味が口の中に広がる……
		PRINTFORMW 「それ、冷凍ですから」
		PRINTFORMW ……どうやらすべて、解凍したお弁当用のおかずらしい……
		PRINTFORMW 「あ、%CNAME("一人称")%のことは気にしないでいいですから」
		PRINTFORMW そう言って%CALLNAME:TARGET%はバランス栄養食を取り出した……
		CALL SET_CEVENT, "初めてのお弁当：冷凍", TARGET
	;ELSEIF TALENT:反抗的 == 0
	ELSE
		PRINTFORMW 
		PRINTFORMW 「……」
		PRINTFORMW 自分の手料理を満足そうに食べる%CALLNAME:MASTER%の横顔を、%CALLNAME:TARGET%はぼーっと眺めていた。
	ENDIF
	TFLAG:地の文スキップ = 1
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	CALL EMOTION_PLUS, "びっくり", TARGET
	PRINTFORMW 「は？ ちょ、なに急におっ始めようとしてんですか？」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K48, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「どうも……%NAME:TARGET%です。よろしく」
	PRINTFORML %CALLNAME:TARGET%は簡素な挨拶だけして、適当に空いた椅子に座り込んだ。
CASE "汗だく"
	PRINTFORML 「%TEXTR(@"あっつ/つかれた/しぬ")%……」
	PRINTFORML %CALLNAME:TARGET%は息も絶え絶えだ……
CASE "寝起き"
	PRINTFORML 「うぅん……んぁ？」
	PRINTFORML 「……何かしました？」
CASE "コスプレ中"
	PRINTFORML 「あんまりじろじろ見ないでくださいよ。こっちは中学生ですからね」
	PRINTFORML %CALLNAME:TARGET%は気丈に振る舞っているが、顔がほんのり赤い……
CASE "おやつの感想"
	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
		PRINTFORML 「ああ、これ。%TEXTR("置いときますから/好きに食べてください")%」
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				PRINTFORML 「あ……おいし……」
				PRINTFORML 無愛想な%CALLNAME:TARGET%の表情が僅かに揺れたように見えた。
			ELSE
				PRINTFORML 「へぇ……上手ですね、これ」
				PRINTFORML %CALLNAME:TARGET%は感心しながらおやつをちまちまと食べている。
			ENDIF
		ELSE
			PRINTFORML 「あ、手作りですか……じゃあ、ひとつだけ」
			PRINTFORML 置かれたおやつをひょいとつまみ、%CALLNAME:TARGET%は席についた。
		ENDIF
	;おやつが手作りでない
	;ELSE
		;PRINTFORML おやつ当番へのコメントもおやつ自体へのコメントも幅が広すぎて難しすぐる…
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K48
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	IF TALENT:相思相愛
		PRINTFORML %CALLNAME:TARGET%は泣き腫らした目のままそっぽを向いている……
	ELSE
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の言葉を聞こえなかったふりをしている……
	ENDIF
CASE "デートの約束"
	IF TALENT:恋慕
		PRINTFORML 「…………～♪」
		PRINTFORML %CALLNAME:TARGET%は小さな声で鼻歌を歌っているようだ。
	ELSE
		PRINTFORML 「もう……日曜はゆっくりしようと思ったのに……」
		PRINTFORML 口では悪態をついてるが、%CALLNAME:TARGET%は少し落ち着きがなさそうに見える……
	ENDIF
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K48, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "抜け出す"
	IF CFLAG:ムラムラ
		PRINTFORML 「もう……早く済ませましょうよ……」
		PRINTFORML %CALLNAME:TARGET%は熱っぽい顔をしている……
	ELSE
		PRINTFORML 「まったく……こんなちみっこい後輩を連れ込んで、何考えてるんです？」
		PRINTFORML %CALLNAME:TARGET%は呆れた目をしている……
	ENDIF
;あなたと一緒に、あなたの部屋に移動した
CASE "あなたの部屋に連れ込まれた"
	IF CFLAG:ムラムラ
		PRINTFORML 「それで……%CNAME("一人称")%に何をするつもりなんですか、%CNAME("二人称")%？」
		PRINTFORML %CALLNAME:TARGET%から熱っぽい視線を感じる……
	ELSE
		PRINTFORML 「なんかありませんか？ 漫画とか、ゲームとか」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%そっちのけで寛ごうとしている……
	ENDIF
;あなたと一緒に、パートナーの部屋に移動した
CASE "自室に招待"
	IF CFLAG:ムラムラ
		PRINTFORML 「……で、%CNAME("一人称")%の部屋になんの用ですか？ %CNAME("二人称")%」
		PRINTFORML %CALLNAME:MASTER%を睨みつける%CALLNAME:TARGET%の顔は少し赤い……
	ELSE
		PRINTFORML 「部屋のもの勝手に散らかしたりしないでくださいよ」
		PRINTFORML 「%TEXTR(@"思春期の部屋なんですから。一応/ちゃんと言うこと聞けますよね？ %CNAME("一人称")%より大人なんだから")%」
	ENDIF
;パートナーが既に居る部屋に、あなたが移動した
CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		IF FIND_COND("真")
			PRINTFORML 「……何しに来たの？ お姉ちゃん」
			PRINTFORML 『もう、またそんな冷たい反応……』
			PRINTFORML 互いの態度に、二人は溜め息をついている……
		ELSEIF FIND_COND("灯")
			PRINTFORML 「なんだ、お兄ちゃんか……」
			PRINTFORML 『おーう、邪魔するぜー』
			PRINTFORML 気安く部屋に上がり込む%CALLNAME:FINDCHARA(NO, 35)%に、%CALLNAME:TARGET%は溜め息をついている……
		ELSE
			PRINTFORML 「あれ。今日は二人ですか？」
			PRINTFORML 同伴者の存在に、%CALLNAME:TARGET%は少し慌てている。
		ENDIF
	ELSE
		PRINTFORML 「%TEXTR(@"うわぁ、来た……/なにずけずけ入ってきてんですか/……何しに来たんです？")%」
		PRINTFORML %CALLNAME:TARGET%は冷たい目で迎えてくれた……
	ENDIF
CASE "アドバイザー部屋訪問"
	SELECTCASE SAVESTR:現在位置
	CASE "真の部屋"
		PRINTFORML 『あれ、莉ちゃん？ 部長さんと一緒に来てくれたんだ？』
		PRINTFORML 「別に……勝手に連れてこられただけだけど」
		PRINTFORML 柔らかな表情の%CALLNAME:FINDCHARA(NO, 34)%から、%CALLNAME:TARGET%は目を逸らした。
	CASE "灯の部屋"
		PRINTFORML 『おーう、お前らか。遊びに来たんか？』
		PRINTFORML 「いや、知らないけど。%CNAME("二人称")%に聞いてよ」
		PRINTFORML %CALLNAME:FINDCHARA(NO, 35)%と%CALLNAME:TARGET%はニヤニヤしながら%CALLNAME:MASTER%を見ている……
	CASEELSE
		PRINTFORML 「どうもー、おじゃましまーす」
	ENDSELECT
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「あーあ、疲れた……」
CASE "オナバレ"
	PRINTFORML 「はぁぁ……狙って見にきてませんか、%CNAME("二人称")%」
	PRINTFORML %CALLNAME:TARGET%から恨みがましい視線が飛んできている……
CASE "ミニスカパンチラ"
	IF EQUIP:パンツ == 0
		PRINTFORML 「――！」
		PRINTFORML 見られたことに気付いた%CALLNAME:TARGET%の顔が、みるみる赤くなっていく……
	ELSE
		PRINTFORML 「ちょ、なに勝手に見てんですかっ」
		PRINTFORML %CALLNAME:TARGET%はとっさにスカートを抑えながら飛び退いた。
	ENDIF
CASE "ローライズヒップ"
	PRINTFORML 「……あんまり見ないでください。ガキのケツですよ」
	PRINTFORML %CALLNAME:TARGET%は咎めるような視線を向けてくる……
CASE "スカートをめくる"
	IF EQUIP:パンツ == 0
		PRINTFORML 「……なんですか。なんか文句あるんですか」
		PRINTFORML %CALLNAME:TARGET%は恥じらい混じりの赤い顔で睨みつけてくる……
	ELSE
		PRINTFORML 「まったく、なんでこんなちみっこい相手のスカートなんかめくるんですか……」
		PRINTFORML 吐き捨てる%CALLNAME:TARGET%の顔は平静を装いながらも赤くなっている……
	ENDIF
CASE "パンツチェック"
	IF COND("合意：パンツチェック")
		PRINTFORML 「……もういいですか？」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%を急かしている。
	ENDIF
CASE "アソコチェック"
	IF COND("合意：アソコチェック")
		PRINTFORML 「……独裁者め……」
		PRINTFORML %CALLNAME:TARGET%は顔を背けて歯噛みしている……
	ENDIF
CASE "性処理を頼む"
	PRINTFORML 「ふぅ……あんまり疲れるようなことさせないでくださいよ……」
	PRINTFORML %CALLNAME:TARGET%はまだ少し息が荒い……
CASE "ながら性処理"
	PRINTFORML 「こんなので嬉しいんですか、%CNAME("二人称")%？」
	PRINTFORML %CALLNAME:TARGET%は呆れ顔で首を傾げている。
CASE "性欲解消を手伝う"
	PRINTFORML 「……その、すみませんね。手間を取らせて」
	PRINTFORML %CALLNAME:TARGET%は目を合わせず口ごもっている……
CASE "お掃除フェラ"
	PRINTFORML 「……っ……ふぅ……満足しました？」
	PRINTFORML %CALLNAME:TARGET%は口元を拭い、さっと%CALLNAME:MASTER%から離れていった。
CASE "買い物する"
	IF COND("巨大バイブ購入") && DEVENT("巨大バイブ感想") == 0
		PRINTFORML 「うわ、でか……」
		IF TALENT:MASTER:オトコ
			PRINTFORML 「男の人って、とりあえずデカけりゃいいと思ってませんか？」
		ELSE
			PRINTFORML 「%CNAME("二人称")%、これくらいのが好きなんですか？ ……大したもんですね」
		ENDIF
		CALL SET_DEVENT, "巨大バイブ感想"
	ELSE
		;単純にいっぱい買ったかどうかで分岐
		SELECTCASE COND("買上品数")
		CASE IS >= 4
			PRINTFORML 「いや、買いすぎでしょ……こんなに……」
			PRINTFORML 「ずいぶん良いご身分ですねぇ……まったく」
		CASEELSE
			PRINTFORML 「またなんか変なの買ってきたんですか？」
			PRINTFORML 「いい歳しておもちゃ遊びが好きなんですねぇ」
		ENDSELECT
	ENDIF
CASE "隠し撮り失敗"
	PRINTFORML 「……あんまりナメないでいただきたいですね、%CNAME("二人称")%さん」
	PRINTFORML %CALLNAME:TARGET%は小さな体で%CALLNAME:MASTER%を見下している……
CASE "裸になった"
	;他に人がいる
	IF CHARA_NUM("室内の邪魔者")
