;【今後がんばりたいこと】
;・なんかおッそろしいほど個別イベントファイルにコメントアウトにしてボツった長文放ってあるけどお前今まで何してたん？？？
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K48, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 部長_部長
	;相手が主人ではない
	ELSEIF NO:PLAYER == 34
		LOCALS = 欧内酱_欧内酱
	ELSEIF NO:PLAYER == 35
		LOCALS = 欧尼酱_欧尼酱
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:男性
				LOCALS = 君_你
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = 前輩_前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 老师_老师
			ELSE
				LOCALS = 貴方_你
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF NAME:PLAYER == "你"
		LOCALS = 前輩_前輩
	ELSE
		LOCALS = %CALLNAME:PLAYER%前輩_%CSTR:PLAYER:ふりがな%前輩
	ENDIF
CASE "助手"
	IF NO:PLAYER == 34
		LOCALS = 欧内酱_欧内酱
	ELSEIF NO:PLAYER == 35
		LOCALS = 欧尼酱_欧尼酱
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:男性
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%君
			ELSE
				LOCALS = %CALLNAME:ASSI%酱_%CSTR:ASSI:ふりがな%酱
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%前輩_%CSTR:ASSI:ふりがな%前輩
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = %CALLNAME:ASSI%老师_%CSTR:ASSI:ふりがな%老师
			ELSE
				LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

