;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K5, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 儂_わし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 主様_ぬしさま
	;相手が主人ではない
	ELSE
		LOCALS = 貴様_きさま
	ENDIF
CASE "主人"
	LOCALS = 主様_ぬしさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
ENDSELECT
RETURNF LOCALS

