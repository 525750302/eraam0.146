	ENDIF
CASE "デート終了"
	;エッチをする場合
	IF TFLAG:ベッドイン
		IF TALENT:恋慕
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ELSE
		IF TALENT:恋慕
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDSELECT

