	CASE "１年生", "初等部", "中等部"
		IF TALENT:ASSI:オトコ
			LOCALS = 后辈君_后辈君
		ELSE
			LOCALS = 后辈酱_后辈酱
		ENDIF
	CASE "２年生"
		IF TALENT:ASSI:オトコ
			LOCALS = 哥哥_哥哥
		ELSE
			LOCALS = 姐姐_姐姐
		ENDIF
	;学生年上
	CASE "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%センパイ_%CSTR:ASSI:ふりがな%せんぱい
