				CASE 1
					CALL PRINT_STR, "「…………っ_HH_」_W"
					CALL PRINT_STR, "「……_H_　……_H_」_W"
					PRINTFORML 　膣に捩じ込んだペニスの先端で%CALLNAME:TARGET%の子宮口を責めると、%CALLNAME:TARGET%の寝息に甘いものが混じった……
			ENDSELECT
		ELSEIF COND("うたた寝")
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, "「ぁ……_H_　ん……っ_H_」_W"
					PRINTFORML 　%CALLNAME:TARGET%の口から吐息のような喘ぎが漏れている……
				CASE 1
					CALL PRINT_STR, "「…………ぁ_HH_」_L"
				CASE 2
					CALL PRINT_STR, "「ん……ぅ……_H_　ぁ……_H_」_W"
					PRINTFORML 　身動ぎする%CALLNAME:TARGET%の膣が、普段とは異なるぎこちなさで%CALLNAME:PLAYER%のペニスを締め付ける……
			ENDSELECT
		ELSE
			; 起きそう
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, "「ふ、ぁ……_H_　んっ_H_　ぁ……ぁ_H_」_W"
					PRINTFORML 　小刻みに腰を動かすと、%CALLNAME:TARGET%の寝顔に心地よさそうな微笑みが浮かんだ……
				CASE 1
					CALL PRINT_STR, "「ぁ……_H_　んっ♪　は、ぁ……_H_」_W"
					PRINTFORML 　寝たままの%CALLNAME:TARGET%が、逃すまいと%CALLNAME:PLAYER%の腰に力の入らぬ脚を絡めてきた……
				CASE 2
					CALL PRINT_STR, "「〜〜〜〜っ_HH_HH_」_W"
					CALL PRINT_STR, "「…………ぁ_H_」_W"
					PRINTFORML 　ひときわ%TEXTR("力強く/乱暴に")%%CALLNAME:TARGET%の一番奥を抉ると、暴れるように身体を逸した%CALLNAME:TARGET%の口から甘い息が零れた……
				CASE 3
					CALL PRINT_STR, "「………………_H_」_W"
					PRINTFORML 　リズミカルに打ち付けられる%CALLNAME:PLAYER%の腰の動きに合わせて、%CALLNAME:TARGET%は細かい寝息を立てている……
			ENDSELECT
		ENDIF

		SIF LINE_MEMO == LINECOUNT
			GOTO K11_SLEEP_LP1

	CASEELSE
		PRINTFORML 「あん……っ　そんな、ダメぇ……」
		$KOJO_SLEEP_K11_LP2
		SELECTCASE RAND:10
			CASE 0
				PRINTFORML 「おに、あくまぁ……初手"強迫"はダメなのぉ……」
			CASE 1
				PRINTFORML 「"神の怒り"……うごご……私のエルフたち……」
			CASE 2
				PRINTFORML 「"稲妻"で……私のバッパラちゃんが……ネギマ……」
			CASE 3
				PRINTFORML 「ふぉーぐふぉぐふぉぐたーぼふぉぐー……」
			CASE 4
				PRINTFORML 「らーんらんらんおーばーらーん……」
			CASE 5
				PRINTFORML 「じゃいぐろ……わたしのじゃいぐろどこ……ここ……？」
			CASE 9
				PRINTFORML 「森……森が一枚も無いなんて……」
		CASEELSE
			GOTO KOJO_SLEEP_K11_LP2
		ENDSELECT
		PRINTFORML 「……%TEXTR("くぅ/すやぁ/Zzz")%」
ENDSELECT
