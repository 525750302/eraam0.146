;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K11, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR
#DIMS LUNCHBOX

MEMO_LINECOUNT = LINECOUNT

$KOJO_K11_GREETING_LP1

SELECTCASE ARGS
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝の新妻プレイ"
		;それ以外
		SELECTCASE RAND:10
			CASE 0
				CALL PRINT_STR, @"「朝からリアラちゃん食べ放題_H_　ですねー……_H_」_W"
			CASE 1
				CALL PRINT_STR, @"「もぉ%TEXTR("……/＿")%_H_　珍しくリアラちゃんが朝ごはん作ってたのに……」_W"
				CALL PRINT_STR,  "「おなかいっぱいで……_H_　もう朝ごはん作れませんよぉ……_HH_」_W"

		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASE "朝騎乗位"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, @"「%TEXTR("はぅ/ぁぅ/あぅ")%……_HH_　朝から%TEXTR("元気/ヤンチャ")%すぎですよぉ……_H_」_W"
					CALL PRINT_STR, @"「どーぞ……_HH_　%TEXTR("いっぱい/たくさん/好きなだけ")%出して_H_　ください、ねー……_HH_」_W"
				CASE 1
					CALL PRINT_STR, @"「%TEXTR("あぅ/ひぐ/ふぐ")%っ_H_　う、うぅ、うっ_H_」_W"
					CALL PRINT_STR,  "「ご、ごめんなさいぃ……っ_H_　ふ、ふつーに、起こそうと_H_　したん、です、よぉ？_H_」_W"
					CALL PRINT_STR, @"「なのに%CNAME("二人称")%の%TEXTR("コレ/ココ")%がぁ……_H_　%TEXTR("元気いっぱい/とっても苦しそう")%だったのでぇ……_H_」_W"
					CALL PRINT_STR,  "「我慢……できませんでした、ぁ……っ_HH_」_W"
				CASE 2
					CALL PRINT_STR, @"「%CNAME("一人称")%はぁ……最近、%TEXTR("ちょーっと/少しだけ")%%TEXTR("ワガママ/独占欲が強い")%のでぇ……」_W"
					CALL PRINT_STR, @"「朝一番の%TEXTR("孕みやすそうな/こってり濃厚な/どっろどろの")%%CNAME("精液")%はぁ……%TEXTR("他の娘に渡したくない/自分で堪能したい")%んですぅ……_H_」_W"
					CALL PRINT_STR,  "「だからぁ、このままナカでぇ……_H_　濃いやつ、くださいぃ……_HH_HH_HH_」_W"
			ENDSELECT
		ENDIF
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0

		;それ以外
		SELECTCASE RAND:10
			CASE 0
				CALL PRINT_STR,  "「朝からこんなに元気で……さすがはオトコノコ、ですねー……_H_」_W"
				CALL PRINT_STR, @"「我慢しないで……んっ_H_　どろっどろな%CNAME("精液")%、%CALLNAME:TARGET%先生のナカに……ぁんっ_HH_HH_」_W"
				CALL PRINT_STR, @"「……あは_H_　あっつぅ……_HH_」_W"
			CASE 1
				CALL PRINT_STR,  "「ぁ_H_　おはよー、ござい、ますっ_H_」_W"
				CALL PRINT_STR,  "「んぅっ_H_　いえいえ、なんだか、溜まってっ_H_　いるみたい、でした_H_　からぁっ_HH_」_W"
				CALL PRINT_STR, @"「とりあえず%CALLNAME:TARGET%ちゃんが絞って_H_　あげようか_H_　なぁって_H_」_W"
				CALL PRINT_STR,  "「ナイジョのコーってやつ、です……よっ_HH_　あぁんっ_HH_」_W"
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASE "朝フェラ", "朝シックスナイン"
		; 恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, @"「%TEXTR("んっ/んぅ/あは/あはぁ")%_HH_　朝の%TEXTR("一番絞り/濃いやつ")%、ごちそうさまでした_HH_」_W"
					CALL PRINT_STR,  "「朝ごはん、一緒に食べませんか？」"
				CASE 1
					CALL PRINT_STR, @"「おはよう%TEXTR("ございます/＿")%_H_　んっ_H_　%TEXTR("こっち/コッチ/おちんちん")%と違ってお寝坊さんですねー……_H_」_W"
				CASE 2
					CALL PRINT_STR, @"「こーやって%TEXTR("フェラ/おくち")%で%TEXTR("起こされる/起こしてもらう")%のって、男の子的にはどうなんです？」_W"
					CALL PRINT_STR, @"「もし嬉しいなら……%CNAME("一人称")%も早起きした甲斐があったなぁ_H_　って_HH_」_W"
				CASE 3
					CALL PRINT_STR,  "「おはようございます、ご主人様_HH_　今日も元気そうで、ん、何よりです_H_」_W"
					CALL PRINT_STR,  "「……ふふ_H_　そーいうプレイもオッケーですからね？」_W"
			ENDSELECT
		ENDIF
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0

		; それ以外
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, @"「%TEXTR("えへへ/あは")%_H_　%CNAME("二人称")%の寝起き%CNAME("精液")%、%CALLNAME:TARGET%ちゃんが堪能しちゃいました_H_」_W"
				CALL PRINT_STR, @"「朝から%TEXTR("濃いし/濃厚だし/ねっとりしてるし")%%TEXTR("量も多いし/たっぷりだし/こぼしちゃうくらいだし")%で、%CALLNAME:TARGET%ちゃん大満足です……_HH_」_W"
			CASE 1
				CALL PRINT_STR, "「えへへ……おはようございます_H_」_W"
				CALL PRINT_STR, @"「溜まってるみたいだったから%TEXTR("ゴックン/おくちで")%しちゃいました♪」_W"
			CASE 2
				CALL PRINT_STR, "「ん……く……_HH_」_W"
				CALL PRINT_STR, "「えへへ……朝からこんなに濃いの飲んだら、朝ごはん食べられなくなっちゃいます……_HH_」_W"
		ENDSELECT

		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 0
		GOTO KOJO_K11_GREETING_LP1
	CASEELSE
		RETURN 0
	ENDSELECT
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

$KOJO_K11_GREETING_LP2
	SELECTCASE RAND:30
		CASE  0
			PRINTFORMW 「おはようございまぁす♪　今日もきっと%TEXTR("素敵な/いい")%一日ですよー」
		CASE  1
			LOCAL = FIND_COND("同棲中")
			SIF LOCAL <= 0
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「今日の朝ごはんは%CALLNAME:LOCAL%ちゃんの%TEXTR("お手製/手作り")%ですよ〜」
			PRINTFORMW 「……え？　%CNAME("一人称")%ですか？」
			IF TALENT:恋慕
				PRINTFORMW 「そ、それはそのっ、また%TEXTR("次の機会/今度")%に……っ」
			ELSE
				PRINTFORMW 「んふー。じゃぁ今度%CALLNAME:TARGET%ちゃん特製朝ごはんをごちそうしてあげますね〜」
				CALL PRINT_STR, "「乞うご期待！　ですよ_HH_」_W"
			ENDIF
		CASE  2
			SIF TALENT:恋慕
				GOTO KOJO_K11_GREETING_LP2
			CALL PRINT_STR, @"「今朝は%CALLNAME:TARGET%ちゃんの%TEXTR("特製/手作り/愛情たっぷり")%朝ごはんですよー_H_」_W"
			CALL PRINT_STR, "「あ〜ん_HH_　ってしてあげますからー、早く起きましょ？」_W"
		CASE  3
			SIF !TALENT:恋慕
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「あのー……朝ごはん、出来ました、よ？」
			PRINTFORMW 「お口に合えばいいんですけど……」
		CASE 10
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「今日は体育の日ですよね？　%TEXTR("ふふん/えへへー")%、準備しておいてあげましたよー♪」
		CASE 11
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「たまーに%CNAME("一人称")%もこっそり体育に混じってたりするんですよ？」
			PRINTFORMW 「こー、周りには認識阻害をちょちょいっと。%CNAME("二人称")%にはもう通じないですけどねー」
		CASE 12
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%CNAME("二人称")%はブルマとか好きですかぁ？」
			PRINTFORMW 「色とかデザインとかー……なにかこだわりがあるなら一応聞いてあげますよ？」
		CASE 13
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「体育の授業は好きですか？　%TEXTR("ふふ/まあ")%、嫌いでないなら結構です」
			PRINTFORMW 「%TEXTR("二人で一緒に運動/ベッドの上で組体操")%するにも体力は要りますからねー」
			PRINTFORMW 「……特に%CNAME("二人称")%の場合、お相手も多いですし？　ふふ、頑張りましょうねー♪」
		CASE 14
			SIF !COND("体育")
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%CNAME("一人称")%の体操服姿……と言うか、ブルマ姿が見たいんですか？」
			IF TALENT:恋慕
				PRINTFORMW 「えっと、その……こ、コスプレならいつでもオッケーですよ？」
				PRINTFORMW 「運動の後は……しゃ、シャワーを浴びてから、ということで……ね？」
				CALL PRINT_STR, @"「汗だくなのがいい？　も、もぉ、%TEXTR("フェチ/ヘンタイさん")%ですよそれはー……_H_」_W"
			ELSE
				PRINTFORMW 「%TEXTR("むふー/んふー")%♪　それはそれは、乞うご期待という奴ですねー♪」
			ENDIF
		CASE 20
			SIF BASE:PLAYER:絶頂 * 3 >= MAXBASE:PLAYER:絶頂
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%TEXTR("んふふ/あらあら/あらぁ")%、こっちもお寝坊さんですねぇ♪」
			PRINTFORMW 「ほーら、早くおっきしましょうねー」
		CASE 21
			SIF BASE:PLAYER:絶頂 * 2 < MAXBASE:PLAYER:絶頂
				GOTO KOJO_K11_GREETING_LP2
			PRINTFORMW 「%TEXTR("あらー/まぁまぁ/うふふ")%♪　こっちは早起きさんですねぇ♪」
			PRINTFORMW 「もーちょっと時間に余裕があれば……残念です」
	CASEELSE
		GOTO KOJO_K11_GREETING_LP2
	ENDSELECT

CASE "一番乗り"

CASE "ただいま"

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「おかえりなさ〜い♪」_W"
			CASE 1
				CALL PRINT_STR, "「遅かったですねー……待ちくたびれちゃいましたよ」_W"
			CASE 2
				CALL PRINT_STR, "「おかえりなさいのちゅーを希望します_H_」_W"
			CASE 3
				CALL PRINT_STR, "「あんまり遅くなっちゃダメですよ？」_W"
		CASEELSE
			GOTO KOJO_K11_GREETING_LP1
		ENDSELECT
$KOJO_K11_GREETING_LP3
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「ごはんにします？　お風呂にします？　それとも……_H_」_W"
				CALL PRINT_STR, "「うふふ♪」_W"
			CASE 1
				CALL PRINT_STR, "「ご飯の準備はできてますから、一緒に食べましょ？」_W"
		CASEELSE
			GOTO KOJO_K11_GREETING_LP3
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「%TEXTR("うふふ/えへへ")%、待ってましたよ〜♪」
				PRINTFORMW 「デキるオンナの%CALLNAME:TARGET%ちゃんの手にかかれば%TEXTR("ベッドメイク/おやつの準備")%も完璧なのです」
			CASE 1
				PRINTFORMW 「%TEXTR("ようこそいらっしゃいました/おまちしておりました")%〜♪」
				PRINTFORM 「%CALLNAME:TARGET%ちゃんと他
				IF CHARA_NUM("室内の邪魔者") > 4
					PRINTFORM 多数
				ELSEIF CHARA_NUM("室内の邪魔者") > 2
					PRINTFORM 数名
				ELSE
					PRINTFORM 一名
				ENDIF
				PRINTFORMW の愛の巣へようこそ〜♪」
			CASE 2
				PRINTFORMW 「もー、%TEXTR("遅いです/遅刻です/待ちわびました")%よー？」
				PRINTFORMW 「誠意の証としてー、ちゅーかぎゅーを要求します」
				PRINTFORMW 「……もちろん、それ以上も大歓迎ですよ？」
			CASE 3
				PRINTFORMW 「お待ちしておりました、%TEXTR("旦那様/ご主人様")%」
				PRINTFORMW 「……なぁんて♪　ふふ、ちょっとくらいは%TEXTR("興奮/ドキドキ")%しましたぁ？」
				IF TALENT:恋慕 == 0
					PRINTFORMW 「%TEXTR("本気で/心から")%そう呼んで欲しいならぁ、頑張って%CALLNAME:TARGET%ちゃんの好感度を稼ぎましょうねー♪」
				ENDIF
		CASEELSE
			GOTO KOJO_K11_GREETING_LP1
		ENDSELECT
	ENDIF

CASE "入室挨拶"
	SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「%TEXTR("ハァイ/は〜い")%、%CALLNAME:TARGET%ちゃんの登場ですよ%TEXTR("＿/ぉ/〜")%♪」
			PRINTFORM 「%TEXTR("ベッド/ソファ")%までエスコートを%TEXTR("希望/お願い")%しちゃいますー
			CALL HEARTMARK
			PRINTFORMW 」

		CASE 1
			PRINTFORMW 「%CALLNAME:TARGET%ちゃんが%TEXTR("＿/今日も/やって")%来ましたよー♪」

		CASE 4
			PRINTFORMW 「ご指名ありがとうございます%TEXTR("＿/ぅ/〜")%。%CALLNAME:TARGET%です♪」
			CALL PRINT_STR, @"「まずは%TEXTR("即尺/即ハメ/即ズリ")%%TEXTR("＿/のオプション")%はいかがですかぁ_H_」_W"
			PRINTFORM 　%CALLNAME:TARGET%はおどけた風に言いながら
			IF EQUIP:スカート
				PRINTFORMW %SKIRT(TARGET, "略")%の裾を摘んで僅かに持ち上げた……
			ELSE
				PRINTFORMW しなを作って%CALLNAME:PLAYER%にもたれかかった。
			ENDIF
	CASEELSE
		GOTO KOJO_K11_GREETING_LP1
	ENDSELECT

CASE "抜け出された"
$K11_GREETING_ESCAPE_LP1
	SELECTCASE RAND:5
		CASE 0
			PRINTFORMW 「%CALLNAME:TARGET%ちゃんとお出かけですかー？」
			PRINTFORMW 「%TEXTR("うふふ/あらあら/んふー")%♪　ごゆっくりー♪」
		CASE 1
			PRINTFORMW 「他の場所でご休憩ですかぁ？」
		CASE 2
			PRINTFORMW 「ふむふむ、二人きりでしっぽりしたいんですね〜」
			PRINTFORMW 「%CNAME("一人称")%のことは気にしないで楽しんできてください〜♪」
		CASE 4
			SIF TALENT:恋慕
				GOTO K11_GREETING_ESCAPE_LP1
			PRINTFORMW 「あのー……%CNAME("一人称")%も一緒に行っていいですか？」
			PRINTFORMW 「あ、はい、ダメですか……うぅ。おとなしくお留守番してますぅ……」
	CASEELSE
		GOTO K11_GREETING_ESCAPE_LP1
	ENDSELECT

CASE "お弁当"
$KOJO_GREETING_K11_LUNCHTIME_LP1
	LUNCHBOX = %KOJO_K11_LOCAL_STRING("お弁当")%

	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR, "「今日はどこで食べましょうねー」_W"
				CALL PRINT_STR, "「あ、お茶なら買ってありますよ♪」_W"
			CASE 1
				CALL PRINT_STR, @"「ふふ♪　そんなに%TEXTR("お弁当/お昼ごはん")%が待ち遠しかったんですか？」_W"
			CASE 2
				CALL PRINT_STR, "「今日は……屋上で食べましょうか」_W"
				CALL PRINT_STR, "「い、いえ、その……た、たまには二人っきりでのんびりっていうのも……」_W"
				CALL PRINT_STR, "「……初心なネンネみたいなコト言わせないでくださいよぉ」_W"
		CASEELSE
			GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				SIF STRCOUNT(LUNCHBOX, "おにぎり") + STRCOUNT(LUNCHBOX, "サンドイッチ")
					GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
				SPLIT LUNCHBOX, "/", LOCALS
				PRINTFORMW 「今日のお弁当は%LOCALS:0%にしてみましたー」
			;CASE 1
			;	SIF !STRCOUNT(LUNCHBOX, "無し")
			;		GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
			CASE 4
				SIF RAND:3 == 0
					PRINTFORMW 「お昼ですよ〜」
				LOCALS = %TEXTR("部室/教室/屋上/中庭/食堂")%
				PRINTFORMW 「今日はどこで食べましょうね」
				PRINTFORMW 「%TEXTR("＿/……/ん〜、")%%TEXTR("＿/そうだ、/")%%LOCALS%にしましょうか」
		CASEELSE
			GOTO KOJO_GREETING_K11_LUNCHTIME_LP1
		ENDSELECT
	ENDIF

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K11, ARGS
;これは主にFOR-NEXTのCOUNT用
#DIM LCOUNT
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

$KOJO_DAILY_K11_LP2

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ハァイ、%CNAME("一人称")%が来ましたー！　ぱちぱちぱちー」
	PRINTFORML 　%CALLNAME:TARGET%がテンション高く入ってきた……

	; 確定でフェリシエルが居る(筈)ので、最低値1
	IF CHARA_NUM("室内の邪魔者") > 1
		; 既に多数ヒロインが居る
		IF CHARA_NUM("室内の邪魔者") >= 4
			PRINTFORML 「おやおやおやぁ？　思ってたより凄いメンツですねー」
		ENDIF
	ELSE
		; 他にはフェリシエルのみ
		IF CHARA_NUM("室内の邪魔者") != CHARA_NUM("集合予定")
			; 他にもヒロインが居る
		ELSE
			; 他のヒロインはまだ居ない
			PRINTFORML 「んにゅ？　……ほー。なるほどなるほど？」
		ENDIF
	ENDIF

	IF CHARA_NUM("室内の邪魔者") >= 2 && CHARA_NUM("陥落済み") >= CHARA_NUM("室内の邪魔者")
		PRINTFORML 「でもちょっと意外ですねー。こー、お気楽にパコパコエンジョイしてるだけだと思ったんですけど」
		PRINTFORML 「割とガチな娘も、ひの、ふの……あ、こーいうのは野暮ですね、ごめんなさい」
	ENDIF

	PRINTFORML 「ま、これからは竿姉妹として仲良くしましょ？」
	PRINTFORML 　%CALLNAME:TARGET%はにこやかにそう言った……
CASE "汗だく"
	; たまに(25%)体育に参加、ということで特殊処理
	;IF RAND:3
		; 汗と汚れをリセットしてランダム会話に飛ばす
	;	CFLAG:汗 = 0
	;	CALL  RESET_STAIN_EX, "全部", TARGET
	;	GOTO KOJO_DAILY_K11_SKIP1
	;ENDIF

	PRINTFORML 「いやー、たまには健全な運動もいいものですねー」
	PRINTFORML 　%CALLNAME:TARGET%は汗だくのままソファに座り込んでいる……
	IF RAND:4
		PRINTFORML 「うーん、汗でびしょびしょ……おやぁ？」
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%の視線に気付いたようだ。
		IF ABL:露出癖 >= 3 && RAND:3
			PRINTFORML 「見たいなら見たいって言ってくれないと、見せてあげませんよ？」
			PRINTFORML 　言いながらも%CALLNAME:TARGET%は脚を開き、汗に蒸れた股間を見せつけてきた……
			IF BASE:欲求不満 >= 50
				PRINTFORML 「見たいのはここですか？　それともこっちですか？」
				IF EQUIP:パンツ
					PRINTFORML  %CALLNAME:TARGET%は自ら指を掛けて股間のブルマをずらし、その奥に隠れた%PANTIES(TARGET)%を顕にした。
