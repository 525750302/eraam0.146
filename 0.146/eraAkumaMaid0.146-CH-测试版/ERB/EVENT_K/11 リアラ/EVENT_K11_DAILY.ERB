								PRINTFORM 、クロッチが無い%PANTIES(TARGET, "パンツ")%を穿いているおかげで%TEXTS("割れ目")%が
							ELSE
								PRINTFORM %PANTIES(TARGET)%が
								CALL SETFLAG_DAILY, "パンツ色確認"
							ENDIF
							PRINTFORML %TEXTR("バッチリ/")%見えている……
						ELSE
							PRINTFORML 下着が見えてしまいそうだ……
						ENDIF
					ENDIF
				CASE 3
					PRINTFORM %CALLNAME:TARGET%は%CALLNAME:PLAYER%の視線に気付くと
					IF EQUIP:スカート
						PRINTFORML %SKIRT(TARGET, "略")%の端を摘んで持ち上げた。
						PRINTFORML 「%TEXTR("お安くないですよ/見えますか/見たいんですかぁ")%ー？　ほらほらー♪」
						IF CHECK_CLO("ノーパン")
							PRINTFORM %SKIRT(TARGET, "略")%の下から顕になったのは、
							;アソコ丸見え
							IF CHECK_CLO("Ｖ露出")
								PRINTFORM 布一枚、糸一本纏わぬ
							;パンスト越し
							ELSE
								PRINTFORM %OUTER(TARGET, "略")%越しにうっすらと透ける
							ENDIF
							PRINTFORM %CALLNAME:TARGET%の%TEXTR("秘所/股間/アソコ")%だった……
							CALL SETFLAG_DAILY, "パンツ形状確認"
						ELSE
							PRINTFORML 今日のチョイスは%PANTIES(TARGET)%のようだ……
							CALL SETFLAG_DAILY, "パンツ色確認"
						ENDIF
					ELSE
						PRINTFORML こちらに背中を向け、腰を振って誘ってきた……
					ENDIF
				CASE 7
					SIF COND("危険日") == 0
						GOTO KOJO_DAILY_K11_LOOP1
					PRINTFORML 　%CALLNAME:TARGET%はどこか%TEXTR("熱を帯びた/蕩けた")%%TEXTR("笑み/顔")%で自らの下腹部に手を添えている……
					PRINTFORML 「%TEXTR("はぁ/もぉ")%……%TEXTR("切ない/寂しい")%ですねぇ」
					PRINTFORML 「早く%TEXTR("お相手して/押し倒して/連れ出して")%欲しいんですけどー……？」
				CASE 8
					PRINTFORM 「
					FOR LCOUNT, 0, RAND:4 + 1
						PRINTFORM %TEXTR("……/――")%
					NEXT
					PRINTFORML 」
					PRINTFORM 　%CALLNAME:TARGET%は%TEXTR("真剣な顔で/パラパラと")%
					IF RAND:4
						LOCALS = 雑誌
						LOCALS:1 = %KOJO_K11_LOCAL_STRING("雑誌：タイトル")%
					ELSE
						LOCALS = 本
						LOCALS:1 = %KOJO_K11_LOCAL_STRING("本：タイトル")%
					ENDIF
					PRINTFORML 『%LOCALS:1%』というタイトルの%LOCALS%を捲っている……
				CASE 9
					IF CHARA_NUM("室内の邪魔者") > 4
						PRINTFORML 「眼福眼福、良きかな良きかな〜♪」
						VARSET LOCAL
						FOR LCOUNT, 1, CHARANUM
							IF COND("現在存在", LCOUNT)
								SIF TALENT:LCOUNT:恋慕 || TALENT:LCOUNT:淫乱
									LOCAL += 1
							ENDIF
						NEXT
						IF LOCAL >= 3
							PRINTFORML 「%TEXTR("盛ってる/サカってる/発情してる")%%TEXTR("女の子/雌/メス")%の顔はイイものですねぇ……♪」
						ELSE
							PRINTFORML 「みんなステキな女の子で、%CNAME("一人称")%幸せです〜」
						ENDIF
					ELSE
						PRINTFORML 「この部屋の空気、いつ吸ってもイイ感じですね〜」
						PRINTFORML 「とーっても濃密な雄と雌の%TEXTS("匂い")%で、もう%CNAME("一人称")%の%TEXTR("胸が/お股/アソコが/オマンコ/子宮が")%きゅんきゅんしちゃいますよぉ」
					ENDIF
				CASEELSE
					GOTO KOJO_DAILY_K11_LOOP1
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:恋慕
		ELSE
			SELECTCASE RAND:10
				CASE  0
					SIF SAVESTR:現在位置 != "ヤリ部屋"
						GOTO KOJO_DAILY_K11_LOOP1
					PRINTFORML 「こーいう隠れスポットって結構あるんですよねー」
					PRINTFORML 「興味があるなら%TEXTR("幾つか教えて/今度連れてって")%あげますよー」

				CASE  1
					SIF SAVESTR:現在位置 != "ヤリ部屋"
						GOTO KOJO_DAILY_K11_LOOP1
					PRINTFORML 「こーんな場所に%CNAME("一人称")%を%TEXTR("連れ込む/誘い込む")%だなんてぇ♪」
					PRINTFORM 「%TEXTR("期待/きゅんきゅん")%しちゃいますよぉー
					CALL HEARTMARK
					PRINTFORML 」
				CASE  2
					SIF LOCALS != "体育倉庫"
						GOTO KOJO_DAILY_K11_LOOP1
					PRINTFORML 「ちょっと埃っぽいですけどー……まぁ、これはこれで♪」
				CASE  3
					SIF LOCALS != "体育倉庫"
						GOTO KOJO_DAILY_K11_LOOP1
					PRINTFORML 「スるならはやくシないとぉ、午後の授業の準備で人が来ちゃいますよぉ〜？」
					PRINTFORM 「なーのーでー♪　%CALLNAME:TARGET%ちゃん的には
					SELECTCASE RAND:3
						CASE 0
							PRINTFORM 壁に手を付いてバックでサクっとするの
						CASE 1
							PRINTFORM お互い立ったまま向かい合ってパコパコ
						CASE 2
							PRINTFORM あえてそこのマットの上で時間ギリギリまでがっつりえっち
					ENDSELECT
					PRINTFORML とかオススメなんですけどぉ……どうします？」
					
				CASE  9
					PRINTFORML 「場所を変えるのも大事ですよ〜」
				CASE 99
					PRINTFORML 
			CASEELSE
				GOTO KOJO_DAILY_K11_LOOP1
			ENDSELECT
		ENDIF
	ENDIF
ENDIF

