			CALL K11_DAYLYCOM_SLEEP_SEX_MENU
			CALL SET_CEVENT("睡眠姦：メモ発見")
			CALL SET_CEVENT("日常：睡眠姦メモ")
		; ２回目以降のメモ
		ELSEIF !CEVENT("日常：睡眠姦メモ") && CEVENT("睡眠姦：メモ発見") && CSTR:没収済パンツ != ""
			SPLIT CSTR:没収済パンツ, "/", LOCALS
			PRINTFORMW %CALLNAME:TARGET%のはいている%LOCALS:0%にメモが挟まれていた……
			CALL K11_DAYLYCOM_SLEEP_SEX_MENU
			CALL SET_CEVENT("日常：睡眠姦メモ")
		ENDIF

		SIF LOCAL != LINECOUNT
			GOTO KOJO_SLEEP_K11_SKIP1

		; 汎用
		LINE_MEMO = LINECOUNT
		IF COND("熟睡")
			SELECTCASE RAND:10
				CASE 0
					CALL PRINT_STR, "「…………」_W"
