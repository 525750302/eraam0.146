;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K52, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, @"「最近構ってくれないからね。たまには%CNAME("一人称")%から誘っちゃおうかなって_HH_」"
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		CALL PRINT_STRL, @"「%TEXTR("はぁ……/んっ……/んぅ……")%%TEXTR("あら、おはよう/ごめんなさい。起こしちゃったかしら")%_HH_」"
		CALL PRINT_STRW, @"「%TEXTR(@"朝から躁动难忍してきちゃったから……折角だもの、一緒に気持ち良くなりましょう/%CNAME("二人称")%を見てたら躁动难忍してきちゃったの。嫌じゃなかったら付き合ってくれないかしら")%_HH_」"
	CASE "朝舐陰"
		CALL PRINT_STRL, @"「%TEXTR("んっ/んぅ")%_HH_　%TEXTR("起こしちゃったかしらね/目が覚めちゃったみたいね")%_HH_」"
		CALL PRINT_STRW, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("就这様楽にしていてね。大姐姐がたっぷりご奉仕して给から/楽にしてていいのよ。隅々まで綺麗にして给から")%_HH_」"
	CASE "朝足交"
		CALL PRINT_STRL, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("おはよう/目は覚めたかしら")%_HH_」"
		CALL PRINT_STRW, @"「%TEXTR("足でナデナデしてたらすっかり硬くなっちゃったみたいね/ちょっと足交の練習してみようかなって思ってね")%_HH_」"
	CASE "朝の新妻PLAY"
		CALL PRINT_STRL, @"「%TEXTR("こらっ/もうっ")%_HH_」"
		CALL PRINT_STRW, @"「%CNAME("一人称")%は%CNAME("二人称")%の%TEXTR("奥桑/お嫁桑")%じゃないのよ_HH_　%TEXTR("ダメよ……ダメだったら/もう……ダメだって言ってるのに")%」"
	CASE "朝乳交"
		IF RAND:2 == 0
			CALL PRINT_STRL, @"「はぁ……はぁ……%TEXTR("んぅ/んっ")%_HH_」"
			CALL PRINT_STRW, @"「%CNAME("一人称")%の%TEXTR("胸/胸部")%%TEXTR("がそんなに好きなのかしら/で挟まれて、こんなに硬くなってイケない子ね/の中でビクビクしちゃって、可愛いんだから")%_HH_」"
		ELSE
			CALL PRINT_STRL, @"「ごめん%TEXTR("ね/なさい")%。%CNAME("二人称")%の……見てたら、%TEXTR("乳交でご奉仕/乳交奉仕/胸部でご奉仕")%したくなってきちゃったの_HH_」"
			CALL PRINT_STRW, @"「……%TEXTR("変態だって軽蔑しないでね。女の子にだって性欲はあるんだから/はしたない女だって思われそうだけど……女の子だって我慢出来ない時はあるのよ")%_HH_」"
		ENDIF
	CASE "朝騎乗位"
		CALL PRINT_STRL, @"「ごめんね_HH_　麦莉爱娜桑の代わりに起こしに……来たら我慢出来なくなっちゃったの_HH_」"
		CALL PRINT_STRW, @"「酱と_HH_　%TEXTR("射精するまで/出るまで/出してくれるまで")%、んっ_HH_　上で動いてあげちゃうから……あっ_HH_　ああっ_HH__」"
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRL, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR("おはよう、やっと目が覚めたみたいね/目は覚めたかしら？")%」"
		PRINTFORML 「……%CNAME("一人称")%ね。ちょっと%TEXTR("一人/自分")%で%TEXTR("お尻/肛門")%%TEXTR("でしてたんだけど/を使ってたんだけど/を慰めてたんだけどね")%……」
		CALL PRINT_STRW, @"「%TEXTR("本物/生ち〇ぽ")%が欲しくなっちゃったから……%CNAME("二人称")%を襲っちゃったの_HH_　%TEXTR("ごめんね/ごめんなさい")%」"
	CASE "朝接吻"
		CALL PRINT_STRL, @"「%TEXTR("ちゅっ……んっ、んぅ、んん/ちゅう……んっ、ちゅぱっ、ちゅ……/レロッ……レロレロッ")%_HH_」"
		CALL PRINT_STRW, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_　%TEXTR(@"ほら、さっさと起きなさい。起きない気なの？　そんなに%CNAME("一人称")%のディープ接吻が気に入ったのかしら/目は覚めたかしら？　それとももっとディープ接吻して欲しいのかしら")%」"
	CASE "朝添い寝"
		CALL PRINT_STRL, @"「%TEXTR("ふぁ……/ん……")%_HH_」"
		CALL PRINT_STRW, @"「%TEXTR("好きよ……/愛しているわ")%_HH_……%TEXTR("むにゃ/")%」"
	CASE "朝口交", "朝六九式"
		IF RAND:2 == 0
			CALL PRINT_STRL, @"「%TEXTR("んっ/んぅ")%……%TEXTR("はぁ、おはよう。/おはよう。")%%TEXTR("じゅるっ、じゅるる/ジュポ、ジュポッ/")%_HH_」"
			CALL PRINT_STRW, @"「朝から、んっ……こんなに大きくさせちゃって%TEXTR("＿/るのね")%_HH_　%TEXTR("とっても辛そうだし、お口でたっぷりご奉仕して给ね/悪い子は大姐姐がお仕置きして给んだから")%_HH_」"
		ELSE
			CALL PRINT_STRL, @"「おはよう_HH_　麦莉爱娜桑の代わりに起こし来了ら大きくなってたから……」"
			CALL PRINT_STRW, @"「ジュプ、ジュプッ_HH_　たっぷりご奉仕しちゃった_HH_　ちゅぷっ、はぁはぁ_HH_　若いから、朝からこんなに出ちゃうんだね_HH_」"
		ENDIF
	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝口交"
		PRINTFORML 「%TEXTR("もう/ダメよ")%……%CNAME("一人称")%だって%TEXTR("女なんだから、ご奉仕したくなっちゃう時もあるんだからね/あんなの見せつけられたらご奉仕したくなっちゃうんだからね")%？」
		CALL PRINT_STRW, @"「%TEXTR(@"%CNAME("二人称")%が嫌じゃないなら……またしてあげても良いけどね/そう、気に入ってくれたのね。褒められたらまたしたくなっちゃうんだから……もう")%_HH_」"
	CASE "朝騎乗位", "朝Ａ騎乗位"
		PRINTFORML 「こほん……」
		CALL PRINT_STRW, @"「%TEXTR("解ってると思うけど/勿論、解っているわよね")%。%TEXTR("今朝/朝")%の事は他の皆には内緒よ。%TEXTR("約束して/黙っておいて")%くれたらまたして给からね_HH_」"
	CASEELSE
		IF RAND:2 == 0
			PRINTFORMW 「%CNAME("二人称")%%TEXTR(@"の寝顔が可愛かったから/")%、朝からちょっとハメを外し過ぎたかなって……イヤじゃなかった？」
		ELSE
			PRINTFORMW 「ごめんなさい……迷惑じゃなかったかしら。%CNAME("一人称")%ったら%TEXTR("老师/人妻/大姐姐/年上")%なのに年甲斐もなく%TEXTR("はしゃいじゃったわ/イケない事してしまったわね")%」
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育なんだから、しっかり頑張りなさいね？」
	ELSE
		CALL PRINT_STRW, @"「%TEXTR("おはよう、酱と学校行かなきゃダメだよ？/おはよう。朝ご飯は出来てるから酱と食べたら学校に行きなさいね")%_HH_」"
	ENDIF
CASE "一番乗り"
	CALL PRINT_STRW, @"「誰も帰って来てないみたいね。今なら……少しぐらい打情骂俏してもいいけど、どうする？_HH_」"
CASE "ただいま"
	PRINTFORML 「%TEXTR("ただいま/お邪魔します")%」
	PRINTFORMW 「何だかココが、私の本当のお家みたいになっちゃってるけど……%TEXTR("それってあんまり良くない事だよね/自宅で一人ぼっちなのは寂しいのよ")%」
CASE "既に入室"
	;家で出迎えてくれる時
	IF PLACE("自宅")
		IF COND("夜")
			CALL PRINT_STRL, @"「%TEXTR("お帰りなさい/待ってたんだからね")%_HH_」"
			CALL PRINT_STRW, @"「%TEXTR("またお邪魔してるけど……私だって暇なわけじゃないのよ、本当なんだからね？/今日の学校で何があったか、良かったら大姐姐に教えてね？_HH_")%」"
		;昼間にいるのは日曜日だけのはず
		ELSE
			PRINTFORML 「今日はお休みの日だから朝からお邪魔しちゃってます」
			PRINTFORML 「折角の休みなんだから、彼女と约会したり友達と玩耍に行ってらっしゃい」
			CALL PRINT_STRW, @"「どうしてもって言うのなら……私が付き合ってあげてもいいけどね_HH_」"
		ENDIF
	ENDIF
CASE "入室打招呼"
	IF CFLAG:呼び出された
;		PRINTFORMW 「%TEXTR("どうかしたの/急ぎの用事だって聞いたけど何")%？もう……私は在宅でお仕事してると言っても何時でも暇な訳じゃないのよ」
		PRINTFORMW 「%TEXTR("どうかしたの/急ぎの用事だって聞いたけど何")%？もう……私だって仕事があるのだから何時でも暇な訳じゃないのよ」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「えっとぉ……お邪魔だったみたいね」
	ELSE
		IF COND("夜")
			PRINTFORMW 「こんばんは。お邪魔しますね」
		ELSE
			PRINTFORMW 「こんにちは。お邪魔します」
		ENDIF
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	IF COND("夜")
		PRINTFORMW 「こんばんは。お邪魔しますね」
	ELSE
		PRINTFORMW 「こんにちは。お邪魔します」
	ENDIF
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「家族団欒って素敵ね、私も兒童は二人以上欲しいかも……」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「呼び出されたから来たんだけど……」
	ELSE
		PRINTFORMW 「お邪魔しちゃいますね。今日は何をしましょうか」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "打招呼返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF COND("夜")
		PRINTFORMW 「こんばんは。お邪魔してます」
	ELSE
		PRINTFORMW 「こんにちは。お邪魔してます」
	ENDIF
	IF FIND_COND("教員", "入室打招呼中")
		;実母も先生にカウントされる不具合
		PRINTFORMW 「老师もお家に来てるのね。勉強熱心なのは良い事だと思います」
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL PRINT_STRW, @"「みんな待ってましたよ。な～んてね_HH_」"
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「%CNAME("一人称")%を呼んでるって聞いたんだけど？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「色々と忙しいのね。何か手伝える事があったら気兼ねなく言ってね？」
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		CALL PRINT_STRW, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_%TEXTR("若いわねぇ/若いんだからしょうがないわね")%」"
CASE "便当"
	PRINT 「
	IF NAME:MASTER == "你"
		IF TALENT:MASTER:男性
			PRINT 你
		ELSE
			PRINT 你
		ENDIF
	ELSE
		PRINTFORM %NAME:MASTER%
		IF TALENT:MASTER:男性
			PRINT 君
		ELSE
			PRINT 酱
		ENDIF
	ENDIF
	CALL PRINT_STRW, @"に頼まれたら断れないもの。一生懸命頑張ったから美味しく食べてね_HH_」"
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	PRINTFORMW 「%TEXTR(@"えっと……%CNAME("一人称")%はお邪魔かしら/何だか気まずいきもするけど")%……」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K52, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	IF COND("夜")
		PRINTFORML 「%CNAME("一人称")%は家族ではないですけど……」
		PRINTFORML 「出来れば皆桑と家族以上に仲良くしていけたらいいなって思ってます」
	ELSE
		PRINTFORML 「小鳥遊静菜と申します。学園にはやってきたばかりなので」
		PRINTFORML 「部活の皆桑は温柔的色々教えてくださいね。宜しくお願いします」
	ENDIF
CASE "浑身是汗"
	PRINTFORML 「%TEXTR("ふぅ……今日も暑いわね/夏なんだからしょうがないと言っても……ちょっと疲れちゃうかも")%」
CASE "刚睡醒"
	PRINTFORML 「むにゃ……まだ眠いけど、おはよう」
CASE "Cosplay中"
	CALL PRINT_STRL, @"「%TEXTR("似合ってるかな……そう、似合ってる？ふふっ、褒めても何も出ませんからね/そんなに喜んでもらえるなんて、もう次は何のCosplayをして欲しいの？")%_HH_」"
CASE "点心の感想"
	;今日のおやつを用意したのは自分
	IF COND("点心当番")
		PRINTFORML 「今日は%CNAME("一人称")%が点心を用意したんだけど、どうかな？」
	;今日のおやつは誰かの手作り
	ELSEIF COND("亲手自制点心")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("点心当番")%の亲手自制") == 0
			;おやつ当番が[料理上手]
			IF COND("点心の作り手が料理擅長")
				PRINTFORML 「%TEXTS("点心当番")%桑のお菓子。凄い%TEXTR("擅長/美味しいの")%ね。%CNAME("一人称")%も負けられないなぁ」
			ELSE
				PRINTFORML 「手造りのお菓子なんて手が込んでいるのね♪」
			ENDIF
		ELSE
			PRINTFORML 「%TEXTR("今日の点心は何かしらね？/手造りの点心なんて贅沢よね♪")%」
		ENDIF
	;おやつが手作りでない
	ELSE
		PRINTFORML 「%TEXTR("今日は何の点心なのかしらね/大家茶にするのは楽しいわね")%」
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K52
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
;2020/12/15　ショップを改変したので書き直し
;	PRINTFORML 「……気持ちは嬉しいんだけどね。少しだけ考える時間をくれると嬉しいかな」
;	PRINTFORML （二人の関係は不倫だけど……%CNAME("二人称")%の本気の気持ちには応えてあげたい……）
CASE "约会的约定"
	PRINTFORML 「勘違い不会うに、これは约会の練習だから」
	PRINTFORML 「本命の彼女と擅長くいくように全力で応援して给から本番のつもりで頑張ってね？」
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	PRINTFORML 「くっくっくっ、契約成立じゃ。魂をよこしなさい」
	PRINTFORML 「悪魔ってこんな感じなのかしら？」
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	IF COND("夜")
		IF RAND:2 == 0
			PRINTFORML 「もう……強引なんだから」
			CALL PRINT_STRL, @"「%CNAME("一人称")%もイヤなわけじゃないけどダメよ、%CNAME("一人称")%は人妻なんですからね_HH_」"
		ELSE
			PRINTFORML 「まったく……」
			CALL PRINT_STRL, @"「他の娘には後で%CNAME("一人称")%から謝っておくから。今は二人っきりの時間を楽しみましょう_HH_」"
		ENDIF
	ELSE
		IF (PLACE("体育倉庫") || PLACE("更衣室")) && RAND:2 == 0
			PRINTFORML 「老师をこんな場所に連れ込んで……」
			PRINTFORML 「正直に言いなさい。何をするつもりなのかしら？」
		ELSEIF PLACE("保健室") && RAND:2 == 0
			PRINTFORML 「気分が悪いって言って老师を騙したの？」
			PRINTFORML 「まあ……騙された老师も悪いけど、ダメよ」
		ELSEIF PLACE("做爱房") && RAND:2 == 0 
			PRINTFORML 「こんな如何わしい場所に老师を連れてきて」
			PRINTFORML 「一体何をするつもりなの？」
		ELSE
			IF RAND:5 == 0
				PRINTFORML 「こらっ……」
				PRINTFORML 「何て言うか、強引なのはダメだからね……」
			ELSEIF RAND:4 == 0
				PRINTFORML 「もう……」
				PRINTFORML 「摆脱なんて……本当はダメよ」
			ELSEIF RAND:3 == 0
				PRINTFORML 「摆脱のは嫌なわけじゃないけど……」
				PRINTFORML 「%CNAME("一人称")%よりずっと若い子達に囲まれてるんだから、少し考えてね？」
			ELSEIF RAND:2 == 0
				PRINTFORML 「摆脱のが嫌って言うんじゃなくて……ね？」
				PRINTFORML 「他の子が嫉妬したりしないか、もう少し考えなさいよね」
			ELSE
				PRINTFORML 「もう……次からはもう少し考えてね？」
				PRINTFORML 「強引なのは好きだけど、他の子も見てるんだからね」
			ENDIF
		ENDIF
	ENDIF
CASE "主人公的房間に連れ込まれた"
	PRINTFORML 「もう……」
	IF RAND:2 == 0
		PRINTFORML 「女の子にあんまりエッチな事ばっかりしてると嫌われちゃうわよ？」
	ELSE
		PRINT 「
		IF NAME:MASTER == "你"
			IF TALENT:MASTER:男性
				PRINTFORM 你
			ELSE
				PRINTFORM 你
			ENDIF
		ELSE
			PRINTFORM %NAME:MASTER%
			IF TALENT:MASTER:男性
				PRINT 君
			ELSE
				PRINT 酱
			ENDIF
		ENDIF
		PRINTFORML だから許して给けど……本当はダメなんだからね？」
	ENDIF
CASE "自宅に招待"
	PRINTFORML 「ようこそ、我が家へ」
	IF RAND:114514 == 0
