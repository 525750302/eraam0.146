	PRINTFORMW 「……手で良かったらして给から%TEXTR("＿/ね")%」
CASE "接吻"
	CALL PRINT_STRL, @"「%TEXTR("ちゅ/ちゅう")%_HH_」"
	CALL PRINT_STRW, @"「%TEXTR("はぁ/んっ")%_HH_……ダメよ、こんな所で接吻なんて……」"
CASE "摸屁股"
	IF TALENT:恋慕 && RAND:2 == 0
		CALL PRINT_STRW, @"「%TEXTR("ふふっ/诶嘿嘿っ")%_HH_……%TEXTR("もっとして/もっと続けて欲しいな")%……」"
	ELSE
		CALL PRINT_STRW, @"「%TEXTR("んっ/んぅ")%_HH_　他の人にバレないようにね」"
	ENDIF
;あなたが物陰フェラやキスやセクハラをしない紳士な場合はここに来ます
CASE "下次去哪里呢？询问"
	PRINTFORMW 「%TEXTR("そうね/何処が良いかしらね")%……」
