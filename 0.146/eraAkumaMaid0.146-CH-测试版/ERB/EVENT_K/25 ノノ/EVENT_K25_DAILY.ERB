			PRINTFORMW  「%CNAME("一人称")%は男の子なのに……」
			CALL PRINT_STRW, @"「……%CNAME("二人称")%はどう思いますか？　%CNAME("一人称")%の今の格好_H_」"
		ELSE
			PRINTFORMW 「……周りの視線が気になります……」
			PRINTFORMW 「別の水着にしたほうが良かったかな……」
		ENDIF
	;その他はだいたいエロ水着です
	CASEELSE
		IF YOKUBO()
			CALL PRINT_STRW, @"「どうですか？　%CNAME("二人称")%_H_」"
			CALL PRINT_STRW, "「こんな格好で泳いだりなんかしたらみんなから変態だって思われちゃうでしょうね……_H_」"
		ELSE
			PRINTFORMW 「%CNAME("一人称")%としては……その……」
			PRINTFORMW 「……次は他のものにしてもいいですか……」
		ENDIF
	ENDSELECT
;物陰フェラです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "ちょっと物陰へ……"
	SIF YOKUBO() || TALENT:恋慕
		PRINTFORMW 「……まったく、%CNAME("二人称")%は本当に底なしの変態なんですから……」
	CALL PRINT_STRW, @"「……%AEGI(TARGET, "要", "Ｍ異物", 5 + RAND:3)%」"
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1
;映画館などで横並びに座った状態での手コキです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "こっそり手コキしてもらう"
	IF YOKUBO()
		CALL PRINT_STRW, @"「……まったくもう、%CNAME("二人称")%のせいで映画に集中できないじゃないですか_H_」"
	ELSE
		PRINTFORMW 「ひ、人前ですし、すぐに終わらせますからね……」
	ENDIF
CASE "キスする"
	IF MARK:屈服刻印 <= 2
		PRINTFORMW 「え、えっ！？　こ、こんな外でキスするんですか！？」
		PRINTFORMW 「……う……べ、別にイヤではないですけど……」
		PRINTFORMW 「……わ、分かりました。いいでしょう」
	ENDIF
	CALL PRINT_STRW, @"「……%AEGI(TARGET, "要", "キス", 3 + RAND:2)%」"
	IF TALENT:相思相愛
		PRINTFORMW 「……こうやってくっついていると、%CNAME("二人称")%とカップルになれたんだなぁって改めて実感できます」
		SIF MARK:屈服刻印 <= 2
			PRINTFORMW 「……同じくらい恥ずかしくもありますが」
		PRINTFORMW 「……あの、%CNAME("二人称")%、もう少しこのままでいてもらっていいですか」
		IF YOKUBO()
			PRINTFORMW 「いつもみたいに理性が吹き飛ぶくらいに気持ちよくして貰うのも勿論大好きですけど、こういう触れ合いも気に入っているんです」
			PRINTFORMW 「……なんて、もう知ってますよね、%CNAME("二人称")%」
		ENDIF
	ELSEIF YOKUBO()
		CALL PRINT_STRW, @"「……こんな往来でキスするなんて、%CNAME("二人称")%ってばなかなかやりますね_H_」"
		PRINTFORMW 「でも、本当にキスだけでいいんですか？」
		CALL PRINT_STRW, @"「……外でいっぱいセックスして、街中でヘンタイ行為しちゃうどうしようもないケダモノになっちゃいたくありませんか_H_」"
	ENDIF
CASE "お尻を触る"
	IF YOKUBO()
		CALL PRINT_STRW, @"「……%CNAME("二人称", "甘え")%_H_　こんなところじゃ周りの人にバレちゃいますよ_H_」"
		CALL PRINT_STRW, @"「……それともいっそバラしちゃいますか？　%CNAME("一人称")%と%CNAME("二人称")%は人前でお尻を触る変態\@TALENT:相思相愛 ? カップル # 同士\@なんだって_H_」"
	ELSEIF TALENT:恋慕
		PRINTFORMW 「%CNAME("二人称")%！　こんな人前でなにするんですか！」
		CALL PRINT_STRW, "「う……い、嫌ではないですけどぉ……_H_」"
	ELSE
		PRINTFORMW 「……まったく、仕方ない%CNAME("二人称")%なんですから……」
	ENDIF
