				CALL PRINT_STRW, "、去年も結構頑張りましたけど、今度はさらに踏み切って、こういうのにしてみませんか_H_」"
			;去年の水着を選んでないパターン
			CASE ""
				CALL PRINT_STRW, "_H_コレ、どうですか_H_」"
			CASEELSE
				CALL PRINT_STRW, "、去年はかわいらしい方向でいってみましたけど、今年は頑張ってこういうのにしてみませんか_H_」"
			ENDSELECT
			PRINTFORMW 「……こんな格好で泳ぎになんて行ったら、いったいどうなっちゃうんでしょうか」
			CALL PRINT_STRW, @"「%CNAME("一人称")%が男の子なのにこんな格好してるってバレちゃったりして_H_」"
		ELSE
			PRINTFORMW 「嗯、」
			PRINTFORMW 「…………こ、この格好して人前に出るんですか……%CNAME("一人称")%……」
			PRINTFORMW 「……………………」
			PRINTFORMW 「……い、いいでしょう……」
		ENDIF
	ENDSELECT

