			CALL PRINT_STRW, @"「……こんな格好で泳ぎに行っちゃったら、『うっかり』%CNAME("一人称")%が男の子だってバレちゃうかもしれませんね_H_」"
		ELSE
			PRINTFORMW 「い、いくらなんでも小さすぎませんか……？」
			CALL PRINT_STRW, @"「こんな格好じゃ%CNAME("一人称")%が男の子だってバレちゃいますよ……_H_」"
		ENDIF
	CASE "ビキニ", "パレオ", "ワンピース水着"
		PRINTFORMW 「……%CNAME("二人称")%、お待たせしました」
		SELECTCASE SWIM(CSTR:今年の水着, "種類")
		;去年の水着を選んでないパターン
		CASE ""
			PRINTFORMW 「デザインが気に入って着てみたんですが……どうですか？」
		CASE "メンズビキニ", "サーフパンツ"
			PRINTFORMW 「前回は男っぽい格好だったので、今回は女の子っぽい格好にしてみました」
		CASE "競泳水着"
			PRINTFORMW 「ぱっと見は前とあまり変わらないかもしれませんが……どうですか？」
		CASEELSE
			IF YOKUBO()
				PRINTFORMW 「去年は過激な水着だったので、今回は逆に落ち着いた感じにしてみました」
				CALL PRINT_STRW, @"「これはこれでイイと思いませんか_H_%CNAME("二人称")%_H_」"
			ELSE
				PRINTFORMW 「去年の水着は……その……刺激的すぎたので、もう少し落ち着いた感じにしてみたんですが……どうですか？」
			ENDIF
		ENDSELECT
	CASE "競泳水着"
		PRINTFORMW 「……%CNAME("二人称")%、お待たせしました」
		IF YOKUBO()
			IF CSTR:今年の水着 == ""
				PRINTFORMW 「選んだときは刺激的にはならないだろうと思っていたんですが……」
				CALL PRINT_STRW, @"「どうですか%CNAME("二人称")%、興奮してきませんか_H_」"
			ELSE
				CALL PRINT_STRW, "「一見健康的なのに、いざ着てみると途端に変態的に見えるギャップに夢中になっちゃったんですよねぇ_H_」"
			ENDIF
		ELSE
			IF CEVENT("デート：競泳水着")
				PRINTFORMW 「えっと……そんな『なんでまたそれを選んだんだ』って顔をしなくても……」
			ELSE
				PRINTFORMW 「スポーティな感じにしてみましたが、どうですか？」
			ENDIF
		ENDIF
	CASE "ハイレグワンピ"
		IF YOKUBO()
			CALL PRINT_STRW, @"「……%CNAME("二人称")%_H_コレ、どうですか_H_」"
			SELECTCASE SWIM(CSTR:今年の水着, "種類")
			;去年の水着を選んでないパターン
			CASE ""
				CALL PRINT_STRW, "「……自分で言うのもなんですが、男なのにこの水着を着るのって、中々倒錯的な光景ですね……_H_」"
			CASE "ワンピース水着", "競泳水着"
				PRINTFORMW 「去年の水着とつくりは似たようなもののハズなのに、こうやって見ると結構違った印象になりますね」
			CASE "ビキニ"
				CALL PRINT_STRW, "「布面積は去年より増えたはずなのに、こっちのほうがいやらしい水着に見えますね……_H_」"
			CASEELSE
				CALL PRINT_STRW, "「思い切って去年のものと変えてみましたけど、これはこれで……_H_」"
			ENDSELECT
		ELSE
			PRINTFORMW 「……こ、この格好は流石に倒錯的ではないですか……？」
			;今更それを言うか
		ENDIF
	CASE "メンズビキニ", "サーフパンツ"
		PRINTFORMW 「……%CNAME("二人称")%、お待たせしました」
		PRINTFORMW 「……？　確かにいつもは女の子の格好してますけど、%CNAME("一人称")%の性別は男な訳ですし、問題ないでしょう？」
		SIF YOKUBO()
			CALL PRINT_STRW, "「……それに、これはこれで興奮すると思いませんか_H_」"
	;その他はだいたいエロ水着です
	CASEELSE
		IF YOKUBO()
			PRINTFORM 「……%CNAME("二人称")%
			SELECTCASE SWIM(CSTR:今年の水着, "種類")
			CASE "競泳水着"
				CALL PRINT_STRW, "、去年は健康的な方向でしたし、次は思い切ってこういうのとかどうですか_H_」"
			CASE "マイクロビキニ", "ハイレグワンピ"
				CALL PRINT_STRW, "、去年も結構頑張りましたけど、今度はさらに踏み切って、こういうのにしてみませんか_H_」"
			;去年の水着を選んでないパターン
			CASE ""
				CALL PRINT_STRW, "_H_コレ、どうですか_H_」"
			CASEELSE
				CALL PRINT_STRW, "、去年はかわいらしい方向でいってみましたけど、今年は頑張ってこういうのにしてみませんか_H_」"
			ENDSELECT
			PRINTFORMW 「……こんな格好で泳ぎになんて行ったら、いったいどうなっちゃうんでしょうか」
			CALL PRINT_STRW, @"「%CNAME("一人称")%が男の子なのにこんな格好してるってバレちゃったりして_H_」"
		ELSE
			PRINTFORMW 「え、」
			PRINTFORMW 「…………こ、この格好して人前に出るんですか……%CNAME("一人称")%……」
			PRINTFORMW 「……………………」
			PRINTFORMW 「……い、いいでしょう……」
		ENDIF
	ENDSELECT

;プールなどで水着姿に着替え、待ち合わせ場所に集合した時です
CASE "水着に着替えた"
	PRINTFORMW 「お待たせしました」
	;どの水着に着替えたか？
	SELECTCASE SWIM(CSTR:今年の水着, "種類")
	CASE "ビキニ",  "ワンピース水着"
		PRINTFORMW 「この格好はどうですか？　%CNAME("二人称")%」
		IF YOKUBO()
			CALL PRINT_STRW, "「もし気に入ってくれたのなら、後でこの水着でエッチなことしませんか_H_」"
		ELSE
			PRINTFORMW 「似合ってる……ですか？　ありがとうございます！」
		ENDIF
	CASE "競泳水着"
		IF YOKUBO()
			CALL PRINT_STRW, "「フフッ_H_見られちゃってますねぇ_H_」"
			CALL PRINT_STRW, @"「この格好だと%CNAME("一人称")%が男だなんて簡単にバレちゃいます……_H_」"
		ELSE
			IF CEVENT("デート：競泳水着") == 0
				PRINTFORMW 「…………お、思ったより目立つ格好ですよね……」
				PRINTFORMW 「なんで大丈夫だと思っちゃったんだろう……」
				CALL SET_CEVENT, "デート：競泳水着"
