;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K25, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	IF YOKUBO()
		PRINTFORMW 「もう、なんでずっとエッチしてくれなかったんですか」
		CALL PRINT_STRW, @"「……代わりに、今日は%CNAME("一人称")%に%CNAME("二人称")%の体、いっぱい使わせてくださいね_H_」"
	ELSE
		PRINTFORMW 「……%CNAME("二人称")%のせいですからね」
		CALL PRINT_STRW, @"「%CNAME("二人称")%のせいで、こんなエッチな体になっちゃったんですからね……_H_」"
	ENDIF
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		IF YOKUBO()
			PRINTFORMW 「おはようございます」
			IF RAND:2
				CALL PRINT_STRW, "「寝てるときに挿れるとどんな感触なんだろうって思ってたら、ついやっちゃいました_H_」"
			ELSE
				CALL PRINT_STRW, "「フフッ_H_寝てる時も良かったですけど、起きている時はやっぱり格別ですね_H_」"
			ENDIF
		ELSE
			PRINTFORMW 「あ……おはようございます」
			PRINTFORMW 「その……朝から用事を済ませたほうが負担も少なくなるかと思って……」
		ENDIF
	CASE "朝クンニ"
		IF YOKUBO()
			PRINTFORMW 「おはようございます」
			PRINTFORMW 「ここからとってもエッチな%TEXTS("匂い")%出しまくってましたからうっかり来ちゃいました」
		ELSE
			PRINTFORMW 「え！　あ……おはようございます」
			PRINTFORMW 「その……出来心といいますか……」
			PRINTFORMW 「あの……もう終わったので……」
		ENDIF
		SIF EXP_GAIN("絶頂経験", MASTER) >= 2
			CALL PRINT_STRW, "「フフ……ここ触るとまだビクビクする……そんなに気持ちよかったですか……_H_」"
	CASE "朝足コキ"
		PRINTFORMW 「\@MARK:屈服刻印 < 3 ? え！　もう……？　その…… # ようやく起きましたか。\@おはようございます」
		IF CFLAG:Ｓっ気フラグ
			CALL PRINT_STRW, @"「%ADANA("ペニス")%を踏まれてるのにこんなに硬くするなんて、%CNAME("二人称")%ってとんでもないヘンタイさんなんですね_H_」"
			CALL PRINT_STRW, @"「フフッ_H_大丈夫ですよ。%CNAME("一人称")%はそんなヘンタイさんな%CNAME("二人称")%が大好きですから_H_」"
		ELSEIF YOKUBO()
			PRINTFORMW 「こんな時間なのに%CNAME("二人称")%の\@TALENT:MASTER:オトコ ? 生意気なおちんちんが勃っていたから # はしたないおまんこがヒクヒクしてたから\@、ちゃんと鎮めてあげているんですよ？」
		ELSE
			PRINTFORMW 「……こんな時間に起きた悪い子にはこうです！」
		ENDIF
	CASE "朝の新妻プレイ"
		PRINTFORMW 「おはようございます」
		IF TALENT:相思相愛 && RAND:2
			PRINTFORMW 「朝ごはんはリビングに置いていますよ」
			CALL PRINT_STRW, @"「それとも……先に%CNAME("一人称")%を食べちゃいますか、%TEXTR(@"あなた/%CALLNAME:MASTER%/\@TALENT:MASTER:オトコ ? ダーリン # ハニー\@")%_H_」"
		ELSEIF YOKUBO()
			CALL PRINT_STRW, "「……このまま朝からしちゃいましょうか_H_」"
		ELSE
			CALL PRINT_STRW, "「えっ、ここでもうしちゃうんですか……エッチ_H_」"
		ENDIF
	CASE "朝パイズリ"
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "通常", 4)%」"
		IF YOKUBO() || TALENT:恋慕
			CALL PRINT_STRW, @"「……あ、おはようございます……%AEGI(TARGET, "要", "通常", 2)%」"
			IF RAND:2
				CALL PRINT_STRW, @"「……フフ_H_%CNAME("二人称")%の%ADANA("ペニス")%がピクピクしてるのが胸の奥にまで響いてきます……_H_」"
			ELSE
				CALL PRINT_STRW, @"「……フフ_H_%CNAME("二人称")%の%ADANA("ペニス")%、とっても熱いですね……_H_」"
			ENDIF
		ELSE
			PRINTFORMW 「……え！？も、もう起きたんですか……？」
			PRINTFORMW 「……そ、その、今%CNAME("一人称")%がやってることは見なかったことにして頂けると……」
		ENDIF
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "通常", 5)%」"
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "通常")%%CNAME("二人称")%_H_%AEGI(TARGET, "要", "Ａ異物")%おはようっ_H_ございましゅっ_H_」"
		IF (YOKUBO() || ABL:欲望 >= 5) && RAND:2
			IF RAND:2
				CALL PRINT_STRW, @"「%CNAME("二人称")%っ_H_%CNAME("二人称")%っ_H_」"
				CALL PRINT_STRW, @"「%CNAME("二人称")%にっ_H_%ADANA("アナル")%開発されたせいでぇっ_H_ガマンできない子になっちゃったじゃないですかぁっ_H_」"
				CALL PRINT_STR, @"「責任とってっ_H_%CNAME("二人称")%の"
				SELECTCASE CONDS("潜在的含むペニスサイズ", PLAYER)
				CASE "短小"
					PRINTFORM %TEXTR("赤ちゃん/かわいい")%
				CASE "普通"
					PRINTFORM %TEXTR("立派な/硬い")%
				CASE "巨根"
					PRINTFORM %TEXTR("おっきい/極太")%
				CASE "馬並み"
					PRINTFORM %TEXTR(@"馬並み/%ADANA("アナル")%いっぱいいっぱいにされちゃう")%
				CASE "腕並み"
					PRINTFORM %TEXTR("腕みたいに大きい/馬より大きな")%
				CASE "規格外"
					PRINTFORM %TEXTR("人外/規格外の")%
				ENDSELECT
				CALL PRINT_STRW, @"「%ADANA("ペニス")%で%CNAME("一人称")%の%ADANA("アナル")%いじめてよぉっ_H_」"
			ELSE
				;煮込みうどんマンさんから頂いたものをベースに書かせていただきました
				;大変ありがとうございます
				CALL PRINT_STRW, @"「アハ_H_%CNAME("二人称")%の今日の童貞、貰っちゃいましたぁ_H_」"
				CALL PRINT_STRW, @"「%ADANA("ペニス")%バッキバキ……_H_%AEGI(TARGET, "要", "通常")%」"
				CALL PRINT_STRW, @"「もう……がっつきすぎですよ……_H_」"
				CALL PRINT_STRW, @"「フフ_H_%CNAME("二人称")%の今日の初物ザーメン、いっぱい搾り取っちゃいます_H_」"
			ENDIF
		ELSE
			IF RAND:2
				CALL PRINT_STRW, @"「ごめんなさいっ_H_ガマンっ_H_できなくてっ_H_もうちょっとだけっ_H_」"
			ELSE
				CALL PRINT_STRW, @"「あっ_H_そんなっ_H_みにゃいでぇっ_H_」"
			ENDIF
		ENDIF
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "通常", RAND:3 + 5)%」"
	CASE "朝フェラ", "朝シックスナイン"
		IF EXP_GAIN("射精経験")
			CALL PRINT_STRW, @"「\@YOKUBO() ? おはようございます。それと……ごちそうさまでした_H_ # あ……その……おはようございます\@」"
		ELSE
			IF YOKUBO()
				PRINTFORMW 「おはようございます。もう少ししたら終わるのでちょっと待っててください」
				SIF TALENT:MASTER:包茎
					PRINTFORMW 「まったく……こんなにチンカスたっぷり貯めちゃって……」
				CALL PRINT_STRW, @"「……こんなエッチな%TEXTS("匂い")%ばら撒きながら寝られたら我慢できないじゃないですか……_H_」"
			ELSE
				PRINTFORMW 「あ……す、すぐ終わらせます……でももう少し……」
			ENDIF
			CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "Ｍ異物", 5)%」"
		ENDIF
	CASE "朝キス"
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "キス", 5)%」"
		PRINTFORMW 「……ハッ！ も、もしかして、%CNAME("二人称")%起きてます……？」
		PRINTFORMW 「そ、その、さっきのは忘れてください……ダメ、ですか？」
	CASE "Ｗ朝フェラ"
		CALL PRINT_STRW, @"「%AEGI(TARGET, "要", "Ｍ異物", 3+RAND:3)%」"
		IF YOKUBO()
			PRINTFORMW 「……あ、おはようございます」
			CALL PRINT_STRW, @"「フフ_H_%CNAME("二人称")%の%ADANA("ペニス", TARGET, MASTER)%が素敵すぎるせいで、%CNAME("パートナー")%もやってきちゃったじゃないですか_H_」"
			CALL PRINT_STRW, @"「責任とって、二人にいっぱいジュポジュポされてくださいね_H_」"
		ELSE
			PRINTFORMW 「……あ！　お、おはようございます……」
			PRINTFORMW 「う…………こ、これは、こう、色々と……」
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORM 「今日は体育がありますね。
		IF COND("夏")
			PRINTFORMW 熱中症にならないようにちゃんと水分を摂ってくださいよ？」
		ELSE
			IF RAND:2
				PRINTFORMW 目いっぱい体を動かしましょう」
			ELSE
				PRINTFORMW ちゃんと体操もしてくださいよ？」
			ENDIF
		ENDIF
	ELSEIF COND("水泳")
		PRINTFORMW 「今日は水泳がありますね。目に水が入らないように気をつけてくださいね」
	;マリーさんの中の人さんのアドバイスにより、ちょっとしたアートうんちくを加えてみることに
	;とりあえずここに放り込む
	ELSEIF RAND:10 == 0 && CEVENT("写真蘊蓄") == 0
		PRINTFORMW 「おはようございます、%CNAME("二人称")%」
		PRINTFORMW 「今日の一時限目は……美術ですか」
		PRINTFORMW 「……そういえば%CNAME("二人称")%は知ってますか？ 写真って美術にものすごい影響を与えたってことを」
		PRINTFORMW 「元々ですね、ルネサンスの時代にアラブ世界からヨーロッパにギリシャ芸術が再び入ってきて以来、絵画といったものは物事を写実的に写すものでした」
		PRINTFORMW 「なぜそのようなことが起きたのかと言うと、それはそれで色々な歴史があるのですが……それはとりあえず置いておいて」
		PRINTFORMW 「もちろん写実的であるという前提はそのままながらも、当時の美術も発展して進化していっていたのですが……写真の登場によってその流れは大きく変わりました」
		PRINTFORMW 「当時の写真というのは、今と比べるとかなり撮影に時間のかかるものではあったようなのですが、それでも現実をそのまま撮影できるという事実は絵画のアドバンテージを大きく損ねるものでした」
		PRINTFORMW 「それにより絵画というものは、あるがままを描くものというよりも、自分の思う景色、主観的な景色を描くものへと変化していきます」
		PRINTFORMW 「『睡蓮』や、『印象、日の出』といった作品が有名な印象派も、写真の影響を受けていたようですね」
		PRINTFORMW 「……まあ、これは今よく見るチューブに入った絵の具が開発されたことにより、どうしても時間の関係上屋内で描かざるを得なかったそれまでの絵画から、屋外でも描けるようになったという変化も大きいようですが」
		PRINTFORMW 「……さて、そろそろ時間ですね。今日も一日頑張りましょうね、%CNAME("二人称")%」
		CALL SET_CEVENT, "写真蘊蓄"
	ELSEIF RAND:2
		IF COND("春")
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「おはようございます……ふわぁ」
			CASE 1
				PRINTFORMW 「おはようございます……もうちょっと寝たくなっちゃいますね……」
			CASEELSE
				PRINTFORMW 「おはようございます。暖かくなってきましたね」
			ENDSELECT
		ELSEIF COND("夏")
			IF RAND:50 == 0
				PRINTFORMW 「暑い……。今日は部活せずに家でダラダラしちゃいましょうよ%CNAME("二人称")%……」
			ELSEIF RAND:2
				PRINTFORML 「おはようございます、%CNAME("二人称")%」
				PRINTFORMW 「今日は暑いので、たくさんお水を飲みましょうね」
			ELSE
				PRINTFORML 「%CNAME("二人称")%…………おはようございます…………」
				PRINTFORMW 「…………暑い」
			ENDIF
		ELSEIF COND("秋")
			PRINTFORMW 「おはようございます。体育祭や文化祭や色々と忙しい時期になってきましたね」
		ELSE
			PRINTFORMW 「すっかり寒くなりましたね。」
		ENDIF
	ELSE
		IF FIND_COND("現在存在", "ツェツィーリア") && COND("相性◎", FIND_COND("ツェツィーリア")) && RAND:2
			PRINTFORMW 「おはようございます。今日は体育がありますね」
			PRINTFORMW 「この前%CNAME("ツェツィーリア")%からヨガを教えてもらったんです。一緒にやってみませんか？」
		ELSE
			PRINTFORMW 「今日もがんばりましょう」
		ENDIF
	ENDIF
CASE "一番乗り"
	SELECTCASE RAND:5
	CASE 0
		PRINTFORMW 「どうやら一番乗りの様ですね」
		PRINTFORMW 「……実は、皆さんと一緒に食べるためにお菓子を買ってきたんですが……」
		PRINTFORMW 「せっかくですし、一足先に食べちゃいましょうか。%CNAME("二人称")%」
	CASE 1
		PRINTFORMW 「どうやら一番乗りみたいですね」
		PRINTFORMW 「……%CNAME("二人称")%、今日、実はトランプ持ってきてるんですよ」
		PRINTFORMW 「後で皆さんとしようと思ってたんですが、一足先にやっちゃいますか？」
	CASE 2
		PRINTFORMW 「早すぎちゃったみたいですね」
		IF TALENT:相思相愛
			CALL PRINT_STRW, @"「……%CNAME("二人称")%、今のうちにイチャイチャしてもいいですか……_H_」"
		ELSE
			PRINTFORMW 「……あ、そうだ。実はとっておきの紅茶を置いてるんですよ」
			PRINTFORMW 「せっかくですし一緒に飲んじゃいましょうか」
		ENDIF
	CASE 3
		PRINTFORMW 「おはようございます、%CNAME("二人称")%。お互い早く来すぎちゃいましたね」
	CASEELSE
		PRINTFORMW 「どうやら早かったみたいですね。皆さんのために先に準備でもしておきましょうか」
	ENDSELECT
CASE "ただいま"
	IF TALENT:相思相愛
		PRINTFORMW 「%CNAME("二人称")%、ただいま戻りました」
		PRINTFORMW 「え？　せっかく恋人なんだからもっと軽い感じでいいって？」
		PRINTFORMW 「ちょ、ちょっとそれは……今からそんなことしちゃったら%CNAME("一人称")%、嬉しいやら恥ずかしいやらで耐え切れなくなりそうで……」
	ELSE
		PRINTFORMW 「た、ただいま戻りました……」
		PRINTFORMW 「アハハ……%CNAME("二人称")%の家に帰っているっていうのがいまいち実感がわかなくて……」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			PRINTFORMW 「お帰りなさい」
		ELSE
			PRINTFORMW 「戻られましたか」
		ENDIF
	ELSE
		IF YOKUBO()
			SELECTCASE RAND:2
			CASE 0
				CALL PRINT_STRW, "「今日の部活も頑張りましょう_H_」"
			CASEELSE
				CALL PRINT_STRW, "「フフ、今日は誰と励むんですか_H_」"
			ENDSELECT
		ELSE
			PRINTFORMW 「今日もしっかり監視しますからね」
		ENDIF
	ENDIF
CASE "入室挨拶"
	PRINTFORMW 「お待たせしました」
	IF DAYEV("ハロウィン")
		PRINTFORMW 「今日はハロウィンですね」
		SIF CFLAG:コスプレ中
			PRINTFORMW 「……あの、コスプレをしてみたんですけど、どうでしょうか……？」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「何か御用ですか？」
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		PRINTFORMW 「……って、い、一体何を……！？」
		SIF YOKUBO()
			CALL PRINT_STRW, "（もしこんなことをされたら……_H_）"
	ELSEIF DAYEV("クリスマス")
		PRINTFORMW 「今日はクリスマスですね」
	ELSEIF DAYEV("バレンタイン")
		PRINTFORMW 「今日はバレンタインですか」
		SIF CDAYEV("バレンタイン") == 0
			PRINTFORMW 「……あの、%CNAME("二人称")%、ちょっと時間を貰ってもいいですか？」
	ELSEIF DAYEV("お正月")
		PRINTFORMW 「年が明けましたね」
		PRINTFORMW 「今年もよろしくお願いしますね、%CNAME("二人称")%」
	ELSEIF RAND:5 == 0
		SELECTCASE TEXTS("季節詳細")
		CASE "春"
			PRINTFORMW 「今日も暖かいですね」
		CASE "初夏"
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「今日は晴れるみたいですね」
			CASEELSE
				PRINTFORMW 「今日も雨みたいですね」
			ENDSELECT
		CASE "夏"
			SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「今日も暑いですね……」
			CASEELSE
				PRINTFORMW 「日差しが強くなってきましたね」
			ENDSELECT
		CASE "初秋"
			PRINTFORMW 「涼しくなってきましたね」
		CASE "秋"
			PRINTFORMW 「今日も涼しいですね」
		CASE "晩秋"
			IF RAND:2
				PRINTFORMW 「日が落ちるのが早くなってきましたね」
			ELSE
				PRINTFORMW 「段々寒くなってきましたね」
			ENDIF
		CASE "冬"
			PRINTFORMW 「今日も寒いですね……」
		ENDSELECT
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「遅れてすみません。生徒会が長引いてしまって」
	ELSEIF CFLAG:呼び出された
		IF RAND:2
			PRINTFORMW 「%CNAME("二人称")%、お待たせしました」
		ELSE
			PRINTFORMW 「%CNAME("二人称")%、なにかご用ですか？」
		ENDIF
	ELSE
		PRINTFORMW 「お待たせしました」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『こんにちは%CNAME(CALLNAME_ACTOR)%』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『今日は遅かったですね』
	ELSE
		PRINTFORMW 『こんにちは\@RAND:2 ? %CNAME(CALLNAME_ACTOR)% # \@』
	ENDIF
CASE "全裸化への反応"
	IF COND("合意：全裸化")
		IF DEVENT("全裸化目撃") == 0
			PRINTFORMW 「……え、ふ、二人ともいきなり何を……？」
			PRINTFORMW 「……ああ、そういうことですか……」
		ELSEIF RAND:10 == 0
			PRINTFORMW 「……いいなぁ……」
		ENDIF
	ELSEIF DEVENT("全裸化目撃") == 0
		PRINTFORMW 「え、え！？　ふ、二人ともいきなり何し始めてるんですか！？」
		PRINTFORMW 「いや、いつもやってることと言われると否定できませんが、そういう問題じゃなくて！」
	ENDIF
CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	;IF CMOVE("居間") && RAND:2

	;ELSE
		IF RAND:2
			PRINTFORML 「ええと、飲み物は……」
			PRINTFORMW 「……あ、%CNAME("二人称")%、居たんですか」
		ELSE
			PRINTFORMW 「あ、ここに居たんですね、%CNAME("二人称")%」
