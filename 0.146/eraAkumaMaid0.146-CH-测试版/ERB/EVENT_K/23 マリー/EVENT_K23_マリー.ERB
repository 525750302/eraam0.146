;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K23, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 你_你
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:肩書
			CASE "実母"
				LOCALS = お母様_おかあさま
			CASEELSE
				LOCALS = 貴方_あなた
		ENDSELECT
	ENDIF
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	IF NO:ASSI == 9
		LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
	ELSEIF NO:ASSI == 25
		LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
	ELSE
		LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
	ENDIF
CASE "調教時二人称"
	;主人と助手の取り扱いが逆になっていたので修正
	;助手
	IF ASSIPLAY 
		IF NO:ASSI == 9
			LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
		ELSEIF NO:ASSI == 25
			LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
		ELSE
			LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
		ENDIF
	;主人
	ELSE
		LOCALS = ご主人様_ごしゅじんさま
	ENDIF
ENDSELECT
RETURNF LOCALS

