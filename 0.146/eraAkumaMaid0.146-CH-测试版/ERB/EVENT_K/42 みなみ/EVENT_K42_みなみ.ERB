;近親好き向けになってます。
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K42, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	;相手が主人ではない時の特殊二人称もここ(ナンバー指定あり)
	IF PLAYER == MASTER
		IF TRUE_SEX(MASTER) != "オトコ"
			LOCALS = お姉ちゃん_おねえちゃん
		ELSE
			LOCALS = お兄ちゃん_おにいちゃん
		ENDIF
	ELSEIF NO:PLAYER == 10
		LOCALS = お母さん_おかあさん
	ELSEIF NO:PLAYER == 6
		LOCALS = さくらお姉ちゃん_さくらおねえちゃん
	ELSEIF NO:PLAYER == 51
		LOCALS = 棗さん_なつめさん
	;相手が主人ではない時の汎用口上
	;学生限定。
	;中等部以下…くんちゃん付け
	;高等部以上…先輩
	;教師…先生
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部"
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%先輩_%CSTR:PLAYER:ふりがな%せんぱい
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%先生_%CSTR:PLAYER:ふりがな%せんせい
			ELSE
				LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
    IF TRUE_SEX(MASTER) != "オトコ"
        LOCALS = お姉ちゃん_おねえちゃん
    ELSE
        LOCALS = お兄ちゃん_おにいちゃん
    ENDIF
CASE "助手"
	IF COND("メリィエーナ", ASSI)
	    LOCALS = お母さん_おかあさん
	ELSEIF COND("さくら", ASSI)
	    LOCALS = %CALLNAME:ASSI%お姉ちゃん_%CSTR:ASSI:ふりがな%おねえちゃん
;	ELSEIF COND("マリー", ASSI)
;		;「私ですか？　私はメスガキ解らせお姉さんです。姉ビーム！！」
;		;【%CALLNAME:TARGET%の中の常識が改変された！！】
;	    LOCALS = %CALLNAME:ASSI%お姉ちゃん_%CSTR:ASSI:ふりがな%おねえちゃん
	ELSEIF NO:PLAYER == 51
		LOCALS = 棗さん_なつめさん
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF
		CASE "１年生", "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%先輩_%CSTR:ASSI:ふりがな%せんぱい
		CASE "教師"
			LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
		CASEELSE
	    	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
		ENDSELECT
   ENDIF
CASEELSE
	;ARGSにキャラ名が入れられてる場合にはキャラへの呼び方なのでチェック
	IF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)

		IF NO:ACTOR == 10
			LOCALS = お母さん_おかあさん
		ELSEIF NO:ACTOR == 6
			LOCALS = さくらお姉ちゃん_さくらおねえちゃん
		ELSEIF NO:ACTOR == 51
			LOCALS = 棗さん_なつめさん
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部"
				IF TALENT:ACTOR:オトコ
					LOCALS = %CALLNAME:ACTOR%くん_%CSTR:ACTOR:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
				ENDIF
			CASE "１年生", "２年生", "３年生", "研究生"
				LOCALS = %CALLNAME:ACTOR%先輩_%CSTR:ACTOR:ふりがな%せんぱい
			CASEELSE
				IF COND("教員", ACTOR)
					LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
				ELSE
					LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
ENDSELECT
RETURNF LOCALS

