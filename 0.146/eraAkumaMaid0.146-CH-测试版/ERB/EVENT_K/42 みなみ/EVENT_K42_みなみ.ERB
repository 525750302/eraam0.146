		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;胸で射精させた
;-------------------------------------------------
ELSEIF SHOOT("胸")
	;初めて
	IF FIRST_EV("絶頂：胸") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:奉仕精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;手で射精させた
;-------------------------------------------------
ELSEIF SHOOT("手")
	;初めて
	IF FIRST_EV("絶頂：手") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:奉仕精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;セックスで射精させた
;-------------------------------------------------
ELSEIF SHOOT("Ｖ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ｖ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ELSEIF SHOOT("Ａ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ａ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
