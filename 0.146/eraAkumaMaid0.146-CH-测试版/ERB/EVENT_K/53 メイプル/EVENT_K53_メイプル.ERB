;【今回がんばったこと】
;・はじまりとおわりとあいぶとくんにとあなるあいぶかいた！　れんぼはいちぶのぞいておみっとした！

;【今後がんばりたいこと】
;・うまぴょいしてねえで　かけ
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K53, ARGS
#FUNCTIONS
;名前を呼ぶ相手のキャラ番号の代入に使う
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 僕_僕
CASE "二人称"
	LOCALS = 你_你
;この下は三人称。
CASE "主人"
	IF NAME:PLAYER == "你"
		LOCALS = 部長_部長
	ELSE
		LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
	ENDIF
CASEELSE
	;名前を呼ぶ相手のキャラ番号をACTORに代入する
	SELECTCASE ARGS
	;相手が助手
	CASE "助手"
		ACTOR = ASSI
	;ここにはARGSにキャラのCALLNAMEが入ったりしている場合(ARGS == "フェリシエル" など)に来る
	CASEELSE
		ACTOR = FIND_NO(ARGS)
	ENDSELECT

	;相手が教員
	IF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%老师_%CSTR:ACTOR:ふりがな%老师

	;自分も相手も学生なら学年比較
	ELSEIF COND("学生") && COND("学生", ACTOR)
		;相手が同学年
		IF COND("学年") == COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が下の学年
		ELSEIF COND("学年") > COND("学年", ACTOR)
			LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
		;相手が上級生
		ELSE
			LOCALS = %CALLNAME:ACTOR%前輩_%CSTR:ACTOR:ふりがな%前輩
		ENDIF

	;それ以外
	ELSE
		LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
	ENDIF
ENDSELECT
RETURNF LOCALS

