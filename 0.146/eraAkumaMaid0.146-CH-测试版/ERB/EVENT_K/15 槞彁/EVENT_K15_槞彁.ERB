@CNAME_K15, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "本名"
	LOCALS = ⊥лыъвьыкцлр
CASE "一人称"
	LOCALS = Ваташ
CASE "一人称翻訳後"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = Аната
	;相手が主人ではない
	ELSE
		LOCALS = Аната
	ENDIF
CASE "二人称翻訳後"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 貴方_あなた
	;相手が主人ではない
	ELSE
		LOCALS = 貴方_あなた
	ENDIF
CASE "主人"
	LOCALS = Ные
CASE "助手"
	LOCALS = %CALLNAME:ASSI%Сан_%CALLNAME:ASSI%Сан
ENDSELECT
RETURNF LOCALS
