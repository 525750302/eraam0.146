;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K100, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人　日本語ではこっちのが自然かもなので変えさせてもらいました
	IF PLAYER == MASTER
		LOCALS = ご主人様_ごしゅじんさま
	;相手が主人ではない
	ELSE
		LOCALS = 你_你
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
ENDSELECT
RETURNF LOCALS

