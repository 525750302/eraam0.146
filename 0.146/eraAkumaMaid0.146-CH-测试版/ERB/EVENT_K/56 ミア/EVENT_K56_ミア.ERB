	LOCALS = 主人様_主人大人
CASE "助手"
	LOCALS = %CALLNAME:ASSI%桑_%CSTR:ASSI:ふりがな%桑
; 以下ローカル追加用
CASE "肛門"
	LOCALS = 肛門_あなる/あなる_あなる/お尻_おしり/けつ穴_けつあな/けつまんこ_けつまんこ/
CASE "肛門"
