;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K49, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"
		PRINTFORMW 「ん……ふ、ぅ……あっ……んっ……」
		PRINTFORMW 快感に目を覚ますと%CALLNAME:TARGET%が%CALLNAME:MASTER%の上に跨っていた。
		PRINTFORMW 「ん…ふ、あっ……起こし、ちゃった……？ごめんね？」
		PRINTFORMW 「ホントは%CNAME("二人称")%の寝顔を見てるだけで良かったんだけどちょっと、ね？」
	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ"
		IF EXP_GAIN("射精経験", PLAYER)
			PRINTFORMW 「んく……んく……ふぅ……」
			PRINTFORMW 「いっぱい出たね？私の奉仕が気持ち良かったんだね」
			PRINTFORMW 「シて良かったって少し安心してるよ」
		ENDIF
		PRINTFORMW 涎と精液で汚れた顔を拭い
		PRINTFORMW 「改めて、おはよ」
		PRINTFORMW 「今日も良い一日だと良いね」
		PRINTFORMW %CALLNAME:TARGET%はそう%CALLNAME:MASTER%に微笑みかけた。
	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER)
			PRINTFORMW 「気持ちよかった？」
			PRINTFORMW 「なんて聞くのは無粋だったかな？」
			PRINTFORMW 「ありがとね」
		ENDIF
		PRINTFORMW 乱れた衣服を整えてから軽く白く長い髪をふわりとさせて
		PRINTFORMW 「改めて、おはよ」
		PRINTFORMW 「今日も良い一日だと良いね」
		PRINTFORMW %CALLNAME:TARGET%はそう%CALLNAME:MASTER%に微笑みかけた。
	ENDSELECT
		PRINTFORMW 「ん、おはよ」
	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("日曜")
	;IF TEXTS("曜日") == "日"
		PRINTFORMW 「今日は安息日だね」
		IF COND("デートの約束")
			PRINTFORMW 「逢引きの準備は万端だよ」
			CALL PRINT_STRW, @"「……楽しみにしてたんだよ？……悪い？_／／／_」" 
			CALL EMOTION_PLUS, "恥じらい", TARGET
			PRINTFORMW %CALLNAME:TARGET%は少しだけ照れ臭そうにそっぽ向いた。
		ELSEIF FIND_COND("デートの約束")
			PRINTFORMW 「確か他の子と約束があるのだっけ？」
			PRINTFORMW 「羨ましい、なんて感情は少し前まで思わなかったけど」
			PRINTFORMW 「今はそう思ってしまうね」
			PRINTFORMW 「だから、出掛けるまで%CNAME("二人称")%を独り占めさせてほしいな」
		ELSE
			PRINTFORMW 「今日は何か予定はないのかい？」
			PRINTFORMW 「だったら、私と少し出掛けないかい？」
			PRINTFORMW 「まぁ、%CNAME("二人称")%さえ良ければ、だけどね」
		ENDIF
	ELSEIF CEVENT("学校に通う死神")
		PRINTFORMW 「朝餉と学校の準備はしておいたから一緒に行こうね？」
	ELSE
		PRINTFORMW 「今日も良い一日だと良いね？」
	ENDIF
	IF COND("体育")
		PRINTFORMW 「今日は体育があったでしょ？」
		PRINTFORMW 「用意してあるから忘れないでね？」
	ENDIF
CASE "一番乗り"
	PRINTFORML 「おや、%CNAME("一人称")%が一番乗りかい？」
	PRINTFORMW 「死神が一番最初に来るのは死と切り離せない%CNAME("二人称")%らしいね」
CASE "ただいま"
	PRINTFORML 「ただいま」
	PRINTFORML 「今、帰ったよ」
	PRINTFORMW 「……うん、ありがと」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			PRINTFORML 「あっ……、おかえり」
			PRINTFORML 「……少しだけ寂しかったって言うと」
			PRINTFORMW 「面倒くさいって思うかな？」
		ELSE
			PRINTFORMW 「やぁ、おかえり」
		ENDIF
	ELSEIF COND("昼")
		IF RAND:2 == 0
			PRINTFORMW 「やぁ、今日は%CNAME("一人称")%の方が先だったね」
		ELSE
			PRINTFORML 「やぁ、駆け付け三杯とは言わないけれど」
			PRINTFORMW 「%CSTR:お茶%でもどうだい？」
		ENDIF
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		PRINTFORML 「ととっ、ごめんね」
		IF RAND:3 == 0
			PRINTFORML 「少し周りをふらふらしてたんだ」
		ELSEIF RAND:2 == 0
			PRINTFORML 「ほら同級のあの子に少し呼び止められていたんだ」
		ELSE
			PRINTFORML 「実は統括機体の子から緊急の業務報告受けてたんだ」
		ENDIF
		PRINTFORMW 「%CNAME("二人称")%に手間かけさせちゃったね」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		IF RAND:3 == 0
			PRINTFORML 「やぁ………ふぅ～ん」
			PRINTFORMW じとーっとした瞳で%CALLNAME:TARGET%は%CALLNAME:MASTER%を見つめている。
		ELSEIF RAND:2 == 0
			PRINTFORML 「やぁ……なるほど」
			PRINTFORML 「なるほどなるほどなるほど」
			PRINTFORMW 笑みを作って頷いているが%CALLNAME:TARGET%の瞳は虹色を帯び全く笑っていない……
		ELSE
			PRINTFORML 「やぁ…っと、どうやらお邪魔みたいだね？」
			PRINTFORMW そのままドアを開けて出て行こうとする%CALLNAME:TARGET%を止めて何とかなった。
		ENDIF
	ELSE
		PRINTFORML 「やぁ、こんにちは」
		IF RAND:3 == 0
			PRINTFORML 「少し散策してから来たから%CNAME("二人称")%の方が先だったね」
		ELSEIF RAND:2 == 0
			PRINTFORML 「少し同級のあの子と話しててね、遅れてしまったね」
		ELSE
			PRINTFORML 「統括機体の子から緊急の業務報告を受けてたから少し遅れちゃった」
		ENDIF
		PRINTFORMW そう言いながら%CALLNAME:TARGET%は指定席の様に自然な流れで%CALLNAME:MASTER%の隣に座った。
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORML 「やぁ、こんにちは」
		PRINTFORMW 「ととっ、もう勢ぞろいだね」
	ELSEIF CFLAG:呼び出された
		PRINTFORML 「ととっ、こんにちは」
		PRINTFORMW 「ごめんね、%CNAME("二人称")%に手間かけさせたみたいだね」
	ELSE
		PRINTFORML 「やぁ、こんにちは」
		PRINTFORMW 軽く挨拶した後、%CALLNAME:TARGET%は%CALLNAME:MASTER%の横に座った。
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("入室挨拶中","ノイ")
		IF DEVENT("ノイを認識") == 0
			PRINTFORML 『機体を検知、ΔΑ-Kyrios-06……』
			PRINTFORML 『検索、検索完了。識別名称ノイ・ドミナ』
			PRINTFORML 『まさかあん畜生から離反した機体が教職をやっているとはね……』
			CALL SET_DEVENT, "ノイを認識", TARGET
		ELSE
			PRINTFORML 『おや、%CSVCALLNAME(NO:ACTOR)%じゃないか』
		ENDIF
		PRINTFORML 『おっと、学校では%CSVCALLNAME(NO:ACTOR)%先生だったね』
		PRINTFORMW 『あらためて、こんにちは』
	ELSEIF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『%CSVCALLNAME(NO:ACTOR)%先生、こんにちは』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『やぁ、%CSVCALLNAME(NO:ACTOR)%。少し遅れたようだね』
	ELSE
		PRINTFORMW 『やぁ、%CSVCALLNAME(NO:ACTOR)%。こんにちは』
	ENDIF
CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「ん、%CNAME("一人称")%に何か用かい？」
			PRINTFORMW 「何て、用が無くても%CNAME("二人称")%に呼び出されたら来るけどね？」
		ELSE
			PRINTFORMW 「ん、呼ばれてやってきたけど何か用でもあるのかな？」
		ENDIF
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ん……戻ったよ」
	ELSE
		IF COND("夜")
			IF TALENT:恋慕
				PRINTFORMW 「やぁ、こんばんは。良い夜だね」
			ELSE
				PRINTFORMW 「ん……。おや、こんばんは」
			ENDIF
		ELSE
			IF TALENT:恋慕
				PRINTFORMW 「やぁ、こんにちは。今日も良い日になるといいね」
			ELSE
				PRINTFORMW 「ん……。おや、こんにちは」
			ENDIF
		ENDIF
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		PRINTFORML 「そうかい、そうかい、そうなのかい」
		PRINTFORMW %CALLNAME:TARGET%の少し膨れた顔が見えた気がする。
	ELSE
		PRINTFORMW 「おや……、ふむ……。いってらっしゃい」
	ENDIF
CASE "お弁当"
	IF TALENT:恋慕
		PRINTFORML 「はい、お弁当だよ」
		IF DEVENT("お弁当同伴初めて") == 0
			PRINTFORML 「ん……？%CNAME("一人称")%もかい？」
			PRINTFORMW 「ちょっと待っててね、購買部で何か買ってくるから」
			CALL SET_DEVENT, "お弁当同伴初めて", TARGET
		ELSE
			PRINTFORML %CALLNAME:MASTER%にお弁当を渡した%CALLNAME:TARGET%は横にちょこんと座り、
			IF RAND:3 == 0
				PRINTFORML 渡されたお弁当とは違う内容のお弁当を広げた。
				PRINTFORMW 「こうやって違う内容のお弁当だったら、交換とか出来るから少しだけ頑張ったんだ」
			ELSEIF RAND:2 == 0
				PRINTFORML 「じゃあ、いただきます。ふふっ♪」
				PRINTFORMW 何故だか%CALLNAME:TARGET%は朗らかに笑っている。
			ELSE
				PRINTFORML 「あーんって言うのをやってみたいんだけど……ダメ、かな？」
				PRINTFORML %CALLNAME:TARGET%はお弁当のオカズを箸で取って%CALLNAME:MASTER%に差し出してきた。
				PRINTFORMW 少し気恥ずかしいが差し出されたオカズを食べた。
			ENDIF
		ENDIF
	ELSE
		PRINTFORML 「はい、頼まれてたお弁当だよ」
		IF DEVENT("お弁当同伴初めて") == 0
			PRINTFORML 「ん……？%CNAME("一人称")%もかい？」
			PRINTFORMW 「ちょっと待っててね、購買部？って場所で何か買ってくるから」
			CALL SET_DEVENT, "お弁当同伴初めて", TARGET
		ELSEIF RAND:3 == 0
			PRINTFORML 「では、いただきます」
			PRINTFORMW 「……この食べ物に感謝するのって嫌いじゃないな」
		ELSEIF RAND:2 == 0
			PRINTFORML 「味の方はどうだい？」
			PRINTFORMW 「%CNAME("二人称")%の口に合えば良いんだけど……」
		ELSE
			PRINTFORMW 「こうやって一緒に食事をするって、存外心地いいね」
		ENDIF
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		PRINTFORML 「ふぅ～ん……ふぅ～ん……」
		PRINTFORMW 気のせいだが%CALLNAME:TARGET%に若干半目で睨まれている気がする……
	ELSE
		PRINTFORML 「お盛んだねぇ……」
		PRINTFORMW %CALLNAME:TARGET%は苦笑している。
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K49, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「ん、これから厄介になるね」
CASE "汗だく"
	IF DEVENT("一言汗だく：はじめて") == 0
		PRINTFORML 「…………なんであん畜生はこんな機能を追加したのだろ？」
		PRINTFORML %CALLNAME:TARGET%は若干不機嫌そうな顔をしている。
		CALL SET_DEVENT, "一言汗だく：はじめて"
	ELSE
		IF RAND:3 == 0
			PRINTFORML 「こう暑いと氷菓子が欲しくなるねぇ」
			PRINTFORML ……最近アイスのゴミが増えたのは%CALLNAME:TARGET%の所為だったのか……
		ELSEIF RAND:2 == 0
			PRINTFORML 「冷房って便利だねぇ」
			PRINTFORML 「今までは暑さとか寒さを感じなかったから、%CNAME("二人称")%達の文明に感謝だよ」
		ELSE
			PRINTFORML 「よく太陽が憎いって言葉を聞いたけどこう言う事だったんだね……」
			PRINTFORML 「確かにここまで不快指数が高いと憎くなっても仕方ないね」
		ENDIF
	ENDIF
CASE "寝起き"
	PRINTFORML 「ん……保全作業完了」
	IF CHOSE_DEVENT("睡眠姦Ｖ中出し")
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「ん……_HH_ねぇ、もっと欲しいなって言ったら、してくれる？」"
			PRINTFORML 少し上気した顔で熱っぽく%CALLNAME:TARGET%は%CALLNAME:MASTER%を見つめている。
			PRINTFORML 【%CALLNAME:TARGET%の欲求が深まった】
			BASE:欲求不満 += 50
		ELSE
			PRINTFORML 「まったく……%CNAME("二人称")%は仕方がないなぁ」
			PRINTFORML 少し呆れつつも%CALLNAME:TARGET%は少し笑みをつくっている。
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦フェラ")
		IF CHOSE_DEVENT("睡眠姦顔射")
			IF TALENT:恋慕
				PRINTFORML 「顔にかけるだけで満足しちゃったんだ……」
				PRINTFORML なんだか%CALLNAME:TARGET%は少し不満気の様だ……
				PRINTFORML 【%CALLNAME:TARGET%の欲求が少し深まった】
				BASE:欲求不満 += 15
			ELSE
				PRINTFORML 「うーん、ちゃんと拭いてくれたみたいだけど、臭いが凄いよ？」
				PRINTFORML %CALLNAME:TARGET%の光輪が少し光を強めた後、精子の臭いが無くなった。
			ENDIF
		ELSEIF CHOSE_DEVENT("睡眠姦精飲")
			IF TALENT:恋慕
				CALL PRINT_STRL, @"「んっく……_HH_%CNAME("一人称")%の身体で気持ち良くなってくれたんだね」"
				PRINTFORML 軽く舌なめずりをして少し熱っぽい瞳で%CALLNAME:TARGET%は微笑んだ。
				PRINTFORML 【%CALLNAME:TARGET%の欲求が少し深まった】
				BASE:欲求不満 += 15
			ELSE
				PRINTFORML 「んく……もご……、口の中が%CNAME("二人称")%のだらけだよ。まったくもう……」
				PRINTFORML 呆れた顔をしているが嫌そうにしてはいないみたいだ……
			ENDIF
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦ティッシュ処理")
		IF TALENT:恋慕
			PRINTFORML 「ちょっと、%CNAME("二人称")%のが欲しかったな……」
			PRINTFORML 期待するような眼差しで%CALLNAME:TARGET%は%CALLNAME:MASTER%を見つめてくる。
			PRINTFORML 【%CALLNAME:TARGET%の欲求が少し深まった】
			BASE:欲求不満 += 15
		ELSE
			PRINTFORML 「ん、ちゃんと処理出来て偉い偉い」
			PRINTFORML %CALLNAME:MASTER%をあやす様に褒めてきた……
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦キス")
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「口づけでなんて、まるで御伽噺みたいだね_／／／_」"
			PRINTFORML %CALLNAME:TARGET%は少しテレテレとしている。
		ELSE
			PRINTFORML 「口づけが好きなんだね。嫌いじゃないよ」
			PRINTFORML そう%CALLNAME:TARGET%は少し朗らかに笑っている。
		ENDIF
	ELSE
		PRINTFORML 「ごめんね。待たせてたみたいかな？」
	ENDIF
CASE "コスプレ中"
	PRINTFORML 「似合ってるかい？」
	PRINTFORML 「折角着たんだから似合っている方が良いのだけど……」
CASE "おやつの感想"
	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
		PRINTFORML 
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				PRINTFORML 
			ELSE
				PRINTFORML 
			ENDIF
		ELSE
			PRINTFORML 
		ENDIF
	;おやつが手作りでない
	ELSE
		PRINTFORML 
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K49
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	IF TALENT:恋慕
		PRINTFORML 「その、ちょっとね……」
		CALL PRINT_STRL, @"「顔の火照りがまだ治まりそうにないかな_／／／_」"
		CALL PRINT_STRL, @"「あ、あんまり見ないでね……？_／／／_」"
	ENDIF
CASE "デートの約束"
	PRINTFORML 「少し楽しみだって言ったら%CNAME("二人称")%は笑うかな？」
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	PRINTFORML 「ん、これで%CNAME("二人称")%の魂に接触できるようになった」
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	IF TALENT:恋慕
	ELSE
;		PRINTFORML 「小さくしたいのかい？何とも面白い事をするね」
	ENDIF
CASE "抜け出す"
	PRINTFORML 
CASE "あなたの部屋に連れ込まれた"
	IF CFLAG:不機嫌指数 >=10 && CHARA_NUM("室内の邪魔者") == 0 && CEVENT("紫煙を燻らせる") == 0
		PRINTFORML 「ん……、%CNAME("二人称")%が良かったらで良いのだけど」
