;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K34, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝袭"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"

	CASE "朝舐陰"

	CASE "朝足交"

	CASE "朝の新妻PLAY"

	CASE "朝乳交"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝接吻"

	CASE "朝添い寝"

	CASE "朝口交", "朝六九式"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "打招呼"
	SELECTCASE CSTR:朝袭
	CASE "朝逆正常位"
		PRINTFORMW 「はっ、はっ……ごめんなさい、%CNAME("二人称")%……」
		PRINTFORMW 「ちょっと、朝から我慢できなくて……」
	CASE "朝舐陰"
		PRINTFORMW 「あ、もう起きちゃった……もっと手早くしておけば……」
		PRINTFORMW 「って、いや！ なんでもないですよ！ あ、あはは」
	CASE "朝足交"
		PRINTFORMW 「あ、やっと起きた」
		PRINTFORMW 「ふふっ……ごめんなさい。今日はちょっとこういう悪戯してみたくて……」
	CASE "朝の新妻PLAY"
		PRINTFORMW 「んもー、折角の料理が……困りますよ%CNAME("二人称")%」
		PRINTFORMW 「まぁこんな格好してた%CNAME("一人称")%が悪いんですけど……その、やってみたかったんです！」
	CASE "朝乳交"
		PRINTFORMW 「えへへ……その、おはようございます」
		PRINTFORMW 「……どうです？ %CNAME("一人称")%にこうされるの、微妙じゃないです？」
	CASE "朝接吻"
		PRINTFORMW 「えへへ……しちゃいました。おはようの接吻」
	CASE "朝添い寝"
		PRINTFORMW 「えへへ……お、お邪魔してまーす」
	CASE "朝口交", "朝六九式"
		PRINTFORMW 「啊……不……」
		PRINTFORMW 「えへへ……お元気でしたね、%CNAME("二人称")%の」
	CASE "朝騎乗位", "朝Ａ騎乗位"
		PRINTFORMW 「あはは……ちょっと、疲れちゃいました……」
		PRINTFORMW 「すみません、このまま少し休ませてくれませんか……」
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「ほら、今日は体育ありますよ。準備はできてます？」
	ELSEIF TALENT:内気
		PRINTFORMW 「那个……哦，早上好。」
	ELSE
		PRINTFORMW 「あ。おはようございます、%CNAME("二人称")%」
	ENDIF
CASE "一番乗り"
	IF TALENT:内気
		PRINTFORMW 「呼！？那个，难道只有两个人吗？」
	ELSE
		PRINTFORMW 「あ……%CNAME("二人称")%だけなんですね」
	ENDIF
CASE "ただいま"
	IF TALENT:内気
		PRINTFORMW 「あっ%CNAME("二人称")%！　た、ただいま帰りましたっ」
	ELSE
		PRINTFORMW 「ただいまー、%CNAME("二人称")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:内気
			PRINTFORMW 「あっ%CNAME("二人称")%！　お、お帰りなさいっ！」
		ELSE
			PRINTFORMW 「あ、%CNAME("二人称")%。お帰りなさい」
		ENDIF
	ELSE
		IF TALENT:内気
			PRINTFORMW 「あ、その、お邪魔してます%CNAME("二人称")%！」
		ELSE
			PRINTFORMW 「あ、%CNAME("二人称")%。お邪魔してますね」
		ENDIF
	ENDIF

CASE "入室打招呼"
	IF CFLAG:呼び出された
		PRINTFORMW 「すいません、お待たせしました……%CNAME("二人称")%います？」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈ的いちゃつき中", MASTER)
		PRINTFORMW 「——啊！？对不起，我看得太仔细了。太失礼了吧……」
		PRINTFORMW %CALLNAME:TARGET%は慌てて目を逸らし、红着脸いる……
	ELSE
		PRINTFORMW 「让您久等了……」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室打招呼"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("对不起，我迟到了……/哇，大家都来了。")%」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「お待たせですー。待ちました？」
	ELSE
		PRINTFORMW 「失礼了……」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "打招呼返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室打招呼中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室打招呼中")
		PRINTFORMW 『老，老师！啊，真是个疲倦的大人……』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室打招呼時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF FIND_COND("入室打招呼中", "灯")
			PRINTFORMW 『あ……遅かったね、兄桑』
		ELSEIF FIND_COND("入室打招呼中", "莉")
			PRINTFORMW 『あ、莉酱。遅かったね？』
		ELSE
			PRINTFORMW 『あ……遅かったですね？』
		ENDIF
	ELSEIF FIND_COND("入室打招呼中", "灯")
		PRINTFORMW 『あ……なんだ、兄桑か』
	ELSEIF FIND_COND("入室打招呼中", "莉")
		PRINTFORMW 『あ、莉酱』
	ELSE
		PRINTFORMW 『啊……不过呢，谢谢』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「すみません、待ちました？」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「ちょっと街道ますね……っと」
	ELSE
		PRINTFORMW 「どうもー、戻りました」
	ENDIF
CASE "抜け出された"
	IF TALENT:内気
		CALL EMOTION_PLUS, "惊嚇", TARGET
		PRINTFORMW 「あ、その……ど、どうぞごゆっくり！」
	ELSE
		PRINTFORMW 「あ……ごゆっくり、%CNAME("二人称")%」
	ENDIF
CASE "便当"
	IF CEVENT("初めての便当") == 0
		PRINTFORMW 「お待たせしました、%CNAME("二人称")%。どうぞ」
		PRINTFORMW %CALLNAME:TARGET%が差し出した弁当は、意外なほど小綺麗に纏まっていた。
		PRINTFORMW 一口食べてみると、少し%TEXTR("薄/濃い")%味に感じるがおいしい……
		PRINTFORMW 「おいしいですか？ あぁ……ならよかった」
		PRINTFORMW 「家族以外に自分の料理を振る舞うのは初めてだったから……」
		PRINTFORMW どうやら%CALLNAME:TARGET%は料理には慣れているらしい。今後も期待できそうだ……
		CALL SET_CEVENT, "初めての便当"
	ELSEIF CEVENT("料理上達") && TALENT:料理擅長 == 1
		PRINTFORMW 「おまたせしましたー！ どうぞ！」
		PRINTFORM いつもと違って自信満々な%CALLNAME:TARGET%が差し出した弁当は、
		IF TALENT:PLAYER:男性
			PRINTFORMW 二段の重箱だった。
			PRINTFORMW 打開盖子，里面有很多饭团和各种各样的菜，散发着淡淡的香味。
			PRINTFORMW 唐油煎包、汉堡肉、高汤卷、那不勒斯汤、炸虾、芦笋卷、青椒肉……
			;PRINTFORMW 半分くらいお子様ランチ
			PRINTFORMW 无论是数量还是种类，都明显是费时费力做出的便当。。
			PRINTFORMW 「えへへ……ちょっと作り過ぎたかなーとは思うんですけど」
		ELSE
			PRINTFORMW 非常可爱。
			PRINTFORMW 以一口大小的三明治为主，花型的煎鸡蛋和甜甜圈型的汉堡肉
			PRINTFORMW 苹果、香肠等所有菜肴都与平时不同。
			PRINTFORMW 「えへへ……今日はちょっと気合いを入れてみました」
		ENDIF
		PRINTFORMW 「ここ最近、自己も料理上達したなって実感してて……」
		PRINTFORMW 「だから%CNAME("二人称")%に成果を味わってもらえたらなと思って」
		PRINTFORMW 「どうぞ、遠慮しないでください。手前味噌ですけど、味は保証しますっ」
		PRINTFORMW %CALLNAME:MASTER%はありがたく、%CALLNAME:TARGET%の成果を味わうことにした……
		TALENT:料理擅長 = 2
	ELSEIF TALENT:料理擅長 == 2
		PRINTFORMW 「お待たせしました！ 今日も%TEXTR("バッチリですよ/気合い入れて作りました/抜かりありません")%！」
		PRINTFORMW %CALLNAME:MASTER%は今日も%CALLNAME:TARGET%の手料理%TEXTR("に舌鼓を打った/で至福の時を過ごした")%……
	ELSEIF CFLAG:弁当日数 >= 40
		PRINTFORMW 「えへへ、おいしいですか？」
		PRINTFORMW %CALLNAME:MASTER%が弁当を食べている姿に、%CALLNAME:TARGET%は満足そうだ。
	ELSEIF CFLAG:弁当日数 >= 20
		PRINTFORMW 「……じーっ……」
		PRINTFORMW %CALLNAME:TARGET%はじっと%CALLNAME:MASTER%の食べる姿を観察している……
	ELSEIF CFLAG:弁当日数 >= 10
		PRINTFORMW 「……%CNAME("二人称")%、どうです？」
		PRINTFORMW %CALLNAME:TARGET%はまじまじと%CALLNAME:MASTER%の表情を伺っている。
	ELSE
		PRINTFORMW 「どうですか%CNAME("二人称")%？ 不味くないです？」
		PRINTFORMW %CALLNAME:TARGET%は少し不安そうにしているが、味に問題は無い……
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	CALL EMOTION_PLUS, "惊嚇", TARGET
	PRINTFORMW 「%TEXTR("わわっ、ちょっとまだ人いますよ/えッ、いまからですか")%！？」
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K34, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「……あっ、えっと、%NAME:TARGET%です！　よよ、よろしくお願いします！」
	PRINTFORML 从刚一进入房间就開始发呆，%CALLNAME:TARGET%慌忙回过神来低下了头。
CASE "浑身是汗"
	;他に人がいる
	IF CHARA_NUM("室内の邪魔者") && RAND:2
		PRINTFORML %CALLNAME:TARGET%在用毛巾擦脸上的汗
		PRINTFORML ……似乎是在假装，悄悄地窥视着别人的身影。
	ELSE
		PRINTFORML 「好……热啊……」
		PRINTFORML %CALLNAME:TARGET%一边仰望天花板一边擦着汗
	ENDIF
CASE "刚睡醒"
	PRINTFORML 「う～ん……%CNAME("二人称", "かな")%？　いまなんじです？」
	PRINTFORML 注意到眼前%CALLNAME:MASTER%身影的同时、%CALLNAME:TARGET%正在畅快的打着哈欠。
CASE "Cosplay中"
	IF CHARA_NUM("室内の邪魔者")
		PRINTFORML %CALLNAME:TARGET%は红了脸并溜め息をついている……
		PRINTFORML 「%TEXTR("あぁも、恥ずかしい/せめて誰也不在ときに")%……」
		PRINTFORML %CALLNAME:TARGET%は人目を気にして縮こまっている。
	ELSE
		IF FINDCHARA(NO, 35) > 0
			PRINTFORML 「こういうのは兄桑の役目なのに……」
		ELSE
			PRINTFORML 「%CNAME("一人称")%、こういうの似合わないのに……」
		ENDIF
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K34
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "梦话"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「对不起，对不起。让你看到了不好的地方……」
	PRINTFORML %CALLNAME:TARGET%擦去眼泪，羞涩地笑着。
CASE "约会的约定"
	PRINTFORM %CALLNAME:TARGET%
	IF TALENT:内気
		PRINTFORML そわそわしている……
	ELSE
		PRINTFORML 鼻歌を歌っている……
	ENDIF
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K34, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "摆脱"
	IF CFLAG:躁动难忍
		PRINTFORML 「……あ、あの……するん、ですよね」
		PRINTFORML %CALLNAME:TARGET%はもじもじし的同时、熱っぽい目で%CALLNAME:MASTER%を待っている……
	ELSEIF TALENT:内気
		PRINTFORML 「あの%TEXTR("……これから、そういう……？/、ここ来了ってことは、その……")%」
		PRINTFORML %CALLNAME:TARGET%は落ち着かない様子で%CALLNAME:MASTER%の顔をチラチラと覗いている……
	ELSE
		PRINTFORML 「%TEXTR(@"ふふ……来ちゃいましたね/これからどうするんですか、%CNAME("二人称")%？")%」
		PRINTFORML %CALLNAME:TARGET%は期待するような目で%CALLNAME:MASTER%を見ている……
	ENDIF
CASE "主人公的房間に連れ込まれた"
	IF CFLAG:躁动难忍
		PRINTFORML 「あ、あのぉ……何か、しますか……？」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に何かを求めるような目をしている……
	ELSEIF TALENT:内気
		PRINTFORML 「嗯，可以坐吗？」
		PRINTFORML %CALLNAME:TARGET%心浮气躁……
	ELSE
		PRINTFORML 「诶嘿嘿……」
		PRINTFORML %CALLNAME:TARGET%は床に腰掛けてもじもじしながらも、楽しそうな顔をしている。
	ENDIF
CASE "自室に招待"
	PRINTFORML 「嗯，随便坐一下没関系。」
	PRINTFORML %CALLNAME:TARGET%似乎很在意%CALLNAME:MASTER%的动向。
CASE "自室に訪問された"
	IF RAND:2
		;アドバイザーを伴っている場合
		IF FIND_COND("現在存在", "助手")
			IF FIND_COND("灯")
				PRINTFORML 「わっ、兄桑だ」
				PRINTFORML 『わってなんだ、わって』
			ELSEIF FIND_COND("莉")
				PRINTFORML 「わっ、莉酱？ まだ起きてたの？」
				PRINTFORML 『……まあ』
			ELSEIF TALENT:内気
				PRINTFORML 「诶？什么，什么？为什么要这样？」
				PRINTFORML 对于突然来访的客人，%CALLNAME:TARGET%感到不知所措。
			ELSE
				PRINTFORML 「啊，今天两位吗？」
				PRINTFORML %CALLNAME:TARGET%笑着迎接%CALLNAME:MASTER%们。
			ENDIF
		ELSEIF TALENT:内気
			PRINTFORML 「诶！？等，等等！请等一下！」
			PRINTFORML %CALLNAME:TARGET%对突然来访的客人感到惊慌，将视线投向整个房间。
			PRINTFORML ……特别是看向地板和书架……
		ELSE
			PRINTFORML 「啊……欢迎光临、%CNAME("二人称")%」
			PRINTFORML %CALLNAME:TARGET%对%CALLNAME:MASTER%露出了羞涩的笑容。
		ENDIF
	ELSE
		PRINTFORML 「%TEXTR(@"あ゛ぁ゛ぁ゛あと{RAND:3 + 1}回だったのにぃ゛ぃ゛/ね゛ぇ゛やだもう{RAND:10 + 10}回目なのにぃ゛ぃ゛/ん゛あ゛ぁ゛それさっきまでなかったじゃんもぉ゛ぉ゛/う゛あ゛ぁ゛またおんなじとこミスってぇ゛ぇ゛")%――」
		PRINTFORML ……%CALLNAME:TARGET%は%CALLNAME:MASTER%の視線を察し、%TEXTR("游戏パッドを手に/キーボードに手を置いたまま/マウスを握りしめて")%固まっている。
		PRINTFORML %TEXTR("お邪魔だったかな/だいぶ熱中していたようだ/今日も元気そうだ/聞かなかったことにしてあげよう")%……
	ENDIF
CASE "指导者部屋訪問"
	SELECTCASE SAVESTR:現在位置
	CASE "灯的房間";個別イベントCEVENTによる分岐はカットしました
		PRINTFORML 『……で？ この面子で何しようってんだ』
		PRINTFORML 「そんなの%CNAME("二人称")%に聞いてよ……」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の行動を待っている……
	CASE "莉的房間"
		PRINTFORML 『……それで、この三人で何をしようって言うんです？』
		PRINTFORML 「何をするって……ねえ……」
		PRINTFORML %CALLNAME:TARGET%は困ったような目で%CALLNAME:MASTER%を見ている……
	CASEELSE
		PRINTFORML 「おじゃましまーす……」
	ENDSELECT
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 「戻りました%TEXTR("ー/よ、っと……")%」
CASE "オナバレ"
	PRINTFORML 「%TEXTR("あぁも、また見られたぁ/もうやだぁ、なんでまたやっちゃうの")%……」
	PRINTFORML %CALLNAME:TARGET%は真っ赤な顔で机に突っ伏している……
CASE "露脐装超短裙胖次チラ"
	PRINTFORML 「%TEXTR("哇，/诶？/")%难道你看到了吗？」
CASE "低腰臀圍"
	PRINTFORML 「%TEXTR("あの、あんまりジロジロ見ないでください/こういうの趣味じゃないんですけど")%ね……」
CASE "掀起裙子"
	PRINTFORML 「んもー、兒童じゃないんだから……」
CASE "检查内褲"
	IF COND("合意：检查内褲")
		PRINTFORML 「%TEXTR("好きですよ/飽きません")%ね、%CNAME("二人称")%……」
		PRINTFORM %CALLNAME:TARGET%红了脸并、呆れたように溜め息をついている。
	ENDIF
CASE "检查阴部"
	IF COND("合意：检查阴部")
		PRINTFORML 「もう、%TEXTR("無茶させるんだから/酷い人ですよね")%……」
		PRINTFORML %CALLNAME:TARGET%は红着脸いる……
	ENDIF
CASE "拜托処理性欲", "进行性処理"
	PRINTFORML 「……満足しました？」
	PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の様子を伺っている……
CASE "性欲解消を手伝う"
	PRINTFORML 「役得……って言うんですかね、こういうのも。あはは」
	PRINTFORML %CALLNAME:TARGET%は有点害臊的乾いた笑いを浮かべている……
CASE "清洁口交"
	PRINTFORML 「ふぅ……綺麗成为了ね」
	PRINTFORML %CALLNAME:TARGET%は唾液でテカる%CALLNAME:MASTER%の一物を見て頷いた。
CASE "買東西"
	;買ってきたアイテムなどで分岐
	IF COND("巨大振動棒購入") && DEVENT("巨大振動棒感想") == 0
		PRINTFORML 「うわぁ……おっきいですね、これ」
		IF TALENT:MASTER:男性
			PRINTFORML 「……誰に使う予定なんです？」
