;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K34, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 我_我
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = 先輩_せんぱい
	;相手が主人ではない
	ELSEIF NO:PLAYER == 35
		LOCALS = 哥哥_哥哥
	ELSEIF NO:PLAYER == 48
		LOCALS = 莉ちゃん_れいちゃん
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:オトコ
				LOCALS = 君_きみ
			ENDIF
		CASE "２年生", "３年生", "研究生"
			LOCALS = 先輩_せんぱい
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 先生_せんせい
			ELSE
				LOCALS = 貴方_あなた
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	IF NAME:PLAYER == "あなた"
		LOCALS = 先輩_せんぱい
	ELSE
		LOCALS = %CALLNAME:PLAYER%先輩_%CSTR:PLAYER:ふりがな%せんぱい
	ENDIF
CASE "助手"
	IF NO:ASSI == 35
		LOCALS = 哥哥_哥哥
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:オンナ || TALENT:ふたなり
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF
		CASE "２年生", "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%先輩_%CSTR:ASSI:ふりがな%せんぱい
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
			ELSE
				IF TALENT:オトコ
					LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

