;-------------------------------------------------
;PLAYERの射精＆絶頂をトリガーにした口上
;それぞれの射精分岐内での初めてかどうかの判定は、FIRST_EV(ARGS)で行う
;例）ELSEIF SHOOT("脚")において初めてかどうかは、FIRST_EV("絶頂：脚")で判定する
;-------------------------------------------------
@KOJO_MESSAGE_SAMEN_AND_EXTACY_K45
;助手が調教した時に口上をスキップする（好みに応じて使う、行頭の;を消すとスキップするようになる）
;SIF ASSIPLAY
;	RETURN 0

;-------------------------------------------------
;髪、顔、股間、腹、尻、くぱぁ、Ａくぱぁへのぶっかけ
;-------------------------------------------------
IF SHOOT("ぶっかけ")
	IF SHOOT("自慰する")
		PRINTFORMW 
	;超大量射精
	ELSEIF COND("絶頂", PLAYER) >= 3
		PRINTFORMW 
	;大量射精
	ELSEIF COND("絶頂", PLAYER) == 2
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;自慰で射精された(ぶっかけではなく床などに射精した)
;-------------------------------------------------
ELSEIF SHOOT("自慰する")
	PRINTFORMW 

;以下は通常の射精区分。
;-------------------------------------------------
;素股、足コキや身体で射精させた
;-------------------------------------------------
ELSEIF SHOOT("脚")
	;初めて
	IF FIRST_EV("絶頂：脚") == 0
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	;二回目以降
	ELSE
		;恋慕
		IF TALENT:恋慕
			PRINTFORMW 
		;それ以外
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ELSEIF SHOOT("素股")
	PRINTFORMW 
;-------------------------------------------------
;口で射精させた
;-------------------------------------------------
ELSEIF SHOOT("口")
	;初めて
	IF FIRST_EV("絶頂：口") == 0
		PRINTFORMW 
	;精液中毒Lv5
	ELSEIF TEQUIP:ディープスロート
		IF CFLAG:口内汚辱 == 2
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	;精液中毒Lv3
	ELSEIF ABL:精液中毒 >= 3
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;胸で射精させた
;-------------------------------------------------
ELSEIF SHOOT("胸")
	;初めて
	IF FIRST_EV("絶頂：胸") == 0
		PRINTFORMW 
	;奉仕精神Lv5
	ELSEIF ABL:奉仕精神 >= 5
		PRINTFORMW 
	;奉仕精神Lv3以上
	ELSEIF ABL:奉仕精神 >= 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;手で射精させた
;-------------------------------------------------
ELSEIF SHOOT("手")
	;初めて
	IF FIRST_EV("絶頂：手") == 0
		PRINTFORMW 「うわっひゃ！？な、なんか、なんか白いのいっぱい出たぁ！」
		PRINTFORMW 「す、凄くネバネバしてる…………そ、それに、く……へ、変な、におい……」
		PRINTFORMW 「……こ、これが精液、な、なんですねぇ……ほぇえ……」
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;奉仕精神Lv3
	ELSEIF ABL:奉仕精神 >= 3
		IF RAND:2
			PRINTFORMW 「%TEXTR("あふっ/ひやっ/うぇっ")%！？……あ、あ、で、出たぁ……」
			PRINTFORMW 「……あ、あの……う、上手くできて、ました？」
			PRINTFORMW 「……えへ、えへへぇ……♪」
		ELSE
			PRINTFORMW 
		ENDIF
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;セックスで射精させた
;-------------------------------------------------
ELSEIF SHOOT("Ｖ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ｖ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
ELSEIF SHOOT("Ａ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ａ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;キスで射精
;-------------------------------------------------
ELSEIF SHOOT("キス")
	PRINTFORMW 
;-------------------------------------------------
;顔面騎乗で射精
;-------------------------------------------------
ELSEIF SHOOT("顔面騎乗")
	PRINTFORMW 
;-------------------------------------------------
;ぱふぱふで射精
;-------------------------------------------------
ELSEIF SHOOT("ぱふぱふ")
	PRINTFORMW 
;-------------------------------------------------
;逆クンニで射精
;-------------------------------------------------
ELSEIF SHOOT("逆クンニ")
	PRINTFORMW 
;-------------------------------------------------
;逆アナル、逆レイプで射精
;-------------------------------------------------
ELSEIF SHOOT("逆レイプ") || SHOOT("逆アナル") || SHOOT("アナル奉仕")
	PRINTFORMW 
;-------------------------------------------------
;PLAYERが女で、イってしまった…
;-------------------------------------------------
ELSEIF SHOOT("調教者がオンナ")
	;強絶頂
	IF COND("絶頂", PLAYER) >= 2
		PRINTFORMW 
	;絶頂
	ELSEIF COND("絶頂", PLAYER) == 1
		PRINTFORMW 
	ENDIF
ENDIF


;-------------------------------------------------
;キャラが調教中に調教者を攻める際などの口上
;-------------------------------------------------
@KOJO_COUNTER_K45, ARGS
SELECTCASE ARGS
CASE "恍惚"
	IF TALENT:恋慕
		IF ABL:サドっ気
			PRINTFORMW 「%CNAME("二人称", "どもり")%……？」
			PRINTFORMW 「%CNAME("二人称")%……ちゃ、ちゃんとしないと」
			PRINTFORMW 「た、たた、食べちゃう、ぞぉーがぉー……」
			PRINTFORMW 「…………た、食べちゃう、よ？いい、よね？言ったもん、ね？」		
			CALL PRINT_STR, "「お、へへ……えへっえへへへへ……えひひっ_HH_」_W"
		ELSE
			PRINTFORMW 「あ、%CNAME("二人称", "どもり")%？」
			PRINTFORMW 「え、えへへ……そんなに気持ちよかったんだね……う、嬉しいなぁ」
			PRINTFORMW 「もっと……もっとしてあげる、からね……♪」
		ENDIF
	ELSE
		PRINTFORMW 
	ENDIF
CASE "押し倒し"
	IF ABL:サドっ気
		PRINTFORMW 「……」
	ELSE
		PRINTFORMW 
	ENDIF
CASE "強制終了"
	PRINTFORMW 
CASE "リザレクション"
	PRINTFORMW 
CASE "恍惚から復帰"
	PRINTFORMW 
CASE "助手が限界"
	PRINTFORMW 
ENDSELECT

RETURN 1


