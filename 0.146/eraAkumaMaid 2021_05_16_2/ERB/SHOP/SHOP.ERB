			SIF PLACE(@"%CSVCALLNAME(NO:MASTER)%の部屋") == 0
				STR:選択肢 += @"%CSVCALLNAME(NO:MASTER)%の部屋/"
			SIF PLACE(@"%CSVCALLNAME(NO:TARGET)%の部屋") == 0
				STR:選択肢 += @"%CSVCALLNAME(NO:TARGET)%の部屋/"
