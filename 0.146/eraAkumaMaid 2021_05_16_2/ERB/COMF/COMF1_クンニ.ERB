	;穴の開いていないパンツをはいている
	SIF CHECK_CLO("Ｖ露出") == 0
		PRINTFORM %CLO("股間")%越しに
	IF RAND:2
		PRINTFORM 舌で
	ELSE
		PRINTFORM 舌先で
	ENDIF
	IF RAND:2 && CHECK_COM("お掃除クンニ") == 0
		PRINTFORM %TEXTR("荒々しく/激しく/休まず")%
	ELSEIF RAND:2
		PRINTFORM 優しく
	ELSE
		PRINTFORM 丁寧に
	ENDIF
	IF CHECK_COM("お掃除クンニ")
		PRINTFORMW お掃除した…
	ELSEIF TALENT:相思相愛 && RAND:3 == 0
		PRINTFORMW 執拗になぞった…
	ELSEIF RAND:4 == 0
		PRINTFORMW 吸い上げた…
	ELSEIF RAND:3 == 0
		PRINTFORMW 舐めあげた…
	ELSEIF RAND:2
		PRINTFORMW 愛撫した…
	ELSE
		PRINTFORMW 味わった…
