SELECTCASE CHECK_EQUIP_69(SELECTCOM)
CASE 1
	;前回の調教がフェラチオ、クンニ強制かシックスナインだとシックスナインに
	IF PREVCOM == 91 || PREVCOM == 98 || PREVCOM == 150 || TEQUIP:シックスナイン
		SIF TEQUIP:シックスナイン == 0
			TEQUIP:シックスナイン = PREVCOM
		TEQUIP:PLAYER:シックスナイン = SELECTCOM
	ENDIF
CASE 2
	;前回の調教がクンニ、フェラするかシックスナインだとシックスナインに
	IF PREVCOM == 1 || PREVCOM == 4 || TEQUIP:顔面騎乗 || TEQUIP:シックスナイン
		;69に移行した時（TEQUIP:PLAYER:シックスナインは0も意味を持つので、TEQUIP:シックスナインで判定。）
		IF TEQUIP:シックスナイン == 0
			IF TEQUIP:顔面騎乗
				TEQUIP:PLAYER:シックスナイン = TEQUIP:顔面騎乗
			ELSE
				TEQUIP:PLAYER:シックスナイン = PREVCOM
			ENDIF
		ENDIF
		TEQUIP:シックスナイン = SELECTCOM
	ENDIF
CASE 3
	;シックスナイン中だと継続
	SIF TEQUIP:シックスナイン
		TEQUIP:PLAYER:シックスナイン = SELECTCOM
CASE 4
	;シックスナイン中だと継続
	SIF TEQUIP:シックスナイン
		TEQUIP:シックスナイン = SELECTCOM
;シックスナインも可能なコマンド(TEQUIP:シックスナインは変更させない)
CASE 6
	RETURN 0
CASEELSE
	IF SELECTCOM != 99
		TEQUIP:シックスナイン = 0
		TEQUIP:PLAYER:シックスナイン = 0
	ENDIF
ENDSELECT

@CHECK_EQUIP_69(ARG)
#FUNCTION

;時間を経過させないコマンドなら変化なし
SIF NOTTIME_PLAY(ARG)
	RETURNF 6

SELECTCASE ARG
;PLAYERの行動　シックスナインに移行する股間弄りコマンド
CASE 1, 4
	RETURNF 1
;TARGETの行動　シックスナインに移行する奉仕コマンド
CASE 91, 98, 150
	RETURNF 2
;PLAYERの行動　シックスナイン体勢で可能な愛撫系の股間弄りコマンド
CASE 0 TO 2, 4, 7 TO 10, 15
	RETURNF 3
;PLAYERの行動　道具コマンドやスパンキング、フィストファック
CASE 30 TO 34, 39, 130, 141 TO 143
	RETURNF 3
;TARGETの行動　シックスナイン体勢で可能な奉仕コマンド
CASE 90 TO 92, 97, 98, 105
	RETURNF 4
;シックスナインも可能なコマンド(TEQUIP:シックスナインは変更させない)
CASE 3, 13, 14, 24, 146, 176, 230 TO 300
	RETURNF 6
CASE 99
	RETURNF 7
ENDSELECT
RETURNF 0


