@COM99
;シックスナイン
CALL PRINT_TRAIN_NAME(SELECTCOM)

IF TEQUIP:シックスナイン
	PRINTL 
	PRINTFORMW \@ TEQUIP:押し倒し ? %CALLNAME:TARGET% # %CALLNAME:PLAYER% \@はシックスナインの体勢をやめた…
	STR:前回コマンド = %STR:今回コマンド%
	TEQUIP:シックスナイン = 0
	TEQUIP:PLAYER:シックスナイン = 0
	PREVCOM = 99
	RETURN 0
ENDIF
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
CALL SHOW_COM_ORDER_COM, 99

;実行できない
SIF TCVAR:やる気 == 0
	RETURN 0
;-------------------------------------------------
;実行決定
;-------------------------------------------------
;相互お掃除プレイ
IF CHECK_COM("お掃除フェラ", 91) + CHECK_COM("お掃除クンニ", 98) && CHECK_COM("お掃除フェラ", 4) + CHECK_COM("お掃除クンニ", 1)
	IF CHECK_COM("お掃除フェラ", 91)
		TEQUIP:シックスナイン = 91
	ELSE
		TEQUIP:シックスナイン = 98
	ENDIF
	IF CHECK_COM("お掃除フェラ", 4)
		TEQUIP:PLAYER:シックスナイン = 4
	ELSE
		TEQUIP:PLAYER:シックスナイン = 1
	ENDIF
ELSE
	;ペニスあるならそっち優先
	IF PENIS(PLAYER)
		TEQUIP:シックスナイン = 91
	ELSE
		TEQUIP:シックスナイン = 98
	ENDIF
	IF PENIS(TARGET)
		TEQUIP:PLAYER:シックスナイン = 4
	ELSE
		TEQUIP:PLAYER:シックスナイン = 1
	ENDIF
ENDIF

CALL TRAIN_MESSAGE_B

IF REFUSE_CHECK()
	TEQUIP:シックスナイン = 0
	TEQUIP:PLAYER:シックスナイン = 0
	RETURN 0
ENDIF

STR:前回コマンド = %STR:今回コマンド%

;顔面騎乗ＯＮ
SIF TEQUIP:顔面騎乗 + TEQUIP:押し倒し && TEQUIP:PLAYER:シックスナイン == 1
	TEQUIP:顔面騎乗 = 1
;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

