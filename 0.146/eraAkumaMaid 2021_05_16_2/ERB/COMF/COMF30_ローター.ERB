@COM30
;ローター

PRINTL ローター

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回コマンド = ローター

RETURN 1

@FLAG_COM30, ARG

@SOURCE_COM30, ARG
CALL VARSET_COMF

CALL SOURCE_DOWNBASE_HP, 30

SOURCE:0:露出 = 120
SOURCE:0:逸脱 = 70

;ABL:Ｃ感覚をみる
SOURCE:0:Ｃ快楽 = MULTIPLY(CALCF("Ｃ刺激", 0), 90 + ITEM:ローター*10)

;アソコが露出していないと快Ｃ減
IF !CHECK_CLO("Ｖ露出") && COND("調教中")
	;ズボン等をはいていると快Ｃ激減
	IF CHECK_OUTER("ズボン") || CHECK_PANTIES("レザー")
		TIMES SOURCE:0:Ｃ快楽, 0.20
		TIMES SOURCE:0:露出, 0.20
	ELSE
		TIMES SOURCE:0:Ｃ快楽, 0.60
		TIMES SOURCE:0:露出, 0.50
	ENDIF
ENDIF

SIF TALENT:Ｃ性向
	CALL CALC, "好みのプレイ", SOURCE:0:Ｃ快楽

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM30
PRINTFORM %CALLNAME:PLAYER%は
;インサート中
IF INSERT("合体中")
	PRINTFORM %TEXTS("合体中", PLAYER)%
ELSEIF TEQUIP:電話中
	PRINTFORM %TEXTS("電話中の")%
ELSEIF !CHECK_CLO("Ｖ露出")
	PRINTFORM %CLO("股間")%の上から、
;ELSE
;	SIF ABL:Ｃ感覚 >= 3 && (MARK:屈服刻印 == 3 || TALENT:恋慕)
;		PRINT 脚を広げて待っている
ENDIF
PRINTFORM %CALLNAME:TARGET%の
IF PENIS(TARGET)
	IF TALENT:淫核 && PALAM:欲情 > 8000
		PRINT 肥大した
	ELSEIF PALAM:快Ｃ > 3000
		PRINT 勃起した
	ENDIF
	PRINT ペニス
ELSE
	IF CALCF("Ｃ発情") >= 3 && RAND:2
		PRINT 肥大した
	ELSEIF CHECK_CLO("Ｖ露出") && TALENT:クリピアス && RAND:2
		PRINT ピアスで飾られた
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT ツンと尖った
	ELSEIF CHECK_CLO("Ｖ露出") && COND("陰毛") >= 6 && RAND:2
		PRINT 陰毛に隠れている
	ELSEIF CALCF("Ｃ発情") >= 2 && RAND:2
		PRINT ぷっくりと膨れた
	ELSEIF TALENT:小柄
		PRINT 小さな
	ENDIF
	PRINT クリトリス
ENDIF
IF PREVCOM == SELECTCOM
	PRINTW を小刻みに振動するローターで執拗に責め続けた…
ELSE
	PRINTW に震えるローターを押し当てた……
ENDIF
