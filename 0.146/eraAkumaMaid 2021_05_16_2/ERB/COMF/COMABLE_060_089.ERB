;80番台,特殊セックス系コマンド
;-------------------------------------------------
@COM_ABLE80
;貝あわせ実行判定
SIF COMABLE_DEF(80)
	RETURN 0
;アソコ剥き出しにできないとダメ
SIF !CHECK_CLO("Ｖ露出可")
	RETURN 0
;オトコだとダメ
SIF TALENT:オトコ || TALENT:PLAYER:オトコ
	RETURN 0
;双頭バイブ以外のバイブ挿入中はダメ(PLAYERが双頭バイブで処女喪失するのはどうなんだろ)
IF TEQUIP:バイブ
	SIF COND("双頭バイブ") == 0
		RETURN 0
	IF TALENT:PLAYER:処女
		IF TEQUIP:押し倒し
			SIF ABL:サドっ気 < 3
				RETURN 0
		ELSE
			SIF ABL:PLAYER:マゾっ気 < 3
				RETURN 0
		ENDIF
	ENDIF
ENDIF
RETURN 1

