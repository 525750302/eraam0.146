@COM73
;対面座位アナル
CALL PRINT_TRAIN_NAME(SELECTCOM)

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回コマンド = %STR:今回コマンド%

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

RETURN 1

@FLAG_COM73, ARG
CALL FLAG_INSERTION, "Ａインサート", ARG, 40

@SOURCE_COM73, ARG
CALL SOURCE_INSERTION, "Ａインサート", ARG, 40

CALL VARSET_COMF

SOURCE:0:露出 = 1300
SOURCE:0:情愛 = 1000

;PALAM:欲情をみる
SELECTCASE PALAM:欲情
CASE IS < PALAMLV:1
	TIMES SOURCE:0:情愛, 0.30
CASE IS < PALAMLV:2
	TIMES SOURCE:0:情愛, 0.60
CASE IS < PALAMLV:3
	TIMES SOURCE:0:情愛, 1.00
CASE IS < PALAMLV:4
	TIMES SOURCE:0:情愛, 1.50
CASE IS >= PALAMLV:4
	TIMES SOURCE:0:情愛, 1.80
ENDSELECT

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM73
#DIM MEMO_LINECOUNT

MEMO_LINECOUNT = LINECOUNT
CALL TRAIN_MESSAGE_INSERTION
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

PRINTFORM %CALLNAME:PLAYER%は
IF COND("前Ｔから対象のみ変更") && VAU_INSERT_PLAY(PREVCOM:1)
	CALL ADD_TEXT, "性器から性器へ"
	PRINTFORM %CALLNAME:TARGET%に正面から抱きつかせながら、アナルでペニスを咥え込まさせ
;穴開きでないパンツをはいていて、ずらされていないなら、必ずずらす
ELSEIF !CHECK_CLO("Ａ露出") && CHECK_CLO("Ａ露出可")
	PRINTFORM %CALLNAME:TARGET%の
	CALL SETFLAG, "ボトムを露出：テキスト有り", TARGET
	PRINTFORM 、正面から抱きつかせながらアナルでペニスを咥え込ませ
ELSE
	PRINTFORM %CALLNAME:TARGET%に正面から抱きつかせながらアナルでペニスを咥え込まさせ
ENDIF

IF (CHECK_COM("キス") && TALENT:恋慕) || TEQUIP:キス
	PRINTFORMW 、キスをしながら腰を揺すった。
	CALL SETFLAG, "追加キスフラグ", TARGET
ELSE
	PRINTW た。
ENDIF
