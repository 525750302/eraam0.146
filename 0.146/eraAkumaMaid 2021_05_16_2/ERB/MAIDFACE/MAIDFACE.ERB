;-------------------------------------------------
;顔を描画する関数群。製作者　ギンギー料理
;-------------------------------------------------
;MAIDFACE{X}はARG:0〜ARG:{X-1}のX人の顔を横に並べて出力する関数。
;画像のピクセル数はとりあえず自動的に150*150に近い物が選択される。
;-------------------------------------------------
@PRINT_MAIDFACE, ARG:0, ARG:1, ARG:2, ARG:3, ARG:4
#DIM LCOUNT
#DIM MEMO_REDRAW
#DIM ACTOR, 5
#DIM NUM_ACTOR

SIF CONFIG("顔グラＯＦＦ") || TFLAG:顔グラスキップ
	RETURN 0

MEMO_REDRAW = CURRENTREDRAW()
REDRAW 0

NUM_ACTOR = 0
VARSET ACTOR
FOR LCOUNT, 0, 5
	SIF LCOUNT >= 1 && ARG:LCOUNT <= 0
		BREAK
	SIF CFLAG:(ARG:LCOUNT):顔グラ == 0
		CONTINUE

	ACTOR:NUM_ACTOR = ARG:LCOUNT
	NUM_ACTOR += 1
NEXT

LOCALS = <nobr>
FOR LCOUNT, 0, 5
	SIF ACTOR:LCOUNT <= 0
		BREAK
	IF TFLAG:顔グラアニメ中
		LOCALS += @"%MAIDFACE_ANIME(ACTOR:LCOUNT, LCOUNT + 1)%"
	ELSE
		LOCALS += @"%MAIDFACE(ACTOR:LCOUNT, LCOUNT + 1)%"
	ENDIF
NEXT

IF LOCALS != "<nobr>"
	IF COND("キャラ紹介中")
		SIF MARK:ARG:淫紋
			LOCALS += @"%CUTIN("淫紋", ARG, 2)%"
		;LOCALS += @"%CUTIN("パンツ", ARG, 2)%"
	ENDIF

	HTML_PRINT LOCALS

	FOR LCOUNT, 1, 100*MAIDFACESIZE()/GETCONFIG("フォントサイズ")/100
		PRINTL 
	NEXT
	TFLAG:顔グラ表示中 = LINECOUNT
ENDIF
REDRAW MEMO_REDRAW

;ARG付近でGETCONFIG("一行の高さ")の整数倍な最小の値を返す
@PICSIZE(ARG)
#FUNCTION
#DIM LCOUNT

;ほんの少しだけ小さい値も許容する
ARG -= GETCONFIG("一行の高さ")*3/10

FOR LCOUNT, 0, GETCONFIG("一行の高さ")
	LOCAL = ARG + LCOUNT
	SIF (100*(ARG + LCOUNT)/GETCONFIG("一行の高さ") )%100 == 0
		BREAK
NEXT
RETURNF LOCAL

;CONFIG("顔グラサイズ")に応じたPICSIZEを返す。だが今は175固定。
@MAIDFACESIZE()
#FUNCTION
RETURNF PICSIZE(175)

SELECTCASE CONFIG("顔グラサイズ")
CASE 0
	RETURNF PICSIZE(150)
CASEELSE
	RETURNF PICSIZE(175)
ENDSELECT


