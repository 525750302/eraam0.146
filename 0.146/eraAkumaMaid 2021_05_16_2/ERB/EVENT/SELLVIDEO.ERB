	SELECTCASE LOCALS:LCOUNT
	CASE ""
		BREAK
	CASEELSE
		SIF STRCOUNT(STR:ビデオジャンル, @"\[%LOCALS:LCOUNT%\]")
			CONTINUE
	ENDSELECT

	STR:ビデオジャンル += @"\[%LOCALS:LCOUNT%\]"

