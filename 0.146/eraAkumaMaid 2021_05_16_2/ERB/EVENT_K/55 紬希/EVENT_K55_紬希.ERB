;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K55, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	IF TALENT:相思相愛
		LOCALS = 私_わたし
	ELSE
		LOCALS = ボク_わたし
	ENDIF
CASE "二人称"
	;相手が主人
	;相手が主人ではない時の特殊二人称もここ(ナンバー指定あり)
	IF PLAYER == MASTER
		LOCALS = 部長_ぶちょう
	ELSEIF NO:PLAYER == 10 && TALENT:相思相愛
		LOCALS = 義母さん_おかあさん
	ELSEIF NO:PLAYER == 10
		LOCALS = メリィエーナさん_めりぃえーなさん
	
	;相手が主人ではない時の汎用口上
	;学生限定。
	;中等部以下…くんちゃん付け
	;高等部以上…先輩
	;教師…先生
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CALLNAME:PLAYER%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CALLNAME:PLAYER%ちゃん
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%先輩_%CALLNAME:PLAYER%せんぱい
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%先生_%CALLNAME:PLAYER%せんせい
			ELSE
				LOCALS = %CALLNAME:PLAYER%さん_%CALLNAME:PLAYER%さん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長_ぶちょう
CASE "助手"
	IF COND("メリィエーナ", ASSI) && TALENT:相思相愛
		LOCALS = 義母さん_おかあさん
	ELSEIF COND("メリィエーナ", ASSI)
		LOCALS = メリィエーナさん_めりぃえーなさん
	ELSEIF COND("森賀棗", ASSI)
		LOCALS = 棗さん_なつめさん
	ELSEIF COND("小鳥遊静菜", ASSI)
		LOCALS = 静菜さん_しずなさん
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:ASSI%くん_%CALLNAME:ASSI%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CALLNAME:ASSI%ちゃん
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:ASSI%先輩_%CALLNAME:ASSI%せんぱい
		CASE "教師"
			LOCALS = %CALLNAME:ASSI%先生_%CALLNAME:ASSI%せんせい
		CASEELSE
	    	LOCALS = %CALLNAME:ASSI%さん_%CALLNAME:ASSI%さん
		ENDSELECT
   ENDIF
CASEELSE
	;ARGSにキャラ名が入れられてる場合にはキャラへの呼び方なのでチェック
	IF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)

		IF NO:ACTOR == 10 && TALENT:相思相愛
			LOCALS = 義母さん_おかあさん
		ELSEIF NO:ACTOR == 51
			LOCALS = 棗さん_なつめさん
		ELSEIF NO:ACTOR == 52
			LOCALS = 静菜さん_しずなさん
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部", "１年生", "２年生"
				IF TALENT:ACTOR:オトコ
					LOCALS = %CALLNAME:ACTOR%くん_%CALLNAME:ACTOR%くん
				ELSE
					LOCALS = %CALLNAME:ACTOR%ちゃん_%CALLNAME:ACTOR%ちゃん
				ENDIF
		CASE "３年生", "研究生"
				LOCALS = %CALLNAME:ACTOR%先輩_%CALLNAME:ACTOR%せんぱい
		CASEELSE
				IF COND("教員", ACTOR)
					LOCALS = %CALLNAME:ACTOR%先生_%CALLNAME:ACTOR%せんせい
				ELSE
					LOCALS = %CALLNAME:ACTOR%さん_%CALLNAME:ACTOR%さん
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
ENDSELECT
RETURNF LOCALS


;-------------------------------------------------
;勧誘イベントのセリフや紹介文
;ここの中に限り、CFLAG,BASE,TALENT,ABL,EXP,CSTRをCSVに書いたものから変更させることが出来ます
;例えばBASE:体力を2000にしたいなら BASE:0:体力 = 2000 、強気にさせたいなら TALENT:0:強気 = 1 として下さい
;また、ABL:信頼を+2させたい場合には ABL:0:信頼 = ABL:信頼 + 2 と書いてください
