		CALL PRINT_STRL, @" 「……%CNAME("二人称")%の、ヘンタイ_HH_」"
	ENDIF
CASE "アソコチェック"
	;人が居るとき分岐させる(つもり)
	IF COND("合意：アソコチェック") && CHARA_NUM("室内の邪魔者") == 1
		PRINTFORML 「%CNAME("二人称")%～……えっちじゃないのにこういうことは、いけないことだと思うんだけどなー……」
	ELSEIF TALENT:恋慕 && CHARA_NUM("室内の邪魔者") == 0
		CALL PRINT_STRL, @" 「あは……_HH_ ここ、見るの好き～？」"
		CALL PRINT_STRL, @" 「いっつも見てくれてるから頑張って手入れしている甲斐があるよ～_HH_」"
