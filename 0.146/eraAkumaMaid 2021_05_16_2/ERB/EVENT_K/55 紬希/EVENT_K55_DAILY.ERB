;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K55, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE  "朝キス", "朝の新妻プレイ", "朝添い寝"
		SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STR, @" 「っん、おはよ……_HH_」_W"
		CASE 1
			PRINTFORML 「んぅ……あ、起きちゃった？」
			IF TEXTS("曜日") == "日"
				PRINTFORMW 「そろそろ朝ご飯にしない？」
			ELSE
				PRINTFORMW 「そろそろ学校行く時間だから、起きなきゃダメだよ」
				CALL PRINT_STRW, @" 「……えっ、もうちょっと？ しょうがないなぁ……_HH_」"
			ENDIF
		CASEELSE
			PRINTFORML 「あ、おはよ、%CNAME("二人称")%。」
		ENDSELECT
	CASE "朝騎乗位", "朝Ａ騎乗位","朝パイズリ", "朝フェラ", "朝足コキ", "朝クンニ"
		PRINTFORMW 「%CNAME("二人称")%っ、起きてよ～」
		CALL PRINT_STRW, @" 「%TEXTR("む～っ、下の体はもう起きてるのにぃ_HH_/朝一番_HH_いっぱい出してね_HH_/えへへ、起きないからぁ……_HH_朝から独り占め_HH_")%」"
	CASE "朝シックスナイン"
		PRINTFORMW 「%CNAME("二人称")%、起きた？」
		CALL PRINT_STRW, @" 「%TEXTR("もう下の方は起きちゃってるけど……_HH_/恋人朝まんこ_HH_味わってね_HH_")%」"
	CASE "朝逆正常位"
		CALL PRINT_STRW,@"「%CNAME("二人称")%、おはよぉ…_HH_」"
		CALL PRINT_STRW,@"「%CNAME("二人称")%%TEXTR("に朝出し_HH_きもちよかったぁ……_HH_/のナカ_HH_朝から独り占めしちゃった_HH_/の寝顔がえっちすぎたから、朝えっち_HH_しちゃった_HH_")%」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育だよ。%CNAME("二人称")%、体操服とかは忘れてないよね？」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「今日は何か予定あるかな？」
		CASE 1
			PRINTFORMW 「ちょっと待ってね、お茶入れてくる」
		CASEELSE
			PRINTFORMW 「おはよ。しっかり眠れた？」
			PRINTFORMW %CNAME("二人称")%は穏やかに微笑んだ。
		ENDSELECT
	ENDIF
CASE "一番乗り"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORMW 「あ、%CNAME("二人称")%」
		PRINTFORMW 「今日は%CNAME("一人称")%が一番だね」
	CASE 1
		PRINTFORMW 「やったー一番乗り！」
		PRINTFORMW 「……今日は何しようかな～♪」
	CASEELSE
		PRINTFORMW 「……他に誰も来てないみたいだね」
	ENDSELECT
CASE "ただいま"
	PRINTFORML 「ただいま戻りました」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜") && TALENT:相思相愛
		PRINTFORMW 「お帰り～」
		CALL PRINT_STRW, @" 「ご飯にする？お風呂にする？……それとも……_HH_」"
		CALL PRINT_STRW, @" 「なんでもないよ、なーいしょっ_HH_」"
	ELSEIF COND("夜")
		PRINTFORMW 「お帰り～」
	ELSE
		PRINTFORMW 「あ、待ってたよ」
	ENDIF
CASE "入室挨拶"
	SELECTCASE CSTR:PREV_DAILYCOM
	CASE "甘い言葉"
		PRINTFORMW (あれっ、もしかして邪魔になっちゃってる？)
	CASE "挨拶クンニ", "挨拶フェラ"
		PRINTFORMW 「ちょ、ちょっと何してるの……？」
	CASEELSE
		PRINTFORMW 「お邪魔しまーすっ」
	ENDSELECT
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「お邪魔します」
		PRINTFORMW 「……あー、もう皆お揃いなんだね」
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 「ね、…今日は何をする？」
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%は軽く会釈した。
		CASEELSE
			PRINTFORMW 「こんにちは。」
		ENDSELECT
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『こんにちは、%CNAME(CALLNAME_ACTOR)%。』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『みんなほとんど揃ってるよ。』
		CASE 1
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、こんにちは。』
		CASEELSE
			PRINTFORMW 『こんにちはー。』
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORMW 『こんにちは。』
		CASE 1
			PRINTFORMW %CALLNAME:TARGET%は軽く会釈した。
		CASEELSE
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%、こんにちは。』
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 「ただいま～」
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "お弁当"
	IF TALENT:相思相愛 && RAND:100 >= 95
		PRINTFORML 「今日はねー、なんとっ！！」
		CALL PRINT_STRL, @" 「愛妻弁当作って来ちゃったっ_HH_」"
		CALL PRINT_STRL, @" 「さあさ、めしあがれっ_HH_」"
	ELSE
		PRINTFORML 「今日は、%TEXTR("海苔弁当/ハンバーグ弁当/おにぎりの詰め合わせ/鮭の塩焼き弁当/")%を作ってきたよ～」
	ENDIF
	
	SELECTCASE RAND:4
	CASE 0
		PRINTFORML 「デザートの%TEXTR("梨/キウイ/みかん/りんご/ヨーグルト")%もどうぞ。」
	CASE 1
		PRINTFORML 「%CNAME("一人称")%の自信作だよ。」
		PRINTFORML 「どーお、おいしい…かな？」
	CASE 2
		PRINTFORML %CALLNAME:TARGET%は弁当の一部を%CALLNAME:MASTER%の口の前に差し出した。
		CALL PRINT_STRW, @" 「は、恥ずかしいけど……、あ～ん_HH_」"
	CASEELSE
		PRINTFORML 「%CNAME("二人称")%、お腹いっぱいになったかな？」
	ENDSELECT
ENDSELECT

;-------------------------------------------------
