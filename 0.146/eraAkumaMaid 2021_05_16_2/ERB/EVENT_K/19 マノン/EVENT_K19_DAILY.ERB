@KOJO_GREETING_K19, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS

CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ"
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「んふ……　\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("おはよう……/おはよ、びっくりした……？/おはよう、こういう起こされ方もたまにはいいでしょ？/ん、ふ……ちゅ、んんっ/おはよ、ぅ……んちゅ")%」
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("べ、べつにせーしなんて……んむっ/んっ、ちゅ、ちゅば/こ、こうしなきゃ、辛そうだから/はぁ、んむっ、ちゅ")%」
			PRINTFORMW 素直さのない悪態に聞こえるが、だいぶ熱心な口淫を続けていた。
		ENDIF
	CASE "朝騎乗位"
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「んふ……　\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("おはよう……んっ/おはよ、んふ……はぁ/あ、んっっ、おはよぅ/んっ、ぁぁおは、よぉ/はぁ、はぁ、んふ、おはよぅ")%」
			PRINTFORMW 目覚めと眠りの狭間で下半身が我慢の効かない遡りに目を覚ましたら、その絶対的質量をゆさぶる、一糸まとわぬマノンと目があった。
			PRINTFORMW どれくらいまたがられていたのだろう…… \@ EXP_GAIN("射精経験", PLAYER) ? もうすぐにでも射精してしまいそうだった。 # 気を抜くとすぐにでも暴発してしまいそうった。\@
			PRINTFORMW 「ん、そのまま……我慢しないで……おまんこの奥で……精液飲ませてぇ、っんぁああ！！」
			PRINTFORMW こちらの限界を悟ったマノンが、自らの絶頂を重ねるように下半身を沈め、最奥へ深々と迎え入れていた。
			;たぶん覚醒まのんの朝騎乗位とか射精を我慢するほうが難しい
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("あ、お、おきちゃっんんっ！！/あ、え、えと、あっ、ぁぁっ！！/ま、まってまってっ/ふぁ、っ！？　ぁうぅ！")%」
			PRINTFORMW 目覚めと眠りの狭間で下半身が我慢の効かない遡りに目を覚ましたら、その絶対的質量をゆさぶる、一糸まとわぬマノンと目があった。
			PRINTFORMW どれくらいまたがられていたのだろう…… \@ EXP_GAIN("射精経験", PLAYER) ? もうすぐにでも射精してしまいそうだった。 # 気を抜くとすぐにでも暴発してしまいそうった。\@
			PRINTFORMW 「や、やぁ、えと、これはその、あのっ、っっうああああ！！」
			PRINTFORMW \@　CEVENT("相思相愛：マノン") ? 想いが通い合っていても初々しい反応を返すマノンがかわいくて、思わず腰を両手で引き付けていた。 # あれだけ反発していても体は渇望に耐えられないらしい。最奥にみっちり先端を押し付けてやった。 \@
		ENDIF
	ENDSELECT
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝シックスナイン"
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? 精を吐き出し切った"それ"からマノンは目を離すことができず、再び口に含んで奉仕を始めた…… # じらすように舌先でつついたり、口に含んだままゆるく舌をはわせたどらせるだけで、こちらを弄ぶかのようにマノンは口淫を続けていた。\@
		ELSE
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? 精液を飲み込んで恍惚としたマノンを、ゆっくりからかうことにした。 # 口腔でゆったり感じさせてもらえる様を見つめながら、朝のひとときを楽しんだ。\@
		ENDIF
	CASE "朝騎乗位"
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? ぐっ、ぐっとよりぴったり、より深く密着しようとするマノンの腰使いに、大量に吐精させられていた。 # マノンはもう、こちらが射精するまで深部を押し付け続けずにはいられなかった。\@
			PRINTFORMW 「お、おは……っっぅ……！！」
			PRINTFORMW マノンは膣内を占める感触に絶頂を抜けられず、だが脱力をこらえながら幾度も腰を沈め続けて、貪欲に精を搾ろうと試みていた。
			;たぶん覚醒まのんの朝騎乗位とか射精を我慢するほうが難しい
		ELSE
			IF EXP_GAIN("射精経験", PLAYER)
				PRINTFORMW 「お、おはっ、んんんっっ！　おはよぉ、やぁ、だめぇ、出しながら動かさないでぇっ」
				PRINTFORMW 大量の吐精任せにマノンの最奥をぐいぐいと突き上げて、困惑する様を愉しませてもらった。
			ELSE
				PRINTFORMW 「お、おはっ、んんんっっ！　だ、だめぇ、もうっ、ぁぁっ！」
				PRINTFORMW とろけるような精感がマノンと結ばれる部分にこみあげるまで、困惑するマノンを幾度も幾度も突き上げた。
			ENDIF
		ENDIF
		CASE "朝の新妻プレイ"
			IF CEVENT("淫魔覚醒：マノン")
				PRINTFORMW \@ EXP_GAIN("射精経験", PLAYER) ? いつにないほど突き抜けるような射精感に、尻肉を押しつぶすくらい腰を押し付けて、どくどくと濃縮された白濁を吐き出していた。 # マノンの狭さと締め付けと柔らかい尻肉を味わうように腰を押し付けた。\@
				PRINTFORMW 「ちょ、ちょっと……っっぅ……！！　っはぁ……はぁ、もう、興奮しすぎぃ……でも、朝ごはんごちそうさまね……」
				PRINTFORMW マノンは蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥の圧迫感にひたっていた。
				;たぶん覚醒まのんの朝騎乗位とか射精を我慢するほうが難しい
			ELSE
				IF EXP_GAIN("射精経験", PLAYER)
					PRINTFORMW 「まって、まって、わ、私こんな、でも、っ、っあああ！！」
					PRINTFORMW この上ないほど強くこみあげるままに、濃縮された精液を注ぎながら、困惑するマノンの尻肉を押しつぶすように腰を擦り付けた。
					PRINTFORMW マノンは蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥にたたきつけられる吐精の感触にひたっていた。
				ELSE
					PRINTFORMW 「ちょ、ちょっとっ！　あっ、すご、すごいって、だ、だめぇ、もうっ、ぁぁっ！」
					PRINTFORMW とろけるような精感がマノンと結ばれる部分にこみあげるまで、困惑するマノンを幾度も幾度も後ろから責め続けた。
					PRINTFORMW マノンは蜜内を占める感触に絶頂を抜けられず、だが脱力しそうな膝を踏みとどめて、最奥の圧迫感にひたっていた。
				ENDIF
			ENDIF
	CASEELSE
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「んふ……　\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("おはよう/おはよ、んふ……ん？　別に何もないよ？/おはぁよ/おはよー")%」
		ELSE
			PRINTFORMW 「\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER%、 # \@ あの、%TEXTR("お、お、おはようございましゅってかんじゃったっ/お、おはよう、ございまｓ……/お、お、おはよぅぅ/お、おはようっ、")%」
		ENDIF
	PRINTL
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1

	IF COND("体育") && CEVENT("学食のお姉さん：マノン")
		;ままのんが体育の発言するのは学校を知っているときなので。
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「%TEXTR("あら、今日は体育のある日なの？/今日は何か浮かれてる？　そっか、体育のある日。男の子じゃ仕方ないか/え、今日は保健のほうの体育なんだ。でもいつも授業してるよね……私たち")%」
		ELSE
			PRINTFORMW 「%TEXTR("きょ、今日は体育のある日？　え、べ、別になにも……/体育のある日、って、ど、どうって、私は学生じゃないけど、でもそれだとお腹すかせて、うん、だからがんばるよ/ほ、ほほほほけんたいいくって、そ、それってつまりえっちな……そ、そそういう意味じゃなくてっ！")%」
		ENDIF
	ENDIF
CASE "一番乗り"
	IF CEVENT("学食のお姉さん：マノン")
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「%TEXTR("今日は私だけだけど……ふふ、他の人来る前にいいところいかない？/今ここにはわたしとあなただけ……それがどういう意味かはわかるよねぇ？/お先してるよーまだ他の人いないし、始めちゃおうっか。何って、ご・っ・く・ん(したのおくち)")%」
		ELSE
			PRINTFORMW 「%TEXTR("おおお、お先して、あ、あの……いえ、なんでも/え、え、私そんなやる気ってわけじゃ、でも、何してるか気になるっていうか/わ、私なんでここにいるのかって、私も、いちおう部員、だから")%」
		ENDIF
	ENDIF
CASE "ただいま"
	IF COND("合意：同棲") && CEVENT("学食のお姉さん：マノン") == 0
		CALL INAUGURATION_SCHOOL_MEAL, "帰宅"
	ELSEIF CEVENT("淫魔覚醒：マノン")
		PRINTFORMW 「ただいまもどったよ〜　\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER% # \@。 %TEXTR("今日もお疲れ様/いつもお疲れ様/少し、待ち遠しかった/待ってた……よ？/ね、今日も……する？")%」
	ELSE
		PRINTFORMW 「た、た、ただいま\@　CALLNAME:MASTER != "あなた" ? 、 %CALLNAME:MASTER% # \@。 あの、%TEXTR("おつかれ、じゃないかな？/えと、その、うぅ……(言葉に詰まる)！/お、おつかれさま！/え、えと……その、する？　あ、あの、もちろん入れちゃ嫌だからね！")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("合意：同棲") && CEVENT("学食のお姉さん：マノン") == 0
			CALL INAUGURATION_SCHOOL_MEAL, "出迎え"
		ELSEIF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「おかえりなさい、%TEXTR("今日もお疲れ様/いつもお疲れ様/少し、待ち遠しかった/待ってた……よ？/ね、今日も……する？")%」
		ELSE
			PRINTFORMW 「あの、お、お、%TEXTR("おかえりなさい！/お、おかえりなさい！/おかえりください！　って、ああ何言ってるのっ/お待ちして……ってそんなえっちじゃないからっ！/お、お、おおぉぉ……あれ？")%」
		ENDIF
	ELSE
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「先に来てたよ、%TEXTR("これからするのよね……ね？/今日私の番だといいなぁ/うん……したいしたいしたいしたい/あー、もう、うずうずするっ/ね、今日も……する？")%」
		ELSE
			PRINTFORMW 「あの、あの、%TEXTR("今日は早くて……え、そんなにしたいかって？　ち、ち、ちがっ/えと、今日は、今日は……き、期待なんてしてな……っ！/おかえりください！　って、ああ何言ってるのっ/お待ちして……ってそんなえっちじゃないからっ！/お、お、おおぉぉ……あれ？")%」
		ENDIF
	ENDIF
CASE "入室挨拶"
	IF CEVENT("淫魔覚醒：マノン")
		PRINTFORMW 「こんにちは〜%CALLNAME:TARGET%到着ぅ。 %TEXTR("お仕事お疲れ様のキスしたいなぁ/なんかおっぱいの注文ばっかり来るのよねぇ……揉ませてあげたかって？　ふふ〜ひみつ♪/ああー、今日は私の番だよね、ね？/ううう、うずうずするぅ？/ね、今日も……する？")%」
	ELSE
		PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、%TEXTR("ごめんなさ、おつかれ、なのは一緒だよね……/えと、その、うぅ……(言葉に詰まる)！/な、なんで来たって、そ、そんなの、わた、私も部員だから/え、えと……その、します？　あ、あの、もちろん入れちゃ嫌ですよっ！/き、期待してなんてっ、ち、ちがうよ！(どきどき)")%」
	ENDIF
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「こんにちは〜%CALLNAME:TARGET%到着ぅ。 あら、みんなお揃いだねぇ」
		ELSE
			PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、皆さんこんにちは」
		ENDIF
	ELSE
		IF CEVENT("淫魔覚醒：マノン")
			PRINTFORMW 「こんにちは〜%CALLNAME:TARGET%到着ぅ」
		ELSE
			PRINTFORMW 「や、やっと終わったあぁ…… あ、あの、こんにちは」
		ENDIF
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;挨拶してきた人にフェリシエルが混じっている
	IF FIND_COND("フェリシエル", "入室挨拶中")
		IF CEVENT("淫魔覚醒：マノン")
			IF RAND:3
				PRINTFORML 「こんにちはメイド」
				PRINTFORMW マノンはそっけなく挨拶したが、\@ CHARA_NUM("入室挨拶中") > 1 ? 真っ先にフェリシエルを選んだ辺り実は一番意識しているようだ。 # 挨拶だけはとりあえず欠かさないようだ。\@
			ELSEIF RAND:2
				PRINTFORML 「駄メイドこんにちは」
				PRINTFORMW マノンはあえて挑発するような挨拶をしている。
			ELSE
				PRINTFORML 「いちおうこんにちは」
				PRINTFORMW とりあえずフェリシエルに対しても挨拶はするようだ。
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORML 「こ、こんにちはメイド」
			ELSE
				PRINTFORML 「い、い、いちおうこんにちは」
				PRINTFORMW とりあえずでも挨拶はするようだ
			ENDIF
		ENDIF
	;挨拶してきた人に先生が混じっている
	ELSEIF FIND_COND("教員", "入室挨拶中")
		;挨拶してきた先生の中から一人選ぶ
		ACTOR = FIND_COND("教員", "入室挨拶中")
		;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

		IF CEVENT("淫魔覚醒：マノン")
			IF RAND:2
				PRINTFORML 「こんにちはぁ%CNAME(CALLNAME_ACTOR)%先生。って、いちお学校関係者だしいてもいいんだよね？」
			ELSE
				PRINTFORML 「%CNAME(CALLNAME_ACTOR)%先生おじゃまさまー、いやほんとにお邪魔とか言わないでね？」
			ENDIF
		ELSE
			IF RAND:2
				PRINTFORML 「こ、こんにちは%CNAME(CALLNAME_ACTOR)%先生」
			ELSE
				PRINTFORML 「あ、ああああの、%CNAME(CALLNAME_ACTOR)%先生こんにちは、おじゃましてます……」
			ENDIF
		ENDIF
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K19, %ARGS%, 遅かったじゃない
		[ENDIF]
	ELSE
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K19, %ARGS%
		[ENDIF]
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"

CASE "お弁当"

ENDSELECT

@INAUGURATION_SCHOOL_MEAL, ARGS
IF ARGS == "帰宅"
	PRINTFORMW その日、マノンはただいまの挨拶も半端に、いつもと違う事項を切り出した。
ELSE
	PRINTFORMW その日、マノンはおかえりの挨拶も半端に、いつもと違う事項を切り出した。
ENDIF
IF CEVENT("淫魔覚醒：マノン")
	PRINTFORMW 「あ、そうそう。\@ TEXTS("曜日") == "土" ? あさってから # 明日から\@私も学校行くことになったの」
	PRINTFORMW 「どうしてかって？　それはやっぱり……昼間もシタいし」
	PRINTFORMW 挑発的な潤み目が心臓に悪い。
	PRINTFORMW 「というのは建前で、本当はただ飯食らいのままじゃよくないでしょう？　だから、何かお金稼げないかって思ったら……ちょうど学食のお仕事があったの。それで、明日が初日というわけ」
	PRINTFORMW 「これからは平日もデキるなんて夢みたいだわ……うん、そういうことなので、よろしくお願いしますね」
	PRINTFORML
	PRINTFORMW なんだかややこしいことになったと思う、%CALLNAME:MASTER%であったが、%CALLNAME:TARGET%が持つその類い稀なる絶対的なふたつの質量が話題を呼び、それが学食の名物になることをこの後知ることになる……
	PRINTFORML
	PRINTFORMW 「そういえば」
	PRINTFORMW 「えっちする部活動してるんだっけ？　私ももちろん参加するからね」
ELSE
	PRINTFORMW 「あ、あ、え、えと、きょ、きょおは、お知らせしないといけないことがあって、その」
	PRINTFORMW 「いい、いつまでもただでご飯食べさせてもらうわけにいかないから、働くことにして」
	PRINTFORMW 「その、勤め先が学校の学食に決まって、\@ TEXTS("曜日") == "土" ? あさってから # 明日から\@、なんだけれど……」
	PRINTFORMW 「い、家じゃなにもできないしせめて……と思ったから」
	PRINTFORMW 「と、突然でごめんなさい、でも、こ、これからもよろしくお願いします」
	PRINTFORML
	PRINTFORMW %CALLNAME:TARGET%は深々と頭を下げたが、その頬に帯びている赤みは、きっと学校でも抱いてほしそうな欲の現れなのだろうと、%CALLNAME:MASTER%は勝手に解釈した。
	PRINTFORML
	PRINTFORMW もっとも、%CALLNAME:TARGET%が持つその類い稀なる絶対的なふたつの質量が話題を呼び、学食の名物になることをこの後知ることになる……のだが、吃音交じりにおどおどとしている様子がかわいいとも評判になるのだった。
	PRINTFORML
	PRINTFORMW なお、彼女は部活動に強制参加である。
	PRINTFORMW 同じ学校の学食に勤めてまで追ってきたのだから当然の処遇といえるだろう。
ENDIF
PRINTL
PRINTFORMW %CALLNAME:TARGET%が「学食のお姉さん」として就職しました。それに伴い、日中も学校で%CALLNAME:TARGET%に出会うことができるようになりました。
CFLAG:夜型 = 0
CSTR:学年 = 学食のお姉さん
CALL SET_CEVENT, "学食のお姉さん：マノン"

