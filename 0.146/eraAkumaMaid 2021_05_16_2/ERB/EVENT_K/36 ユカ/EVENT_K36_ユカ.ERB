;スキンシップすると「あはは。なんだよー」と返してくる、男友達みたいな女友達。
;基本方針はテンポよく一行台詞をポン出しする感じで。
;-------------------------------------------------
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K36, ARGS
#FUNCTIONS
#DIM ACTOR
VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称", "助手"
	SELECTCASE ARGS
	CASE "二人称"
		ACTOR = PLAYER
	CASE "助手"
		ACTOR = ASSI
	ENDSELECT

	IF ACTOR == MASTER
		LOCALS = お前_おまえ
	;さん付けの人には自分からあんまりドーンとはいかない
	ELSEIF NO:ACTOR == 1
		LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
	ELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部","中等部"
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%くん_%CSTR:ACTOR:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ENDIF
		CASE "１年生"
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%くん_%CSTR:ACTOR:ふりがな%くん
			ELSEIF TALENT:ACTOR:陽気
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSEIF AGE(ACTOR) <= 2
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		CASE "２年生"
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSEIF TALENT:ACTOR:陽気 || AGE(ACTOR) <= 2
				LOCALS = %CALLNAME:ACTOR%_%CSTR:ACTOR:ふりがな%
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		CASE "３年生"
			LOCALS = %CALLNAME:ACTOR%先輩_%CSTR:ACTOR:ふりがな%せんぱい
		CASEELSE
			IF COND("教員", ACTOR)
				LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
			ELSEIF AGE(ACTOR) == 1
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う。
CASE "主人"
	LOCALS = %CALLNAME:MASTER%_%CSTR:MASTER:ふりがな%
CASE "あなた"
	LOCALS = %CALLNAME:PLAYER%
ENDSELECT
RETURNF LOCALS

