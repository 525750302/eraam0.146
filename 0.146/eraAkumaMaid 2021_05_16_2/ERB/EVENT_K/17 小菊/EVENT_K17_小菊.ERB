		CASE "ご主人様"
			LOCALS = ご主人様_ごしゅじんさま
		CASE "お父さん"
			LOCALS = お父さん_おとうさん
		CASE "お母さん"
			LOCALS = お母さん_おかあさん
		CASE "パパ"
			LOCALS = パパ_ぱぱ
		CASE "ママ"
			LOCALS = ママ_まま
		CASE "お兄ちゃん"
			LOCALS = お兄ちゃん_おにいちゃん
		CASE "お姉ちゃん"
			LOCALS = お姉ちゃん_おねえちゃん
		CASEELSE
			LOCALS = YOU_YOU
		ENDSELECT
	;相手が主人ではない
	ELSE
		LOCALS = あなた_あなた
	ENDIF
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
ENDSELECT
RETURNF LOCALS

