;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K16, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = アタシ_あたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = アンタ_あんた
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:肩書
		CASE "実母"
			IF TALENT:相思相愛
				LOCALS = 義母さん_かあさん
			ELSE
				LOCALS = おばさん_おばさん
			ENDIF
		CASE "実妹"
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
		CASEELSE
			SELECTCASE CSTR:PLAYER:学年
			CASE "１年生", "初等部", "中等部"
				IF TALENT:PLAYER:オトコ
					LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
				ENDIF
			CASE "２年生"
				IF TALENT:PLAYER:オトコ
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ELSE
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ENDIF
			CASE "３年生", "研究生"
				LOCALS = %CALLNAME:PLAYER%先輩_%CSTR:PLAYER:ふりがな%せんぱい
			CASEELSE
				IF COND("教員", PLAYER)
					LOCALS = %CALLNAME:PLAYER%センセ_%CSTR:PLAYER:ふりがな%せんせ
				ELSE
					LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
				ENDIF
			ENDSELECT
		ENDSELECT
	ENDIF
CASE "主人"
	LOCALS = アイツ_あいつ
CASE "助手"
		SELECTCASE CSTR:ASSI:肩書
		CASE "実母"
			IF TALENT:相思相愛
				LOCALS = 義母さん_かあさん
			ELSE
				LOCALS = おばさん_おばさん
			ENDIF
		CASE "実妹"
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
		CASEELSE
			SELECTCASE CSTR:ASSI:学年
			CASE "１年生", "初等部", "中等部"
				IF TALENT:ASSI:オトコ
					LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
				ENDIF
			CASE "２年生"
				IF TALENT:ASSI:オトコ
					LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
				ELSE
					LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
				ENDIF
			CASE "３年生", "研究生"
				LOCALS = %CALLNAME:ASSI%先輩_%CSTR:ASSI:ふりがな%せんぱい
			CASEELSE
				IF COND("教員", ASSI)
					LOCALS = %CALLNAME:ASSI%センセ_%CSTR:ASSI:ふりがな%せんせ
				ELSE
					IF TALENT:ASSI:オトコ
						LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
					ELSE
						LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
					ENDIF
				ENDIF
			ENDSELECT
		ENDSELECT

ENDSELECT
RETURNF LOCALS

;-------------------------------------------------
;勧誘イベントのセリフや紹介文
;-------------------------------------------------
@EVENT_PROFILE_K16, ARGS

SELECTCASE ARGS
CASE "紹介文"
	STR:1 = あなたの同級生でいつも騒がしく好奇心旺盛な女子
	STR:2 = 良く言えばムードメーカー、悪く言えばトラブルメーカー
	STR:3 = 子供の頃は同じ遊び仲間グループでよく遊んでいた
;	STR:4 = 好きなＭＳはハンブラビ　好きでも嫌いでもないＭＳはドートレス
CASE "勧誘"
	PRINTFORML - %NAME:TARGET% -
	PRINTFORMW 「ねーねーねー、%CNAME("二人称")%　ひょっとして学校でなんかやってる？」
	PRINTFORMW 「最近色んな濃ゆい人達から同じクラスだからって%CNAME("二人称")%の事聞かれるんだよね」
