;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K16, ARGS
#DIM MEMO_LINECOUNT
;挨拶
#DIMS GREETING
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT


SELECTCASE ARGS
CASE "朝拘束"
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「うぇい、おはよう！ 早速だけど あっそぼうぜぇ～～い♪」
			CASE 1
				PRINTFORMW 「へへ～～♪ 油断してるからこ～なるんだぞ～♪」
			CASEELSE
				PRINTFORMW 「バカ面して寝ちゃって～……さては誘ってたろ～！？」
		ENDSELECT
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「あ、起きた？ んじゃホラ！%CNAME("ニ人称")%も%CNAME("一人称")%の舐めて！」
					PRINTFORMW 「こーゆーのはお返しの精神が大事なんだぞ～♪」
				CASE 1
					PRINTFORMW 「あーあ、朝から大洪水じゃん！ こりゃおねしょシーツでも敷いたほうがいいんじゃない？」
				CASEELSE
					PRINTFORMW 「うーん！ ちゃんと無意識下でも女の子できてるねぇ 感心感心！」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「いよう、こないだネットで見たテクの練習に借りてるよ～♪」
		ENDIF

	CASE "朝足コキ"
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「へっへ～どうだ～…ちょ…動くな動くな！ ベッドで片足立ちだから！」
				PRINTFORMW 「踏んじゃう踏んじゃう！ タマ踏んじゃう」
			CASE 1
				PRINTFORMW 「もぐもぐ……あ、%CNAME("一人称")%今朝ごはん中だからさ」
				PRINTFORMW 「まぁ %CNAME("二人称")%はコレでも十分イけるでしょ♪」
			CASE 2
				PRINTFORMW 「うぇ～い おはよー。 早速だが%CNAME("二人称")%のチンコは%CNAME("一人称")%が制圧した！」
				PRINTFORMW 「開放してほしければ%TEXTR("宿題の答えを教えろ/今度どっか遊びに連れてけ/ゲームのレベル上げ手伝え/カワイイって10回言え")%ー！」
			CASE 3
				PRINTFORMW 「へへ～♪ 少しぐらい踏んづけとかないとすぐ悪さするからなー このチンコ！」
			CASEELSE
				PRINTFORMW 「うりうり♪ どーだどーだ～？」
		ENDSELECT
	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「ふぅ…ふぅ… あ～コレ意外と疲れる～～！」
					PRINTFORMW 「パイズリで起こしてあげようなんて思わなきゃよかったー！」
				CASE 1
					PRINTFORMW 「ほれほれ、こんなの他の誰にもやんないんだからな～！ ありがたがれよ～♪」
				CASEELSE
					PRINTFORMW 「へへ～っ ど～だ%CNAME("一人称")%のおっぱい天国は！」
					PRINTFORMW 「寝ても覚めても夢心地だろ～～っ♪」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「これを…こうやって……あれぇ～ 動画みたいに行かないなぁ～」
			PRINTFORMW 「……あ、起きた？ おはよー」
		ENDIF
	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"
		;恋慕
		IF TALENT:恋慕
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「あ、起きた？ んじゃホラ！%CNAME("ニ人称")%も%CNAME("一人称")%の舐めて！」
					PRINTFORMW 「こーゆーのはお返しの精神が大事なんだぞ～♪」
				CASE 1
					PRINTFORMW 「ほれほれ♪ 起きがけで%CNAME("一人称")%にしゃぶって貰えて嬉しかろ～？」
				CASEELSE
					PRINTFORMW 「んもー、朝イチで少し搾っとかないとすぐ溜まるんだからなぁ～」
				ENDSELECT
		;それ以外
		ELSE
			PRINTFORMW 「いよう、こないだネットで見たテクの練習に借りてるよ～♪」
		ENDIF
	ENDSELECT

CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝シックスナイン"
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「へっへ～ どーだ嬉しいかぁ～～？」
			CASE 1
				PRINTFORMW 「いやぁ パンツの隙間から見えてたから 行ったれ！ と思ってさ♪」
			CASEELSE
				PRINTFORMW 「あれ！？ こんなになっといて本気で寝てたの？ ……おはよう！」
		ENDSELECT
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「ん、今日体育あるんだっけか……面倒っちいなぁ～」
			CASE 1
				PRINTFORMW 「あれ…体操着洗ってあったっけか……ま、いっか」
			CASE 2
				PRINTFORMW 「あーあ、体育ある日ってそれだけでユーウツだよねー」
			CASEELSE
				PRINTFORMW 「あーあ！ わざわざ授業で運動とかしたくないなぁ～」
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「ふわぁ～～あ、眠～～い」
			CASE 1
				PRINTFORMW 「あーあ、隕石でも振ってきて学校休みになんないかな～」
			CASE 2
				PRINTFORMW 「うぇい～～っす……ふわぁ…」
			CASE 3
				PRINTFORMW 「あれ、靴下どこいった靴下」
			CASEELSE
				PRINTFORMW 「トイレ入ってるの誰～～？ 早く出て～～っ」
		ENDSELECT
	ENDIF
CASE "一番乗り"
	IF TALENT:相思相愛
		SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「ジャーン！！　只今参じょ……って誰も来てないじゃん」
				PRINTFORMW 「せっかく二人でポーズ取って扉開けたのに これじゃ%CNAME("一人称")%達バカみたいじゃんねえ？」
			CASE 1
				PRINTFORMW 「ありゃ、何だよ誰も来てないし」
				PRINTFORMW 「みんな少しは%CNAME("一人称")%らを見習えってのよねー」
			CASEELSE
				PRINTFORMW 「ありゃ、せっかくラブラブ具合を見せつけてやろーと思ったのにねぇ」
		ENDSELECT
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「お、%CNAME("二人称")%早いじゃーん」
			CASE 1
				PRINTFORMW 「へっへ～ 掃除当番抜け出して来ちった♪」
			CASE 2
				PRINTFORMW 「おー？ 早いねぇ！　やる気あるじゃん、感心感心」
			CASEELSE
				PRINTFORMW 「おっ♪ 人少ないからお気に入りのイス取れる！」
		ENDSELECT
	ENDIF
	
CASE "ただいま"
;土曜日も講義あるじゃないですかやだぁ～～～っ！！
;でもそのまんまでいいや
	IF TEXTS("曜日") == "金"
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「今週もお疲れ様でした！　あしたからお休み！　やったぜぃ！」
			CASE 1
				PRINTFORMW 「今週も%CNAME("一人称")%はやりきった！」
				PRINTFORMW 「そしてついに！ 待ち焦がれた週末が眼の前に！」
			CASE 2
				PRINTFORMW 「イェイイェイ♪ 週末週末～～♪」
				PRINTFORMW 「学校のことなんか全部忘れるくらい遊んでやるんだ～～」
			CASEELSE
				PRINTFORMW 「たっだいまぁ～～♪」
				PRINTFORMW 「知ってる？ 知ってる？ 明日休みだよ？」
		ENDSELECT

	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「うぇ～～い」
			CASE 1
				PRINTFORMW 「たっだいまぁ～～♪」
			CASE 2
				PRINTFORMW 「ヤバいヤバイ！ 見逃す見逃す！！」
			CASEELSE
				PRINTFORMW 「あぁ～～もぉ 疲れたぁ～～！」
		ENDSELECT
	
	ENDIF

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「ありゃ、今帰ってきたの%CNAME("二人称")%？」
			CASE 1
				PRINTFORMW 「おかえり～　どっか寄ってきたの？　お土産は？？」
			CASE 2
				PRINTFORMW 「あー、連絡すれば良かったなぁ　帰り買ってきて欲しいモノあったんだよねー」
			CASEELSE
				PRINTFORMW 「おっかえりぃ～～」
		ENDSELECT
	ELSE
		IF COND("同伴中", MASTER)
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「ありゃ、%CNAME("パートナー")%と一緒なんだ」
			CASE 1
				PRINTFORML 「%CNAME("パートナー")% うぇい～～っす～」
				PRINTFORMW 「ついでに%CNAME("二人称")%も うぇいっす♪」
			CASEELSE
				PRINTFORMW 「%CNAME("パートナー")%、コイツと一緒で大丈夫だったぁ～？」
			ENDSELECT
		ELSE
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「遅ーい！　遅刻だぞ～～ 」
			CASE 1
				PRINTFORMW 「まったく！ たるんどるンじゃないかね チミィ？」
			CASE 2
				PRINTFORMW 「あ、やっと来た！」
			CASEELSE
				PRINTFORMW 「遅れたんだからせめて面白いことしながら入ってくるとかしなさいよ ダメねー」
			ENDSELECT
		ENDIF
	ENDIF

CASE "入室挨拶"
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 「おい～～っす！！ 」
			CASE 1
				PRINTFORMW 「ちゅ～～っす！」
			CASE 2
				PRINTFORMW 「やーやー、どーもどーも！」
			CASE 3
				PRINTFORMW 「動くな！ 警察だ！ ……なんつって！」
			CASEELSE
				PRINTFORMW 「じゃじゃーん！！ 」
			ENDSELECT
			
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			SELECTCASE RAND:6
			CASE 0
				PRINTFORMW 「おい～～っす！！ 」
			CASE 1
				PRINTFORMW 「ちゅ～～っす！」
			CASE 2
				PRINTFORMW 「お～～！ みんな集まってるじゃ～～ん！」
			CASE 3
				PRINTFORMW 「おっ、今日は集まりいいね！ で、何やる何やる！？」
			CASE 4
				PRINTFORMW 「ちょっとみんな聞いてよ～～！ さっきそこでさぁ～～！」
			CASEELSE
				PRINTFORMW 「いやぁ～遅れちゃった　メンゴメンゴ♪」
			ENDSELECT
	ELSE
			SELECTCASE RAND:6
			CASE 0
				PRINTFORMW 「おい～～っす！！ 」
			CASE 1
				PRINTFORMW 「ちゅ～～っす！」
			CASE 2
				PRINTFORMW 「お、今日のメンツはこんなもん？」
			CASE 3
				PRINTFORMW 「お～～っ？ %CNAME("一人称")%がビリじゃなかった！」
			CASE 4
				PRINTFORMW 「お、もう来てたの？ 感心感心♪」
			CASEELSE
				PRINTFORMW 「おっ、やってるねえ諸君！」
			ENDSELECT
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		;先生を発言者として選び直す
		ACTOR = FIND_COND("教員", "入室挨拶中")
		CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%
			SELECTCASE RAND:4
			CASE 0
				CALL EMOTION_PLUS, "びっくり", TARGET
				PRINTFORMW 『ゲッ！ センセ来た！ ヤバいヤバい！ これ隠して隠して！』
			CASE 1
				PRINTFORMW 『サボってませんよー ちゃんといますよー』
			CASE 2
				PRINTFORMW 『どーもー！』
			CASEELSE
					IF RELATION:(NO:ACTOR) >= 150
						PRINTFORMW 『いやー、センセもすっかりココに入り浸るようになっちゃったねぇ～』
					ELSE
						PRINTFORMW 『げ、センセが来た。 やりづらいなぁ～……』
					ENDIF
			ENDSELECT
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 『ういっすぅ！』
			CASE 1
				PRINTFORMW 『ちゅ～～っす！』
			CASE 2
				PRINTFORMW 『おおっ だんだん部室が狭くなってきたぞ』
			CASE 3
				PRINTFORMW 『はいはい、んじゃソコ詰めて詰めて！ スペース作ってあげて！』
			CASEELSE
				PRINTFORMW 『へへ～～っ もっと早くこないといい場所取れないぞ～～♪』
			ENDSELECT
	ELSE
			SELECTCASE RAND:5
			CASE 0
				PRINTFORMW 『ういっすぅ！』
			CASE 1
				PRINTFORMW 『ちゅ～～っす！』
			CASE 2
				PRINTFORMW 『よし、%CNAME(CALLNAME_ACTOR)%来たから何かして遊ぼう！』
			CASE 3
				PRINTFORMW 『ふふーん、どーだ %CNAME("一人称")%に先を越された気分はー！』
			CASEELSE
				PRINTFORMW 『おっ ちょうど良い所に来た！ ねーねーコレなんだけどさぁ～』
			ENDSELECT
	ENDIF
CASE "居間に現れた"
	IF CFLAG:呼び出された
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「なにー？」
			CASE 1
				PRINTFORMW 「お？ どっかからお菓子の贈り物でも届いたのかな？」
			CASEELSE
				PRINTFORMW 「何よもー、今いいところだったのにー」
			ENDSELECT
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「まだ いたの！」
	ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORMW 「あー…お腹へったぁー 何か食べるものない？」
			CASE 1
				PRINTFORMW 「%TEXTR("ティッシュ、ティッシュ/延長コード、延長コード/新品の蛍光灯はどこだぁ/ねー、画鋲ってどっかにあるー？")%……っと」
			CASEELSE
				
			ENDSELECT
	ENDIF
CASE "全裸化への反応"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「うひょー 度胸いいなぁ～」
			CASE 1
				PRINTFORMW 「あひゃひゃひゃ！！ なんで脱ぎ出してんのいきなり～！？」
			CASE 2
				PRINTFORMW 「わお、何で脱いでんの！？」
			CASEELSE
				PRINTFORMW 「お！ で？ で？ 何かやんの？ 裸で！？」
			ENDSELECT

CASE "抜け出された"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「はいはい、どーぞドコにでもいってらっしゃい」
			CASE 1
				PRINTFORMW 「あれ？ もう解散？ 早くない！？」
			CASE 2
				PRINTFORMW 「くぁ～～～～っ！ 何だろうね あの連中！！」
			CASEELSE
				PRINTFORMW 「わっ、こっちの事なんか全然見もしないでやんの」
			ENDSELECT
CASE "お弁当"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORMW 「さあ せっかく早起きして作ったんだから たんと食えコンニャロウ」
			CASE 1
				PRINTFORMW 「おかかちょっとくっついてるのがおかかで そうでないのが昆布ね」
			CASE 2
				PRINTFORMW 「丹精込めて作ったんだぞー 全部冷凍食品だけど」
			CASEELSE
				PRINTFORMW 「たまには%CNAME("二人称")%が作ったって全然いいんだかんね？」
			ENDSELECT
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K16, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「へぇ～……ここが部室なんだ」
	PRINTFORML 「……何か妙に広くない？ ココ」
CASE "汗だく"
			SELECTCASE RAND:4
			CASE 0
				PRINTFORML 「あぁ～～！ んもう暑いぃ～～っ！！」
			CASE 1
				PRINTFORML 「あっっぢぃ～～！ 何でエアコンないのよココぉ～～」
			CASE 2
				PRINTFORML 「ったく！ 今どきマラソンとかアタマおかしいよね 体育教師！」
			CASEELSE
				PRINTFORML 「窓開けてもコレなの？ 全然風入ってこないじゃんココ！」
			ENDSELECT
CASE "寝起き"
	PRINTFORML 「……ん～～… 何だ夢かぁ」
CASE "コスプレ中"
	PRINTFORML 
CASE "おやつの感想"

	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
					PRINTFORML 「さーさー 皆の衆、遠慮なくやってくれぃ♪」
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「うわっ！？ 何かスゴいゴージャスなのが置いてある！ 何だこの食べ物！？」
				CASE 1
					PRINTFORML 「何これ誰か出前取ったの？ なんかスゴいおいしそーなのがあるけど」
				CASE 2
					PRINTFORML 「何かスゴい手の混んだお菓子があるんだけど…食べていいやつ？ 撮影かなんかで使うやつ？」
				CASEELSE
					PRINTFORML 「うひょー、これ誰かの手作り？ はえ～～」
				ENDSELECT
			ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「へー、誰作のお菓子コレ？ 授業かなんかで作ったヤツ？」
				CASE 1
					PRINTFORML 「これ食べていいやつ？ わーい！ いただきマンモス♪」
				CASE 2
					PRINTFORML 「お、また誰かがなんか作って持ってきてる！ だからこの部活すき！」
				CASEELSE
					PRINTFORML 「お、誰作ったのコレ？ いただきー♪（ﾓｼｬﾓｼｬ」
			ENDSELECT
			ENDIF
		
		ELSE
				SELECTCASE RAND:4
				CASE 0
					PRINTFORML 「お、なんだなんだ！ 誰が作ったやつこれ！？ 食べていいの？」
				CASE 1
					PRINTFORML 「あ！ また手作りお菓子発表会やってる！」
				CASE 2
					PRINTFORML 「おや、何か女子力高めのお菓子があるぞ！ どれどれ味は～……」
				CASEELSE
					PRINTFORML 「どんなふうに美味しいのかってさっぱり説明できないから とりあえず大きな声で美味しいって言っとく！」
				ENDSELECT

		ENDIF
	;おやつが手作りでない
	ELSE
			SELECTCASE RAND:5
			CASE 0
				PRINTFORML 「お、おやつやった～～♪ お腹ペッコペコだったんだー！」
			CASE 1
				PRINTFORML 「これ食べていいやつ？ わーい！ いただきマンモス♪」
			CASE 2
