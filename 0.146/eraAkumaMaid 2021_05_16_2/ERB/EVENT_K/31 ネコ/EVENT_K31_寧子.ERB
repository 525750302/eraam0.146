	;学生年下
	CASE "１年生", "初等部", "中等部"
		IF TALENT:ASSI:オトコ
			LOCALS = こーはいくん_こーはいくん
		ELSE
			LOCALS = こーはいちゃん_こーはいちゃん
		ENDIF
	CASE "２年生"
		IF TALENT:ASSI:オトコ
			LOCALS = おにーさん_おにーさん
		ELSE
			LOCALS = おねーさん_おねーさん
		ENDIF
	;学生年上
	CASE "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%センパイ_%CSTR:ASSI:ふりがな%せんぱい
	;相手が同学年、ないし学生では無く教職。
	CASEELSE
		IF STRCOUNTS(CSTR:ASSI:学年, "教師")
			LOCALS = %CALLNAME:ASSI%センセ_%CSTR:ASSI:ふりがな%せんせ
		ELSEIF STRCOUNTS(CSTR:ASSI:学年,  "司書")
