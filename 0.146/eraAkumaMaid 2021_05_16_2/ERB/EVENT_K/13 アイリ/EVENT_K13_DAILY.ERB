;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K13, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	CALL EMOTION_PLUS, "ムラムラ", TARGET
	IF COND("発情中")
		CALL PRINT_STR, "「あ、やっと起きた…_H_」_W"
		PRINTFORMW 「じゃあ、いいわよね？もう始めても、いいわよね…？」
	ELSE
		PRINTFORMW 「その…発情期じゃなくても溜まることはあるっていうか…」
		PRINTFORMW 「と、とにかく！今から%CNAME("一人称")%に付き合ってもらうんだから…！」
	ENDIF
CASE "朝駆け"

CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝パイズリ", "朝足コキ", "朝シックスナイン"
		SIF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「んぅっ……_H_」_W"
		PRINTFORMW 「あ…\@ COND("発情中") ? 起きちゃった…？ # 起こしちゃったか… \@おはよう」
	CASE "朝騎乗位"
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		SIF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, "「ふあぁっ……_H_」_W"
		PRINTFORMW 「…\@ COND("発情中") ? ふふ……♪ # ふー…… \@ようやく起きたわね…まったく…」
	CASE "朝の新妻プレイ"
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		IF COND("発情中")
			CALL PRINT_STR, "「ふーっ……_H_ふーっ……_H_」_W"
			IF RAND:2
				CALL PRINT_STR, "「…もう…朝から、元気すぎ……_H_」_W"
			ELSE
				CALL PRINT_STR, "「…お粗末、様でした……_H_」_W"
			ENDIF
		ELSE
			CALL EMOTION_PLUS, "怒り", TARGET
			IF RAND:2  && TEXTS("曜日") != "日"
				PRINTFORMW 「ったく、遅刻したらどうするのよ……こんな理由でとか、恥ずかしすぎるじゃない…」
			ELSE
				PRINTFORMW 「はぁっ…はぁっ……相っ変わらず、%TEXTR("バカ/スケベ")%なんだから…」
			ENDIF
		ENDIF
	CASE "朝キス"
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		IF !COND("発情中") && RAND:2
			PRINTFORMW 「……おはよう」
			PRINTFORMW 「はぁ……朝から何やってるのかしらね、%CNAME("一人称")%達……」
		ELSE
			PRINTFORMW 「\@ COND("発情中") ? ふふ♪どう…？ # その…す、 \@少しは目、覚めた…？」
		ENDIF
	CASE "朝添い寝"
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		IF COND("発情期") || RAND:2
			CALL PRINT_STR, "「…あ、その…おはよう…_／／／_」_W"
			PRINTFORMW 「ご、ごめん…今、離れるから……！」
		ELSE
			PRINTFORMW 「しまった、うっかり寝ちゃってたわ…」
			PRINTFORMW 「な、ちがっ！何で%CNAME("二人称")%の寝顔なんて見なきゃいけないのよ！」
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	PRINTFORMW 「……おはよう」
	IF COND("体育")
		PRINTFORMW 「…今日は体育のある日ね…%TEXTR("しんどいからできれば見学したいなぁ/球技だったら大歓迎なんだけど")%…」
	ELSEIF COND("水泳")
		PRINTFORMW 「…今日は水泳のある日ね…正直言うと水はちょっと苦手なのよね…」
	ELSE
		IF COND("同棲中") == 0 && RAND:4 == 0
			PRINTFORMW 「早く起きてよね、折角起こしに来てあげたんだから」
		ELSEIF RAND:3 == 0 && TEXTS("曜日") != "日"
			PRINTFORML 「…さ、早く学校に、って…え？一緒に登校するの？」
			PRINTFORMW 「う、噂とかになったらどうするのよ…？」
		ELSEIF RAND:2
			PRINTFORML 「…寝癖ついてるわよ、ほら、そこ」
			PRINTFORMW 「今直してあげるから、じっとしてて…」
		ELSE
			PRINTFORMW 「…さ、朝ごはんにしましょ…抜いたら一日持たないわよ？」
		ENDIF
	ENDIF
CASE "一番乗り"
	IF COND("発情期")
		PRINTFORMW 「…他の人はまだなのね？」
		PRINTFORMW 「…どうしよう…じゃあ今の内にガッと…いやでも途中で誰か来たら…」
		PRINTFORMW %CALLNAME:TARGET%は何かを逡巡している…
	ELSEIF RAND:3 == 0 && TALENT:恋慕
		PRINTFORMW 「あ…二人きり、か…」
		PRINTFORMW 「別に、本当にそうだからそう言っただけよ…」
	ELSEIF RAND:2
		PRINTFORMW 「なんか、静かなのも悪くないかもね」
		SIF CHARA_NUM("集合予定") >= 3
			PRINTFORMW 「ま、どうせすぐ賑やかになるんだろうけど…」
	ELSE
		PRINTFORMW 「誰もいないわね…皆忙しいのかしら？」
	ENDIF
CASE "ただいま"
	IF COND("発情期")
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		PRINTFORMW 「…た、ただいま…」
		PRINTFORMW %CALLNAME:TARGET%はよろめきながらもどうにか帰宅したようだ…
	;週一で夕飯お魚？
	ELSEIF RAND:7 == 0
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「……くんくん…」
		PRINTFORMW 「…はぁ……焼き魚の匂いはお隣だったのね…」
	ELSEIF RAND:3 == 0
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「ただいま帰りました、っと…」
		PRINTFORMW 「…誰の靴よ、これ…？脱いだらちゃんと揃えなさいよね…」
	ELSEIF RAND:2
		PRINTFORMW 「…ただいま」
		PRINTFORMW 帰宅した%CALLNAME:TARGET%の手には猫じゃらしが一本握られている…
		PRINTFORMW 「…み、道端に生えてるの見たらどうしても気になっちゃって…」
	ELSE
		PRINTFORMW 「…ただいま」
		PRINTFORMW 「なんだ、先に帰ってたのね」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF COND("発情中")
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORMW 「あ、おかえりなさい…」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%に駆け寄ると体をすり寄せてきた…
		ELSEIF COND("発情期") 
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORMW 「！！……あ、お、おかえり…」
			PRINTFORMW %CALLNAME:TARGET%は一瞬、待ちわびたと言わんばかりの表情を浮かべたものの、すぐに我に返ったようだ…
		ELSE
			IF RAND:2
				PRINTFORMW 「ん、おかえり」
				PRINTFORMW %CALLNAME:TARGET%は床に寝そべったまま挨拶をしてきた…
			ELSE
				PRINTFORMW 「おかえりなさい…って、%CNAME("二人称")%か…おかえり」
			ENDIF
		ENDIF
	ELSE
		IF COND("発情中")
			CALL EMOTION_PLUS, "甘い言葉", TARGET
			PRINTFORMW 「遅かったじゃない…待ってたんだから…」
			PRINTFORMW 尻尾をピンと立てた%CALLNAME:TARGET%は嬉しそうに%CALLNAME:MASTER%を出迎えた…
		ELSEIF COND("発情期") 
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORMW 「あ…！な、何をもたもたやってたのよ…！」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%が来たことに気づくと急にソワソワし始めた…
		ELSEIF RAND:2
			PRINTFORMW 「遅かったわね、何処で道草食ってたのよ？」
		ELSE
			PRINTFORMW 「あ、ようやく来た…どうしたの？補習でも受けてたの？」
		ENDIF
	ENDIF

CASE "入室挨拶"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
	;トーク
	CASE 1
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甘い言葉"
			CALL EMOTION_PLUS, "不機嫌", TARGET
			PRINTFORMW 「…何？この空気は…？ちょっと入りづらいんだけど…」
		CASE "挨拶クンニ", "挨拶フェラ"
			IF RAND:2
				CALL EMOTION_PLUS, "不機嫌", TARGET
				PRINTFORMW 「そういうのは、人目のない場所でやったら…？」
				PRINTFORMW %CALLNAME:TARGET%は呆れたような表情を浮かべている…
			ELSE
				CALL EMOTION_PLUS, "不機嫌", TARGET
				PRINTFORMW 「見てない見てない、%CNAME("一人称")%は何も見てないわ…」
				PRINTFORMW %CALLNAME:TARGET%は何やらブツブツと呟きながら部屋に入ってきた…
			ENDIF	
		ENDSELECT
	;もてなす
	CASE 2
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "おっぱいリフトする", "キスする"
			CALL EMOTION_PLUS, "不機嫌", TARGET
			PRINTFORMW 「…何やってんの？」
			PRINTFORMW 「……いや、ダメダメ…気にしたら負け、気にしたら負けよ%CNAME("一人称")%…」
		ENDSELECT
	;隙を見せる
	CASE 3
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "Ｈのお誘い", "パンツ見せ", "押し倒し", "抱きつき"
			PRINTFORMW 「えーと……邪魔しちゃったかしら…？」
			PRINTFORMW %CALLNAME:TARGET%は部屋の入り口でおろおろしている…
		ENDSELECT
	;パンツ、性処理
	CASE 4, 6
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORMW 「…一回ガツンと言うべきなのかしら？そういうのやめた方がいい、って…」
		PRINTFORMW %CALLNAME:TARGET%はため息をついている…
	ENDSELECT

	;猫の日判定
	CALL CATDAY_K13

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		IF COND("発情中")
			PRINTFORMW 「……来た、けど」
			PRINTFORMW %CALLNAME:TARGET%は頬を紅潮させながら期待するような目でこちらを見ている…
		ELSE
			PRINTFORMW 「……別に急かさなくてもいいでしょ…ちゃんと来るつもりだったってば」
			PRINTFORMW %CALLNAME:TARGET%はぶつくさ文句を言いながら入室してきた…
		ENDIF
	ELSE
		PRINTFORMW 「…ん、ちょっと遅くなったみたいね」
	ENDIF

;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;既に部員の３分の２が集合済
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*2/3 && RAND:3 == 0
		PRINTFORMW 「わ、もうほとんど全員揃ってるみたいね…」
	ELSE
		PRINTFORMW 「…ん、ちょっと遅くなったみたいね」
	ENDIF
;簡易入室挨拶に対する返し。ここでは括弧に『』を使う事。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		ACTOR = FIND_COND("教員", "入室挨拶中")
		IF RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%先生…忙しかったんですか？』
		ELSE
			PRINTFORMW 『…席とか開けます？』
		ENDIF
	ELSEIF PLACE("自宅")

	;先輩にも敬語使いたいかも？
	ELSE
		IF RAND:2
			PRINTFORMW 『やっと来たわね』
		ELSE
			PRINTFORMW 『遅かったじゃない』
		ENDIF
	ENDIF
	
CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「……何か用？今日は部屋にいたい気分だったんだけど…」
		PRINTFORMW %CALLNAME:TARGET%は\@ COND("発情中") ? おぼつかない足取りで # ぶつくさ文句を言いながら \@入室してきた…
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「……ん…やっぱり、%TEXTR("寝れなかった/目、覚めちゃった")%……」
	ELSE
		PRINTFORMW 「……あ、ここにいたのね」
	ENDIF
CASE "抜け出された"
	IF COND("発情期")
		PRINTFORMW 「……逃げられた…」
		PRINTFORMW %CALLNAME:TARGET%は耳と尻尾をしおれさせている…
	ELSEIF RAND:2
		PRINTFORMW 「用事？ふーん…ま、頑張ってね」
	ELSE
		PRINTFORMW 「おぉ…大胆と言うか、なんと言うか…」
	ENDIF
CASE "お弁当"
	IF RAND:6 == 0 && TALENT:恋慕 && COND("発情期")
		PRINTFORMW 「…ほら、今日の分よ」
		PRINTFORMW %CALLNAME:MASTER%がお弁当箱の蓋を開けると、ご飯の上に海苔で大きく『ＹＥＳ』の文字が描かれていた…
		IF COND("発情中")
			PRINTFORMW %CALLNAME:TARGET%はニヤニヤしながらこちらを見ている…
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は頬を染めながら知らん顔をしている…
		ENDIF
	ELSEIF RAND:5 == 0 && TALENT:恋慕
		PRINTFORMW 「…ま、まずかったら遠慮しないで残してよ……？」
		PRINTFORMW 「あ、特にこれとか！%TEXTR("ちょっと固い/結構味濃い/焦げてる部分ある")%し無理して食べないでもいいから！」
		PRINTFORMW %CALLNAME:TARGET%は料理の出来栄えが気になって仕方ないようだ…
	ELSEIF RAND:4 == 0 && TALENT:恋慕
		PRINTFORMW 「毎度毎度世話の焼ける事ね…」
		PRINTFORMW お弁当箱を持つ%CALLNAME:TARGET%の指には絆創膏が巻かれている…
		PRINTFORMW 「…こ、これは…料理とは全然、無関係だからね…」
	ELSEIF RAND:3 == 0
		PRINTFORMW 「どう？おいしい…？」
		PRINTFORMW 「そりゃ、気になるわよ……そ、そこまで力入れたわけじゃないけどね」
	ELSEIF RAND:2
		PRINTFORMW 「何？おかかはもう飽きた？」
		PRINTFORMW 「だったらこっちにツナマヨとシャケがあるけど…」
		PRINTFORMW おにぎりの中身は%CALLNAME:TARGET%の好物ばかりのようだ…
	ELSE
		PRINTFORMW 「…食べさせて欲しい？何を子供みたいな事言ってるのよ…」
		PRINTFORMW 「しょうがないわね、口開けて…」
		PRINTFORMW 「…」
		PRINTFORMW 「…わかったわよ！あーんしなさい！ほら、あーん！」
	ENDIF

ENDSELECT
CALL CATDAY_K13

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K13, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「…もしかして%CNAME("一人称")%以外の人ともそういう契約してるの？」
	PRINTFORML 「…聞いてないんだけど」
CASE "汗だく"
	IF COND("発情期") 
		PRINTFORML 「…熱い……体、熱いよぉ……」
		PRINTFORML %CALLNAME:TARGET%はものすごい量の汗をかいている…
	ELSEIF RAND:3 == 0 && COND("ブラ透け")
		PRINTFORML 「何…？服が透けてる…？知ってるって、それくらい…」
		PRINTFORML 「暑くてもうそんなの気にしてられないのよ…」
	ELSEIF RAND:2 && CHECK_CLO("体操服姿")
		PRINTFORML 「体操服の色を白って決めた奴、絶対下心ありきで決めたわよね…」
	ELSE
		PRINTFORML 「やだ、尻尾の毛が汗吸っちゃってるし…」
		PRINTFORML 「これ乾かすの時間かかるなぁ…」
	ENDIF
CASE "おやつの感想"
	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
		IF CHARA_NUM("室内の邪魔者") >= 3 && RAND:2
			PRINTFORML 「この人数だと、いくら用意してもすぐなくなっちゃうわね…」
			PRINTFORML 「%TEXTR("取り合いにならないといいけど/もう少し買っておけばよかったかな")%…」
		ELSE
			PRINTFORML %CALLNAME:TARGET%はお菓子が入った器の方を時折ちらちらとみている…
			PRINTFORML おやつの売れ行きが気になっているようだ…
		ENDIF
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				PRINTFORML 「%TEXTR("あ、これおいしい……/…！！")%」
				PRINTFORML %CALLNAME:TARGET%は%TEXTS("おやつ当番")%の手作りおやつの出来に%TEXTR("驚い/感動し")%ているようだ…
			ELSE
				PRINTFORML 「%TEXTR(@"これって%CNAME("一人称")%にも作れるかしら/後でレシピ調べてみようかな")%……」
				PRINTFORML %CALLNAME:TARGET%は%TEXTS("おやつ当番")%の手作りおやつ%TEXTR("に触発されている/を真似してみたくなった")%ようだ…
				;この辺に後々手作りし始めるフラグを入れたい
			ENDIF
		ELSE
			PRINTFORML 「………」
			PRINTFORML %CALLNAME:TARGET%は%TEXTS("おやつ当番")%の作ったおやつを%TEXTR("頬張って/味わって")%いる…
		ENDIF
	;おやつが手作りでない
	ELSE
		PRINTFORML 「……%TEXTR("これって一人いくつまで？/もう一個、もう一個だけ…これで最後だから")%」
		PRINTFORML %CALLNAME:TARGET%は%TEXTS("おやつ当番")%の持ってきたおやつに手を伸ばしている…
	ENDIF
CASE "寝起き"
	PRINTFORML 「ふあぁ〜〜〜〜ぁ…」
	PRINTFORML %CALLNAME:TARGET%は顎が外れないか心配になる程の大口を開けてアクビをした…
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K13
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
CASE "コスプレ中"
	IF COND("発情中")
		PRINTFORML 「こういうのもたまには…ね♪」
		PRINTFORML 「……この格好ならいつもと違う気分で楽しめるかもしれないし」
	ELSE
		PRINTFORML 「ちょ、ちょっと気まぐれで着てみただけだから…」
		PRINTFORML 「似合ってなくてもあんまり気にしないでよ？」
	ENDIF
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「あー…心臓、バクハツしそう……」
CASE "デートの約束"
	IF TALENT:恋慕 && RAND:2
		PRINTFORML 「………♪」
		PRINTFORML %CALLNAME:TARGET%は%TEXTR("尻尾を垂直にピンと立てている/心なしかニコニコしているように見える")%…機嫌が良さそうだ
	ELSE
		PRINTFORML 「………」
		PRINTFORML %CALLNAME:TARGET%は落ち着きなく尻尾を左右に揺らしている…少し緊張しているようだ
	ENDIF
CASE "契約成立"
	PRINTFORML 
CASE "抜け出す"
	IF COND("発情期")
		PRINTFORML 「…ここで、するのよね？」
		PRINTFORML %CALLNAME:TARGET%はじりじりと距離を詰めてくる…
	ELSEIF RAND:2
		PRINTFORML 「こっそり連れ出してどういうつもり？」
		PRINTFORML 「ま、のこのこ着いてく%CNAME("一人称")%も%CNAME("一人称")%だけど…」
	ELSE
		PRINTFORML 「もう、わざわざ手まで繋がなくてもいいでしょ…」
	ENDIF
CASE "あなたの部屋に連れ込まれた"
	IF COND("発情中")
		PRINTFORML 「…で、これからどうするの？」
		PRINTFORML 「一応聞いてみたのよ…一応、ね♪」
	ELSE
		PRINTFORML 「…で、なんでわざわざ場所変えたの？」
