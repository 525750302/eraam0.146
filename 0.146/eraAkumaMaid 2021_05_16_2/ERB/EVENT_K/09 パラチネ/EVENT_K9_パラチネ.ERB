		ENDIF
	;それ以外
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「……っ」
			PRINTFORMW %CALLNAME:TARGET%は膣内に叩きつけられる精液の衝撃に、目を白黒させながら堪えている
		ELSEIF RAND:2
			PRINTFORMW 「……素朴な疑問なんです、が……」
			PRINTFORMW 「割と解剖学的法則を超越してませんかね、あなたの射精、は……」
		ELSE
			PRINTFORMW 「おつかれさまです……ふふ」
			PRINTFORMW 「あ、いや、可愛らしいなと思ったら、つい……」
		ENDIF
	ENDIF
ELSEIF SHOOT("Ａ中出し")
	;初めて
	IF FIRST_EV("絶頂：Ａ中出し") == 0
		PRINTFORMW 
	;恋慕
	ELSEIF TALENT:恋慕
		PRINTFORMW 
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 
	;それ以外
	ELSE
		PRINTFORMW 
	ENDIF
;-------------------------------------------------
;キスで射精
;-------------------------------------------------
ELSEIF SHOOT("キス")
	PRINTFORMW 
;-------------------------------------------------
;顔面騎乗で射精
;-------------------------------------------------
ELSEIF SHOOT("顔面騎乗")
	PRINTFORMW 
