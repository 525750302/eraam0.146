;屈服刻印3くらいが一番好きです
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K9, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = わたし_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = あなた_あなた
	;相手が主人ではない
	ELSE
		LOCALS = あなた_あなた
	ENDIF
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
ENDSELECT
RETURNF LOCALS

