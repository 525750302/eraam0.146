;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K26, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
	CASE "一人称"
		LOCALS = 私_わたし
	CASE "二人称"
		;相手が主人
		IF PLAYER == MASTER
			LOCALS = 先輩_せんぱい
		;相手が主人ではない
		ELSE
			IF STRCOUNTS(CSTR:PLAYER:学年, "２年生","３年生","研究生")
				LOCALS = %CALLNAME:PLAYER%先輩_%CSTR:PLAYER:ふりがな%せんぱい
			ELSEIF STRCOUNTS(CSTR:PLAYER:学年, "教師","司書")
				LOCALS = 先生_せんせい
			ELSEIF CSTR:PLAYER:肩書 == "実母"
				LOCALS = お母さま_おかあさま
			ELSEIF CSTR:PLAYER:肩書 != "１年生" && !TALENT:小柄 && TALENT:PLAYER:小柄
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ELSE
				LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
			ENDIF
		ENDIF
	CASE "主人"
		LOCALS = 先輩_せんぱい
	CASE "助手"
		IF STRCOUNTS(CSTR:ASSI:学年, "２年生","３年生","研究生")
			LOCALS = %CALLNAME:ASSI%先輩_%CSTR:ASSI:ふりがな%せんぱい
		ELSEIF STRCOUNTS(CSTR:ASSI:学年, "教師","司書")
			LOCALS = 先生_せんせい
		ELSEIF CSTR:ASSI:肩書 == "実母"
			LOCALS = お母さま_おかあさま
		ELSEIF CSTR:ASSI:肩書 != "１年生" && !TALENT:小柄 && TALENT:ASSI:小柄
			IF TALENT:ASSI:オトコ
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF
		ELSE
			LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
		ENDIF
	CASE "ペニス"
		IF TALENT:淫乱
			IF RAND:4 ;大体こっち
				LOCALS = おチンポ_おちんぽ
			ELSE
				LOCALS = チンポ_ちんぽ
			ENDIF
		ELSE
			LOCALS = おちんちん_おちんちん
		ENDIF
ENDSELECT
RETURNF LOCALS

;-------------------------------------------------
;勧誘イベントのセリフや紹介文
;-------------------------------------------------
@EVENT_PROFILE_K26, ARGS

SELECTCASE ARGS
	CASE "勧誘"
		IF TIME != 1
			PRINTL
			PRINTW 遡ること数時間――
			PRINTL
		ENDIF
		PRINTFORML 「ここの部長さんですよね」
		PRINTFORML ずっと待っていたのだろうか、呼び止められたのはちょうど部室のドアに手をかけたところだった。
		PRINTFORMW 顔立ちは可愛らしいがどこかミステリアスな雰囲気の女子である。
		PRINTL
		PRINTFORML 「この部っていつも何してるんでしょうか。ひょっとして、女子を連れ込んでたり？」
		PRINTFORML 面倒な相手か。こちらがそう判断した途端、彼女は相好を崩した。
		PRINTFORM 「なんて、冗談です。大体の
		IF GETCHARA(1) > 0 ;死に分岐だが一応
			PRINTFORML ことは%CALLNAME:GETCHARA(1)%先輩に聞いてます」
		ELSE
			PRINTFORML 見当はついてます」
		ENDIF
		IF TALENT:MASTER:オトコ
			PRINTFORML 胡散臭さに眉をしかめるも制服\@TALENT:巨乳 ? 越しに主張する胸 # から伸びる太もも\@が眩しい。男の本能でどうしてもそちらに目がいってしまう。
			PRINTFORML 「%CNAME("主人")%、そんな品定めするような目で女の子を見ちゃだめですよー」
			PRINTFORMW 自分が男からどう見られているのか分かっているのだろう。そう言いながらも無防備に近づいてこちらを見上げてくる。
			PRINTL
		ENDIF
		PRINTFORML 「それで、%CNAME("主人")%さえよければ%CNAME("一人称")%達、仲良くなれると思うんですよね。どうでしょう、%CNAME("一人称")%も入部させてくれせんか？」
		PRINTFORMW そう言うと彼女は答えなど聞かずとも分かっているといった様子でクスクスと笑った。
	CASE "紹介文"
		STR:1 = 学園の内外で巻き起こる事件を密かに解決している（らしい）魔法少女
		STR:2 = 愛欲を魔法の源としており、魔力を消費するとその分性欲が増してしまうのが悩みだとか
	CASE "勧誘直後"
		PRINTL 
		PRINTFORML 「入れてくれるんですか、思い切ってお願いしてよかったです」
		PRINTFORMW 「いい部員になれるようがんばりますので、よろしくお願いしますね」
	CASE "出現条件"
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の"%CSTR:関係%"として、(昼)の部室に顔を出してくれます。
	CASE "自室描写"
		
ENDSELECT

