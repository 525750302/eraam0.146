;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K23, ARGS
#DIM LCOUNT
#DIM MEMO_DESIRE
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
#DIM ACTOR2
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"
		;恋慕
		IF TALENT:恋慕
			;違和感で目を覚ますあなた
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ボンジュール（Bonjour）/おはようございます")%_HH_　%CNAME("主人")%」_L"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%TEXTR(@"朝からムラムラしちゃったので……%CNAME("主人")%にたっぷり種付けしちゃいますね/可愛いお顔を見ていたら……我慢出来なくなってしまったので朝からいっぱい種付けしちゃいます")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("はぁ/んぅ")%_HH_　%TEXTR(@"%CNAME("二人称")%に中出ししちゃいます……受け止めてください、ねっ/もう出そうです……んっ、%CNAME("二人称")%の一番奥に、種付けしちゃいます")%_HH_」"
			ENDIF
		ENDIF
	CASE "朝クンニ"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅっ…じゅるるっ、ちゅっちゅっ/じゅるるっ…んっ_HH_　ちゅっ、ちゅう")%_HH_」_L"
			CALL PRINT_STR, @"「%TEXTR("おはようございます/お目覚めですか、おはようございます")%_HH_　%TEXTR("綺麗にしますから待っててくださいね/綺麗になるまでペロペロしてさしあげますからね")%_HH_」_W"
		ENDIF

	CASE "朝足コキ"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ボンジュール（Bonjour）/おはようございます")%_HH_　%CNAME("主人")%」_L"
			PRINTFORMW 「%TEXTR("お願いされた/頼まれていた")%通りに起こしに来ました。あの……%TEXTR("痛かったらいってくださいね/痛くないでしょうか？")%」
		ENDIF
	CASE "朝の新妻プレイ"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 「%TEXTR("おはようございます/ボンジュール（Bonjour）")%%TEXTR(@"朝ご飯ですよ/今日は%CNAME("一人称")%の手料理ですよ")%」
			PRINTFORML 「%TEXTR("きゃあっ/えっ、何です")%！？　%TEXTR(@"ご飯より先に%CNAME("一人称")%が食べたいんですか……もう/こらっ……してもいいですけど、怖いメイドさんが起きてくるまでですよ？/もう……いっぱい中に出してくれるって約束してくださいね/……本当はダメですからね、今回は特別ですよ")%」
		ENDIF

	CASE "朝パイズリ"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("はぁっ…んっ……/んぅ…んっ……")%　おはようございます、%CNAME("主人")%_HH_」_L"
			CALL PRINT_STR, @"「朝なのに起きてくださらないから……%TEXTR("ちょっと悪戯しちゃいました/ご奉仕して起こしてみました")%_HH_」_W"
		ENDIF
	CASE "朝騎乗位"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			IF RAND:2 == 0
				CALL PRINT_STRL, @"「%TEXTR("あっ、あんっ…ああっ、あっ/んっ、んんっ、んぅっ、んうっ")%_HH_」"
				CALL PRINT_STRW, @"「%TEXTR("おはようございます、/おはよう、ちゅっ_HH_　")%%CNAME("主人")%。%TEXTR("朝から元気いっぱいで、その……したくなってしまって……ごめんなさい/起こしに来たんですけど、起きてくださらないのに立派になっておられたので……はしたないですよね……")%」"
			ELSE
				CALL PRINT_STRL, @"「%TEXTR("ふふっ/うふふっ")%_HH_　%TEXTR("ボンジュール（Bonjour）/おはようございます")%。%CNAME("主人")%」"
				CALL PRINT_STRW, @"「%TEXTR("んっ、あっ/んっ、んっ、んんっ")%、%TEXTR(@"久しく乗馬の練習をしていないなって思って、%CNAME("主人")%に跨っちゃいました/乗馬は聖騎士の嗜みですから、%CNAME("主人")%も乗りこなしてみせますよ")%_HH_」"
			ENDIF
		ENDIF
	CASE "朝Ａ騎乗位"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STRL, @"「%TEXTR("ボンジュール（Bonjour）/おはようございます")%_HH_　%CNAME("主人")%」"
			IF RAND:2 == 0
				CALL PRINT_STRW, @"「%CNAME("一人称")%の……%TEXTR("お尻/アナル/不浄の穴")%が%TEXTR("昨日/昨晩/夜")%から%TEXTR("落ち着かなくって/ウズウズしちゃって")%、%TEXTR(@"自分でも慰めたんですけど……ダメだったんです/どうしても%CNAME("主人")%のが欲しくなっちゃいまいた")%……%TEXTR("あっ/んっ")%_HH_」"
			ELSE
				CALL PRINT_STRW, @"「%TEXTR("ふふっ/うふふっ")%_HH_　%TEXTR("今朝/今日")%の%CNAME("一人称")%の%TEXTR("お尻/アナル/不浄の穴")%の%TEXTR("具合/締めつけ/心地よさ")%はどうですか？　%TEXTR("昨晩/昨日の夜")%から%TEXTR(@"ずっと%CNAME("二人称")%サイズのディルドで拡張してきたんです。こんな悪い子にはお仕置きが必要ですよね？/入れていたプラグを抜いたばかりですから、%CNAME("一人称")%も疼いちゃって……ごめんなさい。はしたないですよね")%」"
			ENDIF
		ENDIF
	CASE "朝キス"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			CALL PRINT_STR, @"「%TEXTR("ちゅ/ちゅっ/ちゅぅ")%_HH_」_L"
			IF RAND:2 == 0
				CALL PRINT_STR, @"「%TEXTR(@"元気が出るキスですよ。今日一日頑張ってくださいね/おはようございます、%CNAME("主人")%/いい朝ですね、今日も一日頑張りましょう")%」_W"
			ENDIF
		ENDIF
	CASE "朝添い寝"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("ボンジュール（Bonjour）/おはようございます")%……%TEXTR(@"起こしに来たのですが寝顔を見ていたら%CNAME("一人称")%も眠たくなってしまいました/起こしにきたつもりが一緒に寝ちゃってましたね")%
			PRINTFORMW 「%TEXTR(@"あの……もう少しだけこうしていてもいいですか？　/あと少しだけ……このままでいさせてください。")%%CNAME("主人")%」
		ENDIF
	CASE "朝シックスナイン"
		;恋慕
		IF TALENT:恋慕
			PRINTFORML 
			PRINTFORML 「%TEXTR("んっ、れろっ、れろ……/んっ……んっ、ちゅっ、ちゅぅ……")%　おはようございます」
			PRINTFORMW 「%TEXTR(@"朝のご奉仕をしていたら%CNAME("一人称")%も濡れてきちゃいました……折角ですから互いにご奉仕しませんか？/中々起きてくれないのでご奉仕させていただきました……あっ、%CNAME("一人称")%のクリちゃん、ダメェ。感じちゃいます")%」
		ENDIF
	CASE "朝フェラ"
		;複数プレイ　これで分岐取れるはず？？？
		;複数キャラいると朝フェラを狙って起こせない。取り合えず書いてコメントアウト処理しておく
		;一緒にプレイしている相手の名前は%CNAME(CALLNAME:PARTNER() )%で呼べるのか？　%CNAME("主人")%形式じゃないとダメなのか、解らん……
		IF PARTNER()
			IF RAND:3 == 0
				PRINTFORML 
				PRINTFORML 「%TEXTR("ボンジュール（Bonjour）/おはようございます")%……%TEXTR("んっ、ちゅ、/ちゅっ、ちゅっ、/れろっ、れろっ、")%%CNAME("主人")%……」
				CALL PRINT_STRW, @"「今日は%CNAME("パートナー")%と%TEXTR("二人で/一緒に")%、%TEXTR("朝のフェラチオ奉仕をさせて/たっぷりご奉仕をさせて/元気になるまでたっぷりご奉仕させて")%……%TEXTR("ちゅっ、ちゅぅっ/んっ、んぅっ/れろれろっ、れろっ")%_HH_　%TEXTR("いただきます/もらいます")%_HH_」"
			ELSEIF RAND:2 == 0
				PRINTFORML 
				PRINTFORML 「%TEXTR("ボンジュール（Bonjour）/おはようございます")%。%CNAME("主人")%」
				CALL PRINT_STRL, @"「%TEXTR("起こしに来たら/おはようの挨拶のタイミングで")%丁度%CNAME("パートナー")%%TEXTR("も来てたので/と鉢合わせたので")%……%TEXTR("ちゅっ、ちゅう/じゅるっ、じゅるる")%_HH_」"
				CALL PRINT_STRW, @"「%TEXTR("今日は/今朝は")%%TEXTR("二人で/一緒に")%%TEXTR("ご奉仕させていただきますね/朝のご奉仕をさせてくださいね")%_HH_」"
			ELSE
				PRINTFORML 
				PRINTFORML 「%TEXTR("ボンジュール（Bonjour）/おはようございます")%。%CNAME("主人")%」
				CALL PRINT_STRW, @"「%CNAME("一人称")%と%CNAME("パートナー")%で、%TEXTR(@"どっちの方がご奉仕が上手か/どちらが先に%CNAME("主人")%をイかせられるか")%……%TEXTR("ちゅっ、ちゅっ/んっ、んぅっ/れろっ、れろれろ")%_HH_　%TEXTR("勝負しようって事になりました/勝負ですよ")%_HH_」"
			ENDIF
		ELSE
			;恋慕
			IF TALENT:恋慕
				PRINTFORML 
				PRINTFORML 「%TEXTR("んっ、ちゅっ……/んぅ……んっ、ちゅぅ……")%　おはようございます。%CNAME("主人")%」
				IF RAND:2 == 0
					CALL PRINT_STRW, @"「%CNAME("主人")%を起こしに来たんですが、ぐっすり寝ておいでだったので……%TEXTR("悪戯しちゃいました/ご奉仕しちゃいました")%_HH_」"
				ELSE
					CALL PRINT_STRW, @"「ちゃんと起きない悪い子は、んっ_HH_……%TEXTR("じゅるるっ、じゅぽっじゅぽっ/ジュルジュルっ、ジュルルっ、グポッグポッ")%、%TEXTR("もっとお仕置きしちゃいますよ/おしゃぶりの刑にしちゃいますよ")%？_HH_」"
				ENDIF
			ENDIF
		ENDIF
	CASE "Ｗ朝フェラ"
		;PARTNERはCSTR:朝駆け = 朝フェラ、TARGETはCSTR:朝駆け = Ｗ朝フェラ となっているため　Ｗ朝フェラの時のみ喋る予定
		;淫乱
		IF TALENT:淫乱 && RAND:2 == 0
			PRINTFORML 
			PRINTFORML 「おはようございます、%CNAME("主人")%」
			CALL PRINT_STRL, @"「%TEXTR("んっ、ちゅ、/ちゅっ、ちゅっ、/れろっ、れろっ、")%%TEXTR(@"朝から欲しくなってしまった%CNAME("一人称")%達のお口にたっぷり朝の濃厚ミルクを種付けしてくださいね/ごめんなさい。%CNAME("一人称")%達はコレが欲しくってたまらなくなってしまいました")%_HH_……」"
			CALL PRINT_STRW, @"「%TEXTR("はぁ/んぅ")%_HH_……%TEXTR("朝のフェラチオ奉仕。毎日でもご命令してくださいね/匂いを嗅いでるだけで……興奮してきちゃいます/お口にいれてるだけで、濡れてきちゃいます")%……%TEXTR("ちゅっ、ちゅぅっ/んっ、んぅっ/れろれろっ、れろっ")%_HH_　%TEXTR(@"とっても素敵です/%CNAME("一人称")%達のご奉仕気持ちいいですか")%、%CNAME("主人")%_HH_」"
		;恋慕を想定
		ELSE
			PRINTFORML 
			PRINTFORML 「おはようございます……%TEXTR("んっ、ちゅ、/ちゅっ、ちゅっ、/れろっ、れろっ、")%%CNAME("主人")%……」
			CALL PRINT_STRW, @"「今日は%CNAME("パートナー")%と%TEXTR("二人で/一緒に")%、%TEXTR("朝のフェラチオ奉仕をさせて/たっぷりご奉仕をさせて/元気になるまでたっぷりご奉仕させて")%……%TEXTR("ちゅっ、ちゅぅっ/んっ、んぅっ/れろれろっ、れろっ")%_HH_　%TEXTR("いただきます/もらいます")%_HH_」"
		ENDIF
	ENDSELECT
CASE "挨拶する"
	;2018/01/15　フラグを整理
	;朝は　調教コマンド側の台詞表示→経験処理表記→事後のココの台詞の順番で表記される
	SELECTCASE CSTR:朝駆け
		CASE "朝キス"
			PRINTFORML 「おはようございます、%CNAME("主人")%」
			IF TEXTS("曜日") == "日"
				PRINTFORML 「今日は%TEXTR("予定とか、ありますか？/お暇でしょうか？")%　%TEXTR("無いのならこのまま一緒にゴロゴロしませんか？/無いのでしたら一緒にお出かけしませんか？")%」
			ELSE
				CALL PRINT_STR, @"「%TEXTR(@"今日も元気に頑張りましょうね　ちゅっ/今日も%CNAME("二人称")%の元気な顔が見れて%CNAME("一人称")%は幸せです")%_HH_」_W"
			ENDIF
		CASE "朝の母乳搾り"
			CALL PRINT_STR, @"「%TEXTR("慣れた手つきになってきましたね/実は……毎日自分で処理しているので助かっちゃいます")%_HH_」_W"
		CASE "朝足コキ"
			PRINTFORMW 「%TEXTR(@"正直、良さが良くわからないのですが%CNAME("主人")%が喜んでくださるならまたしてあげますね/手加減しない方が嬉しいと言われても痛くするのは不本意ですからね")%」
		CASE "朝クンニ"
			CALL PRINT_STR, @"「%TEXTR("んっ/んぅ")%……%TEXTR("じゅるるっ/じゅるっ、ピチャピチャピチャ")%_HH_」_W"
			CALL PRINT_STR, @"「%TEXTR("おはようございます/ボンジュール（Bonjour）")%%CNAME("主人")%……%TEXTR("今、綺麗にしますから待ってくださいね/綺麗にしようと舐めてるのにドンドン溢れてきちゃいますね")%_HH_」_W"
		CASE "朝逆正常位"
			PRINTFORMW 「%CNAME("主人")%……%TEXTR("ごめんなさい/反省しています/怒っておられますか")%」
		CASE "朝騎乗位"
			CALL PRINT_STR, @"「%TEXTR("ちゅっ/ちゅうっ")%_HH_　%TEXTR("おはようございます/ボンジュール（Bonjour）")%」_L"
			IF RAND:2 == 0
				CALL PRINT_STR, @"「%TEXTR("恋人はこうして起こしてあげると喜ぶって聞きました/好きな方へのサプライズになると聞いて、襲っちゃいました")%_HH_」_W"
			ELSE
				CALL PRINT_STR, @"「%TEXTR("学校ではあんまりイチャイチャ出来ない分、家ではたっぷり楽しみましょうね/お家で楽しんだ分、学校ではちゃんと我慢してくださいね")%_HH_」_W"
			ENDIF
		CASE "朝Ａ騎乗位"
			PRINTFORM 「%TEXTR("ごめんなさい/すみません")%……
			IF FINDCHARA(NO, 1) > 0 && RAND:2 == 0
				PRINTFORM フェリシエルに頼まれて
			ENDIF
			PRINTFORML %TEXTR(@"%CNAME("主人")%/%CNAME("二人称")%")%を起こしに来たのですが……」
			CALL PRINT_STR, @"「……%TEXTR("苦しそうなぐらいパンパンに大きくなっていたのが目に入ったので/手では収まってくださらなかったので")%%TEXTR("楽にしてさしあげたいなって、/ご奉仕しなくてはと思い……")%%TEXTR("んっ/ああんっ")%_HH_」_L"
			PRINTFORML 「%TEXTR("思わず……跨っちゃいました/イケナイとは思ったのですが……不浄の穴でご奉仕させていただきました")%……」
			IF TALENT:恋慕 && RAND:2 == 0
				PRINTFORML %TEXTR(@"自分勝手な事をしてごめんなさい。%CNAME("一人称")%の事を嫌いにならないでくださいね/自分でこんな事しちゃう女の子なんて……ちょっと嫌ですよね/%CNAME("一人称")%は聖職者なのにこんなはしたない事をしちゃうなんて……反省しないと")%……」
			ENDIF
		CASE "朝の新妻プレイ"
			PRINTFORML 「%TEXTR(@"朝からたっぷりご馳走されちゃいました/朝から激しくするなんて……もう/%CNAME("一人称")%が可愛いからなんて言っても駄目ですよ")%……」
			PRINTFORMW 「%TEXTR("じゃあ、朝ご飯は一緒にシャワーを浴びてからにしましょう/ご飯の前にシャワーにしましょう。汗かいちゃいましたからね")%」
		CASE "朝添い寝"
			IF TALENT:恋慕 && RAND:2 == 0
				PRINTFORML 「%TEXTR("おはようございます/ボンジュール（Bonjour）")%……実は%TEXTR("怖い夢を見てしまって一人で眠るのが怖くってお邪魔しちゃいました/お部屋を間違えてしまいました")%。%TEXTR("ごめんなさい/迷惑でしたよね")%」
			ELSE
				PRINTFORML 「%TEXTR("いけないっ、起こしに来たのに一緒に寝ちゃってました/すみません、抱き心地が良くって……起こしに来たのに一緒に寝ちゃってました")%」
			ENDIF
		CASE "朝フェラ", "朝パイズリ", "朝シックスナイン", "Ｗ朝フェラ"
			IF TALENT:恋慕
				;包茎
				;IF CHECK_STAIN(STAIN:MASTER:Ｐ, "Ｐ恥垢") && TALENT:MASTER:包茎
				IF TALENT:MASTER:包茎
					IF COND("夏") && RAND:2 == 0
						PRINTFORML 「%TEXTR("くんくん/すんすん")%……」
						PRINTFORML 「%TEXTR(@"まだ、ちょっと匂っちゃってますね。汗と恥垢の匂いが混ざり合って……んっ、%CNAME("一人称")%は好きですよ、この匂い/ご奉仕しても……蒸れているから、汚れがたっぷり溜まっちゃってますね。続きは後で……全部綺麗になるまでたっぷりお口でご奉仕してあげますからね")%」
					ELSE
						;真性包茎
						IF COND("真性包茎", MASTER)
							CALL PRINT_STR, @"「%TEXTR("皮の下に隠れちゃってる亀頭ってちょっと可愛いですよね/おはようございます。すっかり元気になっちゃてるおちんちんの皮を剥いて恥垢をお掃除したらお仕舞いにしましょう")%_HH_」_L"
							IF RAND:2 == 0
								PRINTFORML 「%TEXTR("今日/今朝")%も%TEXTR("溜まってる恥垢/隠れちゃってる所の汚れ")%がちゃんと綺麗になるまで%TEXTR("ペロペロさせてくださいね/ご奉仕させてくださいね")%」
							ELSE
								PRINTFORML 「%TEXTR(@"綺麗にしてあげられると%CNAME("一人称")%も嬉しいんですからね/恥垢が溜まってきたら早めに%CNAME("一人称")%に言ってくださいね？")%」
							ENDIF
						;仮性包茎
						ELSE
							IF RAND:2 == 0
								PRINTFORML 「ふぅ……まだおち〇ちんの皮の下には汚れが溜まっちゃってますね……」
								CALL PRINT_STRW, @"「んっ、もう少し念入りに……全部綺麗にしてあげますから楽にしててくださいね_HH_」"
							ELSE
								PRINTFORML 「隠れてる所には汚れが溜まりやすいんですからね」
								PRINTFORML 「何時でも言ってくだされば……%CNAME("一人称")%が毎日綺麗にしてあげますから……」
								CALL PRINT_STRW, @"「これからは%CNAME("一人称")%のお口を使いたい時は、ご奉仕する様に命令してくださいね_HH_」"
							ENDIF
						ENDIF
					ENDIF
				ELSEIF TEXTS("曜日") == "日"
					PRINTFORML 「しーっ、静かにしてくださいね。これ以上続けると…%TEXTR("家族の方に/怖いメイドさんに")%バレちゃいますから、お仕舞いです」
				ELSE
					PRINTFORML 「%TEXTR("時間がないので/残念ですが")%……%TEXTR("続き/本番")%をする時間はありません」
