;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K52, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = キミ_きみ
	;相手が主人ではない
	ELSE
		LOCALS = %CALLNAME:ASSI%さん_%CSTR:PLAYER:ふりがな%さん
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
CASE "マスター"
	IF NAME:MASTER == "あなた"
		LOCALS = キミ_きみ
	ELSEIF CONDS("本来の性別", MASTER) == "オトコ"
		LOCALS = %NAME:MASTER%くん_%NAME:MASTER%くん
	ELSE
		LOCALS = %NAME:MASTER%ちゃん_%NAME:MASTER%ちゃん
	ENDIF
CASE "調教時二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF NAME:MASTER == "あなた"
			IF TALENT:MASTER:オトコ
				LOCALS = キミ
			ELSE
				LOCALS = あなた
			ENDIF
		ELSE
			PRINTFORM %NAME:MASTER%
			IF TALENT:MASTER:オトコ
				LOCALS = くん
			ELSE
				LOCALS = ちゃん
			ENDIF
		ENDIF
	;相手が主人ではない
	ELSE
		LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
	ENDIF
ENDSELECT
RETURNF LOCALS


