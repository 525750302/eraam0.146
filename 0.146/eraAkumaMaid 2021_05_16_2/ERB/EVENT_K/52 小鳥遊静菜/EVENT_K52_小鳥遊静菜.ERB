;-------------------------------------------------
;コンドーム装着の地の文　ホムラ式コンドームの術改
;-------------------------------------------------
@INSERT_MESSAGE_K52, ARGS
SELECTCASE ARGS
	CASE "ゴム有り"
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%がコンドームを着けるのを見届けると、
	CASE "ゴム無し"
		PRINTFORML %CALLNAME:TARGET%は反り返るほどに勃起した%CALLNAME:MASTER%のペニスを一瞥すると、
	CASE "強行"
		PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%
ENDSELECT
IF ARGS == "強行"
	SELECTCASE SELECTCOM
		CASE 60
			PRINTFORM を仰向けに押し倒すと、その%TEXTS("太股：描写")%足をＭ字に開かせ
		CASE 61
			PRINTFORM をうつ伏せに押し倒すと、その%TEXTS("お尻：視覚")%を鷲掴んで
		CASE 62
			PRINTFORM を押し倒すと、上から覆いかぶさって
		CASE 63
			PRINTFORM を強引に抱き寄せると、その%TEXTS("お尻：感触")%を持ち上げて
		CASE 64
			PRINTFORM を後ろから抱き上げると、その%TEXTS("お尻：感触")%を掴み上げて
	ENDSELECT
	PRINTFORMW 剛直の先端を秘裂にあてがった……
ELSE
	SELECTCASE SELECTCOM
		CASE 60
			PRINTFORMW 自ら足をＭ字に開き、%CALLNAME:PLAYER%を誘った……
		CASE 61
			PRINTFORMW 自ら四つん這いになって、%TEXTS("お尻：視覚")%をこちらへ差し出した……
		CASE 62
			PRINTFORMW ベッドに横たわって身を差し出した……
		CASE 63
			PRINTFORMW %CALLNAME:MASTER%の膝上に腰をおろした…
		CASE 64
			PRINTFORMW 柔らかな尻肉を%CALLNAME:MASTER%のペニスに押し付けながら、%CALLNAME:MASTER%にもたれ掛かった……
		CASE 65
			PRINTFORMW 横たわった%CALLNAME:MASTER%に四つん這いでにじり寄った……
	ENDSELECT
ENDIF

RETURN 1
