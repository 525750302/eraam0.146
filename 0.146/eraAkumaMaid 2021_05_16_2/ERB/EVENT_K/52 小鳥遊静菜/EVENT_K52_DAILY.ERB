	IF TALENT:恋慕 && RAND:2 == 0
		CALL PRINT_STRW, @"「%TEXTR("ふふっ/うふふっ")%_HH_……%TEXTR("もっとして/もっと続けて欲しいな")%……」"
	ELSE
		CALL PRINT_STRW, @"「%TEXTR("んっ/んぅ")%_HH_　他の人にバレないようにね」"
	ENDIF
;あなたが物陰フェラやキスやセクハラをしない紳士な場合はここに来ます
CASE "次は何処に行こうか？と話を振る"
	PRINTFORMW 「%TEXTR("そうね/何処が良いかしらね")%……」
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1
CASE "映画に集中する"
	;PRINTFORML 「映画に集中しなさい……」
	;PRINTFORMW %CALLNAME:TARGET%にこっそりと手をつねられた。
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

;文化祭イベント
;ここでは"デート相手ではなく出し物をしている人"が喋ります。
;本来のデート相手はACTORにキャラ番号が入ってます
CASE "バンド演奏", "演劇", "占いの館", "お化け屋敷", "お悩み相談室", "ぱふぱふ屋", "メイド喫茶", "執事喫茶", "レストラン"


;帰り際のセリフです。さりげなくエッチに誘うのもいいでしょう
;STR:Ｈする場所 で場所をラブホ以外にする事も出来ます。（例 STR:Ｈする場所 = 部室
CASE "帰り際"
	IF TALENT:恋慕
		PRINTFORML 「楽しかったぁ……」
		CALL PRINT_STRL, @"「まるで恋人同士みたいね、私達_HH_」"
		CALL PRINT_STRW, @"「夢の時間は何時か終わるんだろうけど……今だけは%CNAME("二人称")%の腕の中で甘えさせてちょうだいね_HH_」"
	ELSE
		CALL PRINT_STRL, @"「%TEXTR("ふふっ/うふふっ")%_HH_」"
		IF RAND:2 == 0
			PRINTFORML 「デートなんて学生時代にしたっきりだったから……」
			CALL PRINT_STRW, @"「少しはしゃいじゃったけど、これで終わりじゃないわよね？_HH_」"
		ELSE
			PRINTFORML 「ちゃんとしたデートなんて久しぶりでドキドキしちゃった……」
			CALL PRINT_STRW, @"「このまま帰っても良いけど……ねぇ、この後は時間はあるのかしら？_HH_」"
		ENDIF
	ENDIF
CASE "デート終了"
	;エッチをする場合
	IF TFLAG:ベッドイン
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「もう%CNAME("一人称")%も我慢出来なくなっちゃいそうなの……はぁ_HH_」"
			CALL PRINT_STRW, @"「%TEXTR(@"何もかも忘れちゃうぐらい……たっぷり愛してね/全部忘れて……%CNAME("二人称")%だけの女になりたいの/%CNAME("一人称")%の身体に教えてね……%CNAME("一人称")%は%CNAME("二人称")%無しじゃもうダメなんだって")%_HH_」"
		ELSE
			PRINTFORML 「恋人同士だもんね……」
			CALL PRINT_STRW, @"「%TEXTR("わかったわ/いいよ")%_HH_……%TEXTR("たっぷり愛し合いましょうね/二人の愛を確かめ合いましょうか")%_HH_」"
		ENDIF
	ELSE
		IF TALENT:恋慕
			CALL PRINT_STRL, @"「%TEXTR("今日はありがとうね/とっても楽しかったわ")%_HH_」"
			CALL PRINT_STRW, @"「%TEXTR(@"良かったら帰りに%CNAME("一人称")%の家に寄って行かない？/今日はあの人もいないんだけど……良かったら%CNAME("一人称")%の家に来ない？")%_HH_」"
		ELSE
			CALL PRINT_STRL, @"「%TEXTR("とっても楽しかったわ/すっごく楽しかったわ")%_HH_」"
			CALL PRINT_STRW, @"「%TEXTR("人妻なのに/結婚してるのに")%……%TEXTR(@"こんなにドキドキさせちゃうなんて、イケない子なんだから/あんまりドキドキさせちゃダメよ……%CNAME("一人称")%はすぐその気になっちゃうんだから")%_HH_」"
		ENDIF
	ENDIF
