;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K14, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = オレ_おれ
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:オトコ
			LOCALS = お前_おまえ
		ELSE
			LOCALS = あんた_あんた
		ENDIF
	
	;相手が成美
	ELSEIF PLAYER:COND("成美", PLAYER)
		LOCALS = 委員長_いんちょう
		
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "１年生", "初等部", "中等部"
			IF TALENT:ASSI:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
		CASE "３年生", "研究生"
			LOCALS = 先輩_せんぱい
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 先生_せんせい
			ELSE
				LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF

CASE "主人"
	LOCALS = ゴシュジン様_ごしゅじんさま

CASE "助手"
	IF ASSI:COND("成美", ASSI)
		LOCALS = 委員長_いんちょう
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "１年生", "初等部", "中等部"
			IF TALENT:ASSI:オトコ
				LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
		CASE "３年生"
			LOCALS = 先輩_せんぱい
		CASEELSE
			IF COND("教員", ASSI)
				LOCALS = 先生_せんせい
			ELSE
				LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

;-------------------------------------------------
;勧誘イベントのセリフや紹介文
;-------------------------------------------------
@EVENT_PROFILE_K14, ARGS

SELECTCASE ARGS
CASE "紹介文"
	STR:1 = どうしても処女の血が吸いたかった変態吸血鬼に○ッドナイトブリス的な事をされた。
	STR:2 = しかも素質があったのか吸血鬼に変質してしまい、男にも戻れなくなった。

CASE "勧誘"
	PRINTFORML - %NAME:TARGET% -
	PRINTL
	PRINTFORML 「……%CNAMET(FINDCHARA(NO, 1), "二人称")%と性交すれば元に戻れるかもしれません」
	PRINTFORMW そう%CALLNAME:FINDCHARA(NO, 1)%が語ると目の前の少女が項垂れる。
	PRINTL 
