		PRINTFORMW 
	ENDIF
ENDSELECT


;-------------------------------------------------
;お買い物での台詞
;-------------------------------------------------
@KOJO_SHOP_K10, ARGS

SELECTCASE ARGS
CASE "一緒に出発"
	;初回
	IF SEVENT(ARGS) == 0
		CALL PRINT_STR, @"「あらあら、%CNAME("一人称")%と買い物に行きたいだなんて_HH_」_W"
		PRINTFORMW 「なぁに？　何か買って欲しいものでもあるのかしら？」
	;着替えてもらう
	ELSEIF CHECK_CLO("ハダカ") || CHECK_CLO("水着姿") || CHECK_CLO("体操服姿") || CHECK_CLO("下着姿")
		PRINTFORMW 「%CNAME("一人称")%、ちょっと身支度するから待ってね」
		PRINTFORMW 「誰かに会うかもしれないからお化粧はしっかりしていかなくちゃね」
	ELSEIF TALENT:恋慕
		PRINTFORMW 「%TEXTR("フフッ、一緒に手を繋いで恋人みたいね/じゃあ、手を繋いでデートしましょうか/こうしてデートするの新鮮ね")%」
		PRINTFORMW 「%TEXTR("お買い物ついでに面白そうな場所を探してみましょうか/お買い物だけじゃ寂しいから色々行きましょうね")%
	ELSE
		CALL PRINT_STR, @"「%TEXTR("可愛い彼女とデートしてきなさいよ/誘う子いないの？/他の子にマザコンだと思われちゃうわよ")%」_W"
		PRINTFORMW 「どうしてもって言うなら付き合ってあげるけれど……しょうがないわねぇ」
	ENDIF
CASE "お見送り"
	PRINTFORMW 「気をつけていってらっしゃい」
CASE "パンツ没収して出発"
	;初回
	IF SEVENT(ARGS) == 0
		CALL PRINT_STR, @"「フフッ_HH_……これもプレイの一環かしら？」_W"
		PRINTFORMW %CALLNAME:TARGET%は穿いていた%CLOCOLOR(CSTR:パンツ)%%PANTIES(TARGET, "種類")%を%CALLNAME:MASTER%に手渡した
	;既にノーパン
	ELSEIF EQUIP:パンツ == 0
		CALL PRINT_STR, @"「残念ね……今は穿いてないの」_W"
		CALL PRINT_STR, @"「次は%CNAME("二人称")%好みのパンツを穿いておくからね_HH_」_W"
	ELSE
		CALL PRINT_STR, @"「%TEXTR("命令ならしょうがないわね/ご主人様の命令だものね/命じられたら逆らえないものね")%」_W"
		PRINTFORMW %CALLNAME:TARGET%は穿いていた%CLOCOLOR(CSTR:パンツ)%%PANTIES(TARGET, "種類")%を%CALLNAME:MASTER%に手渡した
	ENDIF
CASE "今の格好のまま出発"
	IF CHECK_CLO("水着姿")
		PRINTFORMW 「誰かに見られたら困るかしら……」
		PRINTFORMW 「まあ……大丈夫よね？」
	ELSEIF CHECK_CLO("体操服姿")
		PRINTFORMW 「誰かに見られたら困るかしら……」
		PRINTFORMW 「まあ……大丈夫よね？」
	ELSEIF CHECK_CLO("下着姿") || CHECK_CLO("ハダカ")
		IF SEVENT("ＳＨＯＰ露出") == 0
			PRINTFORMW 「流石に……%CNAME("二人称")%のお父さんともこんなプレイした事ないのよ？」
			CALL PRINT_STR, "「ウフフ_HH_……ばれたらどうなっちゃうのかしら……」_W"
			PRINTFORMW %CALLNAME:TARGET%は上着のコートを羽織るとハイヒールを履いて%CALLNAME:MASTER%に従った
			CALL SET_SEVENT, "ＳＨＯＰ露出"
		ELSE
			CALL PRINT_STR, @"「%CNAME("一人称")%ったら……%TEXTR("前回、命令されてドキドキしてきちゃったのが忘れられないの/恥ずかしいのに逆らえないの/イケナイ事なのに興奮してきちゃったわ")%_HH_」_W"
		ENDIF
	ENDIF
CASE "会計交代"
	;初回
	IF SEVENT(ARGS) == 0 || SEVENT("商品購入") == 0
		PRINTFORMW 「なぁに　恥ずかしいの？」
		PRINTFORMW 「%CNAME("一人称")%が代わりに行ってきてあげるわ」
	ELSE
		CALL PRINT_STR, @"「%TEXTR("お財布を忘れたの？/お金が足りないの？/お小遣い足りないのね？")%」_W"
		PRINTFORMW 「ココは%CNAME("一人称")%が払ってあげるから、後で返しなさいね」
	ENDIF
CASE "商品購入"
	;今買った物
	SELECTCASE STR:購入アイテム
	;ギリギリ通常商品の範疇orあなた用
	CASE "撮影機材", "ローション", "媚薬", "オナホ"
		PRINTFORMW 「すいません、これとこれをお願いね」
		CALL PRINT_STR, @"「%TEXTR("この辺もオススメなの？/あら……これもお買い得ねぇ")%」_W"
	CASE "コンドーム"
		;この商品を初めて買わされた
		IF SEVENT(STR:購入アイテム) == 0
			PRINTFORMW 「コンドームくださいな」
			PRINTFORMW 「その1ダースセットのお買い得品でお願いしますね」
		ELSE
			PRINTFORMW 「フフッ……」
			PRINTFORMW 「最近のコンドームは色んな種類があるのね、どれがいいかしら？」
		ENDIF
	;えっちな商品
	CASEELSE
		PRINTFORMW 「この辺はやっぱり売れ筋なのかしら？」
		PRINTFORMW 「そう…じゃあ、折角だからソレも貰おうかしら」
	ENDSELECT
CASE "店内待機"
	;とりあえず何か一個買った時or33％で喋る
	SIF COND("買上品数") != 1 && RAND:3
		RETURN 0
	;パンツ没収中
	IF COND("買い物パンツ没収中")
		CALL PRINT_STR, @"「%TEXTR("手を離さないでね？/一人にしちゃいやよ……/何だか見られてる気がするんだけど……")%」_W"
		PRINTFORMW %CALLNAME:TARGET%は不安げな視線を%CALLNAME:MASTER%に向けながら、手を繋いだまま買い物に付き合っている
	ELSE
		PRINTFORML %TEXTR("「お買い得品ね」/「セール品みたいよ」/「こんなのもあるのね」/（……使った事があるわ）/（懐かしいわ……）")%
		PRINTFORMW %CALLNAME:TARGET%は店の商品%TEXTR("を一つづつ丁寧に見てまわっている/に興味を引かれ、手に取って見ている/を気に入ったようだ")%
	ENDIF
CASE "お買い物終了"
	;パンツ没収中
	IF COND("買い物パンツ没収中")
		CALL PRINT_STR, @"「%CNAME("一人称")%、%TEXTR("ばれたらどうしようか……心配してたのよ_HH_/はぁ……もう心臓が止まりそう")%」_W"
		PRINTFORMW パンツを返してもらった%CALLNAME:TARGET%は安堵の吐息を漏らして、その場にへたり込んだ
	;なにも買わなかった
	ELSEIF COND("買上品数") == 0
		IF ITEM:コンドーム < 5
			IF COND("合意：本番") || COND("合意：アナルセックス")
				PRINTFORML 「%TEXTR("生はダメよ　解るでしょう？/ちゃんとつけてね？　いい子だから/つけないなら、挿れさせてあげないわよ？")%」
			ELSE
				PRINTFORML 「%TEXTR("ねぇ、何か忘れてるでしょう？/コンドームをするのはエチケットよ/ちゃんとゴムを買っておいてあげたからね")%」
			ENDIF
			PRINTFORMW ＜母親からコンドームを手渡された＞
			ITEM:コンドーム += 5
		ELSE
			PRINTFORMW 「今日は何も買わなくていいの？」
		ENDIF

	ELSEIF CHECK_CLO("水着姿") || CHECK_CLO("体操服姿")
		PRINTFORMW 「ああ言うお店だから、何も言われなかったのかしら？」
		PRINTFORMW 「べ、別に……物足りなかったわけではないのよ」
	ENDIF
ENDSELECT

