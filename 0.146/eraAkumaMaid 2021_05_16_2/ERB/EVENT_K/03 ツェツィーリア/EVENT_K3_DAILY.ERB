			CALL PRINT_STR, "「それはともかく、今は目覚めのキスをするといい_H_」_W"
			CALL PRINT_STR, "「悪い魔法使いを退治する方法だよ_HH_」_W"
			TFLAG:地の文スキップ = 1
		ENDIF
	CASE "朝キス"
		;恋慕
		IF TALENT:恋慕 && RAND:2
			PRINTFORMW %CALLNAME:TARGET%は情事のときのように深く舌を絡めて%CALLNAME:MASTER%をしばらく蹂躙した

			CALL SIMULATE_SHOW_NEWDAY
			CALL AFTERTEXT_NEWDAY
			PRINTL 
			CALL EMOTION_PLUS, "甘い言葉", TARGET

			CALL PRINT_STR, "「おはよう_HH_」_W"
			CALL PRINT_STR, @"「ねえ、今日は%CNAME("一人称")%とずっとこうしていないかい？_H_」_W"
			PRINTFORMW 「お互いのこと以外をすべて忘れて、ただひたすら口づけしてお互いの体を味わって」
			PRINTFORMW 「…いや、実際そうするわけにはいかないが。半分くらいは本気でそうしたいと思っている」
			PRINTFORMW 「目覚めのキスで体のほうが舞い上がってしまうなんて、まったく我ながら%CNAME("二人称")%におかしくなってしまってるよ」
			TFLAG:地の文スキップ = 1
		ENDIF
	CASE "朝フェラ", "朝シックスナイン"
		;恋慕
