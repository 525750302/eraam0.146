@KOJO_DAILY_K3, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「%CNAME("二人称")%が%CNAME("一人称")%の旅路に加わりし者かね？」
CASE "汗だく"
	PRINTFORML 「%CNAME("一人称")%はあまり汗をかかない体質なのだが、ここまでだと流石に辛いな」
	PRINTFORML 「まったくあまり気持ちのいいものではないね」
