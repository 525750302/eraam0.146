	IF MARK:屈服刻印 >= 2 && RAND:2
		LOCALS = クリお豆_くりおまめ
	ELSEIF MARK:屈服刻印 >= 1
		LOCALS = クリ_くり
	ELSE
		LOCALS = クリトリス_くりとりす
	ENDIF
CASE "尿道"
	IF RAND:2
		LOCALS = おしっこの穴_おしっこのあな
	ELSE
		LOCALS = 尿道_にょうどう
	ENDIF
CASE "アナル"
	IF YOKUBO() && RAND:3 == 0
		LOCALS = お尻まんこ_おしりまんこ
	ELSEIF MARK:屈服刻印 >= 2 && RAND:2
		LOCALS = アナル_あなる
	ELSE
		LOCALS = お尻_おしり
	ENDIF
CASE "胸"
	IF YOKUBO() && RAND:2
		LOCALS = おっぱい_おっぱい
	ELSEIF MARK:屈服刻印 >= 1
		LOCALS = 乳首_ちくび
	ELSE
		LOCALS = 胸_むね
	ENDIF
CASE "精液"
	IF YOKUBO() && RAND:4 == 0
		LOCALS = おちんぽミルク_おちんぽみるく
	ELSEIF MARK:屈服刻印 >= 2 && RAND:3 == 0
		LOCALS = スペルマ_すぺるま
	ELSEIF MARK:屈服刻印 >= 1 && RAND:2
		LOCALS = ザーメン_ざーめん
	ELSE
		LOCALS = 精液_せいえき
	ENDIF
CASE "男性器"
	IF CEVENTS("男性器呼") == ""
		SELECTCASE MARK:屈服刻印
		CASE 3
			IF TALENT:淫乱 && RAND:2 && PALAM:欲情 >= PALAMLV:3
				LOCALS = マラ_まら
			ELSE
				LOCALS = おちんぽ_おちんぽ
			ENDIF
		CASE 2
			LOCALS = ちんぽ_ちんぽ
		CASE 1
			LOCALS = おちんちん_おちんちん
		CASEELSE
			LOCALS = 男性器_だんせいき
		ENDSELECT
	ELSE
		LOCALS = %CEVENTS("男性器呼")%
	ENDIF
CASE "女性器"
	IF CEVENTS("女性器呼") == ""
		IF MARK:屈服刻印 >= 2
			LOCALS = おまんこ_おまんこ
		ELSE
			LOCALS = 膣_ちつ
		ENDIF
	ELSE
		LOCALS = %CEVENTS("女性器呼")%
	ENDIF
CASEELSE
;	ARGSだけで判別はできないので、TaiTaを参考にFIND_CONDを使って番号を見つけ、それを使って性別などを確認する
;	わざわざ宣言するのも面倒なのでACTORに代入
;	フェリさんのをパクってあなたが通ってもなんとかなるよう改善
	ACTOR = FIND_NO(ARGS)

	LOCALS = %C_DETAIL(ACTOR)%
ENDSELECT
RETURNF LOCALS

