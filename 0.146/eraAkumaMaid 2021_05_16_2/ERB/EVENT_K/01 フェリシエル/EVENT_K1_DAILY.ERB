;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K1, ARGS
#DIM MEMO_LINECOUNT
;挨拶
#DIMS GREETING
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

IF COND("夜")
	GREETING = こんばんは
ELSEIF COND("昼")
	GREETING = こんにちは
ELSE
	GREETING = おはようございます
ENDIF

SELECTCASE ARGS
CASE "朝拘束"
	CALL PRINT_STRW, "「フフフ……_H_」"
	CALL PRINT_STRW, "「まだまだ夜までは間が有ります……たっぷりと、愉しみましょうね……_H_」"
CASE "朝駆け"

CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STRW, @"「%CNAME("二人称")%、おはようございます……_H_」"
		ELSE
			PRINTFORMW 「%CNAME("二人称")%、おはようございます」
		ENDIF
	CASEELSE
		PRINTFORMW 「%CNAME("二人称")%、おはようございます」
	ENDSELECT

	;何か発言してたらここまで
	;SIF LINECOUNT > MEMO_LINECOUNT
	;	RETURN 1
	IF COND("体育")
		IF RAND:2
			PRINTFORMW 「本日は体育が有る日ですよ。体操着とタオルは大丈夫でしょうか？」
		ELSE
			PRINTFORMW 「本日は体育が有る日ですね。女の子のお尻ばっか見てちゃだめですよ？」
		ENDIF
	ELSEIF RAND:2
		PRINTFORMW 「さてさて本日は何をしましょうかねぇ」
	ELSE
		PRINTFORMW 「お食事の準備が出来ていますよ……ささ、どうぞこちらへ」
	ENDIF
CASE "一番乗り"
	IF RAND:2
		PRINTFORMW 「ふむ。他の方は遅れているようですね」
	ELSE
		PRINTFORMW 「今日は%CNAME("一人称")%達の貸し切りみたいですよ」
		PRINTFORMW 「……なーんて、フフフ♪」
	ENDIF
	;部活の標語
	SELECTCASE SAVESTR:標語
	CASE "部活中は下着姿で"
		PRINTFORMW 標語を横目で確認した%CALLNAME:TARGET%は、衣擦れの音をさせつつ%CALLNAME:MASTER%の横で下着姿になった……
		CALL SETFLAG, "下着姿になる", TARGET
	CASEELSE
		IF COND("Ｈなサプライズ可能") && TALENT:巨乳 && RAND:3 == 0
			PRINTFORMW 「……先に始めちゃいますか？」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕を取り、%TOFULL(CONDS("バストカップ") )%カップの%TALENT_NAME(GETNUM(TALENT, "巨乳"), TALENT:巨乳)%で挟んできた……
		ENDIF
	ENDSELECT
CASE "ただいま"
	PRINTFORMW 「いま戻りましたー」
	PRINTFORMW 「%CNAME("一人称")%が居なくて寂しかったりしませんでしたか？フフ♪」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		MEMO_LINECOUNT = LINECOUNT
		IF COND("Ｈなサプライズ可能") && RAND:3 == 0
			IF TEXTS("曜日") == "土" && EQUIP:服 && ABL:露出癖 >= 3
				CALL SETFLAG, "下着姿になる", TARGET
				CALL EMOTION_PLUS, "交流中"
				PRINTFORMW 「%CNAME("二人称")%、%TEXTR("お疲れさま/今日も一日ご苦労さま")%でした」
				PRINTFORMW ……何故か下着姿で。
				PRINTFORMW 「今週も終わりですしね。頑張った%CNAME("二人称")%へのささやかなサービスってやつですよ」
				PRINTFORMW 「フフ♪」
			ELSEIF PENIS(MASTER) && CFLAG:MASTER:汗 && TALENT:においに弱い
				PRINTFORMW 「%CNAME("二人称")%、%TEXTR("お疲れさま/今日も一日ご苦労さま")%でした」
				PRINTFORMW 「今日は汗かきましたねぇ……ほら、凄い事になってますよ」
				PRINTFORMW %CALLNAME:TARGET%は自然な動作で股間の前に膝をつき、%CALLNAME:MASTER%のペニスを取り出した。
				PRINTFORMW 「フフフ、これこれ。このペニスを取り出し\@ COND("仮性包茎", MASTER) ? て皮を剥い # \@た瞬間の%TEXTR("匂い")%が好きなんですよ」
				PRINTFORMW 「……なんですか、変な顔して」
				PRINTFORMW 「%CNAME("二人称")%だって女性のアソコを押し開いた時の%TEXTR("匂い")%とか、少し嗅ぎたくなるんじゃないですか？」
				CALL PRINT_STR, "「…臭いだけですって？まったく、素直じゃないんですから。そんな人は……こうです_H_」_W"
				PRINTFORMW %CALLNAME:TARGET%はペニス\@ COND("仮性包茎", MASTER) ? の皮をグッと引き下ろし # の先端を舌先でほじり \@、口に咥えて\@ COND("体育") ? 昼の体育で # 一日の間に \@蓄積された汚れをお掃除してきた……
			ENDIF
		ENDIF

		;何か発言してたらここまで
		SIF LINECOUNT > MEMO_LINECOUNT
			RETURN 1

		PRINTFORMW 「%CNAME("二人称")%、%TEXTR("お疲れさま/今日も一日ご苦労さま")%でした」
		IF RAND:5 == 0
			PRINTFORMW 「今日も一日大変でしたね。頑張りすぎちゃ駄目ですよ？」
		ELSEIF RAND:4 == 0
			PRINTFORMW 「ご飯出来てますけど、先にお茶にしましょうか」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「ほらほら、そのまま寛ぐと皺が出来ちゃいますよ」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の後ろに回って上着を脱ぐのを手伝ってくれた
		ELSEIF RAND:2
			PRINTFORMW 「喉が渇いてませんか？お茶淹れてありますよ」
		ELSE
			PRINTFORMW 「お茶入ってますから、手洗いとうがいをなさって来てください」
		ENDIF
		CALL SETFLAG_DAILY, "お茶を用意", TARGET
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("パートナー")%と一緒でしたか。何か面白いお話とかありました？」
		ELSE
			IF RAND:3 == 0
				PRINTFORMW 「来るのが遅いから心配してましたよ……ほっとしました」
			ELSEIF RAND:2
				PRINTFORMW 「あら。重役出勤でしたね、%CNAME("二人称")%♪」
			ELSE
				PRINTFORMW 「%CNAME("二人称")%ったら、何処に寄り道してらしたんですか？」
			ENDIF
		ENDIF
	ENDIF

CASE "入室挨拶"
	IF COND("Ｈないちゃつき中", MASTER)
		IF RAND:2
			PRINTFORMW 「失礼します…………もしかして、本当に失礼しちゃいました？」
			PRINTFORMW 「ああいえ、どうぞどうぞ、%CNAME("一人称")%の事なぞ気にせずに」
		ELSE
			PRINTFORMW 「すいません、遅れました……って」
			PRINTFORMW 「なに、やってるんですか？%CNAME("二人称")%」
			PRINTFORMW 「ほーんと、えっちなんですから！」
		ENDIF
	;その他特定の行動に対する反応
	ELSE
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甘い言葉"
			CALL PRINT_STRW, "「失礼します……フフ_H_」"
			PRINTFORMW %CALLNAME:TARGET%は口説いてる最中の%CALLNAME:MASTER%に微笑んだ。
		ENDSELECT
	ENDIF

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		PRINTFORMW 「はいはい、%CNAME("二人称")%の忠実な僕、%NAME:TARGET%ですよ。何の御用でしょう」
		PRINTFORMW %CALLNAME:TARGET%はパタパタと足音を立てながら入室してきた
	ELSEIF CFLAG:MASTER:お茶 == 0
		PRINTFORMW 「すいません、遅れました……すぐお茶の用意をしますね」
		CALL SETFLAG_DAILY, "お茶を用意", TARGET
	ELSE
		PRINTFORMW 「おはようございまーす」
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 「はいはい、%CNAME("二人称")%の忠実な僕、%NAME:TARGET%ですよ。何の御用でしょう」
		PRINTFORMW %CALLNAME:TARGET%はパタパタと足音を立てながら入室してきた
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;既に部員の３分の２が集合済
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*2/3 && RAND:3 == 0
		PRINTFORMW 「すいません、遅れましたー」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「はいはい、%CNAME("二人称")%の忠実な僕、%NAME:TARGET%ですよ。何の御用でしょう」
		PRINTFORMW %CALLNAME:TARGET%はパタパタと足音を立てながら入室してきた
	ELSE
		PRINTFORMW 「おはようございまーす」
	ENDIF
;簡易入室挨拶に対する返し。ここでは括弧に『』を使う事。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う
	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		ACTOR = FIND_COND("教員", "入室挨拶中")
		IF RAND:2
			PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%こんにちは。お先に失礼してます』
		ELSE
			PRINTFORMW 『あ、%CNAME(CALLNAME_ACTOR)%。お疲れ様です』
		ENDIF
	ELSEIF PLACE("自宅")

	ELSE
		IF RAND:2
			PRINTFORMW 『おはようございます、%CNAME(CALLNAME_ACTOR)%』
		ELSE
			PRINTFORMW 『はい、おはようございます』
		ENDIF
	ENDIF

CASE "抜け出された"
	IF RAND:2
		PRINTFORMW 「む〜〜……」
		PRINTFORMW 「いえ。なんでもございません」
		PRINTFORMW 少しだけ不満を表した%CALLNAME:TARGET%だったが、特に引き留めたりはしなかった
	ELSE
		PRINTFORMW 「あら。%CNAME("パートナー")%との用事がおありでしたか……後片付けは%CNAME("一人称")%がしておきますね」
		PRINTFORMW 「どうぞ楽しんできてください」
		PRINTFORMW %CALLNAME:TARGET%は少しだけ寂しそうに%CALLNAME:MASTER%を見送った
	ENDIF
CASE "お弁当"
	IF RAND:4 == 0
		PRINTFORMW 「一緒にお弁当食べるのって楽しいですね……あら？」
		PRINTFORMW 「ご飯粒ついてますよ、%CNAME("二人称")%」
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の%TEXTR("唇/頬")%に付いたご飯粒を手で摘み上げた。
	ELSEIF RAND:3 == 0
		PRINTFORMW 「こういうの良いですよね。なんか、普通の仲の良い友達みたいです」
		PRINTFORMW 「……ん？友達はお弁当作ったりまではしないですかね？どう思います？」
	ELSEIF RAND:2
		PRINTFORMW 「はい、%CNAME("二人称")%あ〜〜〜ん♪」
		PRINTFORML 「パクッ」
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%が口を開くと、箸をＵターンさせて自分の口に運んで食べてしまった！
		PRINTFORMW 「フフっ……フフフ、冗談ですよ、そんな拗ねた顔なさらないでください♪」
	ELSE
		PRINTFORMW 「どうですか？その%TEXTR("ほうれん草/小松菜/蕪の葉っぱ")%のおひたし」
		PRINTFORMW 「根元のじゃぎじゃぎした食感とか、我ながらとても良い出来栄えだと思うのですが」
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K1, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「只今より%CNAME("一人称")%達の共同生活が始まる訳ですね。フフ」
	PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%のベッドに腰かけて、猫のように目を細めた
CASE "汗だく"
	IF CHECK_CLO("体操服姿") && RAND:2
		IF RAND:2
			PRINTFORML 「%CNAME("二人称")%も一緒にどうですか？たまにすると良いものですよ」
			PRINTFORML 体操服姿の%CALLNAME:TARGET%は、ベッドの上で柔軟体操をしている
		ELSE
			PRINTFORML 「ん？体操着のままなのが気になりますか？」
			PRINTFORML 「いいじゃないですか。お好きでしょう？こういう格好」
			PRINTFORML 運動直後の%CALLNAME:TARGET%からは爽やかなかきたての汗の%TEXTS("匂い")%が漂っている……
		ENDIF
	ELSEIF RAND:2
		PRINTFORML 「う〜ん……ちょっと疲れちゃいました」
		PRINTFORML %CALLNAME:TARGET%は頭の後ろで腕を組み、瑞々しい腋を披露しながら身体を捻って筋を伸ばしている
	ELSE
		PRINTFORML 「ふぅ。汗かいちゃいました」
		PRINTFORML 「……もしかして臭いますか、%CNAME("一人称")%？」
		PRINTFORML %CALLNAME:TARGET%は肩口の%TEXTS("匂い")%をクンクンと嗅いだ
	ENDIF
CASE "寝起き"
	IF RAND:3 == 0 && CHECK_CLO("体操服姿")
		PRINTFORML 「いっちにっ、さんっ、しっ…………」
		PRINTFORML %CALLNAME:TARGET%は軽く体を動かして眠気を飛ばそうとしている
	ELSEIF RAND:2
		PRINTFORML 「…………」
		PRINTFORML %CALLNAME:TARGET%は、まだ眠そうだ……口数少なく動きが鈍い
	ELSE
		PRINTFORML 「う〜〜ん……結構、よく寝ましたね」
		PRINTFORML %CALLNAME:TARGET%は気持ちよさそうに伸びをしている
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K1
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「はぁー……%CNAME("一人称")%が、%CNAME("二人称")%と……」
	CALL PRINT_STR, "「フフっ_H_ 好きな人から愛してるって言われるのって……最高ですねぇ_H_」_L"
	PRINTFORML %CALLNAME:TARGET%は今まで見た中で一番幸せそうな表情で、ニコニコしながら身悶えしている……
CASE "契約成立"
	IF RAND:3 == 0
		PRINTFORML 「ふぅ。これで暫くはノルマを気にせずに暮らせそうです」
		PRINTFORML おどける%CALLNAME:TARGET%だが、ノルマなんて本当にあるのだろうか？
	ELSEIF RAND:2
		PRINTFORML 「しかしリザレクションとは凄いものですね……本当に何ともないのですか？」
		PRINTFORML 「常人なら徐々に衰弱が起きてもおかしくないんですけどねぇ」
	ELSE
		PRINTFORML 「良い取引をなさいましたね……フフ♪」
		PRINTFORML %CALLNAME:TARGET%は魂を集めることが出来て上機嫌な様子だ
	ENDIF
CASE "買い物する"
	;買ってきたアイテムなどで分岐
	IF COND("巨大バイブ購入") && DEVENT("巨大バイブ感想") == 0
		IF COND("買上品数") >= 3
			PRINTFORML 「色々と買ってきましたね」
			PRINTFORML 「これなんか、凄い大きいですけど……いつか入るようになるんでしょうか。どう思います？」
		ELSE
			PRINTFORML 「お疲れさまでした、%CNAME("二人称")%」
			PRINTFORML 「しかしこれ……凄い大きいですけど、いつか入るようになるんでしょうか。どう思います？」
		ENDIF
		PRINTFORML %CALLNAME:TARGET%は戦利品を開封して吟味している%CALLNAME:MASTER%に後ろから抱きつき、頬をくっつけてきた
		CALL SET_DEVENT, "巨大バイブ感想"
	ELSE
		SELECTCASE COND("買上品数")
		CASE IS >= 4
			PRINTFORML 「お買い物お疲れ様です、%CNAME("二人称")%」
			PRINTFORML 「それにしても……いろいろ買ってきましたねぇ」
		CASEELSE
			PRINTFORML 「ふぅ。一仕事終えた感じですね、%CNAME("二人称")%」
			PRINTFORML 「あとは%CNAME("一人称")%にお任せを」
			PRINTFORML %CALLNAME:TARGET%はテキパキと紙袋の中身を仕分けして綺麗に収納していった
		ENDSELECT
	ENDIF
CASE "抜け出す", "あなたの部屋に連れ込まれた", "自室に招待"
	IF RAND:2
		PRINTFORML 「ねぇ%CNAME("二人称")%、これから……なにをなさるんですか？」
		PRINTFORML 「先程の場所では出来なかった事をするって解釈で良いんですよね？」
		PRINTFORML 「……フフフ♪」
	ELSE
		IF RAND:3 == 0
			PRINTFORML 「フフ…一杯可愛がってくださいね？」
		ELSE
			PRINTFORML 「こういうの……ドキドキしますね♪」
		ENDIF
		IF PENIS(MASTER) && RAND:2
			IF RAND:2 == 0
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%のファスナーを口で器用に下ろし、飛び出してきた硬いものにキスをした
			ELSE
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の前に跪くと、腰を抱きしめるようにしてファスナーから飛び出す硬いものに口づけした。
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:TARGET%は、これから起こることを思い描いて頬を染めている
		ENDIF
	ENDIF
CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 「あら%CNAME("二人称")%に%CNAME(CSVCALLNAME(NO:ASSI) )%、いらっしゃいませ」
		PRINTFORML 「どうかなさいました？」
	ELSE
		PRINTFORML 「あら%CNAME("二人称")%。どうかなさいました？」
		CALL PRINT_STRL, @"「もしかして、%CNAME("一人称")%が居なくて寂しくなっちゃいましたか？フフ_H_」"
	ENDIF
CASE "アドバイザー部屋訪問"
	PRINTFORML 「%CNAME(CSVCALLNAME(NO:ASSI) )%、失礼しますね」
	PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%と%CALLNAME:ASSI%が腰を下ろしたのちに、二人の邪魔にならない場所に座った
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	;PRINTFORML 
CASE "オナバレ"
	PRINTFORML 「あ、あのですね%CNAME("二人称")%、さっきのはそういう事ではなく……いえ、えーと……うぅぅ」
	PRINTFORML %CALLNAME:TARGET%は自らの行いを恥じて小さくなってしまっている……
CASE "ミニスカパンチラ"
	;超ミニの時にはスルーしたり
	IF CHECK_SKIRT("超ミニ") && RAND:2
	ELSEIF RAND:2 && PENIS(MASTER)
		PRINTFORML 「%CNAME("二人称")%……見ましたね？」
		PRINTFORML 「とぼけたって駄目ですよ……だってほら、なんでそんなになってるんですか？」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の股間を指さしてくすくす笑っている
	ELSEIF CFLAG:甘い言葉 || CFLAG:褒める || CHECK_SKIRT("超ミニ")
		PRINTFORML 「どうしました？なにか良いものでも見えましたか？」
		CALL PRINT_STR, "「……フフ_H_」_L"
		PRINTFORML %CALLNAME:TARGET%は少しだけ頬を染め、楽しそうに微笑んでいる
	ELSE
		PRINTFORML 「…………？ ……！」
		PRINTFORML 「%CNAME("二人称")%、もしかして……見えちゃいました？」
		PRINTFORML %CALLNAME:MASTER%の視線に気づいた%CALLNAME:TARGET%は、頬を染めて遅まきながら手でガードをした
	ENDIF
CASE "スカートをめくる"
	IF RAND:2
		PRINTFORML 「もう……いけませんよ？女の子のスカートの中を無断で覗いたりしては」
	ELSE
		PRINTFORML 「%CNAME("二人称")%って、本当にパンツがお好きですよねぇ」
	ENDIF
	PRINTFORML 口では呆れを滲ませている%CALLNAME:TARGET%だが、表情は楽しそうだ……怒ってはいないらしい
