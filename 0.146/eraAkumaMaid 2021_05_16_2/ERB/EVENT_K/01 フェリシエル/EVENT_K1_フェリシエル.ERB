;-------------------------------------------------
;メイドの悪魔 口上
;台詞が少ないせいもありキャラが掴みづらいと思われるので、エロシーンなどのセリフが書かれるまでは加筆は待ってください
;-------------------------------------------------
;呼び名の設定。""の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K1, ARGS
#FUNCTIONS
#DIM LCOUNT
#DIM ACTOR

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	ACTOR = PLAYER
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
	ACTOR = ASSI
CASEELSE
	;ARGSにキャラ名が入れられてる場合にはキャラへの呼び方なのでチェック
		ACTOR = FIND_NO(ARGS)
ENDSELECT

SIF LOCALS != ""
	RETURNF LOCALS

;相手が主人
IF ACTOR == MASTER
	LOCALS = ご主人様_ごしゅじんさま
;相手が主人ではない
ELSE
	SELECTCASE CSTR:ACTOR:肩書
	CASE "実母"
		LOCALS = 奥様_おくさま
	CASE "魔貴族", "没落魔貴族"
		LOCALS = %CALLNAME:ACTOR%様_%CSTR:ACTOR:ふりがな%さま
	CASEELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部", "中等部", "１年生"
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ELSE
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ENDIF
		CASE "２年生"
			LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
		CASE "３年生"
			LOCALS = %CONDS("略称", ACTOR)%先輩_%CONDS("略称", ACTOR)%せんぱい
		CASEELSE
			IF COND("教員", ACTOR)
				LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDSELECT
ENDIF

RETURNF LOCALS

