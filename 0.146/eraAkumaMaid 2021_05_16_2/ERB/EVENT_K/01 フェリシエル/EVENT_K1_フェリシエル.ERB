	;挿入コマンド
	IF TRAIN_GENRE("挿入", SELECTCOM)
		;プレイヤーは着ける気満々
		IF TEQUIP:PLAYER:コンドーム
			;初めてのゴム有りセックス
			IF EXP:ゴム性交経験 == 0
				PRINTFORMW 「あら、用意が良いですね。見直しちゃいました」
			ELSE
				PRINTFORMW 「フフ、お心遣い感謝しますよ♪」
			ENDIF
			IF TALENT:PLAYER:童貞
				CALL PRINT_STRW, "「ですが今日は特別な日ですから、よければどうぞ何もつけずに……_H_」"
				IF COND("危険日")
					PRINTFORMW 「危険な日なので何度も求められたら困りますけど、少しでしたら……構いませんよ？」
				ELSE
					PRINTFORMW 「初めてなんですから……避妊具無しで、どんなものか確かめておきたいでしょう？」
				ENDIF
				CALL PRINT_SELECT, "それでも着ける/お言葉に甘えて生挿入！"
				IF RESULT == 0
					PRINTFORMW 「……思ったよりも誘惑に強いんですね」
					PRINTFORMW 「%CNAME("二人称")%の事、甘く見ていたかもしれません……」
				ELSE
					PRINTFORMW 「……フフ」
					PRINTFORMW 「%CNAME("二人称")%の初めて……どうぞ%CNAME("一人称")%で経験しちゃってくださいね♪」
					CALL SETFLAG, "ゴムキャンセル", PLAYER
					SIF COND("危険日")
						TCVAR:ワンモア生挿入中 = 1
				ENDIF
			ELSEIF COND("安全日")
				CALL PRINT_STRW, "「ですが今日は大丈夫な日ですから、よければどうぞ何もつけずに……_H_」"
				CALL PRINT_SELECT, "それでも着ける/お言葉に甘えて生挿入！"
				IF RESULT == 0
					PRINTFORMW 「あらら、断られちゃいました。%CNAME("一人称")%の話術も、まだまだですねぇ♪」
				ELSE
					PRINTFORMW 「……フフっ♪ここだけの話、生でするの好きなんですよねぇ、%CNAME("一人称")%」
					CALL SETFLAG, "ゴムキャンセル", PLAYER
				ENDIF
			ELSE
				PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%がコンドームを着けるのを見て微笑んだ。
			ENDIF
		;避妊処置をしなくて良い事がキャラに分かっている状態
		ELSEIF COND("避妊不要") || TCVAR:ご褒美生Ｈ
			SIF CHOSECOMS("Ｖ挿入") == 0
				PRINTFORMW 「フフ。楽しみましょうね♪」
		;ゴムが無い
		ELSEIF ITEM:コンドーム == 0
			;ゴム付けてやるはずだったけどゴムが切れてしまった！
			IF TCVAR:Ｖゴム挿入許可
				PRINTFORMW 「あら？もしかしてコンドーム無くなっちゃいましたか？」
			ELSE
				PRINTFORMW 「あら……？%CNAME("二人称")%、もしかしてコンドーム用意してないんですか？」
			ENDIF
			IF TALENT:PLAYER:童貞
				PRINTFORMW 「ダメなお人ですねぇ本当に。最低限のエチケットですよ？」
				PRINTFORMW 「と言いたいところですが……フフ」
				CALL PRINT_STRW, "「今日は特別な日ですからねぇ。……いいですよ、このまま何も着けなくても……_H_」"
				IF COND("危険日")
					PRINTFORMW 「危険な日なので何度も求められたら困りますけど、少しでしたら……ね？」
				ELSE
					PRINTFORMW 「初めてなんですから……避妊具無しで、どんなものか確かめておきたいでしょう？」
				ENDIF
				SIF COND("危険日")
					TCVAR:ワンモア生挿入中 = 1
			ELSEIF COND("安全日") || COND("女性主導生セックス") || COND("合意：危険日生セックス")
				PRINTFORMW 「じゃあ……\@ CHOSECOMS("Ｖ挿入") ? 続き # 今日 \@は着けずにしましょうか♪」
				CALL PRINT_STRW, "「フフ_H_」"
			ELSEIF COND("危険日")
				PRINTFORMW 「う～～ん……仕方ないですけど、今日はここまでにしましょう」
				PRINTFORMW %CALLNAME:TARGET%は少し残念そうだ。
				TFLAG:コマンドキャンセル = 1
			ELSE
				PRINTFORMW 「……中に出さなければ、大丈夫だと思うんですよね」
				PRINTFORMW 「という事で、続けませんか？」
				PRINTFORMW 「でも、絶対に絶対に中に出したら駄目ですからね？怒りますよ？」
				CALL SET_CEVENT, "ゴム選択：中に出したら怒りますよ"
			ENDIF
		;安全日に生希望
		ELSEIF COND("安全日")
			SIF CHOSECOMS("Ｖ挿入") == 0
				PRINTFORMW 「フフ。今日は大丈夫な日ですし、楽しみましょうね♪」
		;安全日じゃないけど生ＯＫ
		ELSEIF COND("女性主導生セックス") || COND("合意：危険日生セックス")
			PRINTFORMW 「あら？よろしいんですか、避妊なさらなくても」
			CALL PRINT_STRW, @"「%CNAME("一人称")%ですか？%CNAME("一人称")%は……フフ_H_」"
