;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K30, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝キス"
		PRINTFORML 「ん、ちゅっ…%TEXTR("おはよう/む、起きたか")%、%CNAME("二人称")%」
		IF TEXTS("曜日") == "日"
			PRINTFORMW 「今日は休日だが%TEXTR("何かする予定はあるか？/どうやって過ごす？")%%TEXTR(@"%CNAME("一人称")%は少し出かけるが…何ならついてきても良いのだぞ？/何も無いならむやみに出かけず家で過ごすのも良いだろうな/%CNAME("一人称")%はゆっくり過ごさせてもらうぞ")%」
		ELSE
			PRINTFORMW 「%TEXTR(@"今日も一日張り切っていこうではないか/目覚めのキスはどうだ、%CNAME("二人称")%よ/キスだけで半勃ちか…朝から元気そうで何よりだな")%」
		ENDIF
	CASE "朝フェラ", "朝シックスナイン"
		PRINTFORMW 「んっ…んぐ…ん…ちゅぱっ、ちゅぽっ………む、ようやく起きたか、%CNAME("二人称")%」
		PRINTFORMW 「…残念だが朝はこれだけだぞ？後でたっぷり相手してやる」
	CASE "朝騎乗位"
		CALL PRINT_STRW, "「んぁっ、良い、良いぞっ…くっ、ああぁッ_H_」"
		CALL PRINT_STRW, @"「はっ、はぁ…おはよう、%CNAME("二人称")%よ。ククッ、朝一の騎乗位は効くな…ぁンッ_H_」"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		PRINTFORMW 「今日は体育があるようだな。ほれ、体育着だ」
	ELSE
		IF RAND:2
			PRINTFORMW 「起きろ、この寝坊助め」
			PRINTFORMW 「まだ朝食を取る時間だけはあるぞ。早いところ食べて準備しろ」
		ELSE
			PRINTFORMW 「起きたか、%CNAME("二人称")%よ」
			IF FINDCHARA(NO, 1) > 0
				PRINTFORMW 「もう%CALLNAME:FINDCHARA(NO, 1)%が朝食を作って待っているぞ」
			ENDIF
		ENDIF
	ENDIF
CASE "一番乗り"
	PRINTFORMW 「邪魔するぞ…って何だ、誰もいないのか」
CASE "ただいま"
	PRINTFORMW 「帰ったぞ、%CNAME("二人称")%」
	PRINTFORMW 「%TEXTR(@"隅で見ていたが、今日も一日頑張っていたな。どれ、頭を撫でてやろう/今日の夕食は何だ？腹が減ってしょうがない/今、風呂は誰も入ってないか…%CNAME("一人称")%が先に入っても良いか？")%」
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		PRINTFORMW 「帰ったか、%CNAME("二人称")%よ」
		PRINTFORMW 「%TEXTR("隅で見ていたが、今日も一日頑張っていたな。どれ、頭を撫でてやろう/風呂が沸いているぞ。先に入って来てしまえ/疲れた時には甘い物に限る。ほれ、アメをやろう")%」
	ELSE
		PRINTFORMW 「来たか、%CNAME("二人称")%よ」
		PRINTFORMW 「%TEXTR(@"授業中に居眠りするのは感心せんな。%CNAME("一人称")%が隠れて起こしてなければどうなっていたか…/今日はしっかり勉学に励んでいたな。午後もその調子で頑張るのだぞ/悪いが先に寛がせてもらっていた。…茶でも飲むか？")%」
	ENDIF

CASE "入室挨拶"
	SELECTCASE CFLAG:MASTER:PREV_DAILYCATE
	;トーク
	CASE 1
		SELECTCASE CSTR:MASTER:PREV_DAILYCOM
		CASE "甘い言葉"
			PRINTFORMW 「…%TEXTR("ふむ/ほう")%、口説きの最中だったか」
		CASE "挨拶クンニ", "挨拶フェラ"
			PRINTFORMW 「…%TEXTR("ふむ/ほう")%、日も高いうちからお盛んだな、えぇ？」
		ENDSELECT
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%達をニヤニヤしながら興味深げに見ている。
	;もてなす
	CASE 2
	;隙を見せる
	CASE 3
	;パンツ
	CASE 4
		IF RAND:2
			PRINTFORMW 「…ああいや、%CNAME("一人称")%の事は気にせず続けるが良い」
			PRINTFORMW 「それとも少し席を外した方が良いか？」
		ELSE
			PRINTFORMW 「あーその、%CNAME("二人称")%よ…」
			PRINTFORMW 「…まぁ良いか。ここでやってるうちは誰も咎めんしな」
		ENDIF
	CASE 6
		IF RAND:2
			PRINTFORMW 「…ああいや、%CNAME("一人称")%の事は気にせず続けるが良い」
			IF TALENT:恋慕
				PRINTFORMW 「無論、後で%CNAME("一人称")%も相手してくれるのだろうな？」
			ELSE
				PRINTFORMW 「それとも少し席を外した方が良いか？」
			ENDIF
		ELSE
			PRINTFORMW 「あーその、%CNAME("二人称")%よ…」
			IF TALENT:恋慕
				PRINTFORMW 「%CNAME("一人称")%もその…そういう事してやっても良いのだぞ？」
			ELSE
				PRINTFORMW 「…まぁ良いか。ここでやってるうちは誰も咎めんしな」
			ENDIF
		ENDIF
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF CFLAG:呼び出された
		;IF COND("夜")
			
		;ELSE
			PRINTFORML 「%TEXTR("どうした/何だ/呼んだか/何用だ/只今馳せ参じたぞ")%、%CNAME("二人称")%」
			IF RAND:2
				PRINTFORML 「…%TEXTR("ククッ/全く/はぁ")%、そんなに驚かなくても良いだろうに…%CNAME("二人称")%は可愛いな」
				PRINTFORMW %TEXTR(@"死角から/%CALLNAME:MASTER%の後ろに/扉も窓も通らずに")%音も無く現れた%CALLNAME:TARGET%はクスクスと愉快そうに笑っている…。
			ELSE
				PRINTFORML 「%CNAME("一人称")%%TEXTR("の事が恋しくなったか？ククッ♪/に何か頼みでもあるのか？/の力が必要と見たぞ/はすぐに駆けつけるぞ、大事な契約者の危機かもしれないからな")%」
				PRINTFORMW 呼び出された%CALLNAME:TARGET%は得意げに微笑んでいる。
			ENDIF
		;ENDIF
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR("…出遅れたか/むぅ、既に皆揃っておるようだな")%」
	ELSE
		PRINTFORMW 「邪魔するぞ」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『…授業もこっちもで、教師は大変だな？』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『遅かったな。皆大体出揃っているぞ』
	ELSE
		PRINTFORMW 『%CNAME(CALLNAME_ACTOR)%か。先に寛がせてもらってるぞ』
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "お弁当"
	IF CEVENT("初めてのお弁当") == 0
		PRINTFORMW 「さて、約束通り弁当を作ってきたぞ…。%CNAME("二人称")%の口に合えば良いのだが」
		PRINTFORMW %CALLNAME:TARGET%が自信無さげに手渡してきた弁当は…フルーツサンドだった。
		PRINTFORMW 一口食べてみると、クリームのほど良い甘さと果物の酸味が絶妙で美味しかった…。
		PRINTFORMW 「…その反応から察するに、食べられる代物だったと言うことだな」
		PRINTFORMW 「誰かに自分の作ったものを美味しく食べてもらえる…中々嬉しいものだな」
		PRINTFORMW どこか照れくさそうにはにかむ%CALLNAME:TARGET%をおかずに、弁当を食べきった…。
		CALL SET_CEVENT, "初めてのお弁当", TARGET
	ELSE
		IF RAND:3 == 0
			PRINTFORMW 「今日の弁当は%TEXTR("アップルパイ/イチジクのタルト/バタースコッチシナモンパイ")%だ」
			PRINTFORMW 「…デザートじゃないかと思ったろう？すまない、急に食べたくなってしまったのだ…」
		ELSEIF RAND:2
			PRINTFORMW 「今日の弁当は%TEXTR("カレー風味の/チーズ入りチキン/海鮮ピラフ風")%ライスコロッケだ」
			PRINTFORMW 「父が人間界で一番美味い食べ物だともてはやして、一時期こればかり料理人に作らせていてな…」
		ELSE
			PRINTFORMW 「今日の弁当は%TEXTR("カツサンド/フルーツサンド/タマゴサンド/ＢＬＴサンド")%だぞ」
			PRINTFORMW そう言って%CALLNAME:TARGET%は弁当箱を３つほど差し出してきた…。
			PRINTFORMW 「…張り切って作り過ぎてしまってな。%CNAME("一人称")%は間食もコレになりそうだ」
		ENDIF
	ENDIF
ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K30, ARGS
#DIM MEMO_LINECOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT


SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「悪魔の%CALLNAME:TARGET%だ。よろしく頼む」
	PRINTFORML %CALLNAME:TARGET%は部員達に握手して回っている。
CASE "汗だく"
	IF RAND:2
		PRINTFORML 「ふー、暑いな…。服が汗で重たくなってきてるぞ」
		PRINTFORML %CALLNAME:TARGET%はハンカチでしきりに汗を拭きつつ、手うちわで胸元を扇いでいる。
	ELSE
		PRINTFORML 「%CNAME("一人称")%はあまり汗をかかない体質だったのだがな…」
		PRINTFORML ぐったりとする%CALLNAME:TARGET%は力なく汗をぬぐっている…。
	ENDIF
CASE "寝起き"
	IF CHOSE_DEVENT("睡眠姦キス")
		PRINTFORML 「%TEXTR(@"ふむ…王子様のキス…か/ん…思ったよりも良い目覚めだな/%CNAME("二人称")%よ。唇に%CNAME("一人称")%のルージュが付いてるのはどういう訳だ？ククッ♪")%」
	ELSEIF CHOSE_DEVENT("睡眠姦フェラ")
		IF RAND:2
			PRINTFORML 「んっ、ぅ…ッ、濃いのを飲ませおってからに…ケホッ…」
			IF ABL:精液中毒
				PRINTFORML 文句を垂れる%CALLNAME:TARGET%だが、その頬は緩み、舌は精液を味わうように蠢いている…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%は顔を赤くしながら軽く咳き込んでいる…。
			ENDIF
		ELSE
			PRINTFORML 「…寝ている%CNAME("一人称")%の口は気持ち良かったか？」
			IF ABL:精液中毒
				PRINTFORML %CALLNAME:TARGET%は舌なめずりしながら熱の籠った目で%CALLNAME:MASTER%を見つめている…。
			ELSE
				PRINTFORML %CALLNAME:TARGET%はジトっとした目で%CALLNAME:MASTER%を見つめている…。
			ENDIF
		ENDIF
	ELSEIF CHOSE_DEVENT("睡眠姦Ｖ中出し")
		IF RAND:3 == 0 && ABL:信頼 >= 5 && CEVENT("両親の夢")
			PRINTFORML 「…%TEXTR(@"父が死んだ日の夜に/母の葬儀の後で")%%CNAME("二人称")%に抱かれる夢を見た」
			PRINTFORML 「夢の中の%CNAME("二人称")%はな…泣いている%CNAME("一人称")%に優しく声をかけながら、たっぷり精を注いでくれたぞ…？」
			PRINTFORML 残る涙を指で拭いながら、%CALLNAME:TARGET%は上気した顔で%CALLNAME:MASTER%に身を寄せてきた…。
		ELSEIF RAND:2
			PRINTFORML 「んぅ…誰かさんの精で胎が重いな…。%CNAME("二人称")%よ、何か知らぬか？」
			PRINTFORML %CALLNAME:TARGET%はわざとらしく下腹部を撫でながらそう聞いてきた…。
		ELSE
			PRINTFORML 「む、これは…下着が精液でベトベトではないか」
			PRINTFORML 「ククッ、眠っている%CNAME("一人称")%の膣はさぞ気持ち良かったのだろうなァ…なぁ%CNAME("二人称")%よ」
			PRINTFORML %CALLNAME:TARGET%は精液を優雅に指で弄びながら、%CALLNAME:MASTER%に威圧感たっぷりの笑顔を向けてきた…。
		ENDIF
	ELSE
		;オスカーが夢でうなされているのを一度でも見ると追加
		IF RAND:3 == 0 && CEVENT("両親の夢")||CEVENT("悪夢")
			PRINTFORML 「%TEXTR(@"夢か…夢、なのだな…/…チッ、またあの夢か…/…どうした%CNAME("二人称")%よ。%CNAME("一人称")%の顔に何か付いてるか？")%」
			PRINTFORML %CALLNAME:TARGET%の頬と目元には、うっすらと涙の痕が残っている…。
		ELSEIF RAND:2
			PRINTFORML 「んんーッ…！ああぁ…」
			PRINTFORML 目覚めた%CALLNAME:TARGET%は背を反らせ、豊満な胸を強調するような伸びをしている。
		ELSE
			PRINTFORML 「ふっ…ふわぁぁ〜…むにゅむにゅ…」
			PRINTFORML %CALLNAME:TARGET%は優雅に手で口を隠しながら一つ大きな欠伸をした。
		ENDIF
	ENDIF
CASE "コスプレ中"
	PRINTFORML 
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K30
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K30, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "デートの約束"
	PRINTFORML （さて、今から服の準備をしなくてはな…）
;これは悪魔のみ
CASE "契約成立"
	IF RAND:3 == 0
		PRINTFORML 「しかしまあ、一日たてば一人分以上に魂が再生するとはな…」
		PRINTFORML 「…あのクズ共に知られてなくて本当に良かったぞ」
	ELSEIF RAND:2
		PRINTFORML 「ククッ、良い取引だったぞ♪　次もまた頼もうか」
		PRINTFORML %CALLNAME:TARGET%は至極上機嫌な様子で%CALLNAME:MASTER%に笑いかけてきた。
	ELSE
		PRINTFORML 「うーむ、これはこれは良質な魂だな。では早速…あー、ンッ…」ｺﾞｸﾘｯ
		IF RAND:2
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%に見せつけるように、白く揺らめく魂を艶めかしく舌に絡めながら飲み込んだ…。
			PRINTFORML 「…何だその顔は。取引した%CNAME("二人称")%の魂を美味しく頂いただけではないか…クククッ♪」
		ELSE
			PRINTFORML 「…ふふ、感じるぞ…%CNAME("二人称")%の魂が%CNAME("一人称")%と一つになっていくのが…」
			PRINTFORML %CALLNAME:TARGET%は胸の谷間に両手を置き、満ち足りた様子で息をついた…。
		ENDIF
	ENDIF
CASE "抜け出す"
	PRINTFORML 
CASE "あなたの部屋に連れ込まれた"
	IF RAND:2
		PRINTFORML 「どうした、わざわざ%CNAME("一人称")%を連れ込んだりして」
		PRINTFORML 「…ナニをするつもりか気になるなぁ？ククッ♪」
		SIF BASE:欲求不満 >= 75
			PRINTFORML 冗談のように喉を鳴らして笑う%CALLNAME:TARGET%だが、%CALLNAME:MASTER%を捉える眼光は期待に満ち満ちている…。
	ELSE
		PRINTFORML 「スンスン…部屋の前を通るたび感じていたが、やはり%CNAME("二人称")%の匂いが濃いな」
		PRINTFORML 「青臭い、精力に満ちた若い雄の匂いだ…。ククッ、少なくとも%CNAME("一人称")%は好きな匂いだぞ？」
		SIF BASE:欲求不満 >= 75
			PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%にしなだれかかり、マーキングするように腰をねっとりと擦り付けてくる…。
	ENDIF
CASE "自室に招待"
	IF RAND:2
		PRINTFORML 「%CNAME("二人称")%よ。暇ならこのまま%CNAME("一人称")%の部屋で過ごさんか？」
		PRINTFORML 「%TEXTR("恥ずかしい話だが、寂しくて/何というか、退屈で")%な…。一人で過ごすのは慣れているはずだったのだが…」
	ELSE
		PRINTFORML 「わざわざ来てもらってすまないな、%CNAME("二人称")%」
		PRINTFORML 「いやなに、単純に%CNAME("二人称")%と二人っきりになりたかっただけだ」
		SIF BASE:欲求不満 >= 75
			PRINTFORML そう言う%CALLNAME:TARGET%は%CALLNAME:MASTER%に身を寄せ、艶めかしく%CALLNAME:MASTER%の胸や下腹部を撫で続けている…。
	ENDIF
CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 「%TEXTR("む/おお")%、%CNAME("二人称")%と…%CNAME("助手")%か。%TEXTR("二人共よく来たな/二人揃ってどうした？/何か用か？")%」
	ELSE
		PRINTFORML 「%TEXTR("む/おお")%、%TEXTR("よく来たな/どうした/何か用か、")%%CNAME("二人称")%」
	ENDIF
	IF RAND:2
		PRINTFORML %CALLNAME:TARGET%はロッキングチェアーに鷹揚と腰かけながら%CALLNAME:MASTER%\@ FIND_COND("現在存在", "助手") ? 達を # を \@出迎えた。
	ELSE
		PRINTFORML ベッドから起き上がった%CALLNAME:TARGET%は優雅に小さく欠伸をしてから%CALLNAME:MASTER%\@ FIND_COND("現在存在", "助手") ? 達に # に \@歩みよった。
	ENDIF
	PRINTFORML 「%TEXTR("折角来てもらったのに、生憎茶も菓子もほぼ切らしていてな…すまない/たいして面白い物は無いが、まぁゆっくりしていってくれ/チェスでもやるか？それかベッドの上でヤる運動でも良いぞ？")%」
CASE "アドバイザー部屋訪問"
	PRINTFORML 
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 
CASE "オナバレ"
	PRINTFORML 
CASE "ミニスカパンチラ"
	IF RAND:2
		PRINTFORML 「どうした%CNAME("二人称")%、そんなにこっちを見て…」
		PRINTFORML %CALLNAME:TARGET%はパンチラしていたことに気付いていないようだ…。
	ELSE
		PRINTFORML 「む、その視線…%CNAME("一人称")%のパンツが見えていたか？」
		PRINTFORML 「まぁこれだけ丈が短ければ、さもありなんむべなるかな…」
	ENDIF
CASE "ローライズヒップ"
	PRINTFORML 
CASE "スカートをめくる"
	PRINTFORML 
CASE "買い物する"
	;買ってきたアイテムなどで分岐
	;IF COND("巨大バイブ購入")

