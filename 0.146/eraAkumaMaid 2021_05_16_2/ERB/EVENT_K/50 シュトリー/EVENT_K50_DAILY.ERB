;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K50, ARGS
#DIM MEMO_LINECOUNT
#DIM LCOUNT
#DIM NUM_FIND
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR


MEMO_LINECOUNT = LINECOUNT
ACTOR = 0
NUM_FIND = 0

;文字色変更
CALL CHANGE_OFC, "セット", TARGET

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K50, %ARGS%, %CSTR:朝駆け%
		[ENDIF]
	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K50, %ARGS%, %CSTR:朝駆け%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	IF LINECOUNT > MEMO_LINECOUNT
		CALL CHANGE_OFC, "リセット", TARGET
		RETURN 1
	ENDIF

	IF COND("体育")
		PRINTFORMW 「おはよー！ %CNAME("二人称")%」
		PRINTFORMW 「はい、授業で使う体操服、忘れないでね？」
	ELSE
		PRINTFORMW 「おはよー！ %CNAME("二人称")%」
		IF RAND:3 == 0
			PRINTFORMW 「目は覚めた？ まだぼーっとしてるかな？」
		ELSEIF RAND:2
			PRINTFORM 「のんびりしてると
			IF TEXTS("曜日") != "日" && RAND:2
				PRINTFORMW 遅刻しちゃうよ？」
			ELSE
				PRINTFORMW ごはん冷めちゃうよ？」
			ENDIF
		ELSE
			PRINTFORMW 「んふふー、今の気分はどう？」
		ENDIF
		
	ENDIF
CASE "一番乗り"
	IF COND("同伴中")
		CALL EMOTION_PLUS, "リセット/にっこり/微笑/頬染", TARGET
		IF CHARA_NUM("集合予定") == 3
			PRINTFORMW 「%CNAME("フェリシエル")%はまだ来てないみたいだね！ ……なにしよっか？」
		ELSE
			PRINTFORMW 「皆はまだ来てないみたいだね！ ……なにしよっか？」
		ENDIF
	ELSE
		IF RAND:2 && CHARA_NUM("集合予定") > 3
			CALL EMOTION_PLUS, "リセット/糸目/ふきだしワイワイ", TARGET
			PRINTFORMW 「%CNAME("二人称")%、こんにちは」
			PRINTFORMW 「二番目は%CNAME("二人称")%かー。……次は誰だろね？」
		ELSE
			CALL EMOTION_PLUS, "リセット/糸目/困り眉/頬染/ふきだし汗", TARGET
			PRINTFORMW 「%CNAME("二人称")%、こんにちは」
			PRINTFORMW 「ちょっと早く来すぎちゃったみたい」
		ENDIF
	ENDIF
CASE "ただいま"
	PRINTFORMW 「ただいまー、%CNAME("二人称")%」
	;挨拶にフェリシエルがいる
	IF FINDT_COND_K50(TARGET, "入室挨拶中", "フェリシエル") && RAND:4 == 0
		PRINTFORMW 「%CNAME("フェリシェル")%と買い物してたら遅くなっちゃった」
	;会話に参加しているキャラがいる
	ELSEIF FINDT_COND_K50(TARGET, "入室挨拶中") && RAND:3 == 0
		PRINTFORMW 「帰り道で%CNAME(@"{FINDT_COND_K50(TARGET, "入室挨拶中")}")%とたまたま一緒になったんだよ」
	ENDIF
	CALL SET_CEVENT, "Ｄイベ：帰宅の挨拶", TARGET
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		;翌日でも帰り道の行動が変わらないパターンもあるので連続で発生しないようにする
		SIF SAVESTR:帰り道の行動 != "相合傘" && CEVENT("雨の会話")
			CALL DEL_CEVENT, "雨の会話", TARGET
		IF DAYEV("試験間近")
			PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
			PRINTFORMW 「ねぇ、%CNAME("二人称")%、後で勉強教えてね！」
		ELSEIF SAVESTR:帰り道の行動 == "相合傘" && CEVENT("雨の会話") == 0
			PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
			PRINTFORMW 「雨が降ったけど%CNAME("二人称")%は大丈夫だった？」
			CALL SET_CEVENT, "雨の会話", TARGET
		ELSEIF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("二人称")%、%CNAME("パートナー")%、おかえりなさい」
		ELSE
			;超レアだとは思うけど同居人がシュトリーだけのパターン
			IF RAND:4 == 0 && TALENT:恋慕 && CHARA_NUM("集合予定") == 2
				CALL EMOTION_PLUS, "リセット/紅潮/ふきだしハート", TARGET
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ねぇねぇ、%CNAME("二人称")%♪」
				CALL OFC_PRINT_STRW, "「ちゅっ_CBBH_ ……えへへっ_／／／_ 」", TARGET
				PRINTFORMDW %CALLNAME:TARGET%とお帰りのキスをした
			ELSEIF RAND:3 && CHARA_NUM("集合予定") == 2
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ご飯の準備できてるよ？それとも先にお風呂にする？」
			ELSEIF RAND:2
				PRINTFORML 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				IF TEXTS("季節詳細") == "夏"
					PRINTFORMW 「今日は暑かったね！ 熱中症にならないでね？」
				ELSEIF TEXTS("季節詳細") == "冬"
					PRINTFORMW 「今日は寒かったね！ 風邪をひいちゃうダメだよ？」
				ELSE
					PRINTFORMW 「今日は過ごしやすかったね。いい天気が続くといいな」
				ENDIF
			ELSE
				PRINTFORMW 「%TEXTR("おかえりー/遅かったね/お疲れ様")%、%CNAME("二人称")%」
				PRINTFORMW 「ねっ！ %CNAME("二人称")%、%TEXTR("このゲームでやろうよ！/新刊の漫画買ってきたけど読む？/この配信見た？ 面白いよ！")%」
			ENDIF
		ENDIF
		CALL SET_CEVENT, "Ｄイベ：帰宅の挨拶", TARGET
	;こっちは部室
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
			PRINTFORMW 「%CNAME("二人称")%、%CNAME("パートナー")%、こんにちは」
		ELSE
			IF RAND:4 == 0 && FINDT_COND_K50(TARGET, "おやつ当番")
				CALL EMOTION_PLUS, "リセット/にっこり", TARGET
				PRINTFORMW 「%CNAME("二人称")%、こんにちは」
				PRINTFORMW 「%CNAME(@"{FINDT_COND_K50(TARGET, "おやつ当番")}")%がおやつを用意してくれてるよ！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「%CNAME("二人称")%、こんにちは」
				PRINTFORMW 「%CNAME("一人称")%もさっき着いたところ！」
			ELSEIF RAND:2
				PRINTFORMW 「%CNAME("二人称")%、こんにちは」
				PRINTFORMW 「どこかでつまみ食いでもしてた？」
			ELSE
				PRINTFORMW 「%CNAME("二人称")%、こんにちは」
			ENDIF
		ENDIF
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "リセット/＞＜/怒り眉/ふきだしワイワイ", TARGET
		PRINTFORMW 「はいはーい！ お待たせ！」
		PRINTFORMW 「%CNAME("一人称")%に%TEXTR("何か御用？/して欲しいことでもあるの？")%」
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		IF EXP_GAIN("性処理経験")
			CALL EMOTION_PLUS, "リセット/たれ眉/嘲笑/紅潮/ふきだしハート", TARGET
			PRINTFORMW 「お昼前に%CNAME("一人称")%が%TEXTR("いっぱい出/お世話")%してあげたのに♪」
			CALL OFC_PRINT_STRW, @"「%CNAME("二人称")%ってほんっと元気いいよね_CBBH_ 」", TARGET
		ELSE
			CALL EMOTION_PLUS, "リセット/＞＜/紅潮", TARGET
			PRINTFORMW 「こんにちはー！」
			PRINTFORMW 「%CNAME("二人称")%、今日もえろいねー♪」
			PRINTFORMDW %CALLNAME:PARTNER()%とのやりとりを見られたようだ
		ENDIF
	;全裸の人がいる
	ELSEIF FINDT_COND_K50(TARGET, "全裸状態終了可能")
		CALL EMOTION_PLUS, "＞＜/たれ眉/ムラムラ/ふきだし驚き", TARGET
		PRINTFORMW 「こんにちはー！」
		PRINTFORMW 「わっ！ %CNAME(@"{FINDT_COND_K50(TARGET, "全裸状態終了可能")}")%、はだかだー！ すごーい！」
	;下着の人がいる
	ELSEIF FINDT_COND_K50(TARGET, "薄着状態終了可能")
		CALL EMOTION_PLUS, "＞＜/たれ眉/ムラムラ/ふきだし驚き", TARGET
		PRINTFORMW 「こんにちはー！」
		PRINTFORMW 「わっ！ %CNAME(@"{FINDT_COND_K50(TARGET, "薄着状態終了可能")}")%、すごい格好！」
	ELSEIF CSTR:PREV_DAILYCOM == "トーク"
		PRINTFORMW 「こんにちはー！」
		PRINTFORMW 「なんだか盛り上がってるね♪」
	ELSE
		IF RAND:2
			SIF CFLAG:褒める == 0
				CFLAG:褒める = 2
			PRINTFORMW 「こんにちはー♪」
			PRINTFORMDW どうやら今日の%CALLNAME:TARGET%は機嫌が良いようだ
		ELSE
			PRINTFORMW 「こんにちはー！」
			PRINTFORMDW %CALLNAME:TARGET%は今日も元気みたいだ
		ENDIF
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "リセット/〇〇/怒り眉/頬染/ふきだし驚き", TARGET
		PRINTFORML 「わっ！？ もう皆集まってる！」
		PRINTFORMW 「こんにちはー！」

	ELSEIF CFLAG:呼び出された
		CALL EMOTION_PLUS, "リセット/＞＜/怒り眉/頬染/ふきだしワイワイ", TARGET
		PRINTFORML 「こんにちはー！」
		PRINTFORML 「%CNAME("二人称")%、お待たせ！」
		PRINTFORMDW %CALLNAME:TARGET%は小走りで%CALLNAME:MASTER%のもとへやってきた
	ELSE
		CALL EMOTION_PLUS, "リセット/にっこり", TARGET
		PRINTFORMW 「こんにちはー！」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		CALL EMOTION_PLUS, "リセット/微笑/ふきだしワイワイ", TARGET
		PRINTFORMW 『%CNAME(@"{FIND_COND("教員", "入室挨拶中")}")%、こんにちは』
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL EMOTION_PLUS, "リセット/ふきだしワイワイ", TARGET
		PRINTFORML 『%CNAME(@"{FIND_COND("入室挨拶中")}")%、こんにちは』
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部"
			PRINTFORMW 『遅かったね？ 何かあったの？』
		CASE "１年生", "２年生", "３年生"
			PRINTFORMW 『もう皆あつまってるよ！』
		CASEELSE
			IF TALENT:ACTOR:外見年齢 <= 2 && TALENT:PLAYER:ロリババア == 0
				PRINTFORMW 『遅かったね？ 何かあったの？』
			ELSEIF TALENT:ACTOR:外見年齢 == 4
				WAIT
			ELSE
				PRINTFORMW 『もう皆あつまってるよ！』
			ENDIF
		ENDSELECT
	ELSE
		PRINTFORMW 『%CNAME(@"{FIND_COND("入室挨拶中")}")%、こんにちは』
	ENDIF

CASE "居間に現れた"
	;酔っぱらいがいないか探す
	FOR LCOUNT, 2, CHARANUM
		SIF LCOUNT == TARGET
			CONTINUE
		SIF STATE("酔い", LCOUNT) == 0
			CONTINUE
		NUM_FIND += 1

		SIF RAND:NUM_FIND == 0
			ACTOR = LCOUNT
	NEXT
	;いた場合
	IF ACTOR
			CALL EMOTION_PLUS, "リセット/〇〇/たれ眉/半開き/ふきだし驚き", TARGET
			PRINTFORMW 「……ねぇ、%CNAME("二人称")%」
			PRINTFORMW 「%CNAME(@"{ACTOR}")%、なんか%TEXTR("変じゃない/顔が赤くない/酔ってない")%？」
	ELSEIF CFLAG:呼び出された
			PRINTFORMW 「%TEXTR("なーにー？ /呼んだ？ ")%%CNAME("二人称")%」
	;一度も夜に出会ってないパターン
	ELSEIF CMOVE("居間") + CEVENT("Ｄイベ：帰宅の挨拶") + COND("同伴中") == 0
		CALL EMOTION_PLUS, "リセット/にっこり/ふきだしワイワイ", TARGET
		PRINTFORMW 「%CNAME("二人称")%、帰ってたんだね」
		PRINTFORMW 「おかえりー♪」
	ELSE
		CALL EMOTION_PLUS, "リセット/糸目/たれ眉/ω/頬染", TARGET
		PRINTFORMW 「%CNAME("二人称")%、何か面白いことしない？」
		PRINTFORMW %CALLNAME:TARGET%は暇を持て余しているようだ
	ENDIF
CASE "抜け出された"
	IF COND("欲求不満") >= 2 || TALENT:恋慕
		CALL EMOTION_PLUS, "リセット/困り眉/無口系/紅潮", TARGET
		PRINTFORMW 「あ……%CNAME("二人称")%……」
		PRINTFORMW 「……いってらっしゃい」
		PRINTFORMDW %CALLNAME:TARGET%は寂しそうに%CALLNAME:MASTER%を見送った
	ELSE
		CALL EMOTION_PLUS, "リセット/ジト目/ω/頬染", TARGET
		PRINTFORMW 「%TEXTR("ははーん/ふーん/へー/ほー")% ……いってらっしゃい、%CNAME("二人称")%」
		PRINTFORMDW %CALLNAME:TARGET%はニヤニヤしながら%CALLNAME:MASTER%を見送った
	ENDIF
CASE "お弁当"
	IF CEVENT("初めてのお弁当") == 0
		CALL EMOTION_PLUS, "リセット/困り眉/無口系", TARGET
		IF FINDT_COND_K50(TARGET, "同棲中", "フェリシエル") && COND("同棲中")
			PRINTFORMW 「%CNAME("フェリシエル")%にも手伝ってもらったんだけど……どうかな？」
		ELSE
			PRINTFORMW 「料理の本を見ながら作ったんだけど……どうかな？」
		ENDIF
		PRINTFORMW %CALLNAME:TARGET%の作ってきたお弁当は、卵焼きやウィンナーといった定番のおかずが入ったシンプルなものだ。
		PRINTFORMW 見た目はおかしなところもなく、よくできているようにも見える。
		PRINTFORMW とりあえず、一口、卵焼きを食べてみることにした。
		PRINTFORMW ……少し癖のある独特の味がするが、結構おいしい。
		SIF CONFIG("顔グラＯＮ")
			PRINTL
		CALL EMOTION_PLUS, "リセット/にっこり/たれ眉/紅潮", TARGET
		PRINTFORMW 「おいしい？ えへへっ♪ ……よかった」
		PRINTFORMW 「%CNAME("一人称")%が作るとよく魔界っぽい味付けにしちゃうんだけど、大丈夫みたいだね！」
		PRINTFORMW 「それなら、いろいろ作ってきてあげる♪」
		CALL SET_CEVENT, "初めてのお弁当"
	ELSEIF RAND:3 == 0
		PRINTFORMW 今日のお弁当は……サンドイッチだ！
		SIF CONFIG("顔グラＯＮ")
			PRINTL
		CALL EMOTION_PLUS, "リセット/＞＜/怒り眉/ふきだし満開", TARGET
		PRINTFORMW 「どうどう？ おしゃれでしょ！」
		PRINTFORMW 「%CNAME("二人称")%は具は何が好き？」
		PRINTFORMW 「これ？ んふふー♪ はい、あ～～～ん♪」
	ELSEIF RAND:2
		PRINTFORMW 今日のお弁当は……おかずにコロッケがある！
		SIF CONFIG("顔グラＯＮ")
			PRINTL
		CALL EMOTION_PLUS, "リセット/怒り眉", TARGET
		PRINTFORMW 「全部中身が違うんだよ！」
		PRINTFORMW 「それがポテトで、こっちがカレー味、これにはチーズが入ってるの♪」
		PRINTFORMW 「……あれ？ そっちがポテトだっけ？」
	ELSE
		PRINTFORMW 今日のお弁当は……大きなおにぎりが入っている！
		SIF CONFIG("顔グラＯＮ")
			PRINTL
		CALL EMOTION_PLUS, "リセット/頬染/ふきだし満開", TARGET
		PRINTFORMW 「あははっ、%CNAME("二人称")%、ほっぺたにご飯粒付けてる！」
		PRINTFORMW 「じっとしててね！ とってあげる♪」
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の頬に付いたご飯粒を手で摘み、ぱくっと食べてしまった
	ENDIF
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

;文字色を戻す
CALL CHANGE_OFC, "リセット", TARGET
;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K50, ARGS
#DIM MEMO_LINECOUNT
#DIM LCOUNT
#DIM ACTOR
MEMO_LINECOUNT = LINECOUNT

;文字色変更
CALL CHANGE_OFC, "セット"

;レイアウトもあるので1～3行くらいで最大5あたり？
;書き込むならトーク台詞の方を充実の方がいい気がする
ACTOR = CHARANUM-1

;特殊イベント
;初回台詞
IF ARGS == "初回台詞"
	PRINTFORML 「%NAME:TARGET%です！ 今日からよろしくお願いします♪」
	PRINTFORMDL %CALLNAME:TARGET%は自己紹介をしている
;新入部員が入った時
;夜は対応しない……母親とか妹とかの家族が急に生えてくるのでどうしたものか
ELSEIF STRCOUNT(CSTR:ACTOR:NOW_DEVENT, "初回台詞") && ACTOR != TARGET
	IF COND("昼")
		IF COND("教員", ACTOR)
			PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくお願いします」
		ELSE
			SELECTCASE CSTR:ACTOR:学年
			CASE "初等部", "中等部", "１年生", "２年生", "３年生"
				PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくね♪」
			CASEELSE
				IF TALENT:ACTOR:外見年齢 == 3
					PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくお願いします」
				ELSE
					PRINTFORML 「%CNAME(@"{ACTOR}")%、よろしくね♪」
				ENDIF
			ENDSELECT
		ENDIF
		PRINTFORMDL %CALLNAME:TARGET%は%CALLNAME:ACTOR%に自己紹介をしている
	;夜の場合
	ELSE
