;-------------------------------------------------
;口上テンプレート
;シュトリー口上（キャラクターの名前を入れる、下にコンセプトなど一言あれば入れる）
;製作者　ブリート
;-------------------------------------------------
;まだぜんぜん完成してないよ！
;-------------------------------------------------
;元気で甘えてくる女の子、悪魔ですが容姿だけで出来ることはあまり人間の女の子と変わりません
;女性型の魔物のため魔界にいた頃にえっちに関して勉強をしています。（知識だけ実践はない）
;力が弱いために悪魔祓いを相手にするのはちょっと怖い
;悪魔である事がばれない様に認識障害を使っていますが……精度があまり高くないので看破できる人にはできます
;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K50, ARGS
#FUNCTIONS
#DIM ACTOR

VARSET LOCALS

LOCALS =

SELECTCASE ARGS
CASE "男性器"
	SELECTCASE CEVENTS("男性器呼")
	CASE ""
		LOCALS = おちんちん_おちんちん
	CASEELSE
		LOCALS = %CEVENTS("男性器呼")%
	ENDSELECT
CASE "女性器"
	SELECTCASE CEVENTS("女性器呼")
	CASE ""
		LOCALS = おまんこ_おまんこ
	CASEELSE
		LOCALS = %CEVENTS("女性器呼")%
	ENDSELECT
CASE "一人称"
	LOCALS = わたし_わたし

CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF TALENT:PLAYER:オトコ
			LOCALS = おにいさん_おにいさん
		ELSE
			LOCALS = おねえさん_おねえさん
		ENDIF
	;相手が主人ではない
	ELSE
		IF COND("教員", PLAYER)
			LOCALS = %CALLNAME:PLAYER%先生_%CSTR:PLAYER:ふりがな%せんせい
		ELSE
			SELECTCASE CSTR:PLAYER:学年
			CASE "初等部", "中等部"
				IF TALENT:PLAYER:オトコ
					LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
				ENDIF
			CASE "１年生", "２年生", "３年生"
				LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
			CASEELSE
				IF TALENT:PLAYER:外見年齢 < 3 && TALENT:PLAYER:小柄 && TALENT:PLAYER:ロリババア == 0
					IF TALENT:PLAYER:オトコ
						LOCALS = %CALLNAME:PLAYER%君_%CSTR:PLAYER:ふりがな%くん
					ELSE
						LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
					ENDIF
				ELSE
					LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
				ENDIF
			ENDSELECT
		ENDIF
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長さん_ぶちょうさん
CASE "助手"
	IF COND("教員", ASSI)
		LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
	ELSE
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部", "中等部"
			IF TALENT:ASSI:オトコ
				LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF
		CASE "１年生", "２年生", "３年生"
			LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
		CASEELSE
			IF TALENT:ASSI:外見年齢 < 3 && TALENT:ASSI:小柄 && TALENT:ASSI:ロリババア == 0
				IF TALENT:ASSI:オトコ
					LOCALS = %CALLNAME:ASSI%君_%CSTR:ASSI:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
CASEELSE
	;ARGSに数字だけが入ってる時はその加入番号のキャラ
	;例えばCNAME(@"TARGET")とか
	;ARGSにCALLNAMEが入ってる場合にはそのキャラへの呼び方
	IF ISNUMERIC(ARGS)
		ACTOR = TOINT(ARGS)
	ELSEIF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)
	ENDIF

	IF ACTOR == MASTER
		IF TALENT:ACTOR:オトコ
			LOCALS = おにいさん_おにいさん
		ELSE
			LOCALS = おねえさん_おねえさん
		ENDIF
	ELSEIF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
	ELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部", "中等部"
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%君_%CSTR:ACTOR:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ENDIF
		CASE "１年生", "２年生", "３年生"
			LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
		CASEELSE
			IF TALENT:ACTOR:外見年齢 < 3 && TALENT:ACTOR:小柄 && TALENT:ACTOR:ロリババア == 0
				IF TALENT:ACTOR:オトコ
					LOCALS = %CALLNAME:ACTOR%君_%CSTR:ACTOR:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

