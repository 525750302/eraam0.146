;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K56, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"

;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	;別バリアントを参考（eratohoYMAEM_椛口上参照）
	CASE "朝Ａ騎乗位"
		CALL PRINT_STRW, @"股間からこみ上がる鋭い快感に、あなたは目を覚ました"
		CALL PRINT_STRW, @"「っあぁっ_HH_…あ、おはようございまーす♪」"
			SELECTCASE RAND:7
				CASE 0
					CALL PRINT_STRW, @"「見てください%CNAME("二人称")%、起こさずにこんなに搾っちゃいましたよぉ_HH_…んんっ！」"
					CALL PRINT_STRW, @"ミアが息むと、ペニスと肛門の結合部から精液が垂れ落ちた"
					CALL PRINT_STRW, @"「んふふ、では精液でぬるぬるになった所で、もう一回頂きまーす♪」"
				CASE 1
					CALL PRINT_STRW, @"「%CNAME("二人称")%の朝一搾り頂いてま〜す♪ あっ、はぁっ、また出てるぅ_HH_」"
					CALL PRINT_STRW, @"寝起きで意識が朦朧としたまま、ミアのアナルに夢精のような射精をしてしまう"
					CALL PRINT_STRW, @"「んっ、くぅうん！ うふふ♪ 朝からごちそうさまです_HH_」"
				CASE 2
					CALL PRINT_STRW, @"「朝から大変ご立派でしたので、つい食べちゃいましたぁ♪」"
					CALL PRINT_STRW, @"ミアの直腸はまるで咀嚼するように、あなたのペニスを包み込んでいる"
					CALL PRINT_STRW, @"「んふふ♪ ところでミア、朝ごはんがまだなんですよ…」"
					CALL PRINT_STRW, @"「このままお尻に、おちんちんヨーグルト食べさせてくださぁい_HH_」"
				CASE 3
					CALL PRINT_STRW, @"「やーん♪ トクトクしたまったり射精、素敵です♪」"
					CALL PRINT_STRW, @"起床直後の気怠さと射精中の鋭い解放感が混ざり合い、あなたは朝から悶えた"
					CALL PRINT_STRW, @"「あははっ_HH_ %CNAME("二人称")%のお顔、今とっても可愛いですっ_HH_」"
					CALL PRINT_STRW, @"「んちゅー！ よしよし、いい子ですねぇ_HH_ んふふふ♪」"
					CALL PRINT_STRW, @"ミアの愛情籠ったキスと、絶妙にペニスを撫で上げる腸壁に、あなたは無様に精液を垂れ流し続けた"
				CASE 4
					CALL PRINT_STRW, @"「失礼ながら、寝ている間に精を頂きました〜♪」"
					CALL PRINT_STRW, @"寝起きなのに、射精直後の独特な気怠さがあなたを襲う"
					CALL PRINT_STRW, @"「寝ている間に搾り尽くされるのも、男のロマンじゃないですか？_HH_ んふふ♪」"
				CASE 5
					CALL PRINT_STRW, @"目覚めた直後、睡眠中に何度も果てて搾ぼられたことを理解する"
					CALL PRINT_STRW, @"「起きてる時の元気一杯ビュルビュル射精と…」"
					CALL PRINT_STRW, @"「寝ている時のゆったりトクトク射精…」"
					CALL PRINT_STRW, @"「ミアはどっちも大好きです_HH_」"
				CASE 6
					CALL PRINT_STRW, @"「びっくりしちゃいましたか？ んふふ♪」"
					CALL PRINT_STRW, @"あなたのペニスを肛内に挿れたまま、ミアは小悪魔的な表情であなたに笑いかけてきた"
					CALL PRINT_STRW, @"「けど寝ている%CNAME("二人称")%のおちんちん ずっと元気なままで、ミアぴっくりしちゃいました♪」"
			ENDSELECT
	CASE "朝キス"
		CALL PRINT_STRW, @"口元に感じる柔らかい快感に、あなたは目を覚ました"
		CALL PRINT_STRW, @"「んちゅっ、ちぅ、ん… おはようございます_HH_」"
		CALL PRINT_STRW, @"ミアは覚醒したあなたを気に留めず、ついばむようなバードキスを続けている"
		CALL PRINT_STRW, @"「ちぅ、ちゅぅ、んふふ…」"
		CALL PRINT_STRW, @"「寝顔が可愛かったんで…んふふふ_HH_」"
	CASE "朝添い寝"
		CALL PRINT_STRW, @"目覚めたとき、ミアはあなたの胸にうつぶせで添い寝していた"
		CALL PRINT_STRW, @"「んぅ、おはよぉごじゃぁまぁ…」"
		CALL PRINT_STRW, @"「んむぅぅぅっ」"
		CALL PRINT_STRW, @"まだ寝ぼけているのか、ミアはあなたの胸元に顔をグリグリと押し付けて来る"
		CALL PRINT_STRW, @"「んむぅ…いぃにおぃ…」"
		CALL PRINT_STRW, @"「……すぅ――…」"
		CALL PRINT_STRW, @"あなたを起こすつもりが、あなたに体を預ける居心地良さに、ミアがすっかり二度寝してしまった"
	CASE "朝フェラ", "朝シックスナイン"
		CALL PRINT_STRW, @"股間からこみ上がる鋭い快感に、あなたは目を覚ました"
		CALL PRINT_STRW, @"「…じゅぷっ、じゅるるっ_HH_…あ、おはようございまーす♪」"
			SELECTCASE RAND:6
				CASE 0
					CALL PRINT_STRW, @"「え？何をしてるのかって？」"
					CALL PRINT_STRW, @"「朝ごはんを食べてま〜す♪ %TEXTR("あむっ/じゅるっ")%_HH_」"
				CASE 1
					CALL PRINT_STRW, @"「お先に朝ごはんを食べてま〜す_HH_」"
					CALL PRINT_STRW, @"「ではでは、食事の続きを… %TEXTR("あむっ/じゅるっ")%_HH_」"
				CASE 2
					CALL PRINT_STRW, @"「朝一番の出来立てホヤホヤの%CNAME("精液")%…」"
					CALL PRINT_STRW, @"「一杯ミアに食べさせてぇ_HH_」"
				CASE 3
					CALL PRINT_STRW, @"「朝一番の%CNAME("精液")%ってすっごく美味しいんですよね〜♪」"
					CALL PRINT_STRW, @"「つい我慢できなくなっちゃいました_HH_」"
				CASE 4
					CALL PRINT_STRW, @"「成長期のせいか、ミアって朝からお腹が%TEXTR("空いちゃうん/ぺこぺこなん")%ですよ♪」"
					CALL PRINT_STRW, @"「だから朝でも手加減無しですよ〜_HH_」"
				CASE 5
					CALL PRINT_STRW, @"「%CNAME("二人称")%〜、ミアお腹空きましたぁ…」"
					CALL PRINT_STRW, @"「ごはん%TEXTR("頂戴/ちょーだい")%っ_HH_ %TEXTR("あむっ/かぷっ")%_HH_」"
			ENDSELECT
	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ"
		SELECTCASE RAND:4
			CASE 0
				CALL PRINT_STRW, @"「%TEXTR("ぷはぁ/ぷぁ/ごくん/んく")%っ……ご馳走様でした♪」"
			CASE 1
				CALL PRINT_STRW, @"「%TEXTR("ぷはぁ/ぷぁ/ごくん/んく")%っ……やっぱり朝一は味が違いますねぇ_HH_」"
			CASE 2
				CALL PRINT_STRW, @"「%TEXTR("ぷはぁ/ぷぁ/ごくん/んく")%っ……喉に結構絡んで、飲みにくかったですよ〜」"
				CALL PRINT_STRW, @"「朝から興奮しちゃいました？ %TEXTR("んふふ/いひひ")%_HH_」"
			CASE 3
				CALL PRINT_STRW, @"「%TEXTR("ぷはぁ/ぷぁ/ごくん/んく")%っ……あぁ美味しかった♪」"
				CALL PRINT_STRW, @"「………」"
				CALL PRINT_STRW, @"「…もう一発だけ飲んでもいいですか？_HH_」"
		ENDSELECT
	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K56, %ARGS%, %CSTR:朝駆け%
		[ENDIF]
	CASE "朝Ａ騎乗位"
		SELECTCASE RAND:9
			CASE 0
				CALL PRINT_STRW, @"「ふぅー〜〜っ……良い一日になりそうですね♪」"
			CASE 1
				CALL PRINT_STRW, @"「ふぅー〜〜っ……ご馳走様でした♪」"
			CASE 2	
				CALL PRINT_STRW, @"「ふぅー〜〜っ……朝活って素晴らしいですね♪」"
			CASE 3
				CALL PRINT_STRW, @"「ふぅー〜〜っ……また昼と夜もよろしくお願いしますね♪」"
			CASE 4
				CALL PRINT_STRW, @"「ふぅー〜〜っ……一旦、ここで満足しておきますね」"
			CASE 5
				CALL PRINT_STRW, @"「ふぅー〜〜っ……%CNAME("睾丸")%に残った%CNAME("精液")%は後でまた頂きますね_HH_」"
			CASE 6
				CALL PRINT_STRW, @"「ふぅー〜〜っ……お腹ぽかぽか_HH_ とりあえず満足です♪」"
			CASE 7
				CALL PRINT_STRW, @"「ふぅー〜〜っ……とっても気持ちよかったです♪」"
			CASE 8
				CALL PRINT_STRW, @"「ふぅー〜〜っ……朝から最高でした♪」"
		ENDSELECT
		CALL PRINT_STRW, @"「………」"
		SELECTCASE RAND:5
			CASE 1
				CALL PRINT_STRW, @"「…もう1発だけ出して貰ってもいいですか？_HH_」"
			CASE 2
				CALL PRINT_STRW, @"「…尿道に残った%TEXTR("精液/精子")%が勿体ないので、ぐりぐりしちゃいますね〜_HH_」"
			CASE 3
				CALL PRINT_STRW, @"「…やっぱりあと%TEXTR("2/3")%発%TEXTR("お願いします/頑張って下さい")%っ！_HH_」"
			CASE 4
				CALL PRINT_STRW, @"「…あれれぇ？時間はまだ少しありますねぇ♪ このまま抜かずの連発行きましょ〜_HH_」"
			CASE 5
				CALL PRINT_STRW, @"「…やっぱり最後まで搾り切るのがサキュバスですよね〜_HH_」"
		ENDSELECT
		CALL PRINT_STRW, @"……ようやく解放された時、ミアの肌は%TEXTR("美しく潤っ/ツヤツヤになっ/輝い")%ていた…"
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		CALL PRINT_STRW, @"「%CNAME("二人称")%〜、体育の日ですよ〜」"
		CALL PRINT_STRW, @"「一杯身体を動かして、いい汗流しましょう！」"
	ELSE
		CALL PRINT_STRW, @"「%CNAME("二人称")%は本日どのようなご予定ですか？」"
	ENDIF
CASE "一番乗り"
	IF TALENT:恋慕
		CALL PRINT_STRW, @"「あっ！ %CNAME("二人称")%来た〜！」"
		CALL PRINT_STRW, @"一足早く来て待機していたミアは、待っていたあなたを見るや否や%TEXTR("抱き着いて/飛びついて")%きた"
		CALL PRINT_STRW, @"「んぎゅ〜〜♪」"
	ELSE
		CALL PRINT_STRW, @"「あ、きたきた♪ 早めに来て正解でしたねぇ〜」"
		CALL PRINT_STRW, @"「え？なんでもう居るかって？」"
		CALL PRINT_STRW, @"「他の女に誘惑される前に、%CNAME("二人称")%を搾っちゃおうかな〜って…%TEXTR("あはは/んふふ")%♪」"
	ENDIF
CASE "ただいま"
	CALL PRINT_STRW, @"「ただいま戻りました〜」"
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		CALL PRINT_STRW, @"「お帰りなさ〜い、もうこんな時間ですね」"
	ELSE
		CALL PRINT_STRW, @"「お帰りなさ〜い」"
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		CALL PRINT_STRW, @"「はいは〜い、どうされましたか？」"
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中")
		CALL PRINT_STRW, @"「わ〜お♪ これは失礼しました_HH_」"
	ELSE
		CALL PRINT_STRW, @"「おはこんばんにちは〜♪」"
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL PRINT_STRW, @"「おっと、遅れましたか」"
	ELSEIF CFLAG:呼び出された
		CALL PRINT_STRW, @"「はい、どうしましたか？」"
	ELSE
		CALL PRINT_STRW, @"「お疲れ様です」"
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		CALL PRINT_STRW, @"「先生もこの部室に来られるんですね〜」"
	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		CALL PRINT_STRW, @"「皆さん揃ってますよ〜」"
	ELSE
		CALL PRINT_STRW, @"「お疲れ様です」"
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		PRINTFORMW 
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
		PRINTFORMW 
CASE "お弁当"
	SELECTCASE RAND:2
		CASE 0
			CALL PRINT_STRW, @"「%CNAME("二人称")%のためにお弁当作ってきましたよ〜、ご堪能あれっ♪」"
		CASE 1
			CALL PRINT_STRW, @"「料理と聞いたら手を抜けないのがミアなのです！ お弁当も気合入ってますよ〜」"
	ENDSELECT
;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K56, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	CALL PRINT_STRL, @"「んふふ♪ これからがとっても楽しみですね〜」"
	CALL PRINT_STRL, @"「改めまして、サキュバスのミアです！ よろしくお願いしま〜す」"
CASE "汗だく"
	CALL PRINT_STRL, @"「う〜、汗でベタベタ…シャワー浴びたいな…」"
CASE "寝起き"
	SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STRL, @"「んぁ…、んむぅ……」"
			CALL PRINT_STRL, @"「……あ、おはようごじゃいま〜す」"
		CASE 1
			CALL PRINT_STRL, @"「ん、んぅううう……」"
			CALL PRINT_STRL, @"「…おはようございます%CNAME("二人称")%」"
		CASE 2
			CALL PRINT_STRL, @"「んぅ、んぁあああ…」"
			CALL PRINT_STRL, @"「…すみません、寝落ちしてました」"
	ENDSELECT
CASE "コスプレ中"
	PRINTFORML 
CASE "おやつの感想"
	;今日のおやつを用意したのは自分
	IF COND("おやつ当番")
		CALL PRINT_STRW, @"「ミアはおやつも手を抜きませんよ〜」"
		SELECTCASE RAND:8
			CASE 0
				CALL PRINT_STRW, @"「どうぞ！ ミア特性マグカッププリンで〜す♪」"
			CASE 1
				CALL PRINT_STRW, @"「どうぞ！ ミア特性チョコヌガーバーで〜す♪」"
			CASE 2
				CALL PRINT_STRW, @"「どうぞ！ ミア特性豆乳ミルクもちで〜す♪」"
			CASE 3
				CALL PRINT_STRW, @"「どうぞ！ ミア特性豆腐ドーナッツで〜す、やさしい甘さに仕上げましたよ♪」"
			CASE 4
				CALL PRINT_STRW, @"「どうぞ！ ミア特性カボチャマフィンで〜す♪」"
			CASE 5
				CALL PRINT_STRW, @"「どうぞ！ ミア特性なんちゃってチーズケーキで〜す♪」"
				CALL PRINT_STRW, @"「どういうことかって？ これはチーズ一切不使用のチーズ風ケーキなんですよ！」"
				CALL PRINT_STRW, @"「カロリーの気になる乙女に大ブームのスイーツですよ♪」"
			CASE 6
				CALL PRINT_STRW, @"「どうぞ！ ミア特性オレンジジュレで〜す♪ さっぱり爽やかで美味しいですよ！」"
			CASE 7
				CALL PRINT_STRW, @"「どうぞ！ ミア特性アーモンドチョコサブレで〜す♪」"
		ENDSELECT
	;今日のおやつは誰かの手作り
	ELSEIF COND("手作りおやつ")
		;今日のおやつ当番の人の手作りを初めて食べた
		IF DEVENT(@"%TEXTS("おやつ当番")%の手作り") == 0
			;おやつ当番が[料理上手]
			IF COND("おやつの作り手が料理上手")
				CALL PRINT_STRW, @"「っ……！ これはっ…！」"
				CALL PRINT_STRW, @"「……あとでちょっと作り方教えてくれませんか%CNAME(TEXTS("おやつ当番"))%さん」"
			ELSE
				CALL PRINT_STRW, @"「んっ〜〜……♪ 美味しいっ…！」"
				CALL PRINT_STRW, @"「これはミアも熱くなっちゃいますね〜〜」"
			ENDIF
		ELSE
			CALL PRINT_STRW, @"「いや〜〜美味しいですね〜」"
		ENDIF
	;おやつが手作りでない
	ELSE
		CALL PRINT_STRW, @"「わぁっ！ ありがとうございます%CNAME(TEXTS("おやつ当番"))%さん！」"
	ENDIF
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K56
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	CALL PRINT_STRL, @"「んふふ♪ これからは恋人ですよ%CNAME("二人称")%」"
	CALL PRINT_STRL, @"「今まで以上に、えっちが楽しみになりました_HH_」"
CASE "デートの約束"
	CALL PRINT_STRL, @"「今週の日曜日が楽しみです〜♪」"
;これは悪魔や天使や悪魔祓いのみ
CASE "契約成立"
	SELECTCASE RAND:2
		CASE 0
			CALL PRINT_STRL, @"「はい、これで%TEXTR("完了/終了/満了")%です！」"
		CASE 1
			CALL PRINT_STRL, @"「終わりましたよ！気分は如何ですか？」"
	ENDSELECT
;あなたにお願いされて、自分の力で巨根になってあげた(悪魔と天使のみ)
CASE "巨根化契約成立"
	PRINTFORML 
CASE "短小化契約成立"
	PRINTFORML 
CASE "抜け出す"
	PRINTFORML 
;あなたと一緒に、あなたの部屋に移動した
CASE "あなたの部屋に連れ込まれた"
	SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STRL, @"「女の子を部屋に連れ込んで、一体何をなさるつもりですか〜？_HH_」"
		CASE 1
			CALL PRINT_STRL, @"「へぇ〜、これが%CNAME("二人称")%の部屋ですか」"
