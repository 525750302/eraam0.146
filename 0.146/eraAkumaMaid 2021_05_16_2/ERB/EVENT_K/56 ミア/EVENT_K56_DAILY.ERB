;素質で分岐させる場合にはTALENT:恋慕での分岐がおすすめです
;-------------------------------------------------
@KOJO_DATE_K56, ARGS
#DIM ACTOR
ACTOR = PARTNER()

SELECTCASE ARGS
;あなたを週末デートに誘った
CASE "デートのお誘い"
	CALL PRINT_STRW, @"「%CNAME("二人称")%、%CNAME("二人称")%！ 週末は空いてますか？」"
	CALL PRINT_STRW, @"「ミアと一緒にデートしましょ〜、週末一人は寂しいんですよ〜」"
CASE "デートのお誘い：結果"
	;デート決定！
	;自発的にデートのお弁当作ってあげるよ！って場合はCALL SET_CEVENT, "デートのお弁当"としてください。
	IF COND("デートの約束")
		CALL PRINT_STRW, @"「やったぁ！ありがとうございます！」"
		CALL PRINT_STRW, @"「楽しみですね〜、んふふ♪ 昼も夜も_HH_」"
	;あなたに保留されてしまった
	ELSE
		CALL PRINT_STRW, @"「ぶぅ〜、振られちゃいましたか…ざんね〜ん」"
		CALL PRINT_STRW, @"「またお誘いしますね〜」"
		CALL PRINT_STRW, @"「……じゃあ久しぶりにあの方と会おうかな」"
		CALL PRINT_STRW, @"「………%TEXTR("絶倫/お金持ち/初心/イケメン")%なんだよねぇ_HH_」"
		CALL PRINT_STRW, @"ミアは手にかけた男達を見繕っているようだ"
	ENDIF
;あなたから週末デートに誘われた
;自発的にデートのお弁当作ってあげるよ！って場合はCALL SET_CEVENT, "デートのお弁当"としてください。
CASE "デートに誘われた"
	CALL PRINT_STRW, @"「今週末にデートですか？ もっちろんOKですよ！」"
	CALL PRINT_STRW, @"「%CNAME("二人称")%のエスコートが楽しみです♪」"
;あなたにお弁当を作ってきてと頼まれた
CASE "お弁当を頼まれた"

	;これのコメントアウトを外すと、お弁当を作ってあげません
	;CALL DEL_CEVENT, "デートのお弁当"
;あなたからデート当日にいきなり誘われた
;自発的にデートのお弁当作ってあげるよ！って場合はCALL SET_CEVENT, "デートのお弁当"としてください。
CASE "デートに誘う"
	CALL PRINT_STRW, @"「わ〜お！突然ですね！」"
	CALL PRINT_STRW, @"「たまたま今日は空いてたので大丈夫ですよ〜」"
	;同棲していない相手を誘った時
	IF COND("同棲中") == 0
		CALL PRINT_STRW, @"「後で待ち合わせ場所を教えてくださいね♪」"
	ELSE
		CALL PRINT_STRW, @"「準備しますので、少しお待ちくださいね♪」"
	ENDIF

;デートするにあたり、特殊な条件で着替えたりしたい時に使ってください。台詞無しで着せ替え操作のみするのが推奨です
CASE "デート衣装に着替える"

;地の文はパートナーが遅れて現れる展開になっています
;地の文が用意されているので、軽く１行程度の反応で良いと思います
CASE "待ち合わせ"
	IF COND("同棲中") == 0
		CALL PRINT_STRW, @"「あっ！%CNAME("二人称")%〜！ お待たせしました〜」"
	ELSE
		CALL PRINT_STRW, @"「楽しい一日にしましょう！」"
	ENDIF
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

;地の文が用意されているので、軽く１行程度の反応で良いと思います
CASE "デート本番"
	SELECTCASE SAVESTR:デート
	CASE "水着を買いに行く"
		PRINTFORMW 
	CASE "散歩"
		PRINTFORMW 
	CASE "食べ歩き"
		PRINTFORMW 
	CASE "街でお買い物"
		PRINTFORMW 
	CASE "水族館に行く"
		PRINTFORMW 
	CASE "遊園地に行く"
		PRINTFORMW 
	CASE "映画館に行く"
		PRINTFORMW 
	CASE "図書館に行く"
		PRINTFORMW 
	CASE "お花見"
		PRINTFORMW 
	CASE "泳ぎに行く"
		PRINTFORMW 
	CASE "山に行く"
		PRINTFORMW 
	CASE "紅葉狩りに行く"
		PRINTFORMW 
	CASE "スケート"
		PRINTFORMW 
	CASEELSE
		PRINTFORMW 
	ENDSELECT

	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

CASE "手をつなぐ"
	PRINTFORMW 
CASE "手をつながない"
	PRINTFORMW 
;水着を買いに行った時です
CASE "水着選び開始"
	;マイクロビキニとかもＯＫ
	IF COND("Ｈな水着選択可能") && RAND:2
		PRINTFORMW 
	ELSEIF RAND:2
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
;水着を買いに行って、結果的に選んだ水着に対するセリフです
;もしも去年も水着を選んでいたなら、CSTR:今年の水着 に記録されているので比較する事が出来る
CASE "水着決定"
	SELECTCASE SWIM(STR:選んだ水着, "種類")
	CASE "マイクロビキニ"
		PRINTFORMW 
	CASE "ビキニ"
		PRINTFORMW 
	CASE "競泳水着"
		PRINTFORMW 
	CASE "ハイレグワンピ"
		PRINTFORMW 
	CASE "ワンピース水着"
		PRINTFORMW 
	;その他はだいたいエロ水着です
	CASEELSE
		PRINTFORMW 
	ENDSELECT

;プールなどで水着姿に着替え、待ち合わせ場所に集合した時です
CASE "水着に着替えた"
	;どの水着に着替えたか？
	SELECTCASE SWIM(CSTR:今年の水着, "種類")
	CASE "ビキニ"
		PRINTFORMW 
	CASE "マイクロビキニ"
		PRINTFORMW 
	CASE "ワンピース水着"
		PRINTFORMW 
	CASE "競泳水着"
		PRINTFORMW 
	;その他はだいたいエロ水着です
	CASEELSE
		PRINTFORMW 
	ENDSELECT

;物陰フェラです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "ちょっと物陰へ……"
	IF TALENT:恋慕
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

;映画館などで横並びに座った状態での手コキです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "こっそり手コキしてもらう"

CASE "キスする"

CASE "お尻を触る"
	IF TALENT:恋慕
		CALL PRINT_STRW, @"「%TEXTR("んふふ/いひひ")%、やっと来たぁ♪ もうっ！ まだかまだかと待ってたんですよ〜」"
		CALL PRINT_STRW, @"「この後ホテルで、%CNAME("睾丸")%空っぽにしてあげますね_HH_」"
	ELSE
		CALL PRINT_STRW, @"「%TEXTR("んふふ/いひひ")%♪ そう来なくっちゃ！ 健全な殿方の証拠ですね♪」"
		CALL PRINT_STRW, @"「…この後ホテルで、好きなだけ使っていいんですよ_HH_」"
	ENDIF
;あなたが物陰フェラやキスやセクハラをしない紳士な場合はここに来ます
CASE "次は何処に行こうか？と話を振る", "映画に集中する"

	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

;文化祭イベント
;ここでは"デート相手ではなく出し物をしている人"が喋ります。
;本来のデート相手はACTORにキャラ番号が入ってます
CASE "バンド演奏", "演劇", "占いの館", "お化け屋敷", "お悩み相談室", "ぱふぱふ屋", "メイド喫茶", "執事喫茶", "レストラン"


;帰り際のセリフです。さりげなくエッチに誘うのもいいでしょう
;STR:Ｈする場所 で場所をラブホ以外にする事も出来ます。（例 STR:Ｈする場所 = 部室
CASE "帰り際"
	IF TALENT:恋慕
		CALL PRINT_STRW, @"「今日はとっても楽しかったです！」"
		CALL PRINT_STRW, @"「またすぐにデートしましょうね♪」"
		CALL PRINT_STRW, @"「絶対来週また誘ってくださいよ！ 誘われなくてもミアから誘っちゃいますからね！_HH_」"
	ELSE
		CALL PRINT_STRW, @"「%CNAME("二人称")%のおかげで楽しい週末になりました♪」"
		CALL PRINT_STRW, @"「ありがとうございます！」"
	ENDIF
CASE "デート終了"
	;エッチをする場合
	IF TFLAG:ベッドイン
		IF TALENT:恋慕
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ELSE
		IF TALENT:恋慕
			PRINTFORMW 
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDSELECT

