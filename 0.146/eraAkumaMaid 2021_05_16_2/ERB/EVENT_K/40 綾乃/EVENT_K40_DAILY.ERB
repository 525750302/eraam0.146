	CASE "朝キス"
		PRINTFORMW 「ふふ…おはよ」
		PRINTFORMW 「朝ごはん、もうできてるよ」
	CASE "朝添い寝"
		CALL PRINT_STR, @"「%CNAME("二人称")%の寝顔、堪能しちゃった…_BH_」_W"
	CASE "朝フェラ"
		CALL PRINT_STR, @"「あ、おはよー…_BH_」_W"
		CALL PRINT_STR, @"「朝のミルク、ご馳走様…_BH_」_W"
	CASE "朝騎乗位"
		CALL PRINT_STR, @"「ん、朝のぴゅっぴゅ、とっても元気…_BH_」_W"
		CALL PRINT_STR, @"「この調子なら、今日も一日がんばれそうだね…_BH_」_W"
	ENDSELECT

