;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K29, ARGS
#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = ぼく_ぼく
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF NAME:PLAYER == "あなた"
			LOCALS = 部長_ぶちょう
		ELSEIF TRUE_SEX(MASTER) == "オトコ"
			LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
		ELSE
			LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
		ENDIF
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部", "中等部", "１年生", "２年生"
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ENDIF
		CASE "３年生", "研究生"
			LOCALS = %CALLNAME:PLAYER%先輩_%CSTR:PLAYER:ふりがな%せんぱい
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%先生_%CSTR:PLAYER:ふりがな%せんせい
			ELSEIF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = 部長_ぶちょう
CASE "助手"
	SELECTCASE CSTR:ASSI:学年
	CASE "初等部", "中等部", "１年生", "２年生"
		IF TALENT:PLAYER:オトコ || TRUE_SEX(ASSI) == "オトコ"
			LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
		ELSE
			LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
		ENDIF
	CASE "３年生", "研究生"
		LOCALS = %CALLNAME:ASSI%先輩_%CSTR:ASSI:ふりがな%せんぱい
	CASEELSE
		IF COND("教員", ASSI)
			LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
		ELSEIF TALENT:PLAYER:オトコ
			LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
		ELSE
			LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

