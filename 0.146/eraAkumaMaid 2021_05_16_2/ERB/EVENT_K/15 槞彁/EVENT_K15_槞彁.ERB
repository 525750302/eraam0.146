		;		ELSEIF CFLAG:(ARG:1):CFLAG_LIB("母国報道") > CFLAG:(ARG:1):CFLAG_LIB("母国状況")/10
		;			CFLAG:(ARG:1):CFLAG_LIB("母国報道") -= 1
		;		ENDIF
