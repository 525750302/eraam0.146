  <Row0 Column0="IF CFLAG:好感度 &gt;= 1000 &amp;&amp; COND(&quot;合意：本番&quot;) == 0&#xD;&#xA;	SETCOLOR DEF_COLOR(&quot;ショッキングピンク&quot;)&#xD;&#xA;	PRINTFORML - システムメッセージ -&#xD;&#xA;	PRINTFORML  %CALLNAME:TARGET%から本番の合意を得た。&#xD;&#xA;	PRINTFORML  Ｖ挿入系コマンドが使用可能になります&#xD;&#xA;	PRINTFORMW  (未完成につきイベント無し、発生条件は将来的に変更の可能性あり)&#xD;&#xA;	CALL DEL_CEVENT, &quot;合意禁止：本番&quot;&#xD;&#xA;	CALL SET_GOUI, &quot;本番&quot;&#xD;&#xA; 	RESETCOLOR&#xD;&#xA;ENDIF" Column1="IF CFLAG:好感度 &gt;= 1000 &amp;&amp; COND(&quot;合意：本番&quot;) == 0&#xD;&#xA;	IF CEVENT(&quot;斥责事件&quot;) == 0 &#xD;&#xA;	; 没有素股意外&#xD;&#xA;		PRINTL &#xD;&#xA;		PRINTFORMW 「那个……」&#xD;&#xA;		PRINTFORMW %CALLNAME:TARGET%的脸有些红&#xD;&#xA;		PRINTFORMW 「果然，不能动真格的话，%CNAME(&quot;二人称&quot;)%是不能满足的吧」&#xD;&#xA;		PRINTFORMW 「我作为母亲，可不能眼睁睁看着%CNAME(&quot;二人称&quot;)%难受啊」&#xD;&#xA;		PRINTL &#xD;&#xA;		PRINTFORMW %CALLNAME:TARGET%的眼中满溢着温柔和爱意，向%CALLNAME:MASTER%张开了双臂&#xD;&#xA;		SETCOLOR DEF_COLOR(&quot;ショッキング粉紅&quot;)&#xD;&#xA;		PRINTFORMW 「今后，你想怎么使用我的身体都没关系，%CNAME(&quot;一人称&quot;)%都可以原谅你」&#xD;&#xA;		RESETCOLOR&#xD;&#xA;		PRINTFORMW 「不过，要尽量温和一点哦」&#xD;&#xA;		PRINTFORMW 「自从你的父亲走后，我就再也没有做过这种事情了……」&#xD;&#xA;		PRINTL&#xD;&#xA;		PRINTFORMW 「好了好了，不说他了，你已经等不及了吧……」&#xD;&#xA;	ELSE&#xD;&#xA;	; 有素股意外&#xD;&#xA;		PRINTL&#xD;&#xA;		PRINTFORMW 「唉……%CNAME(&quot;二人称&quot;)%，你老实和我说」&#xD;&#xA;		SETCOLOR DEF_COLOR(&quot;ショッキング粉紅&quot;)&#xD;&#xA;		PRINTFORMW 「你是不是还想和%CNAME(&quot;一人称&quot;)%……做上次那种事……」&#xD;&#xA;		RESETCOLOR&#xD;&#xA;		PRINTL&#xD;&#xA;		PRINTFORMW 「果然呢……其实我也没有办法忘记那次意外」&#xD;&#xA;		PRINTFORMW 「自从你父亲走后，我就再也没做过这种事情了……」&#xD;&#xA;		PRINTFORMW 「没想到居然会是和%CNAME(&quot;二人称&quot;)%……」&#xD;&#xA;		PRINTFORMW %CALLNAME:TARGET%用责备的眼神看了%CALLNAME:MASTER%一眼，轻轻叹了口气&#xD;&#xA;		PRINTL&#xD;&#xA;		PRINTFORMW 「这也都怪我，平时太纵容你了」&#xD;&#xA;		PRINTFORMW 「只是这次的事，如果我不负责的话，你邪恶的欲望不知道还会发泄到哪个无辜的女孩身上……」&#xD;&#xA;		PRINTFORMW 「……所以，就仅限性欲处理的时间，我允许你做这种事」&#xD;&#xA;		PRINTFORMW 「平常，我们还是普通的母子，好吗？」&#xD;&#xA;		PRINTFORMW 看着%CALLNAME:MASTER%欣喜若狂的神情，%CALLNAME:TARGET%的脸颊一片嫣红……&#xD;&#xA;	ENDIF&#xD;&#xA;	SETCOLOR DEF_COLOR(&quot;ショッキング粉紅&quot;)&#xD;&#xA;	PRINTFORML - システムメッセージ -&#xD;&#xA;	PRINTFORML  获得了能和%CALLNAME:TARGET%做爱的协议。&#xD;&#xA;	PRINTFORML  V插入系统指令可以使用&#xD;&#xA;	PRINTFORMW  (事件还没有完成，触发条件将来可能会变更)&#xD;&#xA;	CALL DEL_CEVENT, &quot;合意禁止：本番&quot;&#xD;&#xA;	CALL SET_GOUI, &quot;本番&quot;&#xD;&#xA; 	RESETCOLOR&#xD;&#xA;ENDIF" />
  <Row1 Column0=";-------------------------------------------------&#xD;&#xA;;愛撫&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;IF SELECTCOM == 0&#xD;&#xA;	;初めて&#xD;&#xA;	IF FIRSTCOM(SELECTCOM) == 0&#xD;&#xA;		PRINTFORMW 「%CNAME(&quot;一人称&quot;)%の身体じゃ、今一物足りないかもしれないけど」&#xD;&#xA;		PRINTFORMW 「%CNAME(&quot;二人称&quot;)%が好きな人にしてあげるのを想像して……触ってみなさい」&#xD;&#xA;	;二回目以降&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;フフッ/いいわ/もっと強くしてもいいのよ/優しくしてね&quot;)%_HH_」_W&quot;&#xD;&#xA;			CALL PRINT_STR, @&quot;「%CNAME(&quot;一人称&quot;)%が相手なんだから%TEXTR(&quot;遠慮はいらないからね/どんどん色々試していいのよ&quot;)%」_W&quot;&#xD;&#xA;		ENDIF&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1&#xD;&#xA;ENDIF&#xD;&#xA;" Column1=";-------------------------------------------------&#xD;&#xA;;愛撫&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;IF SELECTCOM == 0&#xD;&#xA;	;初めて&#xD;&#xA;	IF FIRSTCOM(SELECTCOM) == 0&#xD;&#xA;		PRINTFORMW 「%CNAME(&quot;一人称&quot;)%的身体，或许现在有点不足，不过」&#xD;&#xA;		PRINTFORMW 「不过我还是想成为%CNAME(&quot;二人称&quot;)%成为喜欢的人呢……摸摸看吧」&#xD;&#xA;	;二回目以降&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			IF MARK:淫紋 &amp;&amp; RAND:2 == 0&#xD;&#xA;				IF EXP_GAIN(&quot;膣内精液経験&quot;)&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;呼唔/咿唔/嗯呜&quot;)%_HH_ %TEXTR(&quot;现在、不可以这样/这、这太奇怪了/稍稍、有些难受&quot;)%」_L&quot;&#xD;&#xA;					CALL PRINT_STR, @&quot;「%CNAME(&quot;一人称&quot;)%的%TEXTR(&quot;那/子宮&quot;)%里面……那些东西……要被%TEXTR(&quot;按揉着流/儿子亲手按&quot;)%出来了_HH_……」_L&quot;&#xD;&#xA;					PRINTFORMW %CALLNAME:PLAYER%温柔地抚弄着的%CALLNAME:TARGET%的身体，不知不觉间已经有%TEXTR(&quot;精液/精浆&quot;)%缓缓地从%TEXTR(&quot;那里/秘所里&quot;)%溢了出来、%CALLNAME:TARGET%顶着潮红的脸又是无奈、又是幽怨地望了%CALLNAME:PLAYER%一眼……&#xD;&#xA;				ELSE&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;咿、嗯嗯~/唔啊~&quot;)%_HH_　%TEXTR(&quot;不可、以、那里、那里的话/等一下、停、停下、咿&quot;)%_HH_」_L&quot;&#xD;&#xA;					PRINTFORMW 无视了%CALLNAME:TARGET%的哀鸣、%CALLNAME:PLAYER%用手指舔舐般轻揉着她%TEXTR(&quot;下腹部/子宮上面&quot;)%的%TEXTR(&quot;淫秽纹章/淫纹&quot;)%&#xD;&#xA;					PRINTFORMW 每个触摸到粉色纹章的瞬间、%CALLNAME:TARGET%的身体都会不受控制地颤抖、迎接一个接一个的轻微高潮……&#xD;&#xA;				ENDIF&#xD;&#xA;			ELSEIF INSERT(&quot;Ｖ挿入&quot;)&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;唔、嗯嗯~/啊嗯、呼呼~&quot;)%_HH_　%TEXTR(&quot;咻、唔唔、嗯、啊/咿唔、啊、哈~&quot;)%_HH_」_L&quot;&#xD;&#xA;				PRINTFORMW %CALLNAME:TARGET%辛苦地忍耐着上面和下面同时传来的让人疯狂的快乐、勉强自己维持身为母親的威严……&#xD;&#xA;				PRINTFORMW %CALLNAME:PLAYER%看着%CALLNAME:TARGET%的这副样子、反而被激起了更强的逆反心理、上下半身瞬间开足马力!!&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;等一下、咿/等、等等、啊、唔&quot;)%_HH_　%TEXTR(&quot;请、请温柔一点/稍稍、慢一点&quot;)%_HH_」_L&quot;&#xD;&#xA;			;隙あらばキス&#xD;&#xA;			ELSEIF TEQUIP:接吻&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;嗯/嗯唔/啾&quot;)%_HH_　怎么能这样……」_W&quot;&#xD;&#xA;				CALL PRINT_STR, @&quot;「一边与%TEXTR(&quot;亲生儿子/学生/那个人的儿子&quot;)%接吻,还不停被抚摸」_W&quot;&#xD;&#xA;				PRINTFORML 「对不起、我已经回不到、仅仅是%CNAME(&quot;二人称&quot;)%%TEXTR(&quot;母親/妈妈&quot;)%的时候了……」&#xD;&#xA;				CALL PRINT_STR, @&quot;「我爱你_HH_」_W&quot;&#xD;&#xA;			ELSE&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;嗯/唔/唔嗯&quot;)%_HH_　%CALLNAME:PLAYER%、%TEXTR(&quot;喜欢/爱你&quot;)%…%TEXTR(&quot;不那么温柔、也没关系~/感受到了你的热量呢/好像在被你的手指亲吻&quot;)%_HH_」_W&quot;&#xD;&#xA;			ENDIF&#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;呵呵/不错呢/再强硬点也没関系/请温柔点吧&quot;)%_HH_」_W&quot;&#xD;&#xA;			CALL PRINT_STR, @&quot;「%CNAME(&quot;一人称&quot;)%可是你的对手，%TEXTR(&quot;请不要客气/可以不断尝试各种各样的东西哦&quot;)%」_W&quot;&#xD;&#xA;		ENDIF&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1&#xD;&#xA;ENDIF" />
  <Row2 Column0="	;二回目以降&#xD;&#xA;	;押し倒されているとき&#xD;&#xA;	ELSEIF TEQUIP:押し倒し&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE" Column1=";二回目以降&#xD;&#xA;	;开局深吻&#xD;&#xA;	ELSEIF CHECK_COM(&quot;テンダー接吻&quot;)&#xD;&#xA;		IF TEQUIP:推倒&#xD;&#xA;			PRINTFORM #逆推+深吻&#xD;&#xA;		ELSE&#xD;&#xA;			;恋慕&#xD;&#xA;			IF TALENT:恋慕&#xD;&#xA;				IF 1&#xD;&#xA;					PRINTFORMW 「一上来就要接吻吗……」&#xD;&#xA;					PRINTFORM %CALLNAME:TARGET%順從地依偎在%CALLNAME:PLAYER%的怀中、&#xD;&#xA;					IF TALENT:PLAYER:高大 &gt; 0&#xD;&#xA;						PRINTFORM 用她那娇媚的双眸仰望着%CALLNAME:PLAYER%、&#xD;&#xA;					ELSEIF TALENT:PLAYER:娇小 &gt;0&#xD;&#xA;						PRINTFORM 她落在着%CALLNAME:PLAYER%身体上的眼神显得格外魅惑、&#xD;&#xA;					ELSE&#xD;&#xA;						PRINT 径直将她的粉红的舌头探了过来、&#xD;&#xA;					ENDIF&#xD;&#xA;					PRINT 两人立刻开始了&#xD;&#xA;					SIF ABL:欲望&gt;5&#xD;&#xA;						PRINT 如蛇在交配一般的&#xD;&#xA;					PRINTFORMW 深情的拥吻……&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;嗯、唔嗯、哈/唔、咿嘤、哼呼/呼、哈、唔咿&quot;)%、嘶噜~_HH_ %TEXTR(&quot;呵呵~/呼呼~/哼、哦~&quot;)%、好像有口水流下来了呢_HH_……」_W&quot;&#xD;&#xA;				ELSE&#xD;&#xA;					PRINTFORMW 「要用接吻来开始吗？%TEXTR(&quot;呵呵/呼呼&quot;)%、好的呢~」&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;嗯、唔嗯、哈/唔、咿嘤、哼呼/呼、哈、唔咿&quot;)%、嘶噜~_HH_ %TEXTR(&quot;呵呵~/呼呼~/哼、哦~&quot;)%、好像有口水流下来了呢_HH_……」_W&quot;&#xD;&#xA;				ENDIF&#xD;&#xA;			ELSE&#xD;&#xA;				PRINTFORM #无恋慕深吻&#xD;&#xA;			ENDIF&#xD;&#xA;		ENDIF&#xD;&#xA;	;押し倒されているとき&#xD;&#xA;	ELSEIF TEQUIP:推倒&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			CALL KOJO_K10_RAND_VOICE&#xD;&#xA;			IF INSERT(&quot;挿入&quot;)&#xD;&#xA;				CALL PRINT_STR, @&quot;「唔、竟然、%TEXTR(&quot;那么卖力地/像头发情中的野兽一样/那么贪婪地&quot;)%上下一起_HH_ 就那么喜歡%TEXTR(&quot;妈妈/亲生母亲/我这个坏女人&quot;)%吗？%TEXTR(&quot;呼呼~/嘻嘻、/哈…&quot;)%不需要这么急迫哦、因为啊…… 」_W&quot;&#xD;&#xA;				IF TALENT:淫乱&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;妈妈/母亲/我&quot;)%的这副%TEXTR(&quot;身体和灵魂/肉体和灵魂/灵和肉&quot;)%、已经离不开%CNAME(&quot;二人称&quot;)%了哦_HH_」_W&quot;&#xD;&#xA;				ELSE&#xD;&#xA;					CALL PRINT_STR, @&quot;「%TEXTR(&quot;妈妈/母亲/我&quot;)%啊、已经不可能离得开%CNAME(&quot;二人称&quot;)%了哦_HH_」_W&quot;&#xD;&#xA;				ENDIF&#xD;&#xA;			;ムードが90以上（ハート三つ分）なら確定、60未満は現在ムード＞ランダムで180以下に該当する場合&#xD;&#xA;			ELSEIF FLAG:情緒 &gt;= 90 || (FLAG:情緒 &gt;= 60 &amp;&amp; FLAG:情緒 &gt;= RAND:180) &amp;&amp; RAND:2 == 0&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;好喜歡、可以不用停下来哦/請再多做一點、妈妈已经……/呼呼、好想你能一直做下去/別……不要停下來、好孩子、我已经……&quot;)%_HH_」_W&quot;&#xD;&#xA;			ELSE&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;好了好了、可以继续撒娇哦~/請再多做一點/好希望这个瞬间能永远继续下去啊/別……不要停下來&quot;)%_HH_」_W&quot;&#xD;&#xA;			ENDIF&#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE" />
  <Row3 Column0=";-------------------------------------------------&#xD;&#xA;;調教ごとの初Ｖインサート&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;ELSEIF SELECTCOM &gt;= 60 &amp;&amp; SELECTCOM &lt;= 69 &amp;&amp; CHOSECOMS(&quot;Ｖ挿入&quot;) == 0 &amp;&amp; FIRSTCOM(SELECTCOM) &amp;&amp; RAND:3 == 0&#xD;&#xA;	PRINTFORMW 調教ごとの初Ｖインサート&#xD;&#xA;	;巨根&#xD;&#xA;	IF SIZE(&quot;ペニス&quot;, PLAYER) &gt;= 2 &amp;&amp; RANDIF(1, TALENT:恋慕)&#xD;&#xA;		PRINTFORMW &#xD;&#xA;	;恋慕&#xD;&#xA;	ELSEIF TALENT:恋慕&#xD;&#xA;		SELECTCASE SELECTCOM&#xD;&#xA;		;騎乗位&#xD;&#xA;		CASE 65&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;対面座位&#xD;&#xA;		CASE 63&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		CASEELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDSELECT&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		PRINTFORMW &#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1" Column1=";-------------------------------------------------&#xD;&#xA;;調教ごとの初Ｖインサート&#xD;&#xA;;调教中的第一次V插入（RAND存在，并非每次必定触发）&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;ELSEIF SELECTCOM &gt;= 60 &amp;&amp; SELECTCOM &lt;= 69 &amp;&amp; CHOSECOMS(&quot;Ｖ挿入&quot;) == 0 &amp;&amp; FIRSTCOM(SELECTCOM) &amp;&amp; RAND:3 == 0&#xD;&#xA;&#xD;&#xA;	;PRINTFORMW 調教ごとの初Ｖ挿入&#xD;&#xA;	;巨根&#xD;&#xA;	IF SIZE(&quot;阴茎&quot;, PLAYER) &gt;= 2 &amp;&amp; RANDIF(1, TALENT:恋慕)&#xD;&#xA;		PRINTFORMW &#xD;&#xA;	;恋慕&#xD;&#xA;	ELSEIF TALENT:恋慕&#xD;&#xA;		;疑似二穴插入&#xD;&#xA;		IF TEQUIP:肛用振動棒&#xD;&#xA;			PRINTFORML 「%TEXTR(&quot;呜嗯……/哈咿……/啊姆……&quot;)%明明%CNAME(&quot;一人称&quot;)%的%TEXTR(&quot;屁股小穴/肛門&quot;)%里还有振動棒、就被插入%TEXTR(&quot;膣道/阴道/小穴/蜜穴&quot;)%了~~%CNAME(&quot;調教時二人称&quot;)%坏心眼……」&#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;呼/嗯/呜&quot;)%_HH_　%CNAME(&quot;一人称&quot;)%要被%TEXTR(&quot;親兒子/兒子主人/學生兒子/兒子老公&quot;)%变成奇怪的%TEXTR(&quot;色情/好色/淫乱&quot;)%%TEXTR(&quot;媽媽/母親/母愛人&quot;)%了_HH__BHH__HH__BHH__HH__W」&quot;&#xD;&#xA;		ELSEIF CHECK(&quot;Ｖ完美契合&quot;) &#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;呼啊/呜咿/姆唔&quot;)%_HH__BHH__HH__BHH__HH_————~~~！！！%TEXTR(&quot;就、就是这个/好、好舒服/肉体交合的地方传来了让人迷失神志的快乐&quot;)%……_W」&quot;&#xD;&#xA;			CALL PRINT_STR, @&quot;「%CNAME(&quot;一人称&quot;)%的%TEXTR(&quot;膣道/阴道/小穴/蜜穴&quot;)%已经彻底变成了%CNAME(&quot;二人称&quot;)%的形状了呐……%TEXTR(&quot;呼呼~/芙芙~/嘻嘻~/嘿嘿~&quot;)%_HH__W」&quot;&#xD;&#xA;			PRINTFORMW&#xD;&#xA;		ELSE&#xD;&#xA;			SELECTCASE SELECTCOM&#xD;&#xA;			;屈曲位&#xD;&#xA;			CASE 62&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;呼呼~/芙芙~/嘻嘻~/哼哼~&quot;)%、一上来就要用这么认真(播_HH_种)的姿势吗……_BHH__W」&quot;&#xD;&#xA;				CALL PRINT_STR, @&quot;「%CNAME(&quot;一人称&quot;)%觉得很%TEXTR(&quot;开心/高兴/幸福&quot;)%哦…毕竟%CNAME(&quot;二人称&quot;)%那————么努力地想让%TEXTR(&quot;媽媽/母親/老师&quot;)%受孕什么的、%TEXTR(&quot;超级可爱哦/很让人心动啊/不是很感人吗&quot;)%_BHH__W」&quot;&#xD;&#xA;				PRINTFORMW %CALLNAME%用充满诱惑的动作、分开两条泛着粉光的长腿、架在了%CALLNAME:PLAYER%的肩膀上。母子倆十指紧紧相扣、以最易授種的体式开始活塞运动……&#xD;&#xA;			;それ以外&#xD;&#xA;			CASEELSE&#xD;&#xA;				PRINTFORML 「%TEXTR(&quot;呜嗯……/哈咿……/啊姆……&quot;)%%TEXTR(&quot;親生兒子/兒子主人/學生兒子/兒子老公&quot;)%的\@ SIZE(&quot;阴茎&quot;, PLAYER) &gt;= 2 ?大肉棒#肉棒\@进到%CNAME(&quot;一人称&quot;)%的%TEXTR(&quot;膣道/阴道/小穴/蜜穴&quot;)%里惹~~」&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_HH_%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_BHH_%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_HH_%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_BHH__W」&quot;&#xD;&#xA;			ENDSELECT&#xD;&#xA;		ENDIF&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		PRINTFORMW &#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1" />
  <Row4 Column0=";-------------------------------------------------&#xD;&#xA;;全Ｖインサート共通のランダム文&#xD;&#xA;;ここに力を入れると毎回異なる反応をするようになるのでおすすめ&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;ELSEIF SELECTCOM &gt;= 60 &amp;&amp; SELECTCOM &lt;= 69 &amp;&amp; CHOSECOM(SELECTCOM) &amp;&amp; RAND:3 == 0&#xD;&#xA;	PRINTFORMW 全Ｖインサート共通のランダム文&#xD;&#xA;	;イきそうなのに焦らされている&#xD;&#xA;	IF TEQUIP:手加減 &amp;&amp; NOWEX:Ｖ寸止め &amp;&amp; RAND:2&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;好きな体位&#xD;&#xA;	ELSEIF COND(&quot;好きな体位中&quot;) &amp;&amp; SELECTCOM == INSERT(&quot;Ｖ対応コマンド&quot;) &amp;&amp; RAND:2&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		SELECTCASE CONDS(&quot;体位&quot;)&#xD;&#xA;		CASE &quot;正常位&quot;, &quot;屈曲位&quot;&#xD;&#xA;			IF TALENT:恋慕 &amp;&amp; RAND:3 == 0&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSEIF MARK:屈服刻印 &gt;= 2 &amp;&amp; RAND:2&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSE&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ENDIF&#xD;&#xA;		CASEELSE&#xD;&#xA;			IF RAND:4 == 0&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSEIF RAND:3 == 0&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSEIF RAND:2&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSE&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ENDIF&#xD;&#xA;		ENDSELECT&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1" Column1=";-------------------------------------------------&#xD;&#xA;;全Ｖインサート共通のランダム文&#xD;&#xA;;所有V插入共有的随机句子&#xD;&#xA;;ここに力を入れると毎回異なる反応をするようになるのでおすすめ&#xD;&#xA;;在这里多下点功夫的话，每次都会有不同的反应，所以推荐&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;ELSEIF SELECTCOM &gt;= 60 &amp;&amp; SELECTCOM &lt;= 69 &amp;&amp; CHOSECOM(SELECTCOM) &amp;&amp; RAND:3 == 0&#xD;&#xA;	;PRINTFORMW 全Ｖ挿入共通のランダム文&#xD;&#xA;	;イきそうなのに焦らされている&#xD;&#xA;	IF TEQUIP:PLAYER:手下留情 &amp;&amp; NOWEX:Ｖ绝顶前骤停 &amp;&amp; RAND:2&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;好きな体位&#xD;&#xA;	ELSEIF COND(&quot;喜欢的体位中&quot;) &amp;&amp; SELECTCOM == INSERT(&quot;Ｖ対応指令&quot;) &amp;&amp; RAND:2&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;呼呼~/芙芙~/嘻嘻~/哼哼~&quot;)%_HH_ %CALLNAME:PLAYER%选择了%TEXTR(&quot;媽媽/母親/我&quot;)%最喜欢的%TEXTR(&quot;体位/姿势&quot;)%呢_HH__BHH__HH_……_W」&quot;&#xD;&#xA;			CALL PRINT_STR, @&quot;「%TEXTR(&quot;好苦恼啊/有点困扰啊/那就稍微有点麻烦了呢&quot;)%……因为、不得不好好侍奉%CNAME(&quot;二人称&quot;)%作为奖励了哦_BHH__W」&quot;&#xD;&#xA;			PRINTFORMW 不知道是不是错觉、%CALLNAME%说话的时候眼睛里浮现出了粉红色的爱心形状……是\@ MARK:淫紋 &gt; 0 ?淫纹的影响#恶魔的本能\@吗……&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;それ以外&#xD;&#xA;	ELSE&#xD;&#xA;		SELECTCASE CONDS(&quot;体位&quot;)&#xD;&#xA;		CASE &quot;正常位&quot;, &quot;屈曲位&quot;&#xD;&#xA;			IF TALENT:恋慕 &amp;&amp; RAND:3 == 0&#xD;&#xA;				CALL PRINT_STR, @&quot;「吶、你和别的女孩子处理性欲的时候、最好也是先這樣哦_W」&quot;&#xD;&#xA;				CALL PRINT_STR, @&quot;「毕竟这個姿勢的话…很方便恋人之间%TEXTR(&quot;親親/接吻/KISS&quot;)%、不是嗎_BHH__W」&quot;&#xD;&#xA;			ELSEIF MARK:屈服刻印 &gt;= 2 &amp;&amp; RAND:2&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSE&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ENDIF&#xD;&#xA;		CASEELSE&#xD;&#xA;			IF RAND:4 == 0&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSEIF RAND:3 == 0&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSEIF RAND:2&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ELSE&#xD;&#xA;				PRINTFORMW &#xD;&#xA;			ENDIF&#xD;&#xA;		ENDSELECT&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1" />
