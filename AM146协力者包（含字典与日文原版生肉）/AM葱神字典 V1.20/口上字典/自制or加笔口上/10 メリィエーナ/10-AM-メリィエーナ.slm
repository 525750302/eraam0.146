  <Row6 Column0=";-------------------------------------------------&#xD;&#xA;;屈曲位&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;IF SELECTCOM == 62&#xD;&#xA;	;初めて&#xD;&#xA;	IF FIRSTCOM(SELECTCOM) == 0&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;二回目以降&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;屈服刻印Lv2以上&#xD;&#xA;		ELSEIF MARK:屈服刻印 &gt;= 2&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1&#xD;&#xA;ENDIF" Column1=";-------------------------------------------------&#xD;&#xA;;屈曲位&#xD;&#xA;;搜索到的屈曲位与游戏中的描述有明显差别、这里以游戏中的姿势为准&#xD;&#xA;;-------------------------------------------------&#xD;&#xA;IF SELECTCOM == 62&#xD;&#xA;	;初めて&#xD;&#xA;	IF FIRSTCOM(SELECTCOM) == 0&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	;二回目以降&#xD;&#xA;	ELSE&#xD;&#xA;		;恋慕&#xD;&#xA;		IF TALENT:恋慕&#xD;&#xA;			IF TEQUIP:接吻&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_HH_%TEXTR(&quot;喜歡/好喜歡/愛你/好愛你&quot;)%_HH_给%CNAME(&quot;一人称&quot;)%%TEXTR(&quot;親親/接吻/KISS&quot;)%_BHH__W」&quot;&#xD;&#xA;				CALL KOJO_K10_RAND_VOICE&#xD;&#xA;				PRINTFORMW %CALLNAME%迷失在了和%TEXTR(&quot;親生兒子/有血緣的愛人/既是兒子又是老公的主人公&quot;)%疯狂、淫乱、销魂的黏膜接触中、时不时痉挛般轻抬美臀来配合%CALLNAME:PLAYER%的动作……&#xD;&#xA;			ELSE&#xD;&#xA;				CALL PRINT_STR, @&quot;「%TEXTR(&quot;讨厌/咿呀/唔&quot;)%……这个%TEXTR(&quot;姿势/体位/动作&quot;)%_HH_也太方便受孕了_BHH_……_W」&quot;&#xD;&#xA;				CALL KOJO_K10_RAND_VOICE&#xD;&#xA;				PRINTFORMW %CALLNAME%无意识地与%CALLNAME:PLAYER%十指相扣、被死死抵住的美胯时不时跟随和子宫中等待着床的卵子一起颤抖……&#xD;&#xA;			ENDIF&#xD;&#xA;		;屈服刻印Lv2以上&#xD;&#xA;		ELSEIF MARK:屈服刻印 &gt;= 2&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		;それ以外&#xD;&#xA;		ELSE&#xD;&#xA;			PRINTFORMW &#xD;&#xA;		ENDIF&#xD;&#xA;	ENDIF&#xD;&#xA;	RETURN 1&#xD;&#xA;