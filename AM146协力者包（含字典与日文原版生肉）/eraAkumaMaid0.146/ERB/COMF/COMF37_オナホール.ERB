@COM37
;オナホール
PRINTL オナホール

CALL TRAIN_MESSAGE_B

SIF REFUSE_CHECK()
	RETURN 0

STR:前回コマンド = オナホール

RETURN 1

@FLAG_COM37, ARG
;アソコを露出させる
CALL SETFLAG, "ボトムを露出：テキスト無し", TARGET


@SOURCE_COM37, ARG
CALL VARSET_COMF

CALL SOURCE_DOWNBASE_HP, 37

SOURCE:0:露出 = 300
SOURCE:0:逸脱 = 300

;ABL:マゾっ気をみる
SELECTCASE ABL:マゾっ気
CASE 0 TO 5
	SOURCE:0:達成感 = ABL:マゾっ気*500
CASEELSE
	SOURCE:0:達成感 = 2500 + (ABL:マゾっ気 - 5)*200
ENDSELECT

;オトコかふたなり
IF PENIS(TARGET)
	;ABL:Ｃ感覚をみる
	SOURCE:0:Ｃ快楽 = MULTIPLY(CALCF("Ｃ刺激", 0), 90 + ITEM:オナホ*10)
	SOURCE:0:Ｃ快楽 = SOURCE_EXABL(SOURCE:0:Ｃ快楽, "指使い", PLAYER)
ENDIF

CALL CALC, "調教ソース", ARG


@TRAIN_MESSAGE_COM37
PRINTFORMW %CALLNAME:TARGET%のペニスをオナホールに挿入し%TEXTR("て激しく/て巧みに/、緩急をつけて")%扱き上げた…

