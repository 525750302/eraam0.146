;実際にコマンド選択をしたら、これを呼ぶ
@SHOW_COM_ORDER_COM, ARG, ARG:1
;ARG:1が1ならテキストが出力されない
TRYCALLFORM COM_ORDER_COM{ARG}, ARG:1
TCVAR:やる気 = RESULT

@PRINT_COM_ORDER_COM, ARGS, ARG, ARG:1, ARG:2
SELECTCASE ARG
CASE 0
	PRINTFORM %ARGS% = {ARG:1} 
	SELECTCASE ARG:1
	CASE IS < ARG:2
		PRINT <
	CASE ARG:2
		PRINT =
	CASEELSE
		PRINT >
	ENDSELECT
	PRINTFORMW  実行値{ARG:2}
ENDSELECT

;RESULT > 0で実行値クリア
RETURN MAX(ARG:1 - ARG:2 + 1, 0)


