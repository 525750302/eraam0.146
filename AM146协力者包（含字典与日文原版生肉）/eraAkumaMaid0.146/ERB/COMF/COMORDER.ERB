		TCVAR:基本実行値 -= 20
		STR:実行値 = %STR:実行値% - 二人きりじゃない(20)
	ELSEIF PLACE("自宅") == 0 && CHARA_NUM("集合予定") > 2
		TCVAR:基本実行値 -= 10
		STR:実行値 = %STR:実行値% - まだまだ人が来る予定がある(10)
	ENDIF

