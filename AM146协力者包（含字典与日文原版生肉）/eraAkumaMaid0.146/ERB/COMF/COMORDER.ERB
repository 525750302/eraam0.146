;現在地による補正
IF PLACE("野外")
	TCVAR:基本実行値 -= 20
	STR:実行値 = %STR:実行値% - 野外(20)
ELSEIF PLACE("パートナーの部屋")
	TCVAR:基本実行値 += 5
	STR:実行値 = %STR:実行値% + 自分の部屋(5)
ELSEIF PLACE("自室")
	TCVAR:基本実行値 += 5
	STR:実行値 = %STR:実行値% + %CALLNAME:MASTER%の部屋(5)
ELSEIF PLACE("ラブホテル")
	TCVAR:基本実行値 += 10
	STR:実行値 = %STR:実行値% + ラブホテル(10)
ELSEIF PLACE("個室") == 0
	TCVAR:基本実行値 -= 5
	STR:実行値 = %STR:実行値% - 個室ではない(5)
ENDIF

SIF ARG == 0
	PRINTFORM %SUBSTRING(STR:実行値, 3)%

RETURN TCVAR:基本実行値


;ARGSにPLAYERの場所、ARGS:1にTARGETの場所
@COM_ORDER_STAIN(ARGS, ARGS:1)
#FUNCTION
#DIM STAIN_TARGET
#DIM STAIN_PLAYER

;汚れ無視
SIF TALENT:汚れ無視
	RETURNF 0

SELECTCASE ARGS
CASE "Ｍ", "手", "脚", "Ｐ", "Ｖ", "Ａ", "Ｂ"
	STAIN_PLAYER = STAIN:PLAYER:ARGS
CASE "尿道"
	IF PENIS(PLAYER)
		STAIN_PLAYER = STAIN:PLAYER:Ｐ
	ELSE
		STAIN_PLAYER = STAIN:PLAYER:Ｖ
	ENDIF
CASEELSE
	RETURNF 0
ENDSELECT

SELECTCASE ARGS:1
CASE "Ｍ", "手", "脚", "Ｐ", "Ｖ", "Ａ", "Ｂ"
	STAIN_TARGET = STAIN:(ARGS:1)
CASE "尿道"
	IF PENIS(TARGET)
		STAIN_TARGET = STAIN:Ｐ
	ELSE
		STAIN_TARGET = STAIN:Ｖ
	ENDIF
CASEELSE
	STAIN_TARGET = 0
ENDSELECT

VARSET LOCAL

;愛液の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｖ")
	LOCAL:0 = 16
	SIF COND("性別嫌悪")
		LOCAL:0 += 5 * 2
	SIF TALENT:オトコ || TALENT:両刀
		LOCAL:0 -= 8
	SIF TALENT:においに弱い
		LOCAL:0 -= 2
	;場所自体がＶでお風呂上りならペナ軽減
	SIF LOCAL:0 > 0 && ARGS == "Ｖ" && TEQUIP:PLAYER:お風呂上り
		LOCAL:0 /= 2
ENDIF
;ペニスの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｐ")
	LOCAL:1 = 5
	SIF COND("性別嫌悪")
		LOCAL:1 += 5 * COND("性別嫌悪")
	SIF TALENT:においに弱い
		LOCAL:1 -= 2
	;場所自体がＰでお風呂上りならペナ軽減
	SIF LOCAL:1 > 0 && ARGS == "Ｐ" && TEQUIP:PLAYER:お風呂上り
		LOCAL:1 /= 2
	SIF CALCF("ペニス好き度", TARGET) > 0
		LOCAL:1 -= CALCF("ペニス好き度", TARGET)
ENDIF
;精液の汚れ(精液中毒で相殺)
IF CHECK_STAIN(STAIN_PLAYER, "精液")
	SIF COND("性別嫌悪")
		LOCAL:2 += 5 * COND("性別嫌悪")
	SIF ABL:精液中毒
		LOCAL:2 = 6 - ABL:精液中毒*2 - (TALENT:においに弱い > 0)
	SIF TALENT:吸精体質
		LOCAL:2 -= 6
ENDIF
;アナルの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ａ")
	LOCAL:3 = 65 - ABL:マゾっ気*5 - ABL:サドっ気*2
	SIF TALENT:尻穴狂い
		LOCAL:3 -= 20
	SIF TALENT:においに弱い && TALENT:恋慕
		LOCAL:3 /= 2
	;場所自体がＡでお風呂上りならペナ軽減
	SIF LOCAL:3 > 0 && ARGS == "Ａ" && TEQUIP:PLAYER:お風呂上り
		LOCAL:3 /= 2
ENDIF
;母乳の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "乳")
	LOCAL:4 += 1
ENDIF
;尿の汚れ
IF CHECK_STAIN(STAIN_PLAYER, "尿")
	LOCAL:5 += 15
	SIF TALENT:においに弱い && TALENT:恋慕
		LOCAL:5 /= 2
ENDIF

;アソコの恥垢汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｖ恥垢")
	LOCAL:7 = 20
	SIF COND("性別嫌悪")
		LOCAL:7 += 5 * COND("性別嫌悪")
	SIF TALENT:においに弱い && TALENT:恋慕
		LOCAL:7 /= 2
	;場所自体がＶでお風呂上りならペナ軽減
	SIF LOCAL:7 > 0 && ARGS == "Ｖ" && TEQUIP:PLAYER:お風呂上り
		LOCAL:7 /= 2
ENDIF
;ちんかすの汚れ
IF CHECK_STAIN(STAIN_PLAYER, "Ｐ恥垢")
	LOCAL:8 = 20
	SIF COND("性別嫌悪")
		LOCAL:8 += 5 * COND("性別嫌悪")
	SIF TALENT:においに弱い && TALENT:恋慕
		LOCAL:8 /= 2
	;場所自体がＰでお風呂上りならペナ軽減
	SIF LOCAL:8 > 0 && ARGS == "Ｐ" && TEQUIP:PLAYER:お風呂上り
		LOCAL:8 /= 2
	IF CALCF("ペニス好き度", TARGET) > 0
		LOCAL:8 /= CALCF("ペニス好き度", TARGET)
		SIF TALENT:献身
			LOCAL:8 -= 5
		SIF TALENT:倒錯的
			LOCAL:8 -= 5
	ENDIF
ENDIF


;もともとある汚れは無視する＆場所によっては汚れの強さＵＰ
SELECTCASE ARGS:1
CASE "Ｍ"
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 150), LOCAL:3 + 20)
CASE "Ｐ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:2 = 0
	LOCAL:5 = 0
CASE "Ｖ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 200), LOCAL:3 + 50)
CASE "Ａ"
	LOCAL:0 = 0
	LOCAL:1 = 0
	LOCAL:3 = 0
CASE "尿道"
	LOCAL:5 = 0
	LOCAL:3 = MIN(MULTIPLY(LOCAL:3, 200), LOCAL:3 + 50)
ENDSELECT

LOCAL = LOCAL:0 + LOCAL:1 + LOCAL:2 + LOCAL:3 + LOCAL:4 + LOCAL:5 + LOCAL:6 + LOCAL:7 + LOCAL:8

SIF TALENT:汚臭鈍感
	LOCAL = MAX(DIVIDE(LOCAL, 200), LOCAL - 20)
SIF TALENT:汚臭敏感
	LOCAL = MIN(MULTIPLY(LOCAL, 200), LOCAL + 20)

;陶酔中は汚れに鈍感になる
SIF LOCAL > 0 && COND("陶酔")
	LOCAL /= 2

LOCAL = MAX(LOCAL, 0)

RETURNF LOCAL

