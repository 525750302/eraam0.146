@WHOSE_PHOTO(ARGS)
#FUNCTION
#DIM LCOUNT

FOR LCOUNT, 2, CHARANUM
	;名前が最後の場合
	SIF SUBSTRINGU(ARGS, STRLENSU(ARGS) - STRLENSU(CALLNAME:LCOUNT), STRLENSU(CALLNAME:LCOUNT) ) == CALLNAME:LCOUNT
		RETURNF LCOUNT
	;名前が途中の場合
	SIF STRCOUNT(ARGS, @"%CALLNAME:LCOUNT%の")
		RETURNF LCOUNT
NEXT
