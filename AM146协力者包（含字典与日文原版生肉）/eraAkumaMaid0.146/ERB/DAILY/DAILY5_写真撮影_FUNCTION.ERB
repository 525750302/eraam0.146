			EVENT_TABLE += "ペニス見せつけ/"
		SIF TALENT:オトコ == 0
			EVENT_TABLE += "立ちくぱぁ/"
	ENDIF

	;日焼け跡と水着の形状が同じ
	SIF CHECK_CLO("水着姿") && CSTR:日焼け != "" && (TEXTS("日焼け跡") == @"%SWIM(CSTR:ボディスーツ, "種類")%" || TEXTS("日焼け跡") == @"%SWIM(CSTR:パンツ, "種類")%")
