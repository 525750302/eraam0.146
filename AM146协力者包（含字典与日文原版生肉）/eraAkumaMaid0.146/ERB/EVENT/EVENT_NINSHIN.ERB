;---------------------------------------------------------
;妊娠判定チェック
;シチュエーションARGSでARGが妊娠するかどうかを判定する。ARG:1は精液量。
;ARGSは"通常", "調教外", "一般人相手"
;SPERMで子宮に入る精子量、IS_FATHERで誰が父親になるかの番号管理を行う
;---------------------------------------------------------
@CHECK_PREGNANT, ARG, ARG:1, ARGS
#DIM SPERM
#DIM SPERM_DENS
#DIM IS_FATHER

;精液量がない場合
SIF ARG:1 <= 0
	RETURN 0

;調教中かどうかとPLAYERが一般人かどうかをチェック
SELECTCASE ARGS
;指定されている場合にはARGSの変化なし
CASE "一般人相手", "調教外", "通常"
CASEELSE
	IF TALENT:PLAYER:一般人
		ARGS = 一般人相手
	ELSEIF COND("調教中")
		ARGS = 通常
	ELSE
		ARGS = 調教外
	ENDIF
ENDSELECT

;SPERMで子宮に入る精子量、IS_FATHERで誰が父親になるかの番号管理を行う
SPERM = ARG:1 * (10 + RAND:21)
SPERM_DENS = 90 + RAND:21

SELECTCASE ARGS
CASE "一般人相手"
	;処女は判定しない
	SIF TALENT:ARG:処女
		RETURN 0
	IS_FATHER = -4
	;精液量は館のメンバーに比べると少ない
	SPERM = MAX(SPERM/2, 1)

CASEELSE
	SELECTCASE ARG
	;調教者の妊娠チェック
	CASE PLAYER
		IS_FATHER = TARGET

	;TARGETやASSI等の妊娠チェック
	CASEELSE
		SELECTCASE BASE:PLAYER:識別番号
		CASE IS > 0
			IS_FATHER = PLAYER
		CASEELSE
			IS_FATHER = -4
		ENDSELECT

		;絶頂を迎える事で確率上昇
		SIF NOWEX:ARG:絶頂数合計
			SPERM += MIN(NOWEX:ARG:絶頂数合計, 2)/2

		IF COND("子宮挿入フラグ", ARG)
			SPERM *= 10
		;挿入子宮口責めの時には妊娠率上昇
		ELSEIF SELECTCOM == 67
			TIMES SPERM, 1.20
		ENDIF
	ENDSELECT
ENDSELECT

;妊娠判定フラグ
BASE:ARG:胎内精液量 += SPERM * SPERM_DENS

;避妊中で妊娠不可能なら精子は受け入れない
SIF COND("避妊中", ARG) && COND("避妊中だが妊娠可能", ARG) == 0
	SPERM = 0
;人形だと生殖能力無し
SIF IS_FATHER > 0 && COND("種族：人形", IS_FATHER)
	SPERM = 0

;吸精体質の人は精子を頂いちゃう
SIF TALENT:ARG:吸精体質
	SPERM = MAX(MIN(1 + EX:ARG:Ｖ中出し*2, SPERM), SPERM/POWER(2, TALENT:ARG:吸精体質) )
;夜這いなどでの妊娠率を落とす
SIF COND("調教中", ARG) == 0
	TIMES SPERM, 0.66

BASE:ARG:胎内精子量 += SPERM

;父親一時フラグ
IF SPERM && BASE:ARG:胎児 == 0 && COND("妊娠確実", ARG) == 0 && (COND("避妊中", ARG) == 0 || COND("避妊中だが妊娠可能", ARG))
 	IF BASE:ARG:胎内精子量 == 0 || COND("精液の優勢", ARG) == 0 || RAND:3 == 0
 		SELECTCASE IS_FATHER
		CASE IS > 0
			BASE:ARG:精液の優勢 = BASE:IS_FATHER:識別番号
		CASEELSE
			BASE:ARG:精液の優勢 = IS_FATHER
		ENDSELECT
	ENDIF
ENDIF
