;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K14, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ", "朝シックスナイン"
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				CALL PRINT_STR, @"「%TEXTR("んっ……/じゅるっ/ぷはぁっ")%_H_ ……なんだよ%CNAME("一人称")%がしちゃワリぃのかよ？」_L"
				PRINTFORMW %CALLNAME:TARGET%はぺろりと口端についた精液を舐めとった。
			CASE 1
				CALL PRINT_STR, @"「%TEXTR("ぺろっ/んふっ/フフッ")%_H_ どーしたんだ？ %CNAME("主人")%_H_」_L"
				PRINTFORMW %CALLNAME:TARGET%は何事もなかったように朝支度を始めました。
			CASEELSE
				CALL PRINT_STR, @"「%TEXTR("ちゅる/ふはぁ/ん～")%_H_ なんだよ？ こーゆーの好きだろ？」_L"
				PRINTFORMW %CALLNAME:MASTER%が頷くと%CALLNAME:TARGET%は満足そうに片付けを始めた。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「……%TEXTR("んんっ/げほっ")%！？ な、なんだよ……起きてたのかよ……」
				PRINTFORMW %CALLNAME:TARGET%は%TEXTR("気まずそうに/バツが悪そうに/いそいそと")%片付け始めた。
			CASE 1
				PRINTFORML 「……%TEXTR("んんっ/げほっ")%！？ ……%CNAME("一人称")%が早く元に戻る為だから……そういう事だから……」
				PRINTFORMW そういう事らしいです。
			CASEELSE
				PRINTFORML 「……あ ……%CNAME("二人称")%は何も見ていない。%CNAME("一人称")%は何もしていない」
				PRINTFORMW %CALLNAME:TARGET%は足早に%CALLNAME:MASTER%の部屋から出ていきました。
			ENDSELECT
		ENDIF

	CASE "朝騎乗位"
		[IF_DEBUG]
			PRINTFORMW KOJO_GREETING_K14, %ARGS%, %CSTR:朝駆け%
		[ENDIF]
	ENDSELECT

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1

	IF COND("体育")
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「今日は体育があるぜ？」
			PRINTFORMW %CALLNAME:TARGET%は%TEXTR("イキイキ/ウキウキ")%している。
		CASE 1
			PRINTFORML 「今日は体育がある……けど、日差しが強すぎるな」
			PRINTFORMW %CALLNAME:TARGET%は太陽を恨みがましく睨み付けている。
		CASEELSE
			PRINTFORML 「体育があるから体操着忘れんなよ？」
			PRINTFORMW %CALLNAME:TARGET%が%CALLNAME:MASTER%の体操着入れを投げ渡してきた。
		ENDSELECT

	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「%TEXTR("ん……/ふあぁ、/くはぁあ、")%おはよ……」
			PRINTFORMW %CALLNAME:TARGET%は眠気眼をこすっている。
		CASE 1
			PRINTFORML 「ん……はよ」
			PRINTFORMW %CALLNAME:TARGET%は眠そうに朝の挨拶をしてきた。
		CASEELSE
			PRINTFORML 「もうメシ出来てるぜ？」
			PRINTFORMW %CALLNAME:TARGET%はお腹が空いているようです。
		ENDSELECT
	ENDIF

CASE "一番乗り"
	IF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「ん……%CNAME("二人称")%と二人っきりか」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の隣に座った。
		CASE 1
			PRINTFORML 「よっ！ ……って、なんだよ%CNAME("二人称")%だけかよ」
			PRINTFORMW %CALLNAME:TARGET%はそう言いつつもどこか嬉しそうだ。
		CASEELSE
			PRINTFORML 「ウイーっす……お、%CNAME("一人称")%だけか」
			PRINTFORMW %CALLNAME:MASTER%を確認した%CALLNAME:TARGET%は微笑んだ。
		ENDSELECT

	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「なんだ、%CNAME("二人称")%と二人かよ……」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%と少し距離を開けて座った。
		CASE 1
			PRINTFORML 「よっ！ ……って、%CNAME("二人称")%だけか」
			PRINTFORMW %CALLNAME:TARGET%は微妙そうな顔つきだ。
		CASEELSE
			PRINTFORML 「ん……ハァァアア」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の顔を見て盛大なため息を吐いた。
		ENDSELECT
	ENDIF

CASE "ただいま"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「ただいまぁ、腹減ったぁ～」
		PRINTFORMW %CALLNAME:TARGET%はバタバタと帰ってきた。
	CASE 1
		PRINTFORML 「はぁ～疲れた……おっと、ただいま」
		PRINTFORMW %CALLNAME:TARGET%は疲れた様子で帰ってきた。
	CASEELSE
		PRINTFORML 「ただいまっと、……今日は日差しがキツかった」
		PRINTFORMW %CALLNAME:TARGET%はダルそうに帰ってきた。
	ENDSELECT

CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「おかえりぃ……何だよ%CNAME("一人称")%が迎えに来ちゃ悪いのかよ！？」
				PRINTFORMW 玄関まで迎えに来た%CALLNAME:TARGET%は少し恥ずかしそうだ。
			CASE 1
				PRINTFORML 「お帰りなさいませ%CNAME("主人")%……なんつってな」
				PRINTFORMW メイド服姿の%CALLNAME:TARGET%が%CALLNAME:MASTER%を出迎えてくれた。
				IF FIND_COND("フェリシエル", "現在存在")
					PRINTFORML 「フェリシエルさんが用意してくれたんだけど……どうだ？」
				ELSE
					PRINTFORML 「意外と似合ってんだろ？」
				ENDIF
				PRINTFORMW %CALLNAME:TARGET%は%TEXTR("笑顔で/すまし顔で/腰をピンと伸ばしながら")%スカートを翻して一回りして見せた。
				CALL SET_COORDINATE, "メイド服", TARGET
			CASEELSE
				PRINTFORML 「おかえりぃ……寄り道してきたのか？」
				PRINTFORMW どうやら%CALLNAME:TARGET%は帰りが遅かった%CALLNAME:MASTER%を心配していたようだ。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ん～おかえりぃ～」
				PRINTFORMW %CALLNAME:TARGET%は居間でテレビを観ている。
			CASE 1
				PRINTFORML 「おかえりぃ～……あ、クソっ！？」
				PRINTFORMW %CALLNAME:TARGET%はゲームをしながら%CALLNAME:MASTER%にパタパタと手を振った。
			CASEELSE
				PRINTFORML 「おかえりぃ……なんだ寄り道してきたのか？」
				PRINTFORMW どうやら%CALLNAME:TARGET%はお土産を期待しているようだ。
			ENDSELECT
		ENDIF
	ELSE
		IF TALENT:恋慕
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「よっ！ なんだ遅かったじゃないか」
				PRINTFORMW %CALLNAME:TARGET%が出迎えてくれました。
			CASE 1
				PRINTFORML 「おう……%CNAME("二人称")%も食うか？」
				PRINTFORMW %CALLNAME:TARGET%は自分の持っていたお菓子を差し出してきた。
			CASEELSE
				PRINTFORML 「遅かったな？ そうだな……%TEXTR("先生に怒られたな/エロ本でも拾ったな/財布を落としたな")%？」
				PRINTFORMW %CALLNAME:TARGET%は遅れてきた%CALLNAME:MASTER%に的外れな推理を披露し始めた。
			ENDSELECT
		ELSE
			SELECTCASE RAND:3
			CASE 0
				PRINTFORML 「ふっふっ……」
				PRINTFORMW %CALLNAME:TARGET%は筋トレしているようだ。
			CASE 1
				PRINTFORML 「ん、……よぅ」
				PRINTFORMW %CALLNAME:TARGET%は片手間に挨拶してきました。
			CASEELSE
				PRINTFORML 「あ～……ダルい……」
				PRINTFORMW %CALLNAME:TARGET%は日差しを避けるようにダレている。
			ENDSELECT
		ENDIF
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「なんか用か？」
			PRINTFORMW %CALLNAME:MASTER%に呼び出された%CALLNAME:TARGET%が入室してきました。
		ELSE
			PRINTFORML 「……なんか用か？」
			PRINTFORMW %CALLNAME:MASTER%に呼び出された%CALLNAME:TARGET%が%TEXTR("ぶっきらぼうに/面倒くさそうに/あくびをしながら")%入室してきました。
		ENDIF

	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORML 「う～っス……ッケ、お盛んだねぇ」
		PRINTFORML 　%CALLNAME:TARGET%はイチャついてる%CALLNAME:MASTER%達を確認すると目を細めた。
		PRINTFORML 
		CFLAG:好感度 -= 20
		PRINTFORMW 　好感度 -20

	ELSE
		IF TALENT:恋慕
			PRINTFORML 「失礼するぜ」
			PRINTFORMW %CALLNAME:TARGET%が入室してきました。
		ELSE
			PRINTFORML 「……失礼するぜ」
			PRINTFORMW %CALLNAME:TARGET%が%TEXTR("ぶっきらぼうに/面倒くさそうに/あくびをしながら")%入室してきました。
		ENDIF
	ENDIF
	
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「ちわッス」
	ELSEIF CFLAG:呼び出された
		PRINTFORMW 「ちわッス」
	ELSE
		PRINTFORMW 「ちわッス」
	ENDIF
	
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		PRINTFORMW 『あ……先生こんちわッス』

	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 『ちわッス』
		
	ELSE
		PRINTFORMW 『ちわッス』
	ENDIF

CASE "居間に現れた"
	IF CFLAG:呼び出された
		IF TALENT:恋慕
			PRINTFORML 「なんか用か？」
			PRINTFORMW %CALLNAME:MASTER%に呼び出された%CALLNAME:TARGET%が入室してきました。
		ELSE
			PRINTFORML 「……なんか用か？」
			PRINTFORMW %CALLNAME:MASTER%に呼び出された%CALLNAME:TARGET%が%TEXTR("ぶっきらぼうに/面倒くさそうに/あくびをしながら")%入室してきました。
		ENDIF

	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		PRINTFORMW 「あー……喉乾いた」
	ELSE
		PRINTFORMW 「あー……喉乾いた」
	ENDIF

CASE "抜け出された"
	IF TALENT:恋慕
		CALL EMOTION_PLUS, "不機嫌", TARGET
		PRINTFORML 「……色男は良いねぇ」
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%を軽蔑した目で見送った。
		PRINTFORML
		CFLAG:好感度 -= 20
		PRINTFORMW 　好感度 -20
	ELSE
		PRINTFORML 「あ？ 今日はもう終わりで良いのか？」
		PRINTFORMW %CALLNAME:TARGET%は%TEXTR("いそいそと/手早く")%帰り支度を始めました。
	ENDIF

CASE "お弁当"
	SELECTCASE RAND:7
	CASE 0
		PRINTFORML 「全く……%CNAME("一人称")%に弁当なんて頼むなよ」
		PRINTFORMW 　%CALLNAME:TARGET%と全体的に茶色い弁当を一緒に食べました。
	CASE 1
		PRINTFORML 「悪いが今日は簡単なモノだぜ？」
		PRINTFORMW 　%CALLNAME:TARGET%と手作りサンドイッチを一緒に食べました。
	CASE 2
		PRINTFORML 「コレは実質フェリシエルさんの弁当だな」
		PRINTFORMW 　%CALLNAME:TARGET%と昨日の残り物が詰められた弁当を一緒に食べました。
	CASE 3
		PRINTFORML 「うぅ……お腹いっぱいだ……胃が小さくなってんのか？」
		PRINTFORML 　%CALLNAME:TARGET%は途中まで食べてお腹いっぱいになってしまったらしい。
		PRINTFORMW 「……ほら、あーん」
	CASE 4
		PRINTFORML 「今日は偶々早く起きたからな」
		PRINTFORMW 　%CALLNAME:TARGET%の手作り弁当を一緒に食べました。
	CASE 5
		PRINTFORML 「%CNAME("二人称")%は料理とかしないのか？」
		PRINTFORMW 　%CALLNAME:TARGET%と簡素な海苔弁当を一緒に食べました。
	CASEELSE
		PRINTFORML 「男料理なら多少は出来んだがなぁ……」
		PRINTFORMW 　%CALLNAME:TARGET%と日の丸弁当を一緒に食べました。
	ENDSELECT

;同じ部屋にいる他の人がＨを開始した際のセリフです
CASE "他の人がＨ開始"
	IF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			CALL EMOTION_PLUS, "びっくり", TARGET
			PRINTFORML 「ちょっ！？ いきなりおっぱじめんな！」
			PRINTFORMW %CALLNAME:TARGET%は%TEXTR("慌てて/急いで")%退出しました。
			TFLAG:退出する = 1
		CASE 1
			CALL EMOTION_PLUS, "不機嫌", TARGET
			PRINTFORML 「ふーん……ほー……へぇー」
			PRINTFORMW %CALLNAME:TARGET%の視線が%TEXTR("冷たいです/痛いです/突き刺さります")%。
		CASEELSE
			PRINTFORML 「はいはい、いつものね……いや、慣れてしまった自分が嫌だわ……」
			PRINTFORMW %CALLNAME:TARGET%は複雑そうな顔をしている。
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			CALL EMOTION_PLUS, "びっくり", TARGET
			PRINTFORML 「ちょっ！？ いきなりおっぱじめんな！」
			PRINTFORMW %CALLNAME:TARGET%は%TEXTR("慌てて/急いで")%退出しました。
			TFLAG:退出する = 1
		CASE 1
			CALL EMOTION_PLUS, "びっくり", TARGET
			PRINTFORML 「……本当に節操がねぇな！？」
			PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%にツッコミを入れました。
		CASEELSE
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORML 「あー……うん、外出てるわ」
			PRINTFORMW %CALLNAME:TARGET%は%TEXTR("そわそわと/顔を赤らめて")%退出しました。
			TFLAG:退出する = 1
		ENDSELECT
	ENDIF

ENDSELECT

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K14, ARGS
#DIM MEMO_LINECOUNT
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	PRINTFORML 「うぅ……あくまで%CNAME("一人称")%が元に戻るまでだぞ！」
CASE "汗だく"
	IF COND("夏")
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「死ぬ……死ぬっ！」
			PRINTFORML 「太陽と暑さのタブルパンチで%TEXTR("溶ける/灰になる/普通に死ぬ")%ッ！！」
		CASE 1
			PRINTFORML 「ああっづぅう～～～」
			PRINTFORML 「……氷うま……氷うま」
		CASEELSE
			PRINTFORML 「汗ヤバい……」
			PRINTFORML 「……なんだよ？　暑いんだからアッチ行けよ。シッシッ！」
		ENDSELECT
	ELSEIF TALENT:恋慕
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「あぁ～良い汗かいたぜ」
			PRINTFORML 「なんだぁ？ %CNAME("二人称")%は%TEXTR("へばっちまった/疲れちまった")%のか？」
		CASE 1
			PRINTFORML 「今日は日差しが強すぎだ……」
			PRINTFORML 「コレさえなければなぁ……」
		CASEELSE
			PRINTFORML 「……汗臭いな」
			PRINTFORML 「%TEXTR("制汗剤/ボディペーパー/タオル")%はっと……汗臭いんだから近寄るなよ？」
		ENDSELECT
	ELSE
		SELECTCASE RAND:3
		CASE 0
			PRINTFORML 「あぁ～良い汗かいたぜ」
			PRINTFORML 「%CNAME("二人称")%は%TEXTR("運動嫌いな/へばっちまった/疲れちまった")%のか？ ……情けないなぁ」
		CASE 1
			PRINTFORML 「今日は日差しが強すぎだ……」
			PRINTFORML 「早く戻りたいぜ……」
		CASEELSE
			PRINTFORML 「……汗臭いな」
			PRINTFORML 「%TEXTR("制汗剤/ボディペーパー/タオル")%はっと……」
		ENDSELECT
	ENDIF

CASE "寝起き"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「むにゃむにゃ……」
		PRINTFORML 「吸血鬼だから……昼に眠い……」
	CASE 1
		PRINTFORML 「んん……あと五分……」
		PRINTFORML 「ZZzz……すぐ起きるから……」
	CASEELSE
		IF TALENT:相思相愛
			PRINTFORML 「ハァ……夢か……」
			PRINTFORML 「……起きた時に男に戻ってるんじゃないかって最近怖くなるんだ」
		ELSEIF TALENT:恋慕
			PRINTFORML 「んん……あぁ～……%CNAME("二人称")%か」
			PRINTFORML 「はぁ……嫌な夢を見たぜ……」
		ELSE
			PRINTFORML 「んん……ふぁあ……」
			PRINTFORML 「……ハァ……起きたら戻ってるって事はねーか」
		ENDIF
	ENDSELECT
	
CASE "コスプレ中"
	SELECTCASE RAND:3
	CASE 0
		PRINTFORML 「なんで%CNAME("一人称")%がこんなことを……」
		PRINTFORML 「%CNAME("二人称", "どもり")%……%TEXTR("な、殴りてぇ/殴って良いか？/ぶっ飛ばすぞ？")%」
	CASE 1
		PRINTFORML 「な、何で%CNAME("一人称")%がこんな格好を……」
		PRINTFORML 「コ、コラッ！？ %TEXTR("撮影禁止だ/動画を撮るな")%！！」
	CASEELSE
		PRINTFORML 「……」
		PRINTFORML 「%CNAME("一人称")%の尊厳が死んでしまう……」
	ENDSELECT

;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K14
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

;前回コマンド対応台詞
SELECTCASE CONDS("直前行動")
CASE "愛の告白"
	PRINTFORML 「何笑ってんだよバーカ♪」

CASE "デートの約束"
	PRINTFORML 
;これは悪魔のみ
CASE "契約成立"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K14, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "抜け出す"
	PRINTFORML 
CASE "あなたの部屋に連れ込まれた"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K14, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "自室に招待"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K14, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 
	ELSE
		PRINTFORML 
	ENDIF
CASE "アドバイザー部屋訪問"
	PRINTFORML 
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 
CASE "オナバレ"
	PRINTFORML 
CASE "ミニスカパンチラ"
	[IF_DEBUG]
		PRINTFORML KOJO_DAILY_K14, %CSTR:PREV_DAILYCOM%後
	[ENDIF]
CASE "ローライズヒップ"
	PRINTFORML 
CASE "スカートをめくる"
	IF COND("合意：スカートめくり") == 0
