;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K41, ARGS

#FUNCTIONS

VARSET LOCALS

SELECTCASE ARGS
CASE "一人称"
	LOCALS = 私_わたし
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		LOCALS = %CALLNAME:MASTER%_%CALLNAME:MASTER%
	;相手が主人ではない
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "２年生", "１年生", "初等部", "中等部"
			LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
		CASE "３年生", "研究生", "ＯＧ"
			LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
		CASE "不法侵入", "備品"
			LOCALS = %CALLNAME:PLAYER%_%CSTR:PLAYER:ふりがな%
		CASEELSE
			IF COND("教員", PLAYER)
				LOCALS = %CALLNAME:PLAYER%先生_%CSTR:PLAYER:ふりがな%せんせい
			ELSE
				LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = あの人_あのひと
CASE "助手"
	SELECTCASE CSTR:ASSI:学年
	CASE "２年生", "１年生", "初等部", "中等部"
		LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
	CASE "３年生", "研究生" ,"ＯＧ"
		LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
	CASE "不法侵入", "備品"
		LOCALS = %CALLNAME:ASSI%_%CSTR:ASSI:ふりがな%
	CASEELSE
		IF COND("教員", ASSI)
			LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
		ELSE
			LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
		ENDIF
	ENDSELECT
ENDSELECT
RETURNF LOCALS

