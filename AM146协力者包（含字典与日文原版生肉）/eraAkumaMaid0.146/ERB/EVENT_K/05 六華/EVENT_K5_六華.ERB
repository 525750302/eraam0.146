	IF RAND:3 == 0
		PRINTFORMW 「%CNAME("二人称")%がこうして求めてくれて、%CALLNAME%は嬉しいのじゃ」
		CALL PRINT_STR, @"「さぁ%CNAME("二人称")%、%CNAME("一人称")%の身体をたっぷりとご堪能あれ…_H_」_W"
	ELSEIF RAND:2 == 0
		CALL PRINT_STR, @"「%CNAME("二人称")%……_H_」_W"
		PRINTFORMW 「%CALLNAME%は%CNAME("二人称")%のことを考えるだけで、%CNAME("一人称")%はどうにかなってしまいそうなのじゃ」
		PRINTFORMW 「その責任、取らずに逃げればひどいのじゃぞ」
	ELSE
		CALL PRINT_STR, @"「%CNAME("二人称")%……ふふふ、%CNAME("二人称")%のこと、%CNAME("一人称")%はと～っても好いておるのじゃ_H_」_W"
		PRINTFORMW 「今からそれを、たっぷりとその身体に教えてやるからの…」
		PRINTFORMW 「もっとも、逆に%CNAME("一人称")%の方が%CNAME("二人称")%やり込められてしまうやも知れぬがな」
	ENDIF
ELSE
