;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K32, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

$RYOKO_GREETING_LP1
MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
CASE "朝拘束"
	RETURN 0
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください
CASE "朝駆け"
	RETURN 0
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"

	CASE "朝パイズリ"

	CASE "朝騎乗位"

	CASE "朝Ａ騎乗位"

	CASE "朝キス"

	CASE "朝添い寝"

	CASE "朝フェラ", "朝シックスナイン"

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
	SELECTCASE CSTR:朝駆け
	CASE "朝フェラ"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「朝からこんなにするなんて……もう……_BH_」_W"
				CALL PRINT_STR,  "「スッキリさせてあげるわね……んちゅ_BH_」_W"
		ENDSELECT
	CASE "朝騎乗位"
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STR,  "「んっ_BH_　ぁっ_BH_　んっ_BH_　んん～～_BH_」_W"
				CALL PRINT_STR,  "「ぁ、ん、も、もう少し、寝ててもいい……_BH_　わよ_BH_」_W"
				CALL PRINT_STR,  "「勝手、に_BH_　楽しませて、もらう、から、ぁ、ぁあっ_BH_　おぅっ_BH_」_W"
				PRINTFORMW %CALLNAME:PLAYER%に跨った%CNAME("玲子")%は大股開きで結合部を見せつけながら貪欲に快楽を貪っている……
				CALL PRINT_STR, @"（ぅぅぅ_BH_　朝、朝なのにすごい、しゅうごいガチガチ%CNAME("ペニス")%……_BH_）_W"
				CALL PRINT_STR, @"（こんなの、少し、少しでいいから_BH_　ちょっとだけでいいから_BH_　朝イチで%TEXTR("絞って/ヌイて")%あげなきゃ_BH_　きっと苦しい、よね_BH_）_W"
				CALL PRINT_STR, @"「ぅぅ_BH_　%CNAME("ペニス")%%TEXTR("ぷるぷる/びくびく")%してきたぁ……_BH_」_W"
				CALL PRINT_STR, @"「だして_BH_　このまま出して_BH_　射精して_BH_　朝勃ち%CNAME("ペニス")%の新鮮%CNAME("精液")%_BH_　いっぱいどぷどぷして_BH_」_W"
				CALL PRINT_STR,  "「ぅ、うう～～～～_BH_　お～……_BH_　おおー……_BH_」_W"
		ENDSELECT
	CASEELSE
		RETURN 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「あら、遅かったわね」"
				CALL PRINT_STRW,  "「どこか寄り道でもしてきたの？」"
		ENDSELECT
	ENDSELECT

CASE "入室挨拶"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW,  "「失礼します」"
		CASE 1
			CALL PRINT_STRW,  "「遅くなっちゃった……ごめんなさいね」"
	ENDSELECT
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0 && CHARANUM > 6
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「……あら？　ひょっとして結構重役出勤しちゃったかしら」"
				CALL PRINT_STRL,  "（重役っていうかお局よね、正確には……はぁ）"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「ごめんなさい、ちょっと遅れちゃったわね」"
		ENDSELECT
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, "『あら、ご一緒だったんですか？』"
		ENDSELECT

	;相手が現れるのが遅かった（既に部員の４分の３が集合済）
	ELSEIF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "『遅かったわね。みんなもう揃ってるわよ』"
			CASE 1
				CALL PRINT_STRW,  "『どこかに寄ってきたの？』"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW, @"『こんにちは、%CALLNAME_ACTOR%』"
		ENDSELECT
	ENDIF

CASE "居間に現れた"
	;一度居間から別の場所に行って、居間に帰って来た時
	IF CMOVE("居間")
		PRINTFORMW 
	ELSE
		PRINTFORMW 
	ENDIF
CASE "抜け出された"
	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRW,  "「…………意地悪ね、もう」"
				CALL PRINT_STRW,  "（せめてそっと出て行ってくれないかしら……）"
			CASE 1
				CALL PRINT_STRW, @"「お盛んなのは%CNAME("一人称")%も重々承知してるけど……」"
				CALL PRINT_STRW, @"「……%CNAME("一人称")%の分も残しておいてね？_／／／_」"
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_GREETING_LP1
	ENDIF

	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRW, @"「あら、二人でお出かけ？　%CNAME("一人称")%は置いてけぼりかしら」"
			CALL PRINT_STRW,  "（これは……隠れて着いて行くべきかしら？　でもバレたら拙いわね……）"
		CASE 1
			CALL PRINT_STRW,  "「あまり羽目を外しすぎないようにね」"
	ENDSELECT

CASE "お弁当"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR, @"「はい、%TEXTR("サンドイッチ/カレーパン/あんパン/クロワッサン")%。……あ、お弁当の方が良かった？」_W"
			CALL PRINT_STR,  "「そう？　こっちのが食べたかったら言ってね。……ふぅ」_W"
			PRINTFORM コンビニのレジ袋から未開封の%TEXTR("サンドイッチ/カレーパン/あんパン/クロワッサン")%を取り出して%CALLNAME:PLAYER%に手渡した%CNAME("玲子")%は、
			PRINTFORMW %CALLNAME:PLAYER%の隣に座ると自分の分の%TEXTR("サンドイッチ/カレーパン/あんパン/クロワッサン")%を取り出した。
			CALL PRINT_STR,  "「…………はぁ」_W"
			IF TALENT:MASTER:オトコ
				CALL PRINT_STR,  "（制服着て、異性と一緒にお弁当、かぁ……）_W"
			ELSE
				CALL PRINT_STR,  "（制服着て、女の子と一緒にお弁当、かぁ……）_W"
			ENDIF
			CALL PRINT_STR,  "（父さんの言う通り高校通ってたらこういう機会もあったのかなぁ……）_W"
			PRINTFORMW どこか物悲しげな雰囲気の%CNAME("玲子")%だが、一緒にいるのを嫌がっているわけではなさそうだ……
		CASE 1
			CALL PRINT_STR,  "「色々買ってきたけど、何が食べたい？」_W"
			PRINTFORMW %CNAME("玲子")%は手に提げたコンビニのレジ袋から取り出したサンドイッチやおにぎりを%CALLNAME:PLAYER%の前に並べていく……
			CALL PRINT_STR,  "「あら、多かった？　でも午後も体力使うんでしょう？」_W"
			CALL PRINT_STR,  "「……べ、別に間違ったことは言ってないわよ」_W"
		CASE 2
			CALL PRINT_STR,  "「今日はちょっと甘いもの多めね。ホント、糖分が足らなくて……」_W"
			CALL PRINT_STR,  "「……さっきの授業で分からない所があったんだけど、食べながら聞いてもいい？」_W"
			CALL PRINT_STR,  "（中学卒業してからすぐに修行と研修ばっかりだったから正直授業に着いていくの辛いのよね……）_W"
	ENDSELECT
CASEELSE
	DEBUGPRINTFORM KOJO_GREETING_K32, ARGS:%ARGS%
	RETURN 0
ENDSELECT

SIF MEMO_LINECOUNT - LINECOUNT
	RETURN 1
GOTO RYOKO_GREETING_LP1

;-------------------------------------------------
;メイン画面での台詞。いわゆる一言イベント強化版
;ちなみに画面リロードしても同じセリフを喋ります
;選択肢に挟まれた場所で出力するのでWAITの使用は禁止
;-------------------------------------------------
@KOJO_DAILY_K32, ARGS
#DIM MEMO_LINECOUNT

$RYOKO_DAILY_LP1
MEMO_LINECOUNT = LINECOUNT

DEBUGPRINTFORML %__FUNCTION__%:l.{__LINE__}:%ARGS%

SELECTCASE ARGS
;初回のセリフ
CASE "初回台詞"
	IF CHARA_NUM("室内の邪魔者")
		CALL PRINT_STR,  "「あら、こんにちは。はじめまして」_L"
		CALL PRINT_STR, @"「今日から%CNAME("一人称")%もさ、ささ……竿姉妹のお仲間だから、よろしくね」_L"
	ELSE
		CALL PRINT_STR,  "「ふぅん……結構片付いているのね。もっと散らばっているかと思ったわ」_L"
		CALL PRINT_STR,  "「だってヤリ部屋……じゃなくて愛の巣……でもなくて、部室、そう部室ってそういうものじゃない？」_L"
	ENDIF
CASE "汗だく"
	SELECTCASE RAND:3
		CASE 0
			CALL PRINT_STRL, "「…………？　どうかした？」"
			PRINTFORML %CNAME("玲子")%は汗で湿った生地が肌に吸い付き透けているのに気付かず、怪訝な顔で額の汗を拭いている……
		CASE 1
			CALL PRINT_STRL, "（こんなに汗だくになるのも久しぶりね……ちょっと本気出しちゃった）"
			CALL PRINT_STRL, "（訓練にはまだちょっと物足りないけど、やらないよりマシね）"
	ENDSELECT
	SIF LINECOUNT - MEMO_LINECOUNT
		RETURN 1
	GOTO RYOKO_DAILY_LP1
CASE "寝起き"

	; 特殊派生
	IF CHOSE_DEVENT("睡眠姦中出し")
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL, @"（…………なんだかすごい%TEXTR("過激な/えっちな")%夢を見た気がするわ）"
				CALL PRINT_STRL,  "「…………ん_BH_BH_BH_」"
				CALL PRINT_STRL, @"（や、やだ_BH_　なんでこんなに%TEXTR("濡れてる/盛ってる")%のよ、%CNAME("一人称")%……_BH_）"
				CALL PRINT_STRL,  "（まさか……欲求不満とか……？　若い子の性欲に付き合ってるうちに？　う、嘘でしょ……？_／／／_）"
				CALL PRINT_STRL,  "「……ぅー」"
				PRINTFORML %CNAME("玲子")%は恥ずかしそうに顔を赤らめてちらちらと%CALLNAME:PLAYER%を見ている……
		ENDSELECT
		SIF LINECOUNT - MEMO_LINECOUNT
			RETURN 1
		GOTO RYOKO_DAILY_LP1
	ENDIF

	; 汎用
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STRL,  "「………………」"
			PRINTFORML %CNAME("玲子")%は焦点の定まらない寝ぼけ眼のまま自分の胸を枕にして机に突っ伏している……
		CASE 1
			CALL PRINT_STRL,  "「ぅ……ん……。ふわぁあ……」"
			PRINTFORML %CNAME("玲子")%は間の抜けた顔でお大きな欠伸をした……
	ENDSELECT
CASE "コスプレ中"
	PRINTFORML 
;眠っている
CASE "睡眠"
	;寝言
	CALL KOJO_SLEEP_K32
	;寝言用の汎用地の文呼出
	CALL DAILY_MESSAGE, "寝言"
ENDSELECT

;何か発言してたらここまで
SIF LINECOUNT > MEMO_LINECOUNT
	RETURN 1

$RYOKO_DAILY_LP2

;前回コマンド対応台詞
SELECTCASE CSTR:PREV_DAILYCOM
CASE "愛の告白"
	CALL PRINT_STRL, @"（お役目失格よね、%CNAME("一人称")%……）"
	CALL PRINT_STRL,  "（でも全然イヤじゃないというか……えへへ……_／／／_）"
	PRINTFORML %CNAME("玲子")%はどこか上の空で幸せそうに微笑んでいる……
CASE "デートの約束"
	IF TALENT:恋慕
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "「～～～～♪」"
				PRINTFORML %CNAME("玲子")%は上機嫌だ……
			CASE 1
				CALL PRINT_STRL,  "（少しくらいは……おめかしした方がいいかしら）"
				CALL PRINT_STRL,  "（下着も新品下ろして……美容室に行く時間はあるかしら？）"
		ENDSELECT
	ELSE
		SELECTCASE RAND:5
			CASE 0
				CALL PRINT_STRL,  "（デート……デートかぁ……）"
				CALL PRINT_STRL,  "（なにか備えておいた方がいいのかしら……？）"
			CASE 1
				CALL PRINT_STRL, @"（%CNAME("一人称")%とデートって……そういうコトもするのよね、勿論）"
				CALL PRINT_STRL, @"（%TEXTR("エッチな/新品の/過激な")%下着にしたほうがいいのかしら……？）"
		ENDSELECT
	ENDIF
	SIF LINECOUNT == MEMO_LINECOUNT
		GOTO RYOKO_DAILY_LP2

;これは悪魔のみ
CASE "契約成立"
	PRINTFORML 
CASE "抜け出す"
	PRINTFORML 
CASE "あなたの部屋に連れ込まれた"
	IF !CEVENT("初回：部屋連れ込み")
		CALL PRINT_STR,  "「お邪魔します……わね？」_L"
		CALL PRINT_STR,  "「あら……意外と片付いているのね。ひょっとしてメイドさん任せなのかしら？」_L"
		CALL PRINT_STR, @"（……と言うか、そうでもないとひょっとして%CNAME("一人称")%の部屋よりも片付いているんじゃ）_L"
		CALL PRINT_STR,  "「う、ううん。なんでもないわ。ところでわざわざ場所を変えて――」_L"
		CALL PRINT_STR, @"（あら？　ひょっとして%CNAME("一人称")%、異性の部屋に入るのって初めて……？）_L"
		CALL PRINT_STR,  "「～～～～っ_／／／_」_L"
		CALL PRINT_STR, @"（お、落ち着くのよ%CNAME("一人称")%……！　あ、あんなことまでしておいて今更部屋に連れ込まれるのがなんだって言うのよ）_L"
		
		CALL SET_CEVENT, "初回：部屋連れ込み"
		RETURN 1
	ENDIF

	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR,  "（結構片付いているのよね……）_L"
			IF TALENT:MASTER:オトコ
				CALL PRINT_STR,  "（この年齢くらいの男の子の部屋って、もっとごちゃごちゃしてる印象があったんだけど……）_L"
			ELSE
				CALL PRINT_STR,  "（この年齢くらいの女の子の部屋って、もっと可愛いものがいっぱいある印象があったんだけど……）_L"
			ENDIF
			CALL PRINT_STR,  "「……ん？　いいえ、なんでもないわ。気にしないで」_L"
		CASE 1
			CALL PRINT_STR,  "（このベッド……いつもここで寝てるのよね？）_L"
			CALL PRINT_STR,  "（と言うか、多分、女の子とスる時もここで……_／／／_）_L"
			CALL PRINT_STR,  "（ひょ、ひょっとしていきなり押し倒されちゃったりするのかしら……？_／／／_）_L"
	ENDSELECT

CASE "自室に招待"
	IF !CEVENT("初回：自室に招待")
		CALL PRINT_STR,  "「あまり片付いてなくてごめんなさいね」_L"
		CALL PRINT_STR,  "（ヤバかったぁ……見られたらマズイものは全部隠せたわよね……？）_L"
		CALL PRINT_STR,  "「ちょっと、あんまりじろじろ見ないでよ。下着ぐらいしか転がってないわよ？」_L"
		CALL PRINT_STR,  "（いつも使ってるバイブはちゃんと隠したし……き、昨日脱ぎ捨てたぱんつくらいはご愛嬌よね）"

		CALL SET_CEVENT, "初回：自室に招待"
		RETURN 1
	ENDIF

	SELECTCASE RAND:5
		CASE 0
		IF TALENT:MASTER:オトコ
			CALL PRINT_STR,  "「女性の部屋にお邪魔してるんだから、もっと緊張したら？」_L"
			CALL PRINT_STR, @"（%CNAME("一人称")%だけ恥ずかしがってるとか、不公平じゃない……）_L"
		ELSE
			CALL PRINT_STR,  "「同じ女の子と言ってもお邪魔してるんだから、もっと緊張したら？」_L"
			CALL PRINT_STR, @"（%CNAME("一人称")%だけ恥ずかしがってるとか、不公平じゃない……）_L"
		ENDIF
		CASE 1
			CALL PRINT_STR,  "「不幸中の幸い、ね。まだ片付いているときで良かったわ」_L"
			CALL PRINT_STR,  "「……その目は止めて。片付いてるのよ。これでも_／／／_」_L"
	ENDSELECT

CASE "自室に訪問された"
	;アドバイザーを伴っている場合
	IF FIND_COND("現在存在", "助手")
		PRINTFORML 
	ELSE
		PRINTFORML 
	ENDIF
CASE "アドバイザー部屋訪問"
	PRINTFORML 
;自宅移動の後にあなたに連れられて居間に戻ってきた場合など。優先度は低いです
CASE "帰還"
	PRINTFORML 
CASE "オナバレ"
	PRINTFORML 
CASE "ミニスカパンチラ"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR,  "「……こら。見たでしょ、いま」_L"
			CALL PRINT_STR,  "「謝れとは言わないけど……その、見れて嬉しかった？」_L"
			CALL PRINT_STR, @"（って、何聞いてるのよ%CNAME("一人称")%……）_L"
		CASE 1
			CALL PRINT_STR,  "「ぇ……？　見え……？」_L"
			CALL PRINT_STR,  "「～～～～_／／／_」_L"
			CALL PRINT_STR,  "（ああもう……！　短すぎなのよこのスカート……！）_L"
	ENDSELECT
CASE "ローライズヒップ"
	PRINTFORML 
CASE "スカートをめくる"
	SELECTCASE RAND:5
		CASE 0
			CALL PRINT_STR,  "「きゃぁっ！？　……ぅ、ぅぅ_／／／_」_L"
			CALL PRINT_STR, @"「……スカートの中が見たいなら%TEXTR("ちゃんと/そう")%%TEXTR("言って/言いなさい")%よね」_L"
			CALL PRINT_STR, @"「見せてあげないとは言ってないんだから……ほら」_L"
			PRINTFORML %CNAME("玲子")%は自ら%SKIRT(TARGET, "略")%の裾を掴んでたくし上げ、%PANTIES(TARGET)%をじっくりと%TEXTR("堪能させて/楽しませて/観察させて")%くれた……
			CALL PRINT_STR,  "「……ほら、もういい？　いいでしょ？」_L"
			CALL PRINT_STR,  "（これすっごい恥ずかしい……_／／／_　でも、不意打ちされるよりマシね……）_L"
		CASE 1
			CALL PRINT_STR,  "「もう……いつまで見てるのよ……_／／／_」_L"
			CALL PRINT_STR,  "「女の子のぱんつなんて見慣れてるんじゃないの……？」_L"
			CALL PRINT_STR,  "「これはこれで？　……もう_／／／_」_L"
	ENDSELECT
CASE "買い物する"
	;買ってきたアイテムなどで分岐
	;IF COND("巨大バイブ購入")

	;	CALL SET_DEVENT, "巨大バイブ感想"
	;ELSE
		;単純にいっぱい買ったかどうかで分岐
		SELECTCASE COND("買上品数")
		CASE IS >= 4

		CASEELSE

		ENDSELECT
	;ENDIF
CASE "隠し撮り失敗"
	PRINTFORML 
CASE "裸になった"
	;他に人がいる
	IF CHARA_NUM("室内の邪魔者")
		PRINTFORML 
	;まさかの野外
	ELSEIF PLACE("野外")
		PRINTFORML 
	;個室ではない
	ELSEIF PLACE("個室") == 0
		PRINTFORML 
	;個室で二人きり
	ELSE
		PRINTFORML 
