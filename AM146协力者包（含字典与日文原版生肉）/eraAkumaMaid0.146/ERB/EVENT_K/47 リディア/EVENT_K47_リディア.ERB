;-------------------------------------------------
;キャラ追加の手順（必読！）
;１：EVENT_K47_名前.ERBとEVENT_K47_SHOP.ERBとEVENT_K47_DAILY.ERBとCHARA47.csvのファイル名と内部の47をキャラ番号に変更し、名前部分をキャラ名にする

;２：変更後のERBファイルをEVENT_Kフォルダに、CHARA47.csvをCSVフォルダに入れる

;３：他のキャラを参考にCHARA47.csv内部の初期設定を記述する
;番号,名前,呼び名,基礎は必ず全部埋めましょう。CSTRも可能な限り埋めた方が良いです
;CSTR,パンツ～アクセサリは初期衣装なので書かないと初期が全裸です

;４：このファイル内にある、@EVENT_PROFILE_K47を記述する
;CASE "勧誘直後"は別に無くても構いません
;CASE "勧誘前"と"紹介文"は有った方が良いです

;５：CALL_NAME.ERB内の
;CASE 47
;	STR:基本呼び名 = %CNAME_K47(ARGS)%
;の２行先頭の ; を削除する。

;６：以上でキャラ追加の準備が整います
;本編の日替わりタイミングでこんな人が居ますよ～って紹介するイベントが有りますが、それを見ないでもメイン画面[301]で勧誘できます

;７：あとはERBファイルに書けば書くだけ喋るようになります

;-------------------------------------------------
;呼び名の設定。"_"の右にふりがなを書く。
;-------------------------------------------------
@CNAME_K47, ARGS
#FUNCTIONS
#DIM ACTOR
VARSET LOCALS

LOCALS =

SELECTCASE ARGS
CASE "一人称"
	IF ABL:信頼 < 3
		LOCALS = リディア_りでぃあ
	ELSE
		LOCALS = 私_わたし
	ENDIF
CASE "二人称"
	;相手が主人
	IF PLAYER == MASTER
		IF ABL:信頼 < 3
			LOCALS = %CALLNAME:MASTER%さん_%CALLNAME:MASTER%さん
		ELSEIF TRUE_SEX(MASTER) != "オトコ"
			LOCALS = %CALLNAME:MASTER%_%CALLNAME:MASTER%
		ELSE
			LOCALS = 君_きみ
		ENDIF
	;相手が主人ではない
	;初等部の子は多分年下だと思ってる
	ELSE
		SELECTCASE CSTR:PLAYER:学年
		CASE "初等部"
			;仲良くなったら呼び捨てにしたいが保留
			IF TALENT:PLAYER:オトコ
				LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
			ENDIF

		CASE "教師"
				LOCALS = %CALLNAME:PLAYER%先生_%CSTR:PLAYER:ふりがな%せんせい
		CASEELSE
		;一応すでに千紗がいるし学校行ってない幼い子＝未成熟組は初等部と同じ扱いをしておく。小柄2～3で判定しようかと思ったが体（見た目は）は子供、頭脳は大人な方がいっぱいいるので止めておこう
			IF TALENT:未成熟
				IF TALENT:PLAYER:オトコ
					LOCALS = %CALLNAME:PLAYER%くん_%CSTR:PLAYER:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:PLAYER%ちゃん_%CSTR:PLAYER:ふりがな%ちゃん
				ENDIF
			ELSE
				LOCALS = %CALLNAME:PLAYER%さん_%CSTR:PLAYER:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
;"主人"と"助手"は三人称。主に当人と話していない時に使う
CASE "主人"
	LOCALS = ご主人様_ごしゅじんさま
CASE "助手"
		SELECTCASE CSTR:ASSI:学年
		CASE "初等部"
			;仲良くなったら呼び捨てにしたいが保留
			IF TALENT:ASSI:オトコ
				LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
			ENDIF

		CASE "教師"
			LOCALS = %CALLNAME:ASSI%先生_%CSTR:ASSI:ふりがな%せんせい
		CASEELSE
		;一応すでに千紗がいるし学校行ってない幼い子＝未成熟組は初等部と同じ扱いをしておく。小柄2～3で判定しようかと思ったが体（見た目は）は子供、頭脳は大人な方がいっぱいいるので止めておこう
			IF TALENT:未成熟
				IF TALENT:ASSI:オトコ
					LOCALS = %CALLNAME:ASSI%くん_%CSTR:ASSI:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ASSI%ちゃん_%CSTR:ASSI:ふりがな%ちゃん
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ASSI%さん_%CSTR:ASSI:ふりがな%さん
			ENDIF
		ENDSELECT
CASEELSE
;動いてるはず
	IF ISNUMERIC(ARGS)
		ACTOR = TOINT(ARGS)
	ELSEIF FIND_NO(ARGS) > 0
		ACTOR = FIND_NO(ARGS)
	ENDIF
	IF ACTOR == MASTER
		IF TALENT:ACTOR:オトコ
			IF ABL:信頼 < 3
				LOCALS = %CALLNAME:MASTER%さん_%CALLNAME:MASTER%さん
			ELSEIF TRUE_SEX(MASTER) != "オトコ"
				LOCALS = %CALLNAME:MASTER%_%CALLNAME:MASTER%
			ELSE
				LOCALS = 君_きみ
			ENDIF
		ELSE
			IF ABL:信頼 < 3
				LOCALS = %CALLNAME:MASTER%さん_%CALLNAME:MASTER%さん
			ELSEIF TRUE_SEX(MASTER) != "オトコ"
				LOCALS = %CALLNAME:MASTER%_%CALLNAME:MASTER%
			ELSE
				LOCALS = 君_きみ
			ENDIF
		ENDIF
	ELSEIF COND("教員", ACTOR)
		LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
	ELSE
		SELECTCASE CSTR:ACTOR:学年
		CASE "初等部"
			;仲良くなったら呼び捨てにしたいが保留
			IF TALENT:ACTOR:オトコ
				LOCALS = %CALLNAME:ACTOR%くん_%CSTR:ACTOR:ふりがな%くん
			ELSE
				LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
			ENDIF

		CASE "教師"
			LOCALS = %CALLNAME:ACTOR%先生_%CSTR:ACTOR:ふりがな%せんせい
		CASEELSE
		;一応すでに千紗がいるし学校行ってない幼い子＝未成熟組は初等部と同じ扱いをしておく。小柄2～3で判定しようかと思ったが体（見た目は）は子供、頭脳は大人な方がいっぱいいるので止めておこう
			IF TALENT:未成熟
				IF TALENT:ACTOR:オトコ
					LOCALS = %CALLNAME:ACTOR%くん_%CSTR:ACTOR:ふりがな%くん
				ELSE
					LOCALS = %CALLNAME:ACTOR%ちゃん_%CSTR:ACTOR:ふりがな%ちゃん
				ENDIF
			ELSE
				LOCALS = %CALLNAME:ACTOR%さん_%CSTR:ACTOR:ふりがな%さん
			ENDIF
		ENDSELECT
	ENDIF
ENDSELECT
RETURNF LOCALS

