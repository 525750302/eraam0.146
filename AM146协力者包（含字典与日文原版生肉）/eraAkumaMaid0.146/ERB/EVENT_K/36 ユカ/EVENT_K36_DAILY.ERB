		PRINTFORMW 「%CNAME("一人称")%だけエロい水着を着るのって、ちょっと不公平じゃない？」
		PRINTFORMW 「来年は%CNAME("二人称")%もなんか着てよ。男のエロ水着ってどんなのかよく知らんけど」
	ENDSELECT

;物陰フェラです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "ちょっと物陰へ……"
		IF SAVESTR:デート == "水着を買いに行く"
				PRINTFORMW 「%CNAME("一人称")%でムラっと来ちゃったの？　しゃあないなぁ……♪」
		ELSEIF SAVESTR:デート == "泳ぎに行く"
			IF RAND:2
				PRINTFORMW 「もー、勃たせすぎだって。一回抜かないと遊べないじゃんか……♪」
			ELSE
				PRINTFORMW 「こんなに大きくさせちゃってー。欲望に素直なやつだのう……♪」
			ENDIF
		ELSEIF RAND:3 == 0
			PRINTFORMW 「おーおー、大きくしちゃって。一回抜いただけで収まるの？　これ？」
		ELSEIF RAND:2
			PRINTFORMW 「油断すると、すーぐ発情するんだもんなぁ……」
			PRINTFORMW 「わかったわかった。抜いてやるから静かにしてろよな？」
		ELSE
			PRINTFORMW 「あー、はいはい。しゃぶってやるからそんなに押し付けんなっての」
		ENDIF
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	;TFLAG:地の文スキップ = 1

;映画館などで横並びに座った状態での手コキです。地の文が用意されているので、軽く１行程度の反応でも良いと思います
CASE "こっそり手コキしてもらう"
		IF COND("欲求不満") || TALENT:好色 || TALENT:淫乱
			PRINTFORMW 「お口チャックな。静かにしてろよ……♪」
		ELSE
			PRINTFORMW 「おいおい、ここでかよ……」
		ENDIF
CASE "キスする"
	PRINTFORMW 「……♪」
CASE "お尻を触る"
	IF RAND:2 && CEVENT("調教開始：淫乱")
		PRINTFORMW 「もー、そういうのは帰りにしろって」
		PRINTFORMW 「%CNAME("一人称")%までその気になったら収拾がつかなくなるだろ……♪」
	ELSEIF RAND:2
		PRINTFORMW 「あはは。おっさんみたいなことするんだもんなー」
	ELSE
		PRINTFORMW 「こらこら。そういうのは後にしろっての」
	ENDIF
;あなたが物陰フェラやキスやセクハラをしない紳士な場合はここに来ます
CASE "次は何処に行こうか？と話を振る"
	IF RAND:2
		PRINTFORMW 「おっ、%CNAME("一人称")%が決める感じか？　そうだなー……」
	ELSE
		PRINTFORMW 「んー。どこがいいかな……」
	ENDIF
	;地の文を自分で書きたい場合にはコメントアウトを外してください
	TFLAG:地の文スキップ = 1
CASE "真面目に勉強する"
	PRINTFORMW %TEXTR(@"「ふーむ……」/「むんむん……」/「あー、目がしぱしぱしてきた……」/（……おっ。こいつ、珍しく集中してんのな。いいことだけど）")%
CASE "帰り際"
	IF CEVENT("サイクリングデートで雨宿りした")
		PRINTFORMW 「あはは……。まぁこんな日もあるよな？」
	ELSEIF SAVESTR:デート == "泳ぎに行く"
		IF RAND:3 == 0 && CSTR:日焼け != ""
			PRINTFORMW 「えっ、そんなに焼けてる？　%CNAME("二人称")%だってけっこう真っ黒だぞー？」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「どうだった？　%CNAME("一人称")%の水着姿、堪能できた？」
		ELSEIF RAND:2
			PRINTFORMW 「ふぅ、泳いだ泳いだ！　今日は付き合ってくれてありがとな！」
		ELSE
			PRINTFORMW 「んふふー、夏満喫って感じだな！　いい気分転換になったんじゃない？」
		ENDIF
	ELSEIF SAVESTR:デート == "食べ歩き"
		IF RAND:3 == 0
			PRINTFORMW 「ふぅー、食べた食べた！　もう日が暮れちゃうなー」
		ELSEIF RAND:2
			PRINTFORMW 「やっぱり二人でご飯食べるのって楽しいなー♪　またやろうぜ！」
		ELSE
			PRINTFORMW 「今日は当たりだったなー♪　今度はどこ行こうかなー♪」
		ENDIF
	ELSEIF SAVESTR:デート == "散歩"
		IF RAND:2
			PRINTFORMW 「だらだらしてただけでもう日が暮れそう。まぁ、たまにはこんな日曜日もいいかな」
		ELSE
			PRINTFORMW 「おっ、もうこんな時間か……。そろそろ切り上げる？」
		ENDIF
	ELSEIF SAVESTR:デート == "図書館に行く"
		IF RAND:2
			PRINTFORMW 「んふふー。たまには勉強するのもいいもんだろ？」
		ELSE
			PRINTFORMW 「よっしゃ、今日はお疲れさま！　またいつでも付き合ってやるからなー？」
		ENDIF
	ELSEIF SAVESTR:デート == "水着を買いに行く"
		IF COND("夏")
			PRINTFORMW 「ふんふーん♪　来週にでも泳ぎに行く？」
			PRINTFORMW 「せっかく買ったんだし、着てるとこを見たいでしょ？」
		ELSE
			PRINTFORMW 「夏が来るのが楽しみだなー。今年も目いっぱい遊ぶぞー！」
		ENDIF
	ELSEIF RAND:3 == 0
		PRINTFORM 「今日も楽しかったー！
		IF CEVENT("サイクリングデートした")
			PRINTFORM 付き合って
		ELSE
			PRINTFORM 遊んで
		ENDIF
		PRINTFORMW くれてありがとな！」
	ELSEIF RAND:2
		PRINTFORMW 「あー、楽しかった！」
		PRINTFORMW 「%CALLNAME:MASTER%！　今日はありがとな！」
	ELSE
		PRINTFORMW 「ふぅー、遊んだ遊んだ！　もう日が暮れちゃうなー」
	ENDIF
CASE "デート終了"
	CALL SET_CEVENT, "デートした"
	;諸般の都合で弁当は一回限りでリセットされる
	CALL DEL_CEVENT, "デートのお弁当"
	CALL DEL_CEVENT, "自分からデートに誘った"
	;エッチをする場合
	IF TFLAG:ベッドイン
		IF TALENT:相思相愛
			CALL PRINT_STR, "「……_HH_」_W"
		ELSEIF COND("欲求不満")
			PRINTFORMW 「よし。行こうぜ！　ちょうど%CNAME("一人称")%もそんな気分だったし！」
		ELSE
			PRINTFORMW 「おっ、いいじゃん。ついでに行こーぜ！」
		ENDIF
	ELSE
		IF COND("同棲中") == 0
			PRINTFORMW 「……よしっと」
			PRINTFORMW 「じゃあ、ここで解散するか。また明日なー！」
		ELSEIF SAVESTR:デート != "食べ歩き" && RAND:2
			PRINTFORMW 「なんか小腹が空いて来ちゃったなぁ……。家に帰る前にどこか寄っていかない？」
		ENDIF
	ENDIF
ENDSELECT
