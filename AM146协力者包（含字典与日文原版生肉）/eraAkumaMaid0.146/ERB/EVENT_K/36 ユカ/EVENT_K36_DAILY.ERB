;-------------------------------------------------
;各種挨拶
;-------------------------------------------------
@KOJO_GREETING_K36, ARGS
#DIM MEMO_LINECOUNT
;イベント発生中のキャラ
#DIM ACTOR
;イベント発生中のキャラのデフォのCALLNAME
#DIMS CALLNAME_ACTOR

MEMO_LINECOUNT = LINECOUNT

SELECTCASE ARGS
;見ないと思ったらデートの約束がない状態で日曜限定なのか。そりゃ出ないわ。
CASE "朝拘束"
	IF ABL:MASTER:マゾっ気 >= 3 && ABL:サドっ気
		CALL EMOTION_PLUS, "ムラムラ", TARGET
		PRINTFORMW 「いい機会だから、今日は徹底的に搾り尽くしてやるよ」
		PRINTFORMW 「すっからかんになるまで許してやらないから、覚悟しとけよな……♪」
	ELSE
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「おっはよー！　状況は理解できたか？」
		PRINTFORMW 「じゃあ目が覚めたところで、ここからが本番な？」
	ENDIF
;朝のＨイベントでのセリフ。下の"挨拶する"だけ書いてあれば十分だと思います
;ここでCALL SETFLAG, "地の文スキップ"すると地の文、経験表示、本来なら続いて表示される下のCASE "挨拶する"の台詞、を全てすっ飛ばせます(上級者向け)
;詳しくは@EVENT_NEWDAYの注釈などを見てください


CASE "朝駆け"
	SELECTCASE CSTR:朝駆け
	CASE "朝逆正常位"

	CASE "朝クンニ"

	CASE "朝足コキ"

	CASE "朝の新妻プレイ"
	CASE "朝パイズリ"

	CASE "朝騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝駆け")
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORMW 「%TEXTR("あはっ……♪　起きたっ……♪/んっ……♪　もう出そう……？/んふふー、出していいよ……♪")%」
		ENDIF
	CASE "朝Ａ騎乗位"

	CASE "朝キス"
		PRINTFORMW 「%TEXTR("＿/ちゅ")%……♪」
	CASE "朝添い寝"
	IF RAND:2 && CEVENT("初回朝駆け")
		CALL SETFLAG, "地の文スキップ"
		IF RAND:3 && CFLAG:MASTER:デートの約束 == 0 && TEXTS("曜日") == "日" && COND("同棲中")
			PRINTFORMW 朝、ふと目が覚めると、%CALLNAME:TARGET%が布団の中にもぐり込んですやすやと寝息を立てていた。
			PRINTFORMW どうやら%CALLNAME:MASTER%を起こしに来たようだが、休日のため気が緩んでいたのか、釣られて眠ってしまったようだ。
			CALL EMOTION_PLUS, "睡眠", TARGET
			PRINTFORMW 「すぅすぅ……」
			PRINTFORMW 平和そうな%CALLNAME:TARGET%の寝顔をぼんやりと眺めているうちに、こちらも二度寝したい気分になってきた……。
		ELSEIF RAND:2 && COND("同棲中")
			PRINTFORMW 朝、何やら%TEXTR("香ばしい/食欲を刺激する/おいしそうな")%匂いがして目を覚ますと、%CALLNAME:TARGET%が開け放たれた窓枠に腰を降ろしてトーストと%TEXTR("目玉焼き/スクランブルエッグ/ベーコン/ウィンナー")%を%TEXTR("頬張って/食べて")%いた。
			CALL EMOTION_PLUS, "褒める", TARGET
			PRINTFORMW 「もぐもぐ……。%CNAME("二人称")%の分もちゃんと持ってきたぞ。一緒に食べよー！」
		ELSEIF RAND:2
			CALL EMOTION_PLUS, "褒める", TARGET
			PRINTFORMW 「%TEXTR("起きろー！/朝ですよー！")%」
			PRINTFORMW %CALLNAME:TARGET%が部屋のカーテンを思いっきり全開にしたことで、ぐうぐうと%TEXTR("眠りこけている/惰眠を貪っている")%%CALLNAME:MASTER%の顔に%TEXTR("眩しい/清々しい/爽やかな")%朝の日差しが直撃した。
			IF RAND:3 == 0 && COND("同棲中")
				PRINTFORMW 「へい、おはよー！」
				PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
			ELSEIF RAND:2
				PRINTFORMW 「へい！　%CALLNAME:TARGET%さんが起こしに来たぞー！」
			ELSE
				PRINTFORMW 「んふふー、今日もいい天気だぞ！」
			ENDIF
		ELSE
			PRINTFORMW 朝、%CALLNAME:MASTER%が%TEXTR("眠りこけている/惰眠を貪っている")%と、突然誰かに%TEXTR("わきの下/足の裏/首筋")%をこちょこちょとくすぐられて、急に意識が覚醒した。
			PRINTFORMW %TEXTR("寝ぼけ眼をこすりながら身を起こすと/びっくりして飛び起きると")%、%CALLNAME:TARGET%が%TEXTR("いたずらっぽく/にこにこと/楽しそうに")%笑いながら朝の挨拶をしてきた。
			CALL EMOTION_PLUS, "褒める", TARGET
			IF RAND:3 == 0 && COND("同棲中")
				PRINTFORMW 「あははっ♪　目は覚めたか？」
				PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
			ELSEIF RAND:3 == 0
				PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
			ELSEIF RAND:2
				PRINTFORMW 「へい！　%CALLNAME:TARGET%さんが起こしに来たぞー！」
			ELSE
				PRINTFORMW 「あははっ、声かけても全然起きない方が悪いんだぞー？」
			ENDIF
		ENDIF
	ENDIF
	CASE "朝フェラ"
		SIF EXP_GAIN("射精経験", PLAYER) && CEVENT("初回朝駆け")
			PRINTFORMW 「んふふー、起きた？　このまま一回抜いてあげる……♪」

	ENDSELECT
;朝のＨイベント終了後にする挨拶
CASE "挨拶する"
;あんまり唐突過ぎるので、一個会話を入れる。
;OPによると寝込みを襲う番とかなんだかよく分からんもんがあるらしいが、まぁよく分からんし……
	IF CEVENT("初回朝駆け") == 0
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「へへ、おはよー！　目は覚めたか？」
		PRINTFORMW 「%CNAME("二人称")%さぁ。毎朝フェリシエルさんとかに起こしてもらってるらしいじゃん？」
		PRINTFORMW 「%CNAME("一人称")%も気が向いたら来てやることにしたから、ちょっとは楽しみにしてろよなー♪」
		CALL SET_CEVENT, "初回朝駆け", TARGET
		RETURN 1
	ENDIF
;デート前。同棲人数が少ない場合は選ばれる率が上がるのでちょっと頻度落とす。
IF CFLAG:MASTER:デートの約束 && CFLAG:デートの約束 && TEXTS("曜日") == "日" && (RAND:4 || CHARA_NUM("同棲中")  >= 3)
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「おはよー！　%TEXTR("目は覚めたか？/もう朝だぞー！/朝ごはん出来てるぞー！")%」
		PRINTFORMW 「今日は%TEXTR("遊びに/デートに")%行くん%TEXTR("だろ/でしょ")%？　ぐうぐう寝てないでとっとと%TEXTR("顔を洗ってくるのだ！/歯を磨くのだ！/シャワーを浴びるのだ！")%」
;さらに4分の1の確率で下の汎用挨拶に流す。
ELSEIF RAND:4
	SELECTCASE CSTR:朝駆け
	CASE "朝パイズリ"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「もう、出し過ぎだって。ベタベタになっちゃったじゃん……♪」_W"
		ELSE
			PRINTFORMW 「どう？　%CNAME("一人称")%のおっぱいで元気出た？」
		ENDIF
	CASE "朝フェラ", "朝シックスナイン"
		IF EXP_GAIN("射精経験", PLAYER)
			CALL PRINT_STR, @"「おはよー。%TEXTR("朝から濃いなぁ/朝からすごい量/今日も元気そうだな")%……_HH_」_W"
		ELSE
			PRINTFORMW 「おはよー！」
			PRINTFORMW 「……で、どうする、これ？　まだ時間あるし、このまま抜いてやろうか？」
		ENDIF
	CASE "朝騎乗位","朝Ａ騎乗位"
		IF EXP_GAIN("射精経験", PLAYER) && RAND:3
			CALL PRINT_STR, "「ふぅ……_HH_　おはよー♪」_W"
		ELSE
			PRINTFORMW 「ふぅー、いい汗かいた！　一緒にシャワー浴びようぜ！」
		ENDIF
	CASE "朝キス"
		CALL EMOTION_PLUS, "褒める", TARGET
		IF RAND:3 == 0 && TALENT:相思相愛 == 0 && COND("同棲中")
			PRINTFORMW 「あはは。目覚めのキスをするのって、普通逆だよなー」
			PRINTFORMW 「今度、%CNAME("二人称")%の方からこうやって起こしに来ない？　どうよ？」
		ELSEIF RAND:2
			PRINTFORMW 「あはは、なんだよー。照れなくってもいいじゃん？」
		ELSE
			PRINTFORMW 「ほら、起きて起きてー。起きないと、もっかいキスしちゃうぞー？」
		ENDIF
	CASE "朝足コキ"
			IF RAND:3 == 0 && EXP:足コキ経験 >= 30 && EXP_GAIN("射精経験", PLAYER) 
				PRINTFORMW 「んふふー。%CALLNAME:TARGET%さんの足捌きはどうよ？　だいぶ上手になったでしょ？」
			ELSEIF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「あははっ、遅刻しそうなお寝坊さんにはこうだぞー♪」
			ELSE
				PRINTFORMW 「目は覚めた？　早く起きないと、ふみふみーを続行しちゃうぞ？」
			ENDIF
	CASE "朝の新妻プレイ"
			CALL EMOTION_PLUS, "褒める", TARGET
			IF RAND:2 && TEXTS("曜日") != "日"
				PRINTFORMW 「んふふー、どう？　今日も一日%TEXTR("がんばる/勉強する")%気になった？」
			ELSE
				PRINTFORMW 「はぁ、気持ちよかった……♪」
				PRINTFORMW 「じゃあ、次は朝ご飯なっ！　いや、その前にシャワーがいいかなぁ……」
			ENDIF
	ENDSELECT
ENDIF

	;何か発言してたらここまで
	SIF LINECOUNT > MEMO_LINECOUNT
		RETURN 1
	CALL EMOTION_PLUS, "褒める", TARGET
	IF RAND:3 == 0 && COND("同棲中")
			PRINTFORMW 「へい、おはよー！」
		IF COND("水泳")
			IF RAND:3 == 0 && COND("夏")
				PRINTFORMW 「今日も暑くなりそう！　絶好の水泳日和だな！」
			ELSE
				PRINTFORMW 「水着とタオル、ここに置いとくから忘れんなよー？」
			ENDIF
		ELSEIF COND("体育")
			IF RAND:5 == 0
				PRINTFORMW 「何だか天気が怪しいんだよなぁ。体育の授業まで降らないといいけど……」
			ELSEIF RAND:3
				PRINTFORMW 「今日の体育、女子は%TEXTR("サッカー/バレー/バスケ/バドミントン/ソフトボール")%なんだ！　楽しみ！」
			ELSE
				PRINTFORMW 「体操服、ここに置いとくから忘れんなよー？」
			ENDIF
		ENDIF
	ELSE
		IF RAND:3 == 0 && COND("同棲中")
			PRINTFORMW 「おはよー！」
			PRINTFORMW 「朝ごはんができてる%TEXTR("っぽい/みたいだ/＿")%ぞ。一緒に食べよー！」
		ELSEIF RAND:3 == 0
			PRINTFORMW 「おはよー、%CALLNAME:MASTER%！　今日もよろしくな！」
		ELSEIF RAND:2
			PRINTFORMW 「%TEXTR("おう/うむ")%っ。おはよー！」
		ELSE
			PRINTFORMW 「目は覚めたか？　ご飯の前にシャワー行くぞー！」
		ENDIF
	ENDIF

CASE "一番乗り"
	CALL EMOTION_PLUS, "褒める", TARGET
	IF COND("同伴中")
			IF RAND:3 == 0 && COND("Ｈなサプライズ可能") && COND("欲求不満") >= 2
				CALL EMOTION_PLUS, "ムラムラ", TARGET
				PRINTFORMW 「ねぇ、%CALLNAME:MASTER%。ちょうどいいし、今日は%CNAME("一人称")%としない？」
				PRINTFORMW 「今さ、すっごく%CNAME("二人称")%としたい気分なんだよ。……ダメ？」
			ELSE
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				PRINTFORMW 「んー、ちょっと早かったかな？」
				PRINTFORMW 「まぁいいや。%CNAME("二人称")%はそこに座ってろよ。お茶淹れてやるからさ」
			ENDIF
	ELSE
		IF COND("同伴中", MASTER) && RAND:2
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				CALL EMOTION_PLUS, "褒める", TARGET
				PRINTFORMW 「へいらっしゃい！　お茶飲む？」
		ELSE
				;一番乗りの時はすでにシャワーを浴びてる。
				CFLAG:シャワー済 = 1
				CFLAG:汗 = 0
				CALL RESET_STAIN_EX, "老廃物以外全部", TARGET
				;このタイミングで欲求不満気味or淫乱な時はスパッツをキャストオフする
				IF (COND("欲求不満") && CEVENT("調教開始：Ｈに慣れた")) || CEVENT("調教開始：淫乱")
					SIF CHECK_OUTER("スパッツ")
						CALL SET_OUTER, "", TARGET
				ENDIF
			IF COND("汗だく", MASTER)
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				CALL EMOTION_PLUS, "褒める", TARGET
				PRINTFORMW 「うーわ、%CNAME("二人称")%。汗びっしょりだぞー？」
				PRINTFORMW 「拭いてやるからこっち来いよ。カモンカモン♪」
				PRINTFORMW %CALLNAME:TARGET%はタオルを引っぱり出してくると、汗だくの%CALLNAME:MASTER%に手招きをした。
				CFLAG:MASTER:汗 = 0
			ELSEIF COND("夏") && RAND:2
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				SAVESTR:お茶 = 麦茶
				PRINTFORMW 「%CNAME("二人称")%も麦茶飲む？」
				PRINTFORMW 「よしよし。それじゃあ、グラス持ってくるからなー」
			ELSEIF COND("冬") && RAND:2
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				PRINTFORMW 「あったかいものでも飲む？　何がいい？」
			ELSEIF RAND:2
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				PRINTFORMW 「おっ、今日は早いじゃん！　ご褒美にお茶でも淹れてやるかな」
			ELSE
				CALL SETFLAG_DAILY, "お茶を用意", TARGET
				PRINTFORMW 「おっ、今日は%CNAME("二人称")%が二着だな！　お茶淹れるから待ってろなー」
			ENDIF
		ENDIF
	ENDIF
CASE "ただいま"
	IF COND("冬")
		IF RAND:3 == 0
			PRINTFORMW 「%TEXTR("ただいまー/ただいま！/ただいまー♪")%」
			PRINTFORMW 「……っと、手を洗ってこなくっちゃ」
		ELSE
			PRINTFORMW 「ただいまー。あー、さむさむ……」
		ENDIF
	ELSEIF TEXTS("季節詳細") == "晩秋" && RAND:2
		PRINTFORMW 「ただいまっ。もう日が暮れるのがだいぶ早くなったなー」
	ELSEIF COND("夏") && RAND:2
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「はぁ、クーラーが効いてて涼しいのう……♪」
	ELSEIF RAND:2
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「へい！　ただいまっ！」
	ELSE
		PRINTFORMW 「%TEXTR("ただいまー/ただいま！/ただいまー♪")%」
	ENDIF
CASE "既に入室"
	;家で出迎えてくれる時
	IF COND("夜")
		IF TEXTS("曜日") == "土"
			CALL EMOTION_PLUS, "褒める", TARGET
			PRINTFORMW 「おかえり、%CALLNAME:MASTER%！　今週も%TEXTR("お疲れさま！/おつかれー！")%」
		ELSE
			PRINTFORM 「
			SIF COND("同伴中", MASTER)
				PRINTFORM 二人とも、
			PRINTFORMW %TEXTR("おかえりー/おかえり！/おかえり♪/おつかれー！")%」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR("よっす！/おつかれ！/へい！")%」
	ENDIF

CASE "入室挨拶"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "褒める", TARGET
		;おやつ当番が料理上手だとこう。フェリさん作なら確率上げてもいい
		IF COND("おやつの作り手が料理上手") && COND("手作りおやつ") && RAND:2
			PRINTFORMW 「今日は手作りおやつがあると聞いて！　%CNAME("一人称")%、%TEXTR("颯爽と/華麗に")%%TEXTR("参上/登場")%！」
		ELSE
			PRINTFORMW 「%TEXTR(@"お待たせー！/%CNAME("一人称")%、参上！/へい、お待たせ！")%」
		ENDIF
	;あなたが現在のパートナーにＨな悪戯をしている最中に入室した
	ELSEIF COND("Ｈないちゃつき中", MASTER)
		IF RAND:2
			CALL EMOTION_PLUS, "ムラムラ", TARGET
			PRINTFORMW 「おー、今日も盛ってるなー……」
		ELSE
			CALL EMOTION_PLUS, "褒める", TARGET
			PRINTFORMW 「あははっ、何やってんだよー」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす！/%CNAME("一人称")%、参上！/はろはろー！")%」
	ENDIF
;部屋にいる他の人から挨拶を返される挨拶。WAITの使用は一回が望ましい
CASE "簡易入室挨拶"
	;遅れた時のセリフ（既に部員の４分の３が集合済）
	IF CHARA_NUM("入室挨拶時の室内人数") >= CHARANUM*3/4 && RAND:3 == 0
		PRINTFORMW 「%TEXTR(@"あれっ、みんないるじゃん。今日って何かあったっけ？/ごめん。ちょっと遅れちゃった！/よっす！")%」
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす！/%CNAME("一人称")%、参上！/はろはろー！")%」
	ENDIF
;簡易入室挨拶に対する返し。WAITの使用は一回が望ましい
;誰が入室してきた人はCOND("入室挨拶中")がONなのでFIND_COND("入室挨拶中")で検索可能
;また、入ってきた人数はCHARA_NUM("入室挨拶中")で調べられる
CASE "挨拶返し"
	;挨拶してきた人の中から一人選ぶ
	ACTOR = FIND_COND("入室挨拶中")
	;選ばれたキャラのデフォのCALLNAME。このキャラへの呼びかけはCNAME(CALLNAME_ACTOR) で出来る
	CALLNAME_ACTOR = %CSVCALLNAME(NO:ACTOR)%

	;ここでは括弧に『』を使う

	;挨拶してきた人に先生が混じっている
	IF FIND_COND("教員", "入室挨拶中")
		IF RAND:2
			PRINTFORMW 『こんにちはー』
		ELSE
			PRINTFORMW 『お疲れさまです』
		ENDIF
	;絡みやすそうな人たち(独断)
	ELSEIF (CALLNAME_ACTOR == "ハナ" || CALLNAME_ACTOR == "あんず" || CALLNAME_ACTOR == "シキ") && COND("相性○", ACTOR)
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 『おっ、%CNAME(CALLNAME_ACTOR)%じゃん！　%TEXTR(@"よっす！/はろはろー！/こっちこっちー！/待ってたぞー！")%』
	;同級生以下(三十路は弾く)はよっす！
	ELSEIF TALENT:ACTOR:外見年齢 <= 3 && COND("学生", ACTOR) && COND("学年") >= COND("学年", ACTOR)
			PRINTFORMW 『%TEXTR(@"よっす！/お疲れー/おつかれー")%』
	;先輩とか目上相手は敬語。
	ELSE
		IF RAND:2
			PRINTFORMW 『こんにちはー』
		ELSE
			PRINTFORMW 『お疲れさまです』
		ENDIF
	ENDIF


CASE "居間に現れた"
	IF CFLAG:呼び出された
		CALL EMOTION_PLUS, "褒める", TARGET
		PRINTFORMW 「%TEXTR(@"呼んだ？/%CNAME("一人称")%、参上！/何か用かー？")%」
	;一度居間から別の場所に行って、居間に帰って来た時
	ELSEIF CMOVE("居間")
		IF STATE("眠気",MASTER) && RAND:3
			PRINTFORMW 「なんだか眠そうじゃない？　そろそろ部屋に戻ったらどうよ？」
		ELSEIF STATE("酔い",MASTER)
			CALL EMOTION_PLUS, "褒める", TARGET
			PRINTFORMW 「あははっ、まだ飲んでんのかよー」
		ELSE
			PRINTFORMW 「ふんふーん……」
		ENDIF
	ELSE
		PRINTFORMW 「%TEXTR(@"よっす/へい！")%」
	ENDIF
CASE "全裸化への反応"
	IF ABL:欲望 >= 3 && ABL:露出癖 >= 3
		IF TALENT:男の娘
