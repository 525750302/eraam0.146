;=========================
;Demoslaviya20XX_MAPコンストラクタ
;=========================
@Demoslaviya20XX_Map_Constructor
	#DIM DYNAMIC Y_COUNT
	#DIM DYNAMIC X_COUNT
		FOR Y_COUNT, 0, 200
			FOR X_COUNT, 0, 200
				Demoslaviya20XX_MAP:Y_COUNT:X_COUNT:0 = 無
				Demoslaviya20XX_MAP:Y_COUNT:X_COUNT:1 = None
				Demoslaviya20XX_MAP:Y_COUNT:X_COUNT:2 = false
			NEXT
		NEXT
;=========================
;Demoslaviya20XX_Unitコンストラクタ
;=========================
@Demoslaviya20XX_Unit_Constructor
	VARSET Demoslaviya20XX_Unit:0:0, "None"

@Demoslaviya20XX_UnitMovePower_Constructor
	#DIM STATIC LCOUNT
		FOR LCOUNT, 0, 10000
			Demoslaviya20XX_Unit:LCOUNT:3 = %Demoslaviya20XX_GET_UNITDATA(Demoslaviya20XX_Unit:LCOUNT:1,"行動力")%
		NEXT

